          <title level="a" type="main">P66 and P75 on John 1:3/4.</title>
          <author n="1">
            <surname>Miller</surname>
            <forename>E.L.</forename>
          </author>
          <relatedItem type="appearsIn">
            <bibl>
              <ptr target="bibl:480"/>
              <!--ignore - start, i.e. SoSOL users may not edit this-->
