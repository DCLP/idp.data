         <titleStmt>
            <title>Field-by-field register of land at Kerkeosiris</title>
            <author>Menches</author>
         </titleStmt>
         <publicationStmt>
            <authority>APIS</authority>
            <idno type="apisid">berkeley.apis.334</idno>
