    <lb n="1"/><g type="stauros"/> βασιλείας <supplied cert="high" reason="lost">κ</supplied>αὶ <choice><corr>ὑπατείας</corr><sic><hi rend="diaeresis">υ</hi>πατιας</sic></choice> τοῦ θειοτάτου καὶ <expan>εὐ<unclear cert="high" reason="undefined">σεβεστ</unclear><supplied cert="high" reason="lost">άτ<ex cert="high">ου</ex></supplied></expan> 

    <lb n="2"/>ἡμῶν δ<supplied cert="high" reason="lost">εσ</supplied>πότο<supplied cert="high" reason="lost">υ</supplied> <expan>Φλ<ex cert="high">αυίου</ex></expan> Ἰ<supplied cert="high" reason="lost">ο</supplied>υστίν<supplied cert="high" reason="lost">ο</supplied>υ το<supplied cert="high" reason="lost">ῦ αἰ</supplied>ωνί<supplied cert="high" reason="lost">ου Αὐ</supplied>γού<supplied cert="high" reason="lost">σ</supplied>τ<supplied cert="high" reason="lost">ου</supplied> 

    <lb n="3"/>καὶ <expan>Αὐτοκρ<ex cert="high">άτορος</ex></expan> ἔτους <num value="2">β</num> Θὼθ <num value="14">ιδ</num> <expan>ἰνδ<ex cert="high">ικτίονος</ex></expan> <num value="1">α</num> ἐν <app type="BL"><lem resp="7.86"><expan>Ὀξυρ<ex cert="high">ύγχων</ex></expan></lem><rdg><expan>Ὀξυρ<ex cert="high">υγχιτῶν</ex></expan></rdg></app> <expan>πόλ<ex cert="high">ει</ex></expan>. 

    <lb n="4"/>Φλαουίῳ Ἀπ<supplied cert="high" reason="lost">ί</supplied>ωνι τῷ πανευφήμῳ καὶ <expan>ὑπερφ<ex cert="high">υεστάτῳ</ex></expan> 

    <lb n="5"/>ἀπὸ ὑπάτων ὀρδιναρίων καὶ πατρικίῳ <expan>γεουχ<ex cert="high">οῦντι</ex></expan> 

    <lb n="6"/>καὶ <expan>ἐνταῦθ<ex cert="high">α</ex></expan> τῇ λαμπρᾷ Ὀξυρυγχιτῶν πόλει διὰ 

    <lb n="7"/>Μηνᾶ οἰκέτου τοῦ ἐπερωτῶντος καὶ <expan>προσπορί<unclear cert="high" reason="undefined">ζον</unclear><supplied cert="high" reason="lost">το<ex cert="high">ς</ex></supplied></expan> 

    <lb n="8"/>τῷ <hi rend="diaeresis">ἰ</hi>δίῳ δεσπότῳ τῷ αὐτῷ πανευφήμῳ ἀνδρὶ τὴν ἀγωγὴν 

    <lb n="9"/>καὶ ἐνοχὴν <supplied cert="high" reason="lost">Αὐ</supplied>ρήλιος Ἐπίμαχος ὁ καὶ Ἀπίμα <app type="BL"><lem resp="1.299"><seg cert="low" part="N">υἱ<supplied cert="high" reason="lost">ὸ</supplied>ς <unclear cert="low" reason="undefined">Λ</unclear>αμ<unclear cert="low" reason="undefined">ί</unclear>ωνος</seg></lem><rdg>υἱ<supplied cert="high" reason="lost">ὸς</supplied> <unclear cert="high" reason="undefined">Φ</unclear>λαι<supplied cert="high" reason="lost">ου </supplied><gap extent="2" reason="lost" unit="character"/>ωνο<supplied cert="high" reason="lost">ς</supplied></rdg></app> 

    <lb n="10"/>μητρὸς Ἡραίδος ὁρμώμενος ἀπὸ ἐποικίο<unclear cert="high" reason="undefined">υ</unclear> <choice><corr>Ἑκκαιδεκάτου</corr><sic>εκαιδεκα<unclear cert="high" reason="undefined">τ</unclear>ου</sic></choice> 

