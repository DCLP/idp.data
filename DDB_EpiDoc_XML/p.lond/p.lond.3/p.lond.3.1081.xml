    <lb n="1"/><g type="stauros"/> ἀναφέρω τῇ ὑμετέρᾳ πατρικῇ δεσποτείᾳ 

    <lb n="2"/>ὅτι <choice><corr>ἀποψὲ</corr><sic>αποψα</sic></choice> ἔ<unclear cert="high" reason="undefined">γρ</unclear>αψέν μοι ὁ <unclear cert="high" reason="undefined">ε</unclear>ὐκλεέστατος 

    <lb n="3"/>ἀμίρας διὰ τοῦ ἱπποκόμου Ἰβιῶνος <expan>Πανεκτ<unclear cert="high" reason="undefined">υ</unclear>ρέ<ex cert="high">ως</ex></expan> 

    <lb n="4"/>ἵνα πέμψω εἰς τὸ <choice><corr>ἐποίκιον</corr><sic>εποικειον</sic></choice> τοῦ <unclear cert="high" reason="undefined">ἀ</unclear>δ<supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">λφ</unclear><supplied cert="high" reason="lost">οῦ</supplied> 

    <lb n="5"/>μου τοῦ κυρίου <expan>Γερμανο<ex cert="high">ῦ</ex></expan> τοῦ καὶ <expan>λε<supplied cert="high" reason="lost">γο</supplied>μέ<ex cert="high">νου</ex></expan> Θέωνος 

    <lb n="6"/>καὶ λαβεῖν ἐκεῖ τρία <num value="3"/> ἱππ<supplied cert="high" reason="lost">ά</supplied>ρια καὶ δύο <num value="2"/> 

    <lb n="7"/><unclear cert="high" reason="undefined">γ</unclear>αιδάρια καὶ ἐπιδοῦναι τῷ ἱπποκόμῳ 

    <lb n="8"/>καὶ οἴδατε ὅτι οὐ δύνομ<supplied cert="high" reason="lost">αι</supplied> παρα<unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">οῦσαι</supplied> 

    <lb n="9"/><unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">α</supplied>ὶ πρὸς τῷ τὸν δεσπότην μο<supplied cert="high" reason="lost">υ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/><unclear cert="high" reason="undefined">σ</unclear>ημάνω παρακαλῶ δὲ ὑμᾶς <supplied cert="high" reason="lost">ὅπως</supplied> 

    <lb n="11"/><unclear cert="high" reason="undefined">γ</unclear>ένηται <hi rend="diaeresis">ἵ</hi>να μείνητε ἐ<unclear cert="high" reason="undefined">γγ</unclear>ὺ<supplied cert="high" reason="lost">ς ἡμῶν</supplied> 

    <lb n="12"/><supplied cert="high" reason="lost">ἄ</supplied>λλας δύο <num value="2"/> ἡμέρας οὐ <unclear cert="high" reason="undefined">β</unclear>λάπτετε <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/><supplied cert="high" reason="lost">ἀ</supplied>παλλάξητε καλῶς τὴν ἀπόκρισι<supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

