    <lb n="1"/><expan><ex cert="high">πε</ex>ρ<ex cert="high">ὶ</ex></expan> <expan>ἐκπομπ<ex cert="high">ῆς</ex></expan> <expan>να<ex cert="high">υτῶν</ex></expan> <expan><ex cert="high">καὶ</ex></expan> <expan>τ<ex cert="high">ε</ex>χ<ex cert="high">νιτῶν</ex></expan> <expan><ex cert="high">καὶ</ex></expan> <expan>δαπ<ex cert="high">ανῶν</ex></expan> <expan>κού<ex cert="high">ρσου</ex></expan> <expan>Αἰγ<ex cert="high">ύπτου</ex></expan> <expan>ἰ<ex cert="high">νδικτίονος</ex></expan> <num value="9">θ</num> 

    <lb n="1"/><gap desc="Arabic" extent="1" reason="ellipsis" unit="line"/> 

    <lb n="2"/>ἐν ὀνόματι τοῦ Θεοῦ Κορ<g type="apostrophe"/>ρα <expan>βε<ex cert="high">ν</ex></expan> <supplied cert="high" reason="lost">Σ</supplied>ζε<supplied cert="high" reason="lost">ριχ σύμ</supplied>βο<supplied cert="high" reason="lost">υλ</supplied>ος 

    <lb n="3"/>Βασιλείῳ διοικητῇ κώμης Ἀφροδιτώ. εὐχαριστοῦμε<supplied cert="high" reason="lost">ν</supplied> 

    <lb n="4"/>τῷ Θεῷ καὶ μετὰ ταῦτα<g type="apostrophe"/>. <space dim="horizontal" extent="unknown" unit="character"/> <choice><corr>κατέλαβον</corr><sic>κατελαβαν</sic></choice> πρὸς σὲ τὰ ἐντ<supplied cert="high" reason="lost">ά</supplied>για 

    <lb n="5"/>τῆς διαστολῆς τῶν <choice><corr><supplied cert="high" reason="omitted">ν</supplied>αυτῶν</corr><sic>αυτων</sic></choice> καὶ<g type="apostrophe"/> τεχνιτῶν<g type="apostrophe"/> καὶ τῆς δαπάνης 

    <lb n="6"/>αὐτῶν τε καὶ τῆς τῶν μάχων τοῦ κούρ<g type="apostrophe"/>σο<add place="supralinear">υ</add> Αἰγύπτο<add place="supralinear">υ</add> ἰνδικτίονος 

    <lb n="7"/><supplied cert="high" reason="lost">ἐνάτης </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>οὐ<unclear cert="high" reason="undefined">κ</unclear> <unclear cert="high" reason="undefined">εξ</unclear><gap extent="4" reason="illegible" unit="character"/><gap extent="2" reason="illegible" unit="character"/> <unclear cert="high" reason="undefined">αὐ</unclear>τ<supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ῖ</unclear><supplied cert="high" reason="lost">ς </supplied><gap extent="5" reason="lost" unit="character"/><supplied cert="high" reason="lost"> δεχόμενος οὖν τὰ παρόντα γράμματα</supplied> 

    <lb n="9"/>ἀπόστειλον <choice><corr>πρωτοτύπως</corr><sic>προτοτυπως<g type="apostrophe"/></sic></choice> διὰ πάσης συντομ<hi rend="diaeresis">ί</hi>ας 

    <lb n="10"/>τὰς δ<hi rend="diaeresis">ι</hi>ασταλείσας διὰ τῆς<g type="apostrophe"/> διοικήσεώς σο<add place="supralinear">υ</add> δαπάνας <choice><corr>πρὸ</corr><sic>πρω</sic></choice> <expan>το<ex cert="high">ῦ</ex></expan> 

    <lb n="11"/>ἀποβῇ τὸ ὕδωρ τῆς διώρυγος Ἀλεξ<g type="apostrophe"/>ανδρείας καὶ ἀναγκασθῇς 

    <lb n="12"/>δοῦναι τὰ φόρετρα τῶν αὐτῶν δαπαν<g type="apostrophe"/>ῶν ἕως αὐτῆς Ἀλεξανδρεία<supplied cert="high" reason="lost">ς</supplied> 

    <lb n="13"/>οὐ μὴν ἀλλὰ καὶ τοὺς ναύτας γενναίους καὶ ἐμπείρους. ἐπετρέψαμ<unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">ν</supplied> <add place="supralinear"><unclear cert="high" reason="undefined">γ</unclear><supplied cert="high" reason="lost">ὰ</supplied><unclear cert="high" reason="undefined">ρ</unclear></add> 

    <lb n="14"/>τῷ παρόντι ἀποστόλῳ μὴ δοῦναί σοι ἄνεσιν ἕως δ᾽ ἂν εἰς πλῆρες 

    <lb n="15"/>ἐκπέμψῃς εἴ τί ἐστι διὰ τῆς διοικήσεώς σο<add place="supralinear">υ</add>. λοιπὸν 

    <lb n="16"/>μὴ γένηται ἐμπόδιος δ<hi rend="diaeresis">ι</hi>ὰ <expan>σο<ex cert="high">ῦ</ex></expan> καὶ ἐκ μὲν τοῦ σίτο<add place="supralinear">υ</add> καὶ το<add place="supralinear">ῦ</add> <expan>ἄρτο<ex cert="high">υ</ex></expan> 

    <lb n="17"/><choice><corr>ὑστερήσῃς</corr><sic>υστερεσης</sic></choice> τι τὸ σύνολον μήτε μὴν ἀνύσῃς ἐκ τῶν 

    <lb n="18"/>χωρ<hi rend="diaeresis">ί</hi>ων ἀπαρ<g type="apostrophe"/>γυρισ<g type="apostrophe"/>μὸν ὑπὲρ<g type="apostrophe"/> αὐτῶν τὸν ὁτιοῦν εἰ μὴ αὐτὰ τὰ εἴδη. 

    <lb n="19"/>ἐκ δὲ τῶν ὑπολοίπων δαπαν<g type="apostrophe"/>ῶν οἷον εἶδός ἐστι ἐξ αὐτῶν 

    <lb n="20"/>ἐν ἑτοίμῳ <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">ρ</unclear>ὰ τοῖς τῶ<supplied cert="high" reason="lost">ν</supplied> χωρίω<supplied cert="high" reason="lost">ν</supplied> εὐθέως πέμψον, ὃ δὲ 

    <lb n="21"/>ἀδύνατό<unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">ἐ</unclear><supplied cert="high" reason="lost">σ</supplied>τ<supplied cert="high" reason="lost">ιν</supplied> δοῦναι ἐν εἴδεσιν<g type="apostrophe"/> ἀνύων τὸν ἀπαρ<g type="apostrophe"/>γυρισμὸν 

    <lb n="22"/>αὐτο<add place="supralinear">ῦ</add> πρὸς τὴν ἐντεθεῖσαν <unclear cert="high" reason="undefined">δι</unclear><supplied cert="high" reason="lost">α</supplied>τίμησιν ἐν τοῖ<supplied cert="high" reason="lost">ς</supplied> ἡ<supplied cert="high" reason="lost">μ</supplied>ετέρο<supplied cert="high" reason="lost">ι</supplied>ς 

    <lb n="23"/>ἐνταγίοις καὶ ἀπόστειλον π<g type="apostrophe"/>ρὸς ἡμᾶς δι᾽ ἀνθρ<g type="apostrophe"/>ώπου σο<add place="supralinear">υ</add> πισ<g type="apostrophe"/>τοῦ 

    <lb n="24"/>το<add place="supralinear">ῦ</add> ὀφείλοντος καταβαλέσθαι ἡμῖν μὴ παρέχων τοῖς <expan>ὑποδεχομ<unclear cert="high" reason="undefined">έ</unclear><ex cert="high">νοις</ex></expan> 


    <lb n="25"/>πιστικοῖς τὰς τοιαύτας δαπάν<g type="apostrophe"/>ας παν<g type="apostrophe"/>τοῖ<unclear cert="high" reason="undefined">ο</unclear>ν ἀπαργυρισμὸν 

    <lb n="26"/>καὶ μὴ ἀμελήσῃς εἰς τὸ διὰ τάχους πέμψαι τούς τε ναύτας καὶ τεχνίτας 

    <lb n="27"/>καὶ τὰς δαπάνας καὶ δώσῃς κατὰ <expan>σεαυτο<ex cert="high">ῦ</ex></expan> πρόφασιν. <expan>ἐγρ<ex cert="high">άφη</ex></expan> <expan>μη<ex cert="high">νὸς</ex></expan> <expan>Μ<ex cert="high">ε</ex>χ<ex cert="high">εὶρ</ex></expan> <num value="5">ε</num> <expan>ἰνδ<ex cert="high">ικτίονος</ex></expan> <num value="8">η</num> 
