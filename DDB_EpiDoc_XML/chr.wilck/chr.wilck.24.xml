    <lb n="1"/><supplied cert="high" reason="lost">ἀ</supplied>πελύσαμεν ἐξελθε<supplied cert="high" reason="lost">ῖ</supplied>ν εἰς Ἀνατο<supplied cert="high" reason="lost">λὴν </supplied><gap extent="19" reason="lost" unit="character"/> 

    <lb n="2"/>δεδωκότες αὐτοῖς προθεσμίαν μηνῶ<supplied cert="high" reason="lost">ν </supplied><gap extent="11" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀπὸ τῆς σήμερον</supplied> 

    <lb n="3"/><supplied cert="high" reason="lost">ἡ</supplied>μ<supplied cert="high" reason="lost">έ</supplied>ρ<supplied cert="high" reason="lost">ας</supplied>, ἥτι<supplied cert="high" reason="lost">ς</supplied> ἐστὶ <expan>μ<ex cert="high">ηνὸς</ex></expan> <expan>Π<ex cert="high">α</ex>ῦ<ex cert="high">νι</ex></expan> <num value="5">ε</num> <expan>ἰνδ<ex cert="high">ικτίων</ex>ο<ex cert="high">ς</ex></expan> δωδεκάτης <gap extent="7" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="8" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τῆς</supplied> 

    <lb n="4"/>αὐτῆς δωδεκάτης <expan><supplied cert="high" reason="lost">ἰ</supplied>νδ<ex cert="high">ικτίων</ex>ο<ex cert="high">ς</ex></expan>. 

    <lb n="5"/>ὅστις οὖν ὑπαν<supplied cert="high" reason="lost">τ</supplied>ήσῃ αὐτοῖς ἐκ τῶν ὑπουργῶ<supplied cert="high" reason="lost">ν τοῦ</supplied> 

    <lb n="6"/>Ἀμι<supplied cert="high" reason="lost">ρ</supplied>αλμ<supplied cert="high" reason="lost">ο</supplied>υμνὶν τῶν ὄντων <expan>εἴ<ex cert="high">ς</ex></expan> τε Ἀνατολὴν καὶ Αἴγυπτον μετὰ τὴν δ<supplied cert="high" reason="lost">εδομένην</supplied> 

    <lb n="7"/>αὐτοῖς παρ᾽ ἡμῶν προθεσμίαν, τούτους κρατή<supplied cert="high" reason="lost">σ</supplied>ῃ καὶ ἀποστρέψῃ <expan>εἰ<ex cert="high">ς</ex></expan> τ[ὴν <unclear cert="high" reason="undefined">ἰδίαν</unclear>] 

    <lb n="8"/><supplied cert="high" reason="lost">αὐτῶν</supplied>. ἀπαιτήσῃ ἕκαστον <supplied cert="high" reason="lost">α</supplied>ὐτῶν νομίσματα τρία <num value="3"/>. οὕτω γὰρ ἐθεμ<supplied cert="high" reason="lost">ατίσαμεν</supplied> 

    <lb n="9"/><supplied cert="high" reason="lost">αὐτοὺς δο</supplied>ῦναι καὶ πρὸς τὸ δῆλον εἶναι τῷ παρόντι σιγιλλίῳ ἐχρησάμεθ<supplied cert="high" reason="lost">α τοῖς τε</supplied> 

    <lb n="10"/><supplied cert="high" reason="lost">Ἀραβικοῖς</supplied> <expan><ex cert="high">καὶ</ex></expan> Ἑλληνικοῖς γράμμασιν, ἐπιτ<supplied cert="high" reason="lost">ι</supplied>θέντες ἐν αὐτῷ καὶ τὴν συνήθ<supplied cert="high" reason="lost">η ἡμῶν βοῦλλαν</supplied>. 

