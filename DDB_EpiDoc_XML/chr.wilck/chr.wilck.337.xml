    <lb n="11"/><gap extent="4" reason="lost" unit="character"/>το κε<supplied cert="high" reason="lost">κ</supplied>ομισμένου ἤδη <supplied cert="high" reason="lost">αὐ</supplied>τοῦ πυρῶν <expan><ex cert="high">ἀρτάβας</ex></expan> <num value="130">ρλ</num> <num value="1/12">ιβ</num> <gap extent="unknown" reason="lost" unit="character"/> 

