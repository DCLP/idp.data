    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> βούλομαι ἑκουσίως</supplied> 

    <lb n="2"/><supplied reason="lost">καὶ αὐ</supplied><unclear>θ</unclear>α<unclear>ι</unclear><supplied reason="lost">ρέτως μισθώσασθαι</supplied> 

    <lb n="3"/>παρʼ ὑμῶν <supplied reason="lost">ἐ</supplied>πὶ <supplied reason="lost">π</supplied><unclear>εν</unclear><supplied reason="lost">ταετῆ χρό</supplied><unclear>ν</unclear>ον 

    <lb n="4"/>λογιζόμε<supplied reason="lost">ν</supplied>ον ἀπὸ <supplied reason="lost">καρπῶν</supplied> τῆς 

    <lb n="5"/>σὺν <expan>θ<ex>εῷ</ex></expan> ἑνδεκ<unclear>ά</unclear>της <expan>ἰνδ<ex>ικτίωνος</ex></expan> <supplied reason="lost">καὶ αὐ</supplied>τῆς 

