    <lb n="1"/><g type="stauros"/> τριακόσια ὀγδοήκοντα τέσσερα <num value="384"/> σῦκα καταγ<unclear reason="undefined">γ</unclear>ίσας καὶ νῦν εἰς χλουβο<unclear reason="undefined">ὺ</unclear><supplied reason="lost">ς</supplied> 

