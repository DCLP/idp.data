    <lb n="1"/><supplied cert="high" reason="lost">τῷ ἀγα</supplied><unclear cert="high" reason="undefined">πητ</unclear>ῷ κα<supplied cert="high" reason="lost">ὶ</supplied> θ<unclear cert="high" reason="undefined">ε</unclear>οσεβεστάτῳ πατρὶ <space dim="horizontal" extent="unknown" unit="character"/> Ἄπα <choice><corr><supplied cert="high" reason="omitted">Ἀ</supplied>ντίνῳ</corr><sic>ντινος</sic></choice>

    <lb n="2"/>Περγάμιο<supplied cert="high" reason="lost">ς</supplied> <choice><corr>πραιπόσιτος</corr><sic><unclear cert="high" reason="undefined">πρε</unclear>ποσιτος</sic></choice> Εὐλόγιος πρ<unclear cert="high" reason="undefined">ί</unclear>γκιψ. <space dim="horizontal" extent="unknown" unit="character"/>

    <lb n="3"/>ἐδεξάμεθ<unclear cert="high" reason="undefined">α</unclear> <supplied cert="high" reason="lost">γ</supplied>ρά<unclear cert="high" reason="undefined">μματα</unclear> παρὰ τῆς σ<unclear cert="high" reason="undefined">ῆ</unclear>ς <choice><corr>θεοσεβείας</corr><sic>θ<unclear cert="high" reason="undefined">ε</unclear>οσεβιας</sic></choice>, κύριε πάτερ,

    <lb n="4"/>περὶ Ὥρο<unclear cert="high" reason="undefined">υ</unclear> τοῦ ἡμετέρου στρατιώτ<unclear cert="high" reason="undefined">ο</unclear>υ <choice><corr><app type="alternative"><lem>τοῦ</lem><rdg>τῶν</rdg></app></corr><sic>τ<unclear cert="high" reason="undefined">ο</unclear>ν</sic></choice> ἀπὸ φυλάκων

    <lb n="5"/><choice><corr>ὥστε</corr><sic>ωσται</sic></choice> μὴ <choice><corr>τιθέναι</corr><sic>τηθηναι</sic></choice> εἰς τὴν ἀγραρίαν τῆς Ὀάσεως καὶ τῶν τούτων <gap extent="3" reason="illegible" unit="character"/> <note anchored="yes" lang="en" place="not specified">?</note>

    <lb n="6"/><choice><corr>θεοσέβεια <note anchored="yes" lang="en" place="not specified">?</note></corr><sic>θεοσεβιαν</sic></choice> γεγράφηκεν ἡμῖν, ἐσκεψάμεθα ὅπως <choice><corr>τοῦτο</corr><sic>τουτον</sic></choice>

    <lb n="7"/><choice><corr>συγχωρηθῇ</corr><sic>συνχωρηθηναι</sic></choice> αὐτῷ ὅτι μέτριος <choice><corr>τυγχάνει</corr><sic>τυγχανων</sic></choice> καὶ παλαιὸς <supplied cert="high" reason="omitted">σ</supplied>τρα-

    <lb n="8"/>τιώτης. <space dim="horizontal" extent="unknown" unit="character"/> οἶδ<unclear cert="high" reason="undefined">ε</unclear>ν γὰρ ἡ <choice><corr>θεία</corr><sic>θια</sic></choice> πρόνοια μὴ <choice><corr>ἔχομεν</corr><sic>εχοντες</sic></choice> τὸ καθό-

    <lb n="9"/>λον στρατιώτας εἰς τὸ κάστρον ἀλλὰ εἰς ἀγραρίας

    <lb n="10"/>σχολάζουσι<unclear cert="high" reason="undefined">ν</unclear> καὶ διὰ τὴν ἀνάγκη<unclear cert="high" reason="undefined">ν</unclear> τοῦ<unclear cert="high" reason="undefined">τ</unclear>ον ἐθήκαμεν.

    <lb n="11"/>δεξάμενοι οὖν<sic><gap extent="1" reason="illegible" unit="character"/></sic> τὰ γράμματα τῆς <choice><corr>θεοσεβείας</corr><sic>θεοσεβιαν</sic></choice> σου

    <lb n="12"/>εὐθέως <choice><corr>συνεχωρήσαμεν αὐτῷ τὴν ἐπιδημίαν</corr><sic>εσυνεχωρησαμεν αυτον εκ της επιδημιας</sic></choice>

    <lb n="13"/>εἰς <del rend="erasure" status="unremarkable">ὑ̈μετέραν</del> τιμὴν <add place="supralinear">τῆς <choice><corr>θεοσεβείας</corr><sic>θεοσεβια<unclear cert="high" reason="undefined">ς</unclear></sic></choice> σου</add> καὶ καταξίωσον, πάτερ,

    <lb n="14"/>περὶ ὧν βούλῃ κέλευε ἡμῖν ἡδέως <choice><corr>ὑπουργοῦσι</corr><sic>ϋπουργουντες</sic></choice>

    <lb n="15"/>καὶ μνήσθητι <choice><corr>ἡμῶν</corr><sic>ημιν</sic></choice> ἐν <choice><corr>ταῖς ἁγίαις εὐχαῖς</corr><sic>τες αγιες ευχες</sic></choice> <supplied cert="high" reason="omitted">σ</supplied>ου· <handShift new="m2"/> Ἐρρωμέ<sic>ν</sic>-

    <lb n="16"/>νον σε εὔχο<unclear cert="high" reason="undefined">μ</unclear>αι, κύριέ μου τιμιώτατε

    <lb n="17"/>πάτερ, χρόνοις πολλοῖς.

    <lb n="18"/><handShift new="m3"/> <choice><corr>ἐρρωμένον</corr><sic>ερρωμενος</sic></choice> <supplied cert="high" reason="omitted">σε</supplied> <choice><corr>εὔχομαι</corr><sic>ευχομε</sic></choice>

    <lb n="19"/>κύριέ μου τιμιώτατε

    <lb n="20"/>πολλοῖς χρόνοις.

    <lb n="21"/><g type="unintelligible"/>
