    <lb n="48"/>ἔτη τρία <num value="3"/> κατ᾽ ἔτος <unclear cert="high" reason="undefined">ἑκάστης</unclear> ἀρ<unclear cert="high" reason="undefined">ο</unclear>ύρης <unclear cert="high" reason="undefined">σὺν</unclear> <unclear cert="high" reason="undefined">αἷς</unclear> <unclear cert="high" reason="undefined">λήμψομαι</unclear> 

    <lb n="49"/>πυροῦ ἀρτάβης μιᾶς <num value="1"/> πυροῦ ἀρταβῶν ἓξ <num value="6"/> πάντων <expan>μέτρω<ex cert="high">ι</ex></expan> 

