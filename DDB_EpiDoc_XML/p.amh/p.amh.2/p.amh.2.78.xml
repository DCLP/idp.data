    <lb n="12"/><hi rend="diaeresis">ὑ</hi><supplied cert="high" reason="lost">π</supplied>άρχουσι παντοδαπῶς 

    <lb n="13"/>μ<supplied cert="high" reason="lost">ου</supplied> <choice><corr>πλεονεκτεῖ</corr><sic>πλεονεκτι</sic></choice> ἄνθρωπος 

