    <lb n="6"/>εἰς λόγον τῆς κριθῆς εἰ μὴ <choice><corr>ἕνα</corr><sic>εναν</sic></choice> χρύσινον, <hi rend="diaeresis">ὕ</hi>στερον δὲ ἀπέλυσεν 

    <lb n="7"/>τὸ <hi rend="diaeresis">ὑ</hi>πόλοι<supplied cert="high" reason="lost">π</supplied>ον τῆς κριθῆς ἀπὸ Φοιβάμ<g type="apostrophe"/>μωνος τοῦ <supplied cert="high" reason="lost">υ</supplied><hi rend="diaeresis">ἱ</hi>οῦ Ἀπφῦ 

    <lb n="8"/>τοῦ σιτ<supplied cert="high" reason="lost">ο</supplied>μέτρου.  ἤδη δὲ κα<supplied cert="high" reason="lost">ὶ</supplied> ἄλλοτε γεγράφηκα αὐτῇ πε<supplied cert="high" reason="lost">ρ</supplied>ὶ τοῦ σίτ<supplied cert="high" reason="lost">ο</supplied>υ 

    <lb n="9"/>ὅτι ἀπηλλάγη ὁ κοινὸς πατὴρ Ποῦσις τὰ πάντ<supplied cert="high" reason="lost">ω</supplied> <gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">τ</unclear><gap extent="3" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ρ</unclear><gap extent="2" reason="lost" unit="character"/><unclear cert="high" reason="undefined">α</unclear>ς. 

    <lb n="10"/>εἶπεν ὁ αὐτὸς κοινὸς πατὴρ Ποῦσις ὅτι <gap extent="2" reason="illegible" unit="character"/>ο<unclear cert="high" reason="undefined">νομ</unclear><gap extent="15" reason="lost" unit="character"/> 

    <lb n="11"/>ἐπειδὴ Νόν<g type="apostrophe"/>να ἡ ἀδελφὴ Σ<supplied cert="high" reason="lost">υ</supplied>μεῶνος <supplied cert="high" reason="lost">ἡ</supplied> θ<supplied cert="high" reason="lost">υ</supplied>γ<unclear cert="high" reason="undefined">ά</unclear><supplied cert="high" reason="lost">τηρ </supplied><gap extent="12" reason="lost" unit="character"/> 

    <lb n="12"/>Εὐφημίας ἡ γαμήσασα τὸν υ<hi rend="diaeresis">ἱ</hi>ὸν Θαμουλ<gap extent="18" reason="lost" unit="character"/> 

    <lb n="13"/>ἡ μένουσα μετὰ Ἡραείδος τῆς μ<supplied cert="high" reason="lost">η</supplied>τρὸς <gap extent="8" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/>λ<gap extent="8" reason="lost" unit="character"/> 

    <lb n="14"/>ὡς οὐκ <choice><corr>ἀγν<supplied cert="high" reason="omitted">ο</supplied>ῶ</corr><sic>αγνω</sic></choice> εἰ ἔχει φανερὰ κέλ<unclear cert="high" reason="undefined">λι</unclear><supplied cert="high" reason="lost">α </supplied><gap extent="10" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>π<gap extent="8" reason="lost" unit="character"/> 

    <lb n="15"/>δορωφηξ.  πᾶσα μὲν ἡ αὐλὴ α<gap extent="7" reason="lost" unit="character"/> <unclear cert="high" reason="undefined">το</unclear>ῦ <supplied cert="high" reason="lost">ἀ</supplied><unclear cert="high" reason="undefined">δ</unclear><supplied cert="high" reason="lost">ελ</supplied><unclear cert="high" reason="undefined">φ</unclear><supplied cert="high" reason="lost">ο</supplied>ῦ αὐτῆ<supplied cert="high" reason="lost">ς</supplied> ἐστιν, 

    <lb n="16"/>ἔχει δὲ ἡ αὐλὴ κέλλια ἓξ <num value="6"/> κ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ὶ</supplied> <gap extent="4" reason="illegible" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> <choice><corr>ἐ<supplied cert="high" reason="lost">νδ</supplied>υμενεία<supplied cert="high" reason="omitted">ν</supplied></corr><sic>ε<supplied cert="high" reason="lost">νδ</supplied>υμε<unclear cert="high" reason="undefined">ν</unclear>εια</sic></choice> Νόννα 

    <lb n="17"/>ἡ ἀπὸ Ταλεὶ τὰ πάντα ἀπὸ κληρονομίας μητρ<supplied cert="high" reason="lost">ι</supplied>κῆς.  ἔτι δὲ περιόντος 

    <lb n="18"/>τοῦ πατρὸς αὐτῆς ἐνταῦθ<unclear cert="high" reason="undefined">α</unclear> <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">αὶ</supplied> τοῦ ἀδελφοῦ αὐτῆς διεμερίσαντο 

    <lb n="19"/>πρὸς ἑαυτοὺς, ἔλαχεν δὲ <choice><corr>ὁ</corr><sic>τον</sic></choice> <choice><corr>πατὴρ</corr><sic>πατερα</sic></choice> ἀπὸ τῆς αὐτῆς μητρικῆς 

    <lb n="20"/>κληρονομίας εἰς τὴν αὐτὴν <choice><corr>ἔπαυλιν</corr><sic>επαυλην</sic></choice> ἀπὸ δίκης, ὡς λέγει, 

