    <lb n="12"/><supplied cert="high" reason="lost">ὀ</supplied><unclear cert="high" reason="undefined">β</unclear>ολ<supplied cert="high" reason="lost">ο</supplied>ὺς ὀγδοήκοντα <num value="80"/> σύνπα<supplied cert="high" reason="lost">ν</supplied>-

    <lb n="13"/>τ<supplied cert="high" reason="lost">ι</supplied> λό<supplied cert="high" reason="lost">γῳ</supplied>, ὧν καὶ τὴν δ<supplied cert="high" reason="lost">ι</supplied>αγραφὴν 

    <lb n="14"/><choice><corr>ποι<supplied cert="high" reason="lost">ήσ</supplied>ω</corr><sic><unclear cert="high" reason="undefined">π</unclear>οι<supplied cert="high" reason="lost">ηω</supplied><unclear cert="high" reason="undefined">ωι</unclear></sic></choice> κατ<supplied cert="high" reason="lost">ὰ</supplied> μῆν<supplied cert="high" reason="lost">α</supplied> <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ὸ</supplied> αἱρ<supplied cert="high" reason="lost">ο</supplied>ῦν 

    <lb n="15"/>ἐξ <hi rend="diaeresis">ἴ</hi>σου, τῶν ε<supplied cert="high" reason="lost">ἰ</supplied>ς ἑτέρους λό<supplied cert="high" reason="lost">γους</supplied> 

