    <lb n="1"/><num value="10">ι</num> <expan>χ<ex cert="high">άρτης</ex></expan> <num value="9">θ</num> 

    <lb n="2"/>ἔτους ἑνδεκάτου Τιβερίου Κλαυδίου Καίσαρος Σεβαστοῦ Γερμανικοῦ 

    <lb n="3"/>Αὐτοκράτορος μηνὸς Ἀπελλαίου ἐνάτηι Φαῶφι <num value="9">θ</num> ἐν Καρανίδι τῆς <expan>Ἡρ<supplied cert="high" reason="lost">ακλ<ex cert="high">είδου</ex></supplied></expan> 

    <lb n="4"/>μερίδος τοῦ Ἀρσινοείτου νομοῦ.  ἐ<unclear cert="high" reason="undefined">ξ</unclear><supplied cert="high" reason="lost">έδοτο Σ</supplied><unclear cert="high" reason="undefined">ατο</unclear><supplied cert="high" reason="lost">ρ</supplied><unclear cert="high" reason="undefined">νῖλος</unclear> <unclear cert="high" reason="undefined">Σατορνίλο</unclear><supplied cert="high" reason="lost">υ</supplied> 

    <lb n="5"/>Πέρσης τῆς <expan>ἐπιγο<ex cert="high">νῆς</ex></expan> ὡς ἐτῶν τριάκ<supplied cert="high" reason="lost">οντα <num value="30"/> </supplied><gap extent="10" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Θετηλέννει</supplied> 

    <lb n="6"/>Ἰρανούπιος γερδίωι ὡς <expan>ἐτ<ex cert="high">ῶν</ex></expan> τεσ<unclear cert="high" reason="undefined">σ</unclear><supplied cert="low" reason="lost">αράκοντα <num value="40"/> Πέρσηι τῆς ἐπιγονῆς </supplied> 

    <lb n="7"/>τὸν τοῦ μετηλλαχότος αὐτοῦ <supplied cert="high" reason="lost">ἀδελφοῦ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> υἱὸν </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐπὶ ἔτη </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>ὥστε διδαχθῆναι τὴν γερ<unclear cert="high" reason="undefined">δ</unclear><supplied cert="high" reason="lost">ιακὴν τέχνην πᾶσαν ἐντὸς τοῦ</supplied> 

    <lb n="9"/>προκιμένου χρόνου, οὐ γινό<unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ενον ἀπόκοιτον οὐδ᾽ ἀφήμερον</supplied> 

    <lb n="10"/>ἀπὸ τῆς τοῦ Θετηλέννιος οἰκίας, σ<unclear cert="high" reason="undefined">υ</unclear><supplied cert="high" reason="lost">νακολουθοῦντα αὐτῷ κατὰ τὸν</supplied> 

    <lb n="11"/>νόμον καὶ <choice><corr>ἐπιτελοῦντα</corr><sic>επιτελωντα</sic></choice> π<supplied cert="high" reason="lost">άντα τὰ ἐπιταχθησόμενα, τρεφο</supplied>-

