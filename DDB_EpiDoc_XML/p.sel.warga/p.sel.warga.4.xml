    <lb n="1"/>Ἁρποκρατίωνι <expan>στρα<ex cert="high">τηγῷ</ex></expan> 

    <lb n="2"/><expan>Ἀρσι<ex cert="high">νοείτου</ex></expan> Θεμίστου καὶ <expan>Πολ<ex cert="high">έμωνος</ex></expan> <expan>μερίδ<ex cert="high">ων</ex></expan> 

