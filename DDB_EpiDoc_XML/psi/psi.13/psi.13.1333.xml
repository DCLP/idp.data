    <lb n="6"/>Μᾶρκος Αὐρήλιος Κο<hi rend="diaeresis">ί</hi>ντ<supplied reason="lost">ος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>Κίσσος Ἀρχελ<gap reason="illegible" quantity="3" unit="character"/>τειμ<gap reason="lost" extent="unknown" unit="character"/> 

