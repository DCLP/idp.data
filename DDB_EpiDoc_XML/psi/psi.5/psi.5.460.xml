    <lb n="1"/>παρὰ τοῦ χωματεπείκτου 

    <lb n="2"/><expan>δι<ex cert="high">ὰ</ex></expan> τοῦ βοηθοῦ <app type="BL"><lem resp="5.123"><expan>κωμάρχ<ex cert="high">οις</ex></expan></lem><rdg><abbr>κωμαρχ</abbr></rdg></app> 

