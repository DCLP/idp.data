    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" extent="1" unit="character"/> τ<unclear reason="undefined">ῷ</unclear> ὑμ<unclear reason="undefined">ετέρ</unclear>ῳ 

