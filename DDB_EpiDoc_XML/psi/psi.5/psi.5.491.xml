    <lb n="1"/>Ἀγρεοφῶν Ζήνωνι χαίρειν. ὑπογέγραφά <unclear cert="high" reason="undefined">σ</unclear><supplied cert="low" reason="lost">οι τῆς παρὰ Ἐφαρμόστου πρὸς ἡμᾶς ἐπιστολῆς</supplied> 

    <lb n="2"/>τὸ ἀντίγραφο<supplied cert="high" reason="lost">ν</supplied>. καλῶς οὖν ποιήσεις τάς τ᾽ ἐπιστ<supplied cert="high" reason="lost">ολὰς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>καὶ πυθόμενος δὲ τίνα ἐστὶν ἃ ἀντιγράφει Ἀπο<supplied cert="low" reason="lost">λλώνιος</supplied><gap extent="4" reason="lost" unit="character"/>απο<gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀ</supplied>-

    <lb n="4"/><unclear cert="high" reason="undefined">πορεῖ</unclear> γὰ<supplied cert="high" reason="lost">ρ </supplied><gap extent="1" reason="lost" unit="character"/>μοι<unclear cert="high" reason="undefined">α</unclear><gap extent="12" reason="lost" unit="character"/><unclear cert="high" reason="undefined">γ</unclear>ραφετ<gap extent="1" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="3" reason="illegible" unit="character"/><gap extent="5" reason="lost" unit="character"/> τα<gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>παρεχομένου εἰς τὰ πράγματα· πρὸς γὰρ τοὺς πολλ<unclear cert="high" reason="undefined">οὺ</unclear><supplied cert="high" reason="lost">ς</supplied> <unclear cert="high" reason="undefined">ὁ</unclear> φόβος χρησι<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/>μάλιστα ἑαυτὸν ἐντείνειν εἴωθεν ὅταν <choice><corr>φιλανθρωπότατ<supplied cert="high" reason="omitted">α</supplied></corr><sic>φιλανθρωποτατ</sic></choice> αὐτῶι <unclear cert="high" reason="undefined">μο</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/><add place="supralinear">ἐὰν δή τις λό<supplied cert="high" reason="lost">γο</supplied><unclear cert="high" reason="undefined">ς</unclear> γίνηται, καλῶς ποιήσεις</add> <del rend="erasure" status="unremarkable">καλῶς ποιήσεις ἐάν τε ὁ καιρὸς γένηται μν<unclear cert="high" reason="undefined">η</unclear>σθεὶς <unclear cert="high" reason="undefined">αὐ</unclear>τῶι καὶ σοί, δέομαι δὲ</del> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>αὐτοῦ καθὰ καὶ μέχρι τοῦ νῦν.  ἔρρωσο. <expan><ex cert="high">ἔτους</ex></expan> <num value="28"><unclear cert="high" reason="undefined">κ</unclear>η</num>, Δύστρου <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>Ἐφάρμοστος Ἀγρεοφῶντι. λαβὼν τὰς ἐπιστολὰς ἃς κομίζει Πα<unclear cert="high" reason="undefined">ι</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/>οὔσας τὰς πάσας ὀκτὼ <num value="8"/> σὺν τῆι παρ᾽ Ἱππονίκου καὶ Ζωίλου τοῦ τραπ<supplied cert="high" reason="lost">εζίτου </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/>λόγους ἀπόδος διὰ Ζήνωνος Ἀπολλωνίωι τῶι διοικητῆι καὶ τουτ<gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> καὶ μὴ ἄλλως ποι</supplied>-

    <lb n="12"/><choice><corr>ήσῃς</corr><sic><supplied cert="high" reason="lost">ποι</supplied>ησηις</sic></choice> <foreign lang="la">?.</foreign> ἐπιμελὲς δέ σοι γενέσθω τοῦ Ζήνωνος πυθέσθαι τίνα ἐσ<supplied cert="low" reason="lost">τὶν ἃ ἀντιγράφει Ἀπολλώνιος</supplied> 

    <lb n="13"/>πρὸς τὰς παρ᾽ ἡμῶν ἐπιστολάς· πάλιν γὰρ παραπλήσια γέγραφεν Ἱ<unclear cert="high" reason="undefined">π</unclear><supplied cert="low" reason="lost">πόνικος </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> πρό</supplied>-

    <lb n="14"/>τερον ἐπέστειλεν Ἀθηνάδου παραγενομένου πρὸς ἡμᾶς. 

    <lb n="15"/>ἔρρωσο. <expan><ex cert="high">ἔτους</ex></expan> <num value="28">κη</num>, Μεχὶρ <num value="13">ιγ</num>. 
