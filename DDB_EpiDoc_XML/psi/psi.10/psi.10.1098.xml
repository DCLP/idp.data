    <lb n="43"/><handShift new="m2"/> Ἀρίστων Π<unclear cert="high" reason="undefined">ύρ</unclear>ρ<unclear cert="high" reason="undefined">ο</unclear>υ τῶν διὰ Χομήνιος κλ<supplied cert="high" reason="lost">η</supplied>ρ<supplied cert="high" reason="lost">ο</supplied>ύχων ἱππέων <expan>μεμίσ<unclear cert="high" reason="undefined">θ</unclear><ex cert="high">ωκα</ex></expan> 

    <lb n="44"/>ἀπὸ τοῦ <supplied cert="high" reason="lost">ὑπ</supplied>άρχοντός μοι κ<supplied cert="high" reason="lost">λ</supplied>ήρου γῆς <unclear cert="high" reason="undefined">ἀ</unclear><supplied cert="high" reason="lost">ρούρ</supplied>ας <choice><corr>τρεῖς</corr><sic>τρις</sic></choice> <num value="3"/> πε<unclear cert="high" reason="undefined">ρὶ</unclear> Τεβ<supplied cert="high" reason="lost">τῦνιν εἰς</supplied> 

    <lb n="45"/><unclear cert="high" reason="undefined">τ</unclear>ὸ <choice><corr>δεύτερον</corr><sic>τευτε<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ον</supplied></sic></choice> ἔτος ἐκφορίου τοῦ πα<supplied cert="high" reason="lost">ντὸς πυροῦ</supplied> ἀρτ<supplied cert="high" reason="lost">αβῶν</supplied> εἰκο<supplied cert="high" reason="lost">σιεπτά</supplied> <num value="27"/> </ab></div></div></body></text></TEI.2>