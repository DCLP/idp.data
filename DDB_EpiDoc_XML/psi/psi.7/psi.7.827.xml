    <lb n="1"/><gap extent="unknown" reason="lost" unit="character"/> ὅτε 

    <lb n="2"/><gap extent="3" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>α<unclear>τ</unclear>ο Ὡριγένη<unclear>ς</unclear> ἕνε

