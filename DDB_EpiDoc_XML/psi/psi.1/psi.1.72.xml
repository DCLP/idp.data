    <lb n="2"/>παρὰ κεράτια δ<supplied cert="high" reason="lost">έ</supplied>κα ἰδιωτικῷ 

    <lb n="3"/>ζυγῷ <expan>νομιτ<ex cert="high">ευόμενα</ex></expan>, <expan>γί<ex cert="high">νονται</ex></expan> <expan>χρ<ex cert="high">υσοῦ</ex></expan> <expan>νο<ex cert="high">μίσματα</ex></expan> <num value="2">β</num> <expan>κερ<ex cert="high">άτια</ex></expan> <num value="10">ι</num> <expan>ἰδ<ex cert="high">ιωτικῷ</ex></expan> <expan>νομ<ex cert="high">ίσματα</ex></expan>, 

