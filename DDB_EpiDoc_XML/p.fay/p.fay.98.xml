    <lb n="1"/><supplied cert="high" reason="lost">ἔ</supplied>του<supplied cert="high" reason="lost">ς ὀ</supplied><unclear cert="high" reason="undefined">γδ</unclear><supplied cert="high" reason="lost">ό</supplied>ου Αὐτοκράτο<supplied cert="high" reason="lost">ρο</supplied>ς Καίσαρος 

    <lb n="2"/>Τρα<supplied cert="high" reason="lost">ια</supplied>νοῦ Ἁδριανοῦ Σεβαστοῦ, μην<supplied cert="high" reason="lost">ὸ</supplied>ς Περιτίου <num value="5">ε</num> 

    <lb n="3"/>Χοίαχ <num value="5">ε</num>, ἐν Εὐημερείᾳ τῆς Θεμίστου μερίδος 

    <lb n="4"/>τοῦ Ἀ<supplied cert="high" reason="lost">ρ</supplied>σ<supplied cert="high" reason="lost">ι</supplied>νοείτου νομοῦ.  ὁμολογοῦσι Ἡρακλείδης 

    <lb n="5"/>Φιλα<supplied cert="high" reason="lost">δέ</supplied>λφου ἀνα<supplied cert="high" reason="lost">γρ</supplied>αφόμενος ἐπ᾽ ἀμφόδου Ἱερᾶς 

    <lb n="6"/>Πύλη<supplied cert="high" reason="lost">ς</supplied> ὡς ἐτῶν τριάκοντα ἓξ <num value="36"/> ο<supplied cert="high" reason="lost">ὐ</supplied>λὴ <supplied cert="high" reason="lost">γ</supplied>όνατι ἀριστερῷ 

    <lb n="7"/>καὶ Νεῖλος Ἥρων<supplied cert="high" reason="lost">ο</supplied>ς ἀναγραφόμενος ἐπ᾽ <expan>ἀμφόδο<ex cert="high">υ</ex></expan> 

    <lb n="8"/>Διονυσίου <subst><add place="inline">Τόπων</add><del rend="corrected" status="unremarkable"><gap extent="1" reason="illegible" unit="character"/>οπων</del></subst> <supplied cert="high" reason="lost">ὡς ἐτ</supplied>ῶν εἴκοσι πέντε <num value="25"/> ἄσημος 

