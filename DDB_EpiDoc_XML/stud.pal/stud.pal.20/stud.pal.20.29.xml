    <lb n="1"/>ἔτους ἑβδόμου Αὐτοκράτ<supplied cert="high" reason="lost">ορος Καίσαρος Μάρκου</supplied> 

    <lb n="2"/>Αὐρηλίου Σεουήρου Ἀλεξάνδ<supplied cert="high" reason="lost">ρου Εὐ</supplied>σ<supplied cert="high" reason="lost">εβοῦς Εὐτυχοῦς</supplied> 

    <lb n="3"/>Σεβαστοῦ ἐφ᾽ ἱερέων τῶν ὄντων <supplied cert="high" reason="lost">ἐ</supplied><unclear cert="high" reason="undefined">ν</unclear> <supplied cert="high" reason="lost">Ἀλεξανδρείᾳ</supplied> 

    <lb n="4"/>καὶ τῶν ἄλλων τῶν γραφομένων κοινῶν <supplied cert="high" reason="lost">μηνὸς</supplied> 

    <lb n="5"/>Ἁδριανοῦ Χ<supplied cert="high" reason="lost">οι</supplied>ὰκ δεκάτῃ δι᾽ ἐπιτηρ<supplied cert="high" reason="lost">η</supplied>τῶν <supplied cert="high" reason="lost">ἀγορανομί</supplied>-

    <lb n="6"/>ας περὶ Τέκμει τοῦ ὑπὲρ Μέμφιν Ἡρακ<supplied cert="high" reason="lost">λε</supplied>ο<supplied cert="high" reason="lost">π</supplied><unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">λίτου</supplied>. 

    <lb n="7"/>ὁμολογεῖ Αὐρήλιος Ψεναμοῦνις Διο<supplied cert="high" reason="lost">γέ</supplied>νους <supplied cert="high" reason="lost">μητρὸς</supplied> 

    <lb n="8"/>Ταμμεῖτος ἀπὸ κώμης Τοσαχμεω<unclear cert="high" reason="undefined">ς</unclear> ὡς ἐτ<supplied cert="high" reason="lost">ῶν </supplied><gap extent="4" reason="lost" unit="character"/> 

    <lb n="9"/>κοντα ἓξ οὐλὴ ἀντικνημί<unclear cert="high" reason="undefined">ῳ</unclear> δεξιῷ ἐκ<unclear cert="high" reason="undefined">κεχ</unclear><supplied cert="high" reason="lost">ωρηκέναι</supplied> 

    <lb n="10"/>ἀπὸ τοῦ νῦν ἐπὶ τὸν ἀεὶ χρόνον τῇ ἑαυτοῦ <supplied cert="high" reason="lost">γυναικὶ</supplied> 

    <lb n="11"/>Αὐρηλίᾳ Ταῶτι Ὀν<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">ώ</supplied>φριος μητρὸς Τασκ<supplied cert="high" reason="lost">λαταρίου</supplied> 

    <lb n="12"/>ἀπὸ κώμης <app type="BL"><lem resp="7.259">Τανασὼ</lem><rdg>Ταναμ<gap extent="1" reason="lost" unit="character"/></rdg></app> ὡς ἐτῶν τριάκοντα τ[ριῶν <num value="33"/> λευ]-

    <lb n="13"/>κοχρόῳ χωρὶς κυρίου χρηματιζούσῃ κατὰ τὰ <supplied cert="high" reason="lost">Ῥωμαίων</supplied> 

    <lb n="14"/>ἔθη τέκνων δικαίῳ ἀντὶ μέρους ἧς κατέχρησ<supplied cert="high" reason="lost">ε φερ</supplied>-

    <lb n="15"/>νῆς αὐτῆς ἐν δραχμαῖς <choice><corr>τρισχιλίαις</corr><sic>τρισχειλ<unclear cert="high" reason="undefined">ι</unclear>αις</sic></choice> πεντ<supplied cert="high" reason="lost">ακοσίαις</supplied> <num value="3500"/> 

    <lb n="16"/>ἀπὸ τῶν ὑπαρχόντων α<supplied cert="high" reason="lost">ὐτ</supplied>ῷ πρότερον τοῦ <app type="editorial"><lem>ὁ<supplied cert="high" reason="lost">μογνησίου</supplied></lem><rdg>ὁ<supplied cert="high" reason="lost">μοπατρίου</supplied></rdg></app> 

    <lb n="17"/>αὐτοῦ ἀδελφοῦ Διο<unclear cert="high" reason="undefined">στε</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/><supplied cert="high" reason="lost"> πε</supplied>ρὶ κώμην Τοσα<supplied cert="high" reason="lost">χμιν ἐκ</supplied> 

    <lb n="18"/>τοῦ Ἀπολλωνίου κλήρου ἀπὸ ἀρο<supplied cert="high" reason="lost">υρ</supplied>ῶν τεσσ[άρων <num value="4"/> ἀρού]-

    <lb n="19"/>ρας τρεῖς ἥμισυ <num value="3  1/2 "/> <choice><corr>γείτονες</corr><sic>γιτονες</sic></choice> καθὼς αὐτοὶ ὑπη<supplied cert="high" reason="lost">γόρευσαν</supplied> 

    <lb n="20"/>νότου Νεπωτιανοῦ, βορρᾶ ὁδὸς, ἀπηλιώτ<supplied cert="high" reason="lost">ου Θεοδώ</supplied>-

    <lb n="21"/>ρου, λιβὸς ὅριον κλήρου καὶ περὶ Πεεντε<gap extent="1" reason="illegible" unit="character"/> <supplied cert="high" reason="lost">ἐκ τοῦ </supplied><gap extent="3" reason="lost" unit="character"/>-

    <lb n="22"/>ρέους κλήρου κατο<supplied cert="high" reason="lost">ίκ</supplied>ων ἀρούρας τρεῖς <num value="3"/> νότου <unclear cert="high" reason="undefined">κλη</unclear><supplied cert="high" reason="lost">ρο</supplied>-

    <lb n="23"/>νόμων Μάρωνος, βορρᾶ κληρονόμων Διδυμιανο<supplied cert="high" reason="lost">ῦ</supplied>, 

    <lb n="24"/>ἀπηλιώτου λιβὸς Ἡλιοδώρου καὶ τοῦ ἀδελφοῦ 

    <lb n="25"/>καὶ περὶ τὴν αὐτὴν Τοσαχμιν ἐκ τοῦ Διονυσίου κλ<supplied cert="high" reason="lost">ή</supplied>-

    <lb n="26"/>ρου ἀρούρας δύο ἥμισυ <num value="2  1/2 "/> ὁμοίως <choice><corr>γείτονες</corr><sic>γιτονες</sic></choice> νότ<supplied cert="high" reason="lost">ου</supplied> Ἡσι<gap desc="vestiges" extent="unknown" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="27"/><space dim="horizontal" extent="unknown" unit="character"/> 

    <lb n="28"/>καὶ περὶ τὴν αὐτὴν ἐκ τοῦ Ὑπερβάστου <supplied cert="high" reason="lost">κλή</supplied>ρου ἄρ<supplied cert="high" reason="lost">ου</supplied>-

    <lb n="29"/>ραν μίαν <num value="1"/> ὁμοίως νότου <space dim="horizontal" extent="unknown" unit="character"/> 

    <lb n="30"/><space dim="horizontal" extent="unknown" unit="character"/> πρὸς τὸ ἀπὸ τοῦ νῦν <supplied cert="high" reason="lost">κρα</supplied>-

    <lb n="31"/>τεῖν καὶ κυριεύειν καὶ δεσπόζειν καὶ ἐξ<supplied cert="high" reason="lost">ουσίαν ἔχειν</supplied> 

    <lb n="32"/>τοῦ πᾶσαν οἰκονομί<supplied cert="high" reason="lost">αν</supplied> κατ᾽ αὐτῶν ἐπιτελ<supplied cert="high" reason="lost">εῖν ἀνεμ</supplied>-

    <lb n="33"/>ποδίστως μὴ προσδεομένης τῆς τοῦ Ψεν<supplied cert="high" reason="lost">αμούνιος</supplied> 

    <lb n="34"/>παρουσίας ἐκ συνευδο<supplied cert="high" reason="lost">κή</supplied>σεω<supplied cert="high" reason="lost">ς</supplied> ἀρκουμ<supplied cert="high" reason="lost">ένου τῇδε</supplied> 

    <lb n="35"/>τῇ ἐκχωρήσει ἣν καὶ βεβα<supplied cert="high" reason="lost">ι</supplied>οῖ πάσῃ βεβ<unclear cert="high" reason="undefined">αι</unclear>ώ<supplied cert="high" reason="lost">σει</supplied> 

    <lb n="36"/>πάντα τὸν ἐπελευσόμ<supplied cert="high" reason="lost">εν</supplied>ον ἢ ἐμποιησό<supplied cert="high" reason="lost">μενον</supplied> 

    <lb n="37"/>αὐτὸν ἀποστήσειν παραχρῆμα τοῖς <supplied cert="high" reason="lost">ἰδ</supplied>ίοις 

    <lb n="38"/>ἀναλώμασι, μένοντος δ<supplied cert="high" reason="lost">ὲ τοῦ λόγ</supplied>ου α<supplied cert="high" reason="lost">ὐτῇ</supplied> π<supplied cert="high" reason="lost">ρὸς τὸν</supplied> 

    <lb n="39"/>ἄνδρα αὐτῆς Ψεναμοῦν<supplied cert="high" reason="lost">ιν περὶ τ</supplied>ῶν <gap extent="1" reason="illegible" unit="character"/><gap extent="13" reason="lost" unit="character"/> 

    <lb n="40"/>φερνῆς ἀκολούθω<supplied cert="high" reason="lost">ς </supplied><gap extent="20" reason="lost" unit="character"/><supplied cert="high" reason="lost"> περὶ</supplied> 

    <lb n="41"/>δὲ τοῦ ταῦτα ὀρθῶς καὶ <supplied cert="high" reason="lost">καλῶς γεγενῆσθαι ἐπερωτη</supplied>-

    <lb n="42"/>θέντες ὡμολόγησαν. 
