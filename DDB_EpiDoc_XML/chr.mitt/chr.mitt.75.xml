    <lb n="1"/><app type="BL"><lem resp="cf. 9.93"><supplied cert="high" reason="lost">Αὐρηλίῳ Ἡρακλείῳ τῷ καὶ</supplied></lem><rdg><gap extent="unknown" reason="lost" unit="character"/></rdg></app> <unclear cert="high" reason="undefined">Ἀ</unclear>στερίῳ <supplied cert="high" reason="lost">στρατηγῷ</supplied> Ὀξ<unclear cert="high" reason="undefined">υ</unclear>ρυγχείτου 

