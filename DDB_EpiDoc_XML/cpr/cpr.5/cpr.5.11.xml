    <lb n="1"/>Ἀμμωνοθέωνι ἐπισκόπῳ <gap extent="10" reason="lost" unit="character"/> 

    <lb n="2"/>Αὐρήλιος Βῆσις Ἀκώριος ἀπὸ τοῦ <gap extent="1" reason="illegible" unit="character"/><gap extent="10" reason="lost" unit="character"/> 

    <lb n="3"/><space dim="horizontal" extent="unknown" unit="character"/>        χαίρειν. <space dim="horizontal" extent="unknown" unit="character"/> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>ἐπειδὴ σήμερον ἐχειροτονήθ<supplied cert="high" reason="lost">ην εἰς τὴν σὴν</supplied> 

    <lb n="5"/>διακονίαν καὶ προφοράν σοι ἐξεδ<supplied cert="high" reason="lost">όμην πρὸς</supplied> 

    <lb n="6"/>τὸ ἀπαράβλητόν με εἶναι τῆς <choice><corr>ἐπισ<supplied cert="high" reason="lost">κοπῆς</supplied></corr><sic>επεισ<supplied cert="high" reason="lost">κοπης</supplied></sic></choice> <supplied cert="high" reason="lost">σου,</supplied> 

    <lb n="7"/>διὰ τοῦτο ὁμολογῶ διὰ τοῦδε τοῦ γρα<supplied cert="high" reason="lost">μματίου μὴ</supplied> 

    <lb n="8"/>ἐνκαταλείπειν σε μηδὲ με<unclear cert="high" reason="undefined">τέρχ</unclear><supplied cert="high" reason="lost">εσθαι </supplied><gap extent="5" reason="lost" unit="character"/> 

    <lb n="9"/>ἐπισκόπου ἢ πρεσβυτέρου ἢ <unclear cert="high" reason="undefined">κ</unclear><gap extent="10" reason="lost" unit="character"/> 

    <lb n="10"/>μὴ συνπείθ<unclear cert="high" reason="undefined">ο</unclear>ι διὰ τὸ ἐμὲ <choice><corr>ἐπὶ</corr><sic>επει</sic></choice> το<supplied cert="high" reason="lost">ύτοις </supplied><gap extent="5" reason="lost" unit="character"/> 

    <lb n="11"/>ἐὰν δὲ θελήσω ἀποστῆναι <gap extent="1" reason="illegible" unit="character"/><gap extent="12" reason="lost" unit="character"/> 

    <lb n="12"/>σου ἢ καὶ χωρὶς γραμμάτων <unclear cert="high" reason="undefined">μ</unclear><gap extent="10" reason="lost" unit="character"/> 

    <lb n="13"/>ρου <choice><corr>τυγχάνειν</corr><sic>τυχανιν</sic></choice> δῆθεν ἐγὼ τῆς δια <gap extent="1" reason="illegible" unit="character"/><gap extent="10" reason="lost" unit="character"/> 

    <lb n="14"/>λα λαικῆς κοινωνίας μεταξιω <gap extent="1" reason="illegible" unit="character"/><gap extent="10" reason="lost" unit="character"/> 

    <lb n="15"/>μην σοι τὸ γραμματῖον τοῦτο πρ<unclear cert="high" reason="undefined">ὸ</unclear><supplied cert="high" reason="lost">ς ἀσφάλειαν</supplied> 

    <lb n="16"/>καὶ ὃ μὴ εἴη ἐὰν πράξω τοῦτο ὑπε<unclear cert="high" reason="undefined">ύ</unclear><supplied cert="high" reason="lost">θυνος ἔσο</supplied>-

    <lb n="17"/>μαι τοῖς μετ<unclear cert="high" reason="undefined">α</unclear>ξὺ <choice><corr>ὡρισθεῖσι</corr><sic>ωρισθισει</sic></choice> καὶ <expan>ἐπερ[ωτηθ<ex cert="high">εὶς</ex></expan> <expan>ὡμολ<ex cert="high">όγησα</ex></expan>.] 

    <lb n="18"/>καὶ μὴ ἐξ<unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">ῖναι </supplied><choice><corr><supplied cert="high" reason="lost">κοι</supplied>νωνεῖν</corr><sic><supplied cert="high" reason="lost">κοι</supplied><unclear cert="high" reason="undefined">ν</unclear>ωνιν</sic></choice> με μυ<unclear cert="high" reason="undefined">ς</unclear><gap extent="10" reason="lost" unit="character"/> 

    <lb n="19"/>γενομένῳ <gap extent="1" reason="illegible" unit="character"/><gap extent="5" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> στη ἐὰν μὴ ς<gap extent="1" reason="illegible" unit="character"/><gap extent="10" reason="lost" unit="character"/> 

