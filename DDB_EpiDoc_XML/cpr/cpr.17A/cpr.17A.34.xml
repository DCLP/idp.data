    <lb n="1"/><supplied cert="high" reason="lost">παρὰ Αὐρηλίων Λυσιμάχου καὶ Χωοῦτος καὶ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> καὶ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τῶν</supplied> 

    <lb n="1"/>π<supplied cert="high" reason="lost">άντω</supplied>ν ἀπὸ κώμη<supplied cert="high" reason="lost">ς Πρήκτεως τοῦ αὐτοῦ νομοῦ, προ</supplied>-


    <lb n="2"/>εστώτων τῆς <expan><ex cert="high">αὐτῆς</ex></expan> κ<supplied cert="high" reason="lost">ώμης.  ἀναδίδομεν καὶ εἰσαγγέλλομεν</supplied> 

    <lb n="3"/>τοὺς ἑξῆς ἐγγεγραμέ<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">ους συνπνέοντας ἡμῖν ποιησομένους</supplied> 

    <lb n="4"/>ἅπαντα τὰ πρὸς τ<unclear cert="high" reason="undefined">ὴν</unclear> <supplied cert="high" reason="lost">κώμην ζητούμενα δημόσια ἐπιτάγ</supplied>-

    <lb n="5"/>ματα καὶ ἀπαίτη<supplied cert="high" reason="lost">σιν τῶν δημοσίων τελεσμάτων ὄντας</supplied> 

    <lb n="6"/>εὐθέτους καὶ ἐπιτηδ<supplied cert="high" reason="lost">είους πρὸς τὴν ἑκάστου χρείαν κινδύνῳ</supplied> 

    <lb n="7"/>ἡμῶν τῶν τὴν ὀν<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">μασίαν αὐτῶν πεποιημένων.</supplied> 

    <lb n="8"/><supplied cert="high" reason="lost">εἰσὶ δέ·</supplied> 

    <lb n="9"/>Πόλυς Σιλ<unclear cert="high" reason="undefined">βαν</unclear><supplied cert="high" reason="lost">οῦ δἰ ἐμοῦ Λυσιμάχου</supplied> 

    <lb n="10"/>Φ<supplied cert="high" reason="lost">ῖ</supplied>βις Ἡρ<unclear cert="high" reason="undefined">ακλ</unclear><supplied cert="high" reason="lost">ῆτος δι᾽ ἐμοῦ χωοῦτος</supplied> 

    <lb n="11"/>Κάστωρ Πολ<unclear cert="high" reason="undefined">υ</unclear><supplied cert="high" reason="lost">δεύκους δι᾽ ἐμοῦ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="12"/>Σαρμάτης Σιλβαν<unclear cert="high" reason="undefined">οῦ</unclear> δι᾽ ἐ<supplied cert="high" reason="lost">μοῦ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/><supplied cert="high" reason="lost">οὕσπ</supplied>ερ ἐγ<supplied cert="high" reason="lost">γυώμεθα καὶ παραστήσομεν ἀμέμπτως σὺν</supplied> 

    <lb n="14"/>ἡμ<unclear cert="high" reason="undefined">ῖ</unclear><supplied cert="high" reason="lost">ν</supplied> ὑπ<unclear cert="high" reason="undefined">ακού</unclear><supplied cert="high" reason="lost">σαντας ἅπαντα τὰ πρὸς τὴν κώμην</supplied> 

    <lb n="15"/><supplied cert="high" reason="lost">ζητούμενα ἐπιτάγματα.  ἐὰν δὲ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

