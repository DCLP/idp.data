    <lb n="1"/><gap extent="unknown" reason="lost" unit="character"/>νου ως<gap extent="unknown" reason="lost" unit="character"/>τατ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/><supplied reason="lost">ἐν τῇ π</supplied>όλει ἀγορ<supplied reason="lost">αν</supplied><gap extent="3" reason="lost" unit="character"/> ἀπὸ <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/><gap extent="7" reason="lost" unit="character"/>ης Ἀμμων<gap extent="3" reason="lost" unit="character"/><supplied reason="lost"> τέτ</supplied>αρτον μ<supplied reason="lost">έρος </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>τρι<gap extent="5" reason="lost" unit="character"/> πάντων ἐπ᾽ ἀμ<supplied reason="lost">φ</supplied>όδου Φανησίο<supplied reason="lost">υ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>σὺν κ<gap extent="3" reason="lost" unit="character"/><supplied reason="lost"> νό</supplied>του δημοσία πλατεῖα, βορρᾶ ἑτέρων ομο<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/>τετ<supplied reason="lost">άχ</supplied>θαι καθὼς αἱ περὶ αὐτῶν προκτήσεις περιέχουσι <supplied reason="lost">καὶ ἔχειν τὴν τιμὴν</supplied> 

    <lb n="7"/><supplied reason="lost">τὴν πρὸς ἀλλήλους συμ</supplied>πεφωνημένην τοῦ πεπραμένου καθὼς πρόκειτα<supplied reason="lost">ι ἀργυρίου δραχμὰς</supplied> 

    <lb n="8"/><gap extent="3" reason="lost" unit="character"/> ἑκατὸν <num value="100"/> διὰ τῆς Διδύμου τραπέζης Φανησίου καὶ βεβ<supplied reason="lost">αιώσει τῇ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>καὶ τοῖς παρ᾽ α<supplied reason="lost">ὐτῆς</supplied> τὸ πεπραμένον τέταρτον μέρος τ<supplied reason="lost">ῆς οἰκίας </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/><supplied reason="lost">ἀνέπα</supplied>φον καὶ ἀνενεχύραστον καὶ <choice><corr>ἀνεπιδάνειστον</corr><sic>ανεπιδανιστον</sic></choice> καὶ καθα<supplied reason="lost">ρὸν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/>ἀπό <choice><corr>τε</corr><sic>δε</sic></choice> λαογραφιῶν μεχρὶ ἑτέρας κατ᾽ οἰκίαν ἀπογρ<supplied reason="lost">αφῆς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="12"/>μηδέ<supplied reason="lost">να ἐ</supplied>μποιούμενον αὐτοῦ μηδὲ μέρους μηδε<supplied reason="lost">νὸς </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied reason="lost"> εἰσοδεύ</supplied>-

    <lb n="13"/>οντας καὶ ἐξοδεύοντας καὶ καθαιροῦντας καὶ <app type="BL"><lem resp="8.99">ἀνοικοδ<supplied reason="lost">ομοῦντας</supplied></lem><rdg><choice><corr>ἀντικατ<supplied reason="lost">αλάσσοντας</supplied></corr><sic>αντικαδ<supplied reason="lost">αλασσοντας</supplied></sic></choice></rdg></app><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="14"/>ἃς καὶ πρὸς ταῦτα τὸν Ἀμμώνιον μὴ ἐνκαλεῖν μηδὲ ἀ<supplied reason="lost">μφισβητεῖν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="15"/><choice><corr>τιμὴν</corr><sic>τειμην</sic></choice> μεθ᾽ ἡμιολίας καὶ τὰ βλάβη καὶ ἀνηλώματα διπλᾶ καὶ <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="16"/>τῶν <expan>προγεγρ<ex>αμμένων</ex></expan> <expan>ὑπογρ<ex>αφεῖς</ex></expan> τοῦ μὲν Ἀμμωνίου Χαιρήμων Χαιρήμονος <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="17"/><handShift new="m2"/> Ἀλ<supplied reason="lost">έξ</supplied>α<supplied reason="lost">νδρ</supplied>ος Διοσκόρου ὁμολογῶ πεπρακέναι τὸ προκ<supplied reason="lost">είμενον </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="18"/>Ἀμμωνίου τέταρτον μέρος οἰκίας κοινὸν καὶ ἀδιαίρετον πρὸς <gap extent="unknown" reason="lost" unit="character"/><supplied reason="lost"> μετὰ παντὸς αὐτῶν</supplied> 

    <lb n="19"/>τοῦ δικαίου ὧν καὶ αἱ γειτνίαι πρόκινται καὶ ἀπέχω τὴν <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="20"/>ἀμφόδου Φανησίου καὶ βεβαιώσω πάσῃ <choice><corr>βεβαιώσει</corr><sic>βεβαιωσι</sic></choice> καὶ καθαρὰ <supplied reason="lost">παρέξω ὡς</supplied> πρόκι<supplied reason="lost">ται </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="21"/>σα Νικάριον Ἀμμωνίου μετὰ κυρίου τοῦ ἀδελφοῦ Σαρ<gap extent="4" reason="lost" unit="character"/> <choice><corr>ἐώνημαι</corr><sic>εωνημε</sic></choice> <gap extent="1" reason="illegible" unit="character"/>ν<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="22"/>ἔγραψα ὑπὲρ αὐτῶν Τιμόθεως Μάρωνος μὴ <choice><corr>εἰδότων</corr><sic>ιδοτων</sic></choice> γράμματα. </ab></div></body></text></TEI.2>