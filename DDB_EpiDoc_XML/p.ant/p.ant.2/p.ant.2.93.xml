    <lb n="27"/>καὶ ῥοδ<gap extent="3" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="6" reason="lost" unit="character"/><unclear>τι</unclear><gap extent="1" reason="lost" unit="character"/><unclear>οσα</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="28"/><unclear>γ</unclear>ιμεν<unclear>ον</unclear>κ<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="29"/>ἔχων τ<gap extent="8" reason="lost" unit="character"/> <unclear>πρ</unclear>οι<unclear>κισα</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="30"/>προσδοκη<gap extent="1" reason="illegible" unit="character"/><gap extent="5" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/>χρου πλοίου 

    <lb n="31"/>ἀποσ<unclear>τέλλω</unclear> σοι <choice><corr>ἑπτακελλάριον</corr><sic>επτακελλαρον</sic></choice> 

    <lb n="32"/>μεστὸν σπανοῦ καὶ τὰ κερβικάρια 

    <lb n="33"/>καὶ εἴ τι ἐὰν εὕρω. δήλωσον εἰ 

    <lb n="34"/>ἐδέξω τὸν δακτύλιον χρυσοῦν 

    <lb n="35"/>παρὰ Δωροθέου βοηθοῦ Ἀνυσίου 

    <lb n="36"/><unclear>με</unclear>τὰ τοῦ πιναρίου Ἀρεθουσίου διδασ

    <lb n="37" type="worddiv"/>κάλου <supplied reason="lost">ἵν</supplied><unclear>α</unclear> ἀγοράσῃς ἀρώματα. 

    <lb n="38"/>ἐρχό<supplied reason="lost">μεν</supplied><unclear>ος</unclear> <unclear>φέρ</unclear>ω καὶ τὰ ὑποδήματα 

    <lb n="39"/>Π<gap extent="1" reason="illegible" unit="character"/><gap extent="5" reason="lost" unit="character"/>ιας· καὶ εἴ τι χρήσιμον 

    <lb n="40"/>ἀ<unclear>ν</unclear><supplied reason="lost">εύρηκες πρ</supplied><unclear>ὸ</unclear>ς γάμους, γράψον μοι 

    <lb n="41"/>ἵνα <supplied reason="lost">ἐ</supplied><unclear>ρχό</unclear>μενος <choice><corr>ἀνενέγκω</corr><sic>ανενηγκω</sic></choice>. 

    <lb n="42"/><choice><corr>οὐδεμί<supplied reason="omitted">α</supplied></corr><sic>ουδε<unclear>μι</unclear></sic></choice> <unclear>γ</unclear>ὰρ διαφορὰ <choice><corr>μεταξὺ</corr><sic>μετοξυ</sic></choice> αὐτῆς 

    <lb n="43"/>καὶ τῆ<supplied reason="lost">ς</supplied> νύμφης. 

    <lb n="44"/><choice><corr>ἐρρῶσθαι</corr><sic>ερρωσθε</sic></choice> ὑμᾶς 

    <lb n="45"/>εὔχομαι πολλοῖς χρόν<supplied reason="lost">ο</supplied>ι<supplied reason="lost">ς</supplied>. 
