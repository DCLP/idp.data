    <lb n="40"/><expan>γί<ex cert="high">νεται</ex></expan> <expan>χρ<ex cert="high">υσ</ex>ο<ex cert="high">ῦ</ex></expan> <expan>νο<ex cert="high">μίσματα</ex></expan> <num value="4">δ</num> <expan>εὔστ<ex cert="high">α</ex>θ<ex cert="high">μα</ex></expan> <expan><unclear cert="high" reason="undefined">ζ</unclear><ex cert="high">υγῷ</ex></expan> <expan><supplied cert="high" reason="lost">Ἀλε</supplied>ξανδρ<ex cert="high">είας</ex></expan>, ἥνπερ <expan>προκειμέ<ex cert="high">νην</ex></expan> 

