    <lb n="9"/>τῇ <expan>ἁγί<ex cert="high">ᾳ</ex></expan> <expan>ἐκκλ<ex cert="high">ησίᾳ</ex></expan> τῶν ἁ<unclear cert="high" reason="undefined">γίων</unclear> <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> καὶ</supplied> 

