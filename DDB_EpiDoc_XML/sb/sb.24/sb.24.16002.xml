    <lb n="4"/><supplied cert="high" reason="lost">ὁμολογεῖ Ἀσκληπιάδης </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τοῦ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ο</supplied><unclear cert="high" reason="undefined">υ</unclear>ς ὡ<unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">ἐ</unclear>τῶν τριάκοντα ἓξ <num value="36"/> οὐλὴ <supplied cert="high" reason="lost">ὀ</supplied>φρύ<supplied cert="high" reason="lost">ει <app type="alternative"><lem>δεξιᾷ</lem><rdg>ἀριστερᾷ</rdg></app> Πτολλαροῦτι τῇ καὶ Ἰσιάδι</supplied>

    <lb n="5"/><supplied cert="high" reason="lost">τῇ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τοῦ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ὡς ἐτῶν </supplied><gap extent="unknown" reason="lost" unit="character"/> ἀσήμῳ μετὰ κυρίου τοῦ ἀνδρὸ<unclear cert="high" reason="undefined">ς</unclear> <supplied cert="high" reason="lost">αὐτῆς </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τοῦ </supplied><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="6"/><supplied cert="high" reason="lost">τοῦ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ὡς ἐτῶν </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> πεπρακέναι τὸν ὁμολ</supplied>ογοῦντα Ἀσκληπιάδην τῇ Πτολλ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ροῦτι τῇ καὶ Ἰσιάδι ἀπὸ τοῦ νῦν</supplied>

    <lb n="7"/><supplied cert="high" reason="lost">ἐπὶ τὸν ἅπαντα χρόνον τὰ ὑπάρχοντα τῷ ὁμολογοῦντι Ἀσκλ</supplied>ηπιάδῃ ἐκ διαιρέσεως ἧς δίκαιον <supplied cert="high" reason="lost">ἦλθεν εἰς τὸν ὁμολογοῦντα Ἀσκληπιάδην</supplied>

