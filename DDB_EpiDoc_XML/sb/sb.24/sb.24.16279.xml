    <lb n="1"/><g type="chirho"/>Κυρί<unclear cert="high" reason="undefined">ῳ</unclear> <unclear cert="high" reason="undefined">μ</unclear>ου <expan>ἀδελφ<ex cert="high">ῷ</ex></expan> <choice><corr>Ἑρμίνῳ</corr><sic>ερμειν<unclear cert="high" reason="undefined">ω</unclear></sic></choice> <expan><unclear cert="high" reason="undefined">ἀ</unclear>κτουαρ<ex cert="high">ίῳ</ex></expan> <expan>Φλ<ex cert="high">άουιος</ex></expan> <unclear cert="high" reason="undefined">Μ</unclear>ηνᾶς <expan>Φοιβ<unclear cert="high" reason="undefined">ά</unclear><supplied cert="high" reason="lost">μμω</supplied><ex cert="high">νος</ex></expan>.

    <lb n="2"/>Ἐδεξ<unclear cert="high" reason="undefined">άμ</unclear>ην <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">α</supplied>ρὰ σοῦ ἀπὸ τῶν ἀννωνοκαπίτ<unclear cert="high" reason="undefined">ο</unclear>υ μου διὰ <choice><corr>Μαωίου</corr><sic>μαθιας</sic></choice> συγ<gap extent="1" reason="illegible" unit="character"/>

    <lb n="3"/><gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/> Φιλαδέλφου, καὶ ἄλλα φ<unclear cert="high" reason="undefined">α</unclear>νερῶν κερατίων χρυσοῦ <supplied cert="high" reason="lost">ν</supplied><unclear cert="high" reason="undefined">ο</unclear>μ<unclear cert="high" reason="undefined">ι</unclear>σ<unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">άτια</supplied>

    <lb n="4"/>πέντε παρὰ κεράτια ἑπτά, <expan>γί<ex cert="high">νεται</ex></expan> <expan>χρ<ex cert="high">υσοῦ</ex></expan> <expan>νο<ex cert="high">μισμάτια</ex></expan> <num value="5">ε</num> <expan>π<ex cert="high">αρὰ</ex></expan> <expan>κερ<ex cert="high">άτια</ex></expan> <num value="7"><unclear cert="high" reason="undefined">ζ</unclear></num>, καὶ ταῦτα ἑτοίμως

    <lb n="5"/><unclear cert="high" reason="undefined">ἔχ</unclear><supplied cert="high" reason="lost">ω λογίσα</supplied><unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">θα</supplied><unclear cert="high" reason="undefined">ί</unclear> σοι ἀπὸ τῶν ἐμῶν ἀννωνῶν καὶ καπίτων λό<unclear cert="high" reason="undefined">γο</unclear>υ

