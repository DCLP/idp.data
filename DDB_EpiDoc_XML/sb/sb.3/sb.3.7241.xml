    <lb n="58"/>ἐν <expan>ψω<ex cert="high">μίοις</ex></expan> <expan>μ<ex cert="high">ηνῶν</ex></expan> <num value="5">ε</num> <expan>ἀρτ<ex cert="high">άβαι</ex></expan> <num value="350">τν</num> τοῦ <num value="8">η</num>· <expan>ὀσπρέ<ex cert="high">ου</ex></expan> <expan>ἀρτ<ex cert="high">άβαι</ex></expan> <num value="81">πα</num> <num value="2/3">𐅷</num> τοῦ <num value="10">ι</num>· ἐλαίου <expan>μέ<ex cert="high">τρα</ex></expan> <num value="49">μθ</num> τοῦ <expan>μέ<ex cert="high">τρου</ex></expan> <expan>νο<ex cert="high">μίσματος</ex></expan> <num value="1/3">γ</num> <num value="1/12">ιβ</num>· 

