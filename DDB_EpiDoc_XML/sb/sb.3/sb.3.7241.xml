    <lb n="1"/><gap extent="unknown" reason="lost" unit="line"/> 

    <lb n="1"/>αὐτούς, εἴ τι ἐθεματίσ<supplied cert="high" reason="lost">α</supplied>μεν ἐν τοῖς <unclear cert="high" reason="undefined">ἡ</unclear>μ<supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">έ</supplied><unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">οι</supplied><unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">ἐ</unclear>νταγίο<supplied cert="high" reason="lost">ις</supplied>, 

    <lb n="2"/>αὐτῶν μέντοι παρεχόντων,<g type="apostrophe"/> εἴ τί <add place="supralinear">ἐστι</add> δι᾽ αὐτῶν δημόσιά τε 

    <lb n="3"/>καὶ ἐκστραόρ<g type="apostrophe"/>δινα, <g type="high punctus"/> λαμβάνων ἀντιφωνητὰς αὐτῶ<hi rend="acute">ν</hi> 

    <lb n="4"/>εὐπόρ<g type="apostrophe"/>ους ὀφείλοντας ἀποδοῦναι <g type="apostrophe"/> τὴν ἀντιφών<unclear cert="high" reason="undefined">η</unclear><supplied cert="high" reason="lost">σ</supplied><unclear cert="high" reason="undefined">ι</unclear><supplied cert="high" reason="lost">ν</supplied> <expan>αὐτ<ex cert="high">ῶν</ex></expan>, 

    <lb n="5"/>εἰ δόξῃ τιν<g type="apostrophe"/>ὰ <g type="apostrophe"/> ἀποδρᾶσαι ε᾿ξ αὐτῶν, πέμπων πρὸ<supplied cert="high" reason="lost">ς ἡμᾶς</supplied> 

    <lb n="6"/>τὸ κατάγραφον τῆς ὀνομασίας καὶ <g type="apostrophe"/> <choice><corr>πατρωνυμίας</corr><sic>πατρωνοιμιας</sic></choice> 


    <lb n="7"/>καταχωρίου<unclear cert="high" reason="undefined">ς</unclear> τῶν αὐτ<unclear cert="high" reason="undefined">ῶ</unclear>ν ν<g type="high punctus"/>αυτῶν <g type="apostrophe"/> καὶ τεχνιτῶν, φ<g type="apostrophe"/>ανερο-

    <lb n="8"/>ποιῶν ἡμῖν <g type="apostrophe"/> ἐν τῷ αὐτῷ καταγράφῳ τοὺς <choice><corr>ἐξελθό<supplied cert="high" reason="omitted">ν</supplied>τας</corr><sic>εξελθοτας</sic></choice> 

    <lb n="9"/>ὑπὲρ τῶν ἰδίων χ<supplied cert="high" reason="lost">ω</supplied><unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ί</supplied><unclear cert="high" reason="undefined">ων</unclear> καὶ τοὺς μισθωθέντας ὑπὲρ 

    <lb n="10"/>ἑτέρων, καὶ εἴ τι <choice><corr>ἐδόθ<supplied cert="high" reason="lost">η</supplied></corr><sic>εδωθ<supplied cert="high" reason="lost">η</supplied></sic></choice> <supplied cert="high" reason="lost">ὑπὲρ</supplied> <choice><corr>τῶ<supplied cert="high" reason="omitted">ν</supplied></corr><sic>τω</sic></choice> <g type="high punctus"/> αὐτῶν ὑπὲρ μισθοῦ, μὴ 

    <lb n="11"/><choice><corr>ἐκδίδους</corr><sic>εδκιδων</sic></choice> τινὶ τὸ κ<unclear cert="high" reason="undefined">ε</unclear>φ<unclear cert="high" reason="undefined">ά</unclear>λ<unclear cert="high" reason="undefined">αιο</unclear>ν αὐτῶν, <supplied cert="high" reason="lost">ἀ</supplied><unclear cert="high" reason="undefined">λλὰ</unclear> <gap extent="1" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="12"/>ἀποστέλλων·  <g type="apostrophe"/> τὰς δὲ δαπάνας τὰς <choice><corr>ταγείσας</corr><sic>ταγ<unclear cert="high" reason="undefined">ισας</unclear></sic></choice> διὰ τῆς διοικήσεώς σου 

    <lb n="13"/>ἀπόστειλον εἰς πλῆρε<supplied cert="high" reason="lost">ς π</supplied><unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">π</supplied>οιημένας, <g type="high punctus"/> <choice><corr>τυπώνων</corr><sic>τυπωννων</sic></choice> 

    <lb n="14"/>ἐπάνω <g type="high punctus"/> αὐτῶν <g type="apostrophe"/> ἀ<unclear cert="high" reason="undefined">ν</unclear>θρώπ<supplied cert="high" reason="lost">ους χρησ</supplied><unclear cert="high" reason="undefined">ίμο</unclear><supplied cert="high" reason="lost">υς</supplied> <unclear cert="high" reason="undefined">πι</unclear><supplied cert="high" reason="lost">σ</supplied><unclear cert="high" reason="undefined">τ</unclear>ού<unclear cert="high" reason="undefined">ς</unclear> τε κα<supplied cert="high" reason="lost">ὶ</supplied> 

    <lb n="15"/>εὐπόρους <g type="apostrophe"/> μαρτ<supplied cert="high" reason="lost">υ</supplied>ρ<supplied cert="high" reason="lost">ῶν </supplied><gap extent="3" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">ι</unclear> <supplied cert="high" reason="lost">ὑ</supplied>πὲρ ἑτέρων πιστῶν <supplied cert="high" reason="lost">καὶ</supplied> 

    <lb n="16"/>μὴ εἶναι αὐτοὺς φυρ<supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ὰς ἢ</supplied> ῥᾳδιουρ<g type="apostrophe"/>γούς, <g type="high punctus"/> καὶ ἐπίτρεψο[ν <expan>α]<unclear cert="high" reason="undefined">ὐτ</unclear><ex cert="high">οῖς</ex></expan> 

    <lb n="17"/>πληρωθῆναι <g type="high punctus"/> τὴν τ<supplied cert="high" reason="lost">οιαύτ</supplied><unclear cert="high" reason="undefined">η</unclear><supplied cert="high" reason="lost">ν δαπά</supplied>νην ἐκ τῶν <expan>χωρ<ex cert="high">ίων</ex></expan> 

    <lb n="18"/>μετὰ τοῦ φόβου τοῦ θεοῦ <gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ὅρῳ</supplied> 

    <lb n="19"/>δικαίῳ καὶ σταθμῷ ἴσ<supplied cert="high" reason="lost">ῳ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="20"/>πρακτηθῆναί τινὰ ἐν τ[ῇ τοιαύτῃ διαστολῇ <unclear cert="high" reason="undefined">διὰ</unclear>] 

    <lb n="21"/>τὴν διοίκησίν σου καὶ <unclear cert="high" reason="undefined">ἐπι</unclear>σ<unclear cert="high" reason="undefined">υνα</unclear>πτομ<unclear cert="high" reason="undefined">έ</unclear>νω<supplied cert="high" reason="lost">ν</supplied> <gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="22"/>πάντων διὰ σεαυτοῦ μὴ καταθαρρῶν <g type="high punctus"/> τινί. <choice><corr>καὶ αὐτῶν</corr><sic>καυτων</sic></choice> 

    <lb n="23"/>εἰς πλῆρες ὑποδεχομένων τὴν τοιαύτην δαπάνην 

    <lb n="24"/>ὡς εἴρηται ἐκ τῶν χωρ<supplied cert="high" reason="lost">ίων κα</supplied><unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">α</supplied>σκεύασον ποιῆσ<supplied cert="high" reason="lost">αι</supplied> <expan><unclear cert="high" reason="undefined">αὐ</unclear>τ<ex cert="high">οῖς</ex></expan> 

    <lb n="25"/>ἀποδείξεις ὑπὲρ αὐτῆς, <unclear cert="high" reason="undefined">γ</unclear><supplied cert="high" reason="lost">ρ</supplied>άφων ἡμῶν τὸ <hi rend="diaeresis">ἴ</hi>σον τῆς παραδοθείσης 

    <lb n="26"/>δαπάνης ἑκάστῳ πιστικῷ καὶ πέμπων. μὴ εὕρωμέν σε δὲ 

    <lb n="27"/>πέμψαντα ἀπαργυρισμὸν <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ὸ</supplied> σύνολον ὑπ<unclear cert="high" reason="undefined">ὲρ</unclear> <gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">τ</unclear>ο<unclear cert="high" reason="undefined">ι</unclear><supplied cert="high" reason="lost">ο</supplied>υ <gap extent="2" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/> 

    <lb n="28"/>εἰ μὴ <g type="apostrophe"/> αὐτὸ τὸ σωμάτιον· <supplied cert="high" reason="lost">ὑ</supplied><unclear cert="high" reason="undefined">π</unclear>ὲρ τούτων <choice><corr>ἀποστρέφοντες</corr><sic>αποστρεφωντες</sic></choice> <expan>ἀποδίδουμέ<ex cert="high">ν</ex></expan> σοι 

    <lb n="29"/>ἀνταπόδωσιν βλάπτουσάν σε·<g type="high punctus"/> οὐκ ἔχεις γὰρ ἀφορμὴν τὴν οἵαν οὖν 

    <lb n="30"/>περὶ τοῦ σωματίου. ἐνδρανῶς <add place="supralinear">δὲ</add> ἐνέχθητι εἰς τὸ πρᾶγμα τῶν 

    <lb n="31"/>δαπανῶν, κρατῶν τοὺς τῶν χωρίων ἀποδοῦναι, εἴ τι ἐτάγη δι᾽ <expan>αὐτ<ex cert="high">ῶν</ex></expan> 

    <lb n="32"/>ἐν εἴδεσιν, <g type="apostrophe"/> ἄνευ μέντοι τοῦ ἑψήματος· τοῦτο γὰρ <choice><corr>ὡρίσαμεν</corr><sic>ορισαμεν</sic></choice> σταλ<unclear cert="high" reason="undefined">ῆν</unclear>αι 

    <lb n="33"/>ἐν ἀπαργυρισμῷ πρὸς <add place="supralinear">τὴν</add> <subst><add place="inline">ἐντεθεῖσαν</add><del rend="corrected" status="unremarkable">εντηθεισαν</del></subst> <g type="apostrophe"/> <choice><corr>διατίμησιν</corr><sic>διατημισιν</sic></choice> ἐν τοῖς <choice><corr>ἡμετ<supplied cert="high" reason="omitted">έ</supplied>ροις</corr><sic>ημετροις</sic></choice> <expan>ἐντ<ex cert="high">α</ex>γ<ex cert="high">ίοις</ex></expan>· 

    <lb n="34"/>λοιπὸν μὴ <g type="apostrophe"/> πέμψῃς ἕψημα τὸ σύνολον, ει᾿ μὴ <g type="apostrophe"/> τὸν ἀπαρ<g type="high punctus"/>γυρισμόν, 

    <lb n="35"/>οὐ μὴν ἀλλὰ καὶ <g type="apostrophe"/> εἴ τι δ᾽ ἂν ἀδυνατήσωσιν δοῦναι ἐν εἴδεσιν <g type="high punctus"/> 

    <lb n="36"/>ἔκ τε τῆς ἁλυκῆς <g type="high punctus"/> καὶ τοῦ ὄξ<supplied cert="high" reason="lost">ο</supplied>υς, μὴ συγχωρῶν ἀνυσθῆναί τι ἐκ περιττοῦ 

    <lb n="37"/>τῆς <choice><corr>θεματισθείσης</corr><sic>θεματισθησης</sic></choice> διατιμήσεως, <g type="high punctus"/> πέμπων <choice><corr>τὸ<supplied cert="high" reason="omitted">ν</supplied></corr><sic>το</sic></choice> ἀνυόμενον 

    <lb n="38"/>ἀπαργυρισμὸν <hi rend="diaeresis">ὑ</hi>πὲρ δαπάνης μετὰ <choice><corr>ἀνθρώπου</corr><sic>ανθρωπω</sic></choice> σου πιστοῦ ὀφείλοντος 

    <lb n="39"/>καταβαλέσθαι <g type="high punctus"/> εἰς πλῆρες <g type="high punctus"/> καὶ μηδὲν <g type="high punctus"/> παρέχων τοῖς ὑποδ<unclear cert="high" reason="undefined">εχ</unclear>ομένοις 

    <lb n="40"/>πιστικοῖς <g type="high punctus"/> τὴν λεχθεῖσαν δαπάνην ὑπὲρ ἀπαρ<g type="high punctus"/>γυρισμοῦ, ἀλλὰ τὰ μάλιστα 

    <lb n="41"/>τούτους ἐξασφαλιζόμενος δεόντως <g type="high punctus"/> δι᾽ ἐγγράφου <unclear cert="high" reason="undefined">ὁμ</unclear><supplied cert="high" reason="lost">ο</supplied>λ<supplied cert="high" reason="lost">ογ</supplied><unclear cert="high" reason="undefined">ί</unclear><supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">ς</unclear> 

    <lb n="42"/>αὐτῶν καὶ μαρ<g type="apostrophe"/>τυρῶν, ὅπως <hi rend="diaeresis">ἰ</hi>διοκινδύνως παραδώσωσιν 

    <lb n="43"/>τὸ <choice><corr>παραδοθὲν</corr><sic>παραδωθεν<g type="apostrophe"/></sic></choice> αὐτοῖς, <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">α</supplied>χωρίων παριστούντ<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">ν σο</supplied><unclear cert="high" reason="undefined">ι</unclear><g type="apostrophe"/> 

    <lb n="44"/>ἀντιφωνητὰς χρησίμους <g type="high punctus"/> καὶ εὐπόρους <g type="high punctus"/> τούτου ἕνεκα. 

    <lb n="45"/>μὴ ἀμελῇς <choice><corr>δὲ</corr><sic>τε</sic></choice> εἰς τὸ ἀποστεῖλαι διὰ πάσης συντομία<supplied cert="high" reason="lost">ς</supplied>, εἴ τι <g type="apostrophe"/> 

    <lb n="46"/>διεστείλαμεν διὰ τῆς διοικήσεώς σου λόγῳ τοῦ αὐτοῦ κούρσου 

    <lb n="47"/>ἀπό τε ναυτῶν καὶ τεχνιτῶν καὶ δαπανῶν·<g type="apostrophe"/> ἐπετρέψαμεν γὰρ<g type="apostrophe"/> τῷ 

    <lb n="48"/>παρόντι ἀποστόλῳ ἡμῶν μὴ ἀποστῆναι ἐκ σοῦ καί ἐστι διὰ σοῦ 

    <lb n="49"/>τί ποτε τὸ καθόλου ἐν <choice><corr>λοιπάδι</corr><sic>λοιπαδει</sic></choice>. λοιπὸν μὴ γένηται <choice><corr>ἀμέλεια</corr><sic>αμελι<unclear cert="high" reason="undefined">α</unclear>ς</sic></choice> 

    <lb n="50"/>καὶ <choice><corr>ὑστερηεθῇ</corr><sic>υστερεθη</sic></choice> τι· ἔσῃ γὰρ ἐ<unclear cert="high" reason="undefined">π</unclear>ιστάμενος, <g type="high punctus"/> ὡς, εἰ ἀπομείνῃ τι <g type="apostrophe"/> ἐν <g type="apostrophe"/> 

    <lb n="51"/><choice><corr>λοιπάδι</corr><sic>λοιπαδει</sic></choice>, μέλλομεν κελεύσει θεοῦ ἀπαιτῆσαί σε αὐτὸ ἐν διπλῇ 

    <lb n="52"/>ποσότητι ὑπὲρ παρακοῆς καὶ καταφρονήσεως. <choice><corr><add place="supralinear">γιγνώσκων</add></corr><sic>γιν<g type="apostrophe"/>ωσκων<g type="high punctus"/></sic></choice> τοίνυν, <g type="apostrophe"/> 

    <lb n="53"/>ὡς ἐπισπούδαστόν ἐστι τὸ πρᾶγμα τοῦ τοιούτου κούρσου, <supplied cert="high" reason="lost">ἀ</supplied>μελλητὶ 

    <lb n="54"/>ἐκτέλεσον, <g type="apostrophe"/> μὴ <choice><corr>δίδους</corr><sic>διδων</sic></choice> κατὰ σοῦ τὴν οἵαν οὖν ἀφορμήν. <g type="apostrophe"/> 

