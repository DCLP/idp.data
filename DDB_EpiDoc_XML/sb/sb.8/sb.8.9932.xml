    <lb n="1"/><g type="stauros"/> βασιλείας τοῦ θειοτάτου ἡ<supplied cert="high" reason="lost">μῶν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>ἔτους ἑπτακαιδεκάτου, <unclear cert="high" reason="undefined">μ</unclear><supplied cert="low" reason="lost">ηνὸς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/><g type="stauros"/> Φλαουίῳ Γερμανῷ τῷ λ<supplied cert="high" reason="lost">αμπροτάτῳ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τοῦ τῆς</supplied> 

    <lb n="4"/>λαμπρᾶς μνήμης Φ<gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Ὑπερέχιος Δωροθέου πρεσβύτερος</supplied> 

    <lb n="5"/>ἀπὸ τῆς Ἑρμου<unclear cert="high" reason="undefined">πο</unclear>λιτῶν <gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ὁμολογῶ μεμισθῶσθαι παρὰ</supplied> 

    <lb n="6"/>τῆς σῆς λαμπρό<supplied cert="high" reason="lost">τητος, ἐφ᾽ ὃν βούλει χρόνον, λογιζόμενον ἀπὸ τῆς σήμερον,</supplied> 

    <lb n="7"/>ἥτις ἐστὶν Τῦβι ὀγ<supplied cert="high" reason="lost">δόη τῆς παρούσης δευτέρας ἰνδικτίωνος τὸ ὑπάρχον σοι</supplied> 

    <lb n="8"/>στάβλον ὁλόκληρον <gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> σὺν</supplied> 

    <lb n="9"/>καμάρᾳ καὶ λοι<unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">οῖς χρηστηρίοις πᾶσιν καὶ δικαίοις ἐξ ὅλου τοῦ στάβλου δια</supplied>-

    <lb n="10"/>κείμενον ἐπὶ τὴς αὐ<supplied cert="high" reason="lost">τῆς Ἑρμοῦ πόλεως </supplied><gap extent="25" reason="lost" unit="character"/><supplied cert="high" reason="lost"> πρὸς ἐμὴν</supplied> 

    <lb n="11"/>χρῆσιν, <choice><corr>ἐνοικίου</corr><sic>ενοικειου</sic></choice> <unclear cert="high" reason="undefined">κατὰ</unclear> <supplied cert="high" reason="lost">μῆνα χρυσίου νομισματίου </supplied><gap extent="9" reason="lost" unit="character"/><supplied cert="high" reason="lost">, ὅπερ ἐνοίκιον ἀπο</supplied>-

    <lb n="12"/>δώσω τῷ ὑμετέρῳ εὐ<supplied cert="low" reason="lost">δοκίμῳ</supplied><gap extent="7" reason="lost" unit="character"/><supplied cert="high" reason="lost"> πρὸς λῆξιν ἑκάστου μηνός, καὶ ὁπόταν βου</supplied>-

    <lb n="13"/>ληθείη ἡ σὴ λαμπρ<unclear cert="high" reason="undefined">ότ</unclear><supplied cert="high" reason="lost">ης ἔχειν, παραδώσω σοι τὸ αὐτὸ στάβλον σὺν ἐφεστώσαις</supplied> 

    <lb n="14"/>αὐτῷ θύραις δυσι <num value="2"/> <del rend="erasure" status="unremarkable">η</del> ἐν <supplied cert="high" reason="lost">μηδενὶ καταβλάψας. ἡ μίσθωσις κυρία καὶ <expan>ἐπερωτ<ex cert="high">ηθεὶς</ex></expan> <expan>ὡμολόγ<ex cert="high">ησα</ex></expan>.</supplied> 

    <lb n="15"/><handShift new="m2"/> <g type="stauros"/> Ὑπερέχιος Δωροθ<supplied cert="high" reason="lost">έου ἀπὸ <expan>Ἑρ<ex cert="high">μοῦ πόλεως</ex></expan> μαρτυρῶ τῇ μισθώσει. <handShift new="m3"/> <g type="stauros"/></supplied><gap extent="11" reason="lost" unit="character"/> 

    <lb n="16"/><expan><unclear cert="high" reason="undefined">Ἀν</unclear>τιόχο<ex cert="high">υ</ex></expan> ἀπ<unclear cert="high" reason="undefined">ὸ</unclear> <expan>Ἑρ<ex cert="high">μοῦ πόλεως</ex></expan> μαρτ<supplied cert="high" reason="lost">υρῶ τῇ μισθώσει. <handShift new="m4"/> <g type="stauros"/></supplied><gap extent="11" reason="lost" unit="character"/> 

    <lb n="17"/>Ἀμμωνίου <expan>ὑπ<unclear cert="high" reason="undefined">οδι</unclear><supplied cert="high" reason="lost">ά</supplied><unclear cert="high" reason="undefined">κ</unclear><ex cert="high">ονος</ex></expan> <expan><unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">ρτ</unclear><supplied cert="high" reason="lost">υ</supplied><unclear cert="high" reason="undefined">ρ</unclear><ex cert="high">ῶ</ex></expan> <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ῇ μισθώσει. <handShift new="m5"/> <g type="stauros"/> </supplied><gap extent="9" reason="lost" unit="character"/> 

    <lb n="18"/>Σ<supplied cert="high" reason="lost">α</supplied>ραπίω<supplied cert="high" reason="lost">νος ἀ</supplied><unclear cert="high" reason="undefined">π</unclear>ὸ <expan>Ἑρ<ex cert="high">μοῦ πόλεως</ex></expan> μαρτυρῶ τῇ <supplied cert="high" reason="lost">μισθώσει. </supplied><gap extent="unknown" reason="lost" unit="character"/> 

