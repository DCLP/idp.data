    <lb n="11"/>ἀρουρῶν πέντε τ<unclear>έ</unclear><supplied reason="lost">ταρτον <num value="5"/><num value="1/4"/>  </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>ἐπὶ τοῦ <choice><corr>Νεοφύτου</corr><sic>νεωφυτου</sic></choice> χ<supplied reason="lost">ωρίου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

