    <lb n="1"/>[<g type="stauros"/> ἐν ὀνόμ]<unclear cert="high" reason="undefined">α</unclear>τι <supplied cert="high" reason="lost">τ</supplied>οῦ κυρίου καὶ <supplied cert="high" reason="lost">δεσπότου Ἰησοῦ Χριστοῦ τοῦ θεοῦ καὶ σ</supplied>ωτῆρος

    <lb n="2"/>ἡμῶν καὶ τῆ<supplied cert="high" reason="lost">ς</supplied> δεσποίνης ἡμ<supplied cert="high" reason="lost">ῶν τῆς ἁγίας θεοτόκου καὶ ἀει</supplied><unclear cert="high" reason="undefined">παρ</unclear>θένου

    <lb n="3"/><supplied cert="high" reason="lost">Μα</supplied>ρίας καὶ <supplied cert="high" reason="lost">πά</supplied>ντων τῶν ἁγί<supplied cert="high" reason="lost">ων </supplied><gap extent="unknown" reason="lost" unit="character"/> <expan>ἰ<unclear cert="high" reason="undefined">ν</unclear><ex cert="high">δικτίωνος</ex></expan> <g type="stauros"/>

    <lb n="4"/>ἡμεῖς Ἀβραάμι<supplied cert="high" reason="lost">ος</supplied> <unclear cert="high" reason="undefined">υ</unclear>ἱὸς Ἰωάννου ν[ <gap extent="unknown" reason="illegible" unit="character"/> καὶ <gap extent="unknown" reason="illegible" unit="character"/> ]ος <expan>πρ<unclear cert="high" reason="undefined">αγ</unclear>μ<ex cert="high">α</ex>τ<ex cert="high">ευτὴς</ex></expan>

    <lb n="5"/>υἱὸς Ἀναστασίου <supplied cert="high" reason="lost">ὁρμ</supplied><unclear cert="high" reason="undefined">ώ</unclear>μενοι ἀ<supplied cert="high" reason="lost">π᾽ Ἀρσινοιτῶν πόλεως ἀ</supplied><unclear cert="high" reason="undefined">π</unclear>᾽  <expan>ἀμφ<ex cert="high">ό</ex>δ<ex cert="high">ου</ex></expan>

    <lb n="6"/>Ἀβρααμ<gap extent="10" reason="lost" unit="character"/>νω Ἀρσ<unclear cert="high" reason="undefined">ιν</unclear><supplied cert="high" reason="lost">ο</supplied><gap extent="3" reason="lost" unit="character"/>οικ<unclear cert="high" reason="undefined">ο</unclear><gap extent="8" reason="lost" unit="character"/><supplied cert="high" reason="lost"> μαρτυ</supplied><unclear cert="high" reason="undefined">ροῦ</unclear>μεν Φὶβ

    <lb n="7"/>υἱὸν Νααραῦ ἀπὸ κώμης Πελκεήσε<unclear cert="high" reason="undefined">ως</unclear> καὶ Κι<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">μοῦλ υἱὸν </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> πο</supplied>τὲ μὲν ἀπὸ

    <lb n="8"/>κώμης Παρεμβολῆς τοῦ Ἀφροδιτοπολίτου <unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">ομοῦ νῦν δὲ τὴ</supplied><unclear cert="high" reason="undefined">ν</unclear> οἴκησιν <expan>ἔχ<ex cert="high">οντα</ex></expan>

    <lb n="9"/>ἐπὶ τῆς αὐτῆς κώμης Πελκεήσεως ἐπιλέξα<supplied cert="high" reason="lost">σθαι ἡμᾶς ἐκ τοῦ</supplied> <expan>κομπρομ<ex cert="high">ίσσου</ex></expan>

    <lb n="10"/>μετὰ προστίμου χρυσοῦ νομισμάτ<supplied cert="high" reason="lost">ων</supplied> δύο <num value="2"/> ἀκο<supplied cert="high" reason="lost">λούθως τοῖς τῆς ὑ</supplied><unclear cert="high" reason="undefined">π</unclear>οθέσεως

    <lb n="11"/>καὶ ἀπαλλάξ<unclear cert="high" reason="undefined">ειν</unclear> αὐτούς, καὶ δὴ ἀκροα<supplied cert="high" reason="lost">σά</supplied><unclear cert="high" reason="undefined">μ</unclear>ενοι τῶ[ν μεταξὺ αὐτῶν <expan>ἀ]μφιβαλλομ<ex cert="high">ένων</ex></expan>

    <lb n="12"/>καὶ ἐγνωκότες πᾶσαν τὴν τοῦ κατ᾽ αὐτοῦ πράγ<supplied cert="high" reason="lost">ματος ἀλήθει</supplied><unclear cert="high" reason="undefined">α</unclear>ν, καθὼς

    <lb n="13"/>ὁ <supplied cert="high" reason="lost">δ</supplied><unclear cert="high" reason="undefined">εσπό</unclear>της θεὸς <choice><corr>ἐχωρήγησεν</corr><sic>εχορηγησεν</sic></choice> τῷ ταπεινῷ ἡμῶν <unclear cert="high" reason="undefined">λ</unclear><supplied cert="high" reason="lost">ογισμ</supplied><unclear cert="high" reason="undefined">ῷ</unclear>, <unclear cert="high" reason="undefined">δί</unclear>καιον

    <lb n="14"/>σ<supplied cert="high" reason="lost">υν</supplied><unclear cert="high" reason="undefined">εί</unclear>δα<supplied cert="high" reason="lost">μεν ὥ</supplied>στε αὐτο<unclear cert="high" reason="undefined">ὺ</unclear>ς παραγενέσθαι ἐπὶ τῆς <supplied cert="high" reason="lost">αὐτῆς κώμ</supplied><unclear cert="high" reason="undefined">ης</unclear> <expan><unclear cert="high" reason="undefined">Π</unclear>ελκεήσε<ex cert="high">ως</ex></expan>

    <lb n="15"/>κ<unclear cert="high" reason="undefined">αὶ</unclear> <unclear cert="high" reason="undefined">ἐπ</unclear>ερ<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">τῆσαι</supplied> Κοσμᾶν <supplied cert="high" reason="lost">υἱὸν</supplied> <unclear cert="high" reason="undefined">Ψο</unclear>ύρου καὶ Οὐενά<unclear cert="high" reason="undefined">φ</unclear><supplied cert="high" reason="lost">ριο</supplied><unclear cert="high" reason="undefined">ν</unclear> <unclear cert="high" reason="undefined">υἱ</unclear><supplied cert="high" reason="lost">ὸ</supplied><unclear cert="high" reason="undefined">ν</unclear> Φ<supplied cert="high" reason="lost">οιβά</supplied>μμωνος

    <lb n="16"/>καὶ <supplied cert="high" reason="lost">Μ</supplied><unclear cert="high" reason="undefined">α</unclear>κ<unclear cert="high" reason="undefined">άρ</unclear><supplied cert="high" reason="lost">ιον</supplied> ἑτέρου Φ<unclear cert="high" reason="undefined">ο</unclear>ι<unclear cert="high" reason="undefined">β</unclear>άμμωνος, καὶ ἐὰν εἴπ<unclear cert="high" reason="undefined">ω</unclear>σιν ὅτι μ<unclear cert="high" reason="undefined">αρ</unclear>τυροῦμεν

    <lb n="17"/>ἵνα <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">αρ</supplied><unclear cert="high" reason="undefined">άσχ</unclear><supplied cert="high" reason="lost">ῃ ὁ</supplied> <unclear cert="high" reason="undefined">εἰρ</unclear>ημένος Φὶβ τῷ αὐτῷ Κιαμοῦλ χρ<unclear cert="high" reason="undefined">υ</unclear>σοῦ νομίσματα πέντε <num value="5"/>

    <lb n="18"/>δίχα <choice><corr>ἀμφιβολίας</corr><sic>αμ<unclear cert="high" reason="undefined">φι</unclear>βολειας</sic></choice>, ἐφ᾽ ᾧ τὸν εἰρημένον Φὶβ τα<unclear cert="high" reason="undefined">ῦ</unclear>τ<unclear cert="high" reason="undefined">α</unclear> ἀποδοῦναι τῷ αὐτῷ Κ<unclear cert="high" reason="undefined">ια</unclear>μοῦλ,

    <lb n="19"/>εἰ δὲ μὴ ο<unclear cert="high" reason="undefined">ὕτ</unclear>ως μαρτυρήσωσιν, ἐφ᾽ ᾧ τὰ δύο <num value="2"/> μ<unclear cert="high" reason="undefined">έρ</unclear>η πρὸς ὃ λ<unclear cert="high" reason="undefined">έγ</unclear>ουσιν οἱ <expan>εἰρημέ<ex cert="high">νοι</ex></expan>

    <lb n="20"/>τρεῖς <num value="3"/> ἄνδρες περὶ τοῦ αὐτοῦ χρέους ἐξακολου<unclear cert="high" reason="undefined">θῆ</unclear>σαι τῇ φωνῇ αὐτῶν. ο<unclear cert="high" reason="undefined">ὕ</unclear>τως γὰρ

    <lb n="21"/>συνείδαμεν δίκαιον εἶναι. <g type="stauros"/>

    <lb n="22"/><g type="stauros"/> δι᾽ ἐμ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ῦ</supplied> Ἰ<unclear cert="high" reason="undefined">ούστο</unclear>υ συμβολαιογράφου ταύτης τῆς <expan>Ἀρσινοιτ<ex cert="high">ῶν</ex></expan>

    <lb n="23"/>πόλεως ἐγράφη ὁ παρὼν ὅρος ἐκ φωνῆς τῶν εἰρημένων

    <lb n="24"/>δι<unclear cert="high" reason="undefined">κ</unclear>α<unclear cert="high" reason="undefined">στῶ</unclear>ν. <g type="unintelligible"/> <g type="stauros"/></ab></div></body></text></TEI.2>