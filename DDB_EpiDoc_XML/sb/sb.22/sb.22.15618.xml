    <lb n="2"/><supplied cert="high" reason="lost">Αὐρηλία Ἰουλιανὴ θυγάτηρ Ὕμιος τοῦ δεῖνα μητρὸς τῆς δεῖνα ἀπὸ κώμης Ἀλαβ</supplied><unclear cert="high" reason="undefined">αστρί</unclear><supplied cert="high" reason="lost">ν</supplied><unclear cert="high" reason="undefined">η</unclear><supplied cert="high" reason="lost">ς τοῦ Ἀντινοίτου</supplied> νομ<supplied cert="high" reason="lost">ου Αὐ</supplied>ρη<unclear cert="high" reason="undefined">λ</unclear>ί<unclear cert="high" reason="undefined">ῳ</unclear> <choice><corr>Σερήνῳ</corr><sic>σε<supplied cert="high" reason="lost">ρη</supplied><unclear cert="high" reason="undefined">ν</unclear>ος</sic></choice> <unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">αύτ</supplied><unclear cert="high" reason="undefined">ῃ</unclear> <note anchored="yes" lang="en" place="not specified">?</note>

    <lb n="3"/><supplied cert="high" reason="lost">ἀπὸ τῆς αὐτῆς κώμη</supplied><unclear cert="high" reason="undefined">ς</unclear> χαίρειν. <supplied cert="high" reason="lost">ὁμ</supplied>ολ<supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">γ</unclear><supplied cert="high" reason="lost">ῶ πεπ</supplied><unclear cert="high" reason="undefined">ρ</unclear>ακέναι σ<supplied cert="high" reason="lost">οι</supplied> κα<supplied cert="high" reason="lost">ὶ κ</supplied><unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ταγεγρ</supplied><unclear cert="high" reason="undefined">αφ</unclear>ηκέναι σοι <supplied cert="high" reason="lost">ἀπὸ τοῦ νῦν κ</supplied>υριευτικῶς <unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">ἰς</supplied> <unclear cert="high" reason="undefined">τ</unclear>ὸν ἀεὶ ἅπ<supplied cert="high" reason="lost">α</supplied>ντα χρόν<supplied cert="high" reason="lost">ο</supplied>ν <sic>σου</sic>

    <lb n="4"/><supplied cert="high" reason="lost">τὸ ὑπάρχον</supplied> μοι καὶ <choice><corr>ἐλθὸν</corr><sic>ελθων</sic></choice> εἰς ἐμὲ <unclear cert="high" reason="undefined">ἀ</unclear><supplied cert="high" reason="lost">πὸ</supplied> <choice><corr>δικαίου</corr><sic>δοκαιου</sic></choice> κληρο<supplied cert="high" reason="lost">νο</supplied><unclear cert="high" reason="undefined">μ</unclear>ίας τοῦ πατρός μου πρότερον τῶν ἀπὸ κώμης <unclear cert="high" reason="undefined">Τ</unclear>ερύθε<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">ς</supplied> Ἐβύ<add place="supralinear">θ</add>εω<unclear cert="high" reason="undefined">ς</unclear> τοῦ <sic>ερ</sic>

    <lb n="5"/><unclear cert="high" reason="undefined">Ἑ</unclear>ρμουπ<unclear cert="high" reason="undefined">ο</unclear>λίτου <supplied cert="high" reason="lost">ν</supplied>ομοῦ περὶ πρακτορίαν κ<unclear cert="high" reason="undefined">ώ</unclear><supplied cert="low" reason="lost">μης </supplied> <space dim="horizontal" extent="unknown" unit="character"/> ὅσου ἐστὶν ἀρουρῶν σὺν <choice><corr>τῇ χερσασπόρῳ</corr><sic>τω χερσασπορου</sic></choice> κα<supplied cert="high" reason="lost">ὶ σ</supplied>ὺν τῷ λά<unclear cert="high" reason="undefined">κ</unclear>κῳ

    <lb n="6"/>καὶ<choice><corr>παντοίοις δένδροις</corr><sic>παντοιων δενδρων</sic></choice>, οὗ <choice><corr>γείτωνες</corr><sic>γιτωνες</sic></choice> καθὼς ἡ <choice><corr>πωλοῦσα ὑπηγόρευσε</corr><sic>πολου<unclear cert="high" reason="undefined">ν</unclear>τα υπηγορευσαμεν</sic></choice> βορρᾶ <choice><corr>Δίου</corr><sic>διος</sic></choice> Πευὼτ ἀπὸ Ἀλαβαστρίνης, λι<supplied cert="high" reason="lost">β</supplied>ὸς κοιν<unclear cert="high" reason="undefined">ο</unclear>ῦ

    <lb n="7"/>τῆς κώμης Τευὼ <choice><corr>Νεανίσκων</corr><sic>νεανισκου</sic></choice> τοῦ Ἑρμοπολίτου νομοῦ, <unclear cert="high" reason="undefined">ν</unclear>ό<unclear cert="high" reason="undefined">τ</unclear>ου <choice><corr>Βήκιος</corr><sic>βηκις</sic></choice> Ὕμιος ἀπὸ τῆς αὐτῆς <supplied cert="high" reason="lost">Τ</supplied>ευώ, ἀπηλιώτου τοῦ λάκ<supplied cert="high" reason="lost">κ</supplied>ου <choice><corr>γεωργῶν</corr><sic>γε<unclear cert="high" reason="undefined">ωρ</unclear>γου</sic></choice>

    <lb n="8"/>συνόδου κώμης Ἀλαβαστρίνης, τιμῆς καὶ παραχωρ<supplied cert="high" reason="lost">η</supplied>τικοῦ <choice><corr>τῶν</corr><sic>του</sic></choice> πρὸς ἀλλήλους συμπεφωνημένων χρυσοῦ <choice><corr>νομισματίων εὐστάθμων</corr><sic>νομισ<unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">α</supplied>τια ευ<unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">τα</supplied>θμα</sic></choice>

    <lb n="9"/>ἀριθμῷ εἴκοσι πέντε, <expan><ex cert="high">γίνεται</ex></expan> <expan>χρυ<ex cert="high">σοῦ</ex></expan> <expan>νο<ex cert="high">μισμάτια</ex></expan> <num value="25">κε</num>, <choice><corr>ἅπερ</corr><sic>ασπερ</sic></choice> αὐτόθι <unclear cert="high" reason="undefined">ἀ</unclear>πέσχη<unclear cert="high" reason="undefined">κ</unclear>α παρὰ σοῦ τὴν τιμὴν ἐκ πλ<supplied cert="high" reason="omitted">ή</supplied>ρους διὰ χειρός, <choice><corr>κρατεῖν</corr><sic>κρατιν</sic></choice>οὖν σε καί κυριεύειν

    <lb n="10"/>σὺν τέκνοις καὶ ἐγκόνοις καὶ <choice><corr>τοῖς</corr><sic>της</sic></choice> παρ᾽ <choice><corr>ἐμοῦ</corr><sic>εμη</sic></choice> <unclear cert="high" reason="undefined">π</unclear>αραλημψομέ<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">ο</supplied>ις <choice><corr>τὸ πεπραμένον σοι</corr><sic>των πεπραμενων συ</sic></choice> ὑπ᾽ <choice><corr>ἐμοῦ</corr><sic>εμη</sic></choice> ὅσου ἐστὶν ἀρουρῶν σὺν τῷ χέρσ<supplied cert="high" reason="lost">ῳ</supplied> καὶ τῷ

    <lb n="11"/>λάκκῳ <supplied cert="high" reason="omitted">καὶ</supplied> σὺν τῇ <choice><corr>αὐλυδρίῳ</corr><sic>α<unclear cert="high" reason="undefined">υ</unclear>ληδριω</sic></choice> <unclear cert="high" reason="undefined">ὡς</unclear> <choice><corr>πρόκειται</corr><sic>προκιται</sic></choice> εἰ<unclear cert="high" reason="undefined">ς</unclear> τὸν ἀεὶ χρόνον καὶ τὰ ἐξ αὐτῶν περιγιγνόμενα ἀπὸ κανό<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">ο</supplied>ς τῆς εὐτυχοῦς ἑνδε<unclear cert="high" reason="undefined">κά</unclear>της

    <lb n="12"/>ἰνδικτίονος, τελοῦντα δὲ τὰ ὑπὲρ αὐτῶν τελούμενα ἐτήσια δημόσια τελέσματα πάντα <sic>πάντα</sic> παντοῖα ἀπὸ κανόνος τῆς ε<supplied cert="high" reason="lost">ὐ</supplied>τυχοῦς

    <lb n="13"/>ἑνδεκάτης <expan><unclear cert="high" reason="undefined">ἰ</unclear>νδικτί<ex cert="high">ονος</ex></expan> καί <choice><corr>σοι εἴη</corr><sic>συ ιη</sic></choice> καὶ <choice><corr>ἐξουσία</corr><sic>εξουσι<unclear cert="high" reason="undefined">α</unclear>ν</sic></choice> χρ<unclear cert="high" reason="undefined">ᾶσ</unclear>θαι καὶ <choice><corr>γεωργεῖν</corr><sic>γεωργουντα</sic></choice> καὶ <choice><corr>διοικεῖν</corr><sic>διοικουντα</sic></choice> περὶ αὐτοῦ καὶ ἑτέροις <choice><corr>παραχωρεῖν</corr><sic>παραχωριν</sic></choice> ᾧ ἐ<unclear cert="high" reason="undefined">ὰν</unclear> αἱρῇ τρό-

    <lb n="14"/>πῳ ἀνεμποδίστως ἐφ᾽ ὃ καὶ μὴ ἐπιπορεύεσθαί <choice><corr><app type="alternative"><lem>σέ με</lem><rdg>ἐμε μηδὲ</rdg></app></corr><sic>σε μη</sic></choice> <sic><unclear cert="high" reason="undefined">μη</unclear></sic> ἄλλον τινὰ ὑπὲρ <choice><corr>ἐμοῦ</corr><sic>σου</sic></choice> τρόπῳ μηδενί, ἀλλὰ καὶ ἐπάναγκες <choice><corr>παρέξομαί</corr><sic>παρεξομε</sic></choice> <supplied cert="high" reason="lost">σ</supplied>οι <choice><corr>τοῦτο</corr><sic>τουτου</sic></choice>

    <lb n="15"/>βέβαιον ἀπὸ πάντων πάσῃ <choice><corr>βεβαιώσει</corr><sic>βαιβεωσω</sic></choice> καὶ καθαρ<unclear cert="high" reason="undefined">ὸ</unclear><supplied cert="high" reason="lost">ν</supplied> ἀπό <choice><corr>τε</corr><sic>δη</sic></choice> δημοσία<supplied cert="high" reason="omitted">ς</supplied> καὶ <sic>οι</sic> ἰδιωτικῆς <choice><corr>ὀφειλῆς</corr><sic>οφιλης</sic></choice> καὶ κατοχῆς καὶ οὑντινοσοῦν

    <lb n="16"/>ἄλλου διὰ παντὸς καὶ ἀπὸ παντὸς καὶ πάντα τὸν ἐπελευσόμενον ἢ ἐμποιησόμενον τούτου χάριν ἀποστήσω παραχ<supplied cert="high" reason="lost">ρ</supplied>ῆμα <choice><corr>τοῖς</corr><sic>της</sic></choice> ἰδ<unclear cert="high" reason="undefined">ί</unclear><supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ι</unclear>ς <choice><corr>μου</corr><sic>σου</sic></choice> ἀνα-

    <lb n="17"/>λώμασι καὶ <choice><corr>δαπανήμασι</corr><sic>δ<unclear cert="high" reason="undefined">α</unclear>πανηματα</sic></choice> καὶ διπλώνω τὴν τιμὴν ὡς ἴδιον χρέως. περὶ δὲ τοῦ ταῦτα οὕτως ὀρθῶς καλῶς <choice><corr>πεπρᾶχθαι</corr><sic>πεπρακχθαι</sic></choice> <sic>καὶ</sic> ἐπερωτη-

    <lb n="18"/>θεῖσ<supplied cert="high" reason="omitted">α</supplied> ὡμολόγησα. ἡ πρᾶσις κυρία πανταχοῦ ἐπιφερομέ<unclear cert="high" reason="undefined">νη</unclear> πρὸς <choice><corr>ἀσφάλειαν</corr><sic>ασφαλιαν</sic></choice>. <handShift new="m2"/> Αὐρηλία <supplied cert="high" reason="omitted">Ἰουλια</supplied>νὴ <choice><corr>θυγάτηρ</corr><sic>θυκατρος</sic></choice> Ὕμιος ἡ προκειμένη

    <lb n="19"/>πέπρακα <choice><corr>τὸ</corr><sic>δ<unclear cert="high" reason="undefined">ο</unclear></sic></choice> προκείμενον <choice><corr>γεώργιον</corr><sic>γεωρικο<unclear cert="high" reason="undefined">ν</unclear></sic></choice> ὡς περι<supplied cert="high" reason="lost">έ</supplied>χει σὺν τῷ λάκκῳ καὶ <choice><corr>ἀπέσχον</corr><sic>απεσχυ</sic></choice> τὴν τιμὴν ἐκ <supplied cert="high" reason="omitted">π</supplied>λήρου<supplied cert="high" reason="lost">ς</supplied>

    <lb n="20"/>χρυσοῦ <choice><corr>νομισμάτια</corr><sic>νομεισματια</sic></choice> εἴκοσι πέντε καὶ β<unclear cert="high" reason="undefined">ε</unclear>βαιώσω ὡς πρόκειται. Αὐρήλιος Ἀντωνῖνος Οὐαλεντ<supplied cert="high" reason="lost">ι</supplied>νόου

    <lb n="21"/>πρεσβύτερος <choice><corr>ἀξιωθεὶς ἔγραψα</corr><sic>αξιωθι<add place="supralinear">ς</add> εκρψα</sic></choice> ὑπὲρ αὐτῆς <unclear cert="high" reason="undefined">γ</unclear>ράμμα<supplied cert="high" reason="omitted">τα</supplied> <choice><corr>μὴ εἰδυίας</corr><sic>μοι ει<unclear cert="high" reason="undefined">τ</unclear>ηειοις</sic></choice> καὶ μαρτυρῶ.</ab></div></body></text></TEI.2>