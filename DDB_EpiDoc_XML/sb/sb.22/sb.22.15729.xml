    <lb n="1"/><g type="stauros"/> <supplied reason="lost">ἐν ὀνόματ</supplied>ι <supplied reason="lost">τοῦ κυρίου καὶ δεσπότου</supplied>

    <lb n="2"/>Ἰ<supplied reason="lost">η</supplied><unclear reason="undefined">σ</unclear>οῦ Χριστ<unclear reason="undefined">ο</unclear><supplied reason="lost">ῦ το</supplied><unclear reason="undefined">ῦ</unclear> <supplied reason="lost">οῦ κα</supplied><unclear reason="undefined">ὶ</unclear> <supplied reason="lost">σωτῆρος ἡμῶν</supplied>

    <lb n="3"/>β<supplied reason="lost">α</supplied>σιλείας το<unclear reason="undefined">ῦ</unclear> <expan>εὐσεβ<ex>εστάτου</ex></expan> ἡμῶ<supplied reason="lost">ν δεσπ</supplied>ότου <expan><supplied reason="lost">Φλ<ex>αουίου</ex></supplied></expan>

    <lb n="4"/><unclear reason="undefined">Ἡ</unclear><supplied reason="lost">ρ</supplied><unclear reason="undefined">α</unclear>κλείου τοῦ αἰωνίου <expan><supplied reason="lost">Αὐγούσ<ex>του</ex></supplied></expan> καὶ

    <lb n="5"/>Α<unclear reason="undefined">ὐ</unclear><supplied reason="lost">τ</supplied><unclear reason="undefined">ο</unclear>κράτορ<unclear reason="undefined">ο</unclear><supplied reason="lost">ς</supplied> <unclear reason="undefined">ἔτ</unclear><supplied reason="lost">ο</supplied>υς <num value="29">κθ</num> <unclear reason="undefined">Ἐ</unclear>πεὶ<unclear reason="undefined">φ</unclear> <num value="19">ιθ</num>

    <lb n="6"/>ἀρχ<supplied reason="lost">ῇ</supplied> <num value="13">ιγ</num> <expan>ἰν<ex>δικτίωνος</ex></expan> ἐν <expan>Ἀρ<ex>σινοιτῶν πόλει</ex></expan>·

    <lb n="7"/>Αὐρήλιοι Γε<supplied reason="lost">ώ</supplied>ργιος ἀ<unclear reason="undefined">να</unclear>φά<unclear reason="undefined">λα</unclear>κρος υἱὸς

    <lb n="8"/>Φοιβάμμωνος καὶ Σαμβ<unclear reason="undefined">ᾶς</unclear> υἱὸς <unclear reason="undefined">Π</unclear>εκυσίου

    <lb n="9"/>ἀπὸ κώμης Ἡρ<unclear reason="undefined">α</unclear>κλείας τοῦ <unclear reason="undefined">Ἀρ</unclear>σιν<supplied reason="lost">ο</supplied><unclear reason="undefined">ί</unclear>του νο<unclear reason="undefined">μ</unclear>οῦ

    <lb n="10"/>Αὐρηλίοις Φοιβάμμωνι τῷ καὶ <unclear reason="undefined">Π</unclear>ατόκῳ

    <lb n="11"/>υἱῷ Κοσμᾶ καὶ <choice><corr>ὁμωνύμῳ</corr><sic>ομονυμω</sic></choice> Γεωργ<unclear reason="undefined">ί</unclear>ῳ <expan>Φοιβ<ex>άμμωνος</ex></expan>

    <lb n="12"/>ἀπὸ τῆς Ἀρσινοιτῶν πόλεως <expan>χ<ex>αίρειν</ex></expan>· <expan>ὁμολογοῦ<unclear reason="undefined">μ</unclear><ex>εν</ex></expan>

    <lb n="13"/>ἐξ ἀλληλεγγύης ἐκμεμισθωκέναι ὑμ<unclear reason="undefined">ῖ</unclear>ν

    <lb n="14"/>τὰς διαφερούσας <choice><corr>ἡμῖν</corr><sic>υμιν</sic></choice> ἐν πεδίῳ τῆς <expan>ἡμετέρ<ex>ας</ex></expan>

    <lb n="15"/>κώμης ἐν τόπῳ κλήρου <expan>καλουμ<ex>ένου</ex></expan> Πλαια<unclear reason="undefined">τυ</unclear>

    <lb n="16"/>ἐκ βορρᾶ τοῦ κλήρου τοῦ υἱοῦ Θ<unclear reason="undefined">αι</unclear>σ<unclear reason="undefined">α</unclear>ρίου <expan>ἀρούρ<ex>ας</ex></expan>

    <lb n="17"/>ὀκτὼ <expan>πλήρ<ex>ης</ex></expan>, <expan><ex>ἀρούρας</ex></expan> <num value="8">η</num> <expan>πλήρ<ex>ης</ex></expan>, πρὸς <choice><corr>τὸ</corr><sic>τ<unclear reason="undefined">ω</unclear></sic></choice> <supplied reason="lost">ὑμ</supplied><unclear reason="undefined">ᾶ</unclear>ς ταύτας

    <lb n="18"/><unclear reason="undefined">σ</unclear>π<unclear reason="undefined">ε</unclear><supplied reason="lost">ί</supplied>ρειν καὶ καρ<supplied reason="lost">ποῦ</supplied>σθαι <supplied reason="lost">κα</supplied>ὶ προσοδε<unclear reason="undefined">ύε</unclear><supplied reason="lost">σθ</supplied><unclear reason="undefined">αι</unclear>

    <lb n="19"/><unclear reason="undefined">ἐ</unclear>νι<unclear reason="undefined">α</unclear>υσ<supplied reason="lost">ίως ἀ</supplied><unclear reason="undefined">π</unclear>ὸ καρ<unclear reason="undefined">π</unclear>ῶν τῆς <expan>εἰσιούσ<ex>ης</ex></expan>

    <lb n="20"/><supplied reason="lost">τεσσαρεσκαι</supplied>δεκάτης <expan><supplied reason="lost">ἰ</supplied>ν<ex>δικτίωνος</ex></expan> <unclear reason="undefined">κ</unclear><supplied reason="lost">α</supplied>ὶ <choice><corr>παρασχεῖν</corr><sic>παρασχομεν</sic></choice>

    <lb n="21"/><supplied reason="lost">ἡμῖν ὑπὲρ τοῦ</supplied> φόρ<supplied reason="lost">ου</supplied> α<unclear reason="undefined">ὐ</unclear>τῶ<supplied reason="lost">ν</supplied> ἐνιαυσίως

    <lb n="22"/><supplied reason="lost">χρυσ</supplied>οῦ νομ<unclear reason="undefined">ισμά</unclear>τια τέσ<unclear reason="undefined">σαρα</unclear> <num value="4"/> <supplied reason="lost">π</supplied>α-

    <lb n="23"/><supplied reason="lost">ρὰ</supplied> <unclear reason="undefined">κε</unclear>ρατίων εἴκοσι τριῶν <num value="23"/> <choice><corr>ἐπὶ</corr><sic>επει</sic></choice> <expan>μη<ex>νὶ</ex></expan>

    <lb n="24"/>Ἐπεὶφ νεομηνίᾳ καὶ μὴ δύνασθαι <choice><corr>ὑμᾶς</corr><sic>ημας</sic></choice>

    <lb n="25"/>ἀγνωμονῆσαι περὶ τὴν δόσι<supplied reason="lost">ν</supplied> τοῦ αὐτοῦ φόρου

    <lb n="26"/>ἐνιαυσίως καὶ ἐὰν συμβῇ ἐπιθεῖναι

    <lb n="27"/>προσθήκην ἐπάνω τῶν ἀρ<supplied reason="lost">ο</supplied>υρῶν Μηνᾶ υἱοῦ

    <lb n="28"/>Βενιαμῖν ὥστε καὶ <choice><corr>ὑμᾶς</corr><sic>ημας</sic></choice> παρασχεῖν τὴν <expan>προσθήκ<ex>ην</ex></expan>

    <lb n="29"/>τῶν αὐτῶν ἀρουρῶν ἀναλόγως καὶ ἐὰν συμβῇ

    <lb n="30"/>τὰς αὐτὰς ἀρούρας ἀναλωθῆναι ὑπὸ τοῦ ὕδατος

    <lb n="31"/>ὥστε <choice><corr>ὑμᾶς</corr><sic>ημας</sic></choice> ὑποδεῖξαι <choice><corr>ἡμῖν</corr><sic>υμιν</sic></choice> αὐτὰς καὶ εἶναι

    <lb n="32"/>αὐτὰς χωρὶς δημοσίου καὶ πάλιν ἐὰν σωθῶσι

    <lb n="33"/>ἀπὸ τοῦ ὕδατος καὶ συμβῇ τὰς ἐκ λιβὸς καὶ <expan>ἀνατολ<ex>ῶν</ex></expan>

    <lb n="34"/>καὶ βορρᾶ αὐτῶν ἀρούρας σπαρῆναι ὥστε

    <lb n="35"/><choice><corr>ὑμᾶς</corr><sic>ημας</sic></choice> πάλιν παρασχεῖν <choice><corr>ἡμῖν</corr><sic>υ<unclear reason="undefined">μ</unclear>ιν</sic></choice> τὰ αὐτὰ τέσσαρ<supplied reason="lost">α</supplied> <num value="4"/> <expan>νομ<ex>ισμάτια</ex></expan>

    <lb n="36"/>τοῦ φόρου ἐτησίως. <choice><corr>κατεβάλομεν</corr><sic>καταβαλομεν</sic></choice> δὲ ἐφ᾽ ὑμᾶς

    <lb n="37"/>καὶ τὰ τιμήματα κατὰ μίμησιν τοῦ αὐτοῦ

    <lb n="38"/>Μηνᾶ καὶ ἐὰν ἐθελήσητε <choice><corr>ἰδιοσπορήσειν</corr><sic>ιδιοσπορησι</sic></choice>

    <lb n="39"/>αὐτὰς ἄδειαν ὑμᾶς ἔχειν εἰς το<supplied reason="lost">ῦ</supplied>το. ἡ μίσθωσις

    <lb n="40"/>κυρία <expan><ex>καὶ</ex></expan> <expan>ἐπερ<ex>ωτηθέντες</ex></expan> <expan>ὡμ<ex>ολογήσαμεν</ex></expan>. <g type="stauros"/> Γεώργιος <expan><ex>καὶ</ex></expan> Σαμβᾶς στοι-

    <lb n="41"/>χεῖ ἡμῖν ὡς <expan>πρόκ<ex>ειται</ex></expan>.

    <lb n="42"/><handShift new="m2"/><g type="stauros"/> <foreign lang="la">Di emu Georgiu etelioth</foreign>.
