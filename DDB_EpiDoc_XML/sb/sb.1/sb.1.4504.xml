    <lb n="1"/>[<g type="stauros"/> ἐν ὀνόμα]τι τῆς ἁγίας <supplied cert="high" reason="lost">καὶ ὁμοουσίου</supplied> 

    <lb n="2"/>τρι<supplied cert="high" reason="lost">άδος π</supplied>ατρὸς καὶ υ<hi rend="diaeresis">ἱ</hi>οῦ καὶ ἁγίου 

    <lb n="3"/>πνεύματος, βασιλείας τοῦ γαλ<supplied cert="high" reason="lost">ην</supplied>οτ<supplied cert="high" reason="lost">άτου</supplied> 

    <lb n="4"/>ἡμῶν <expan>δεσπότ<ex cert="high">ου</ex></expan> <expan>Φλ<ex cert="high">αουίου</ex></expan> Ἡρακλείο<supplied cert="high" reason="lost">υ τ</supplied>οῦ 

    <lb n="5"/><choice><corr>αἰωνίου</corr><sic>αιωνειου</sic></choice> Αὐγούστου καὶ Αὐ<supplied cert="high" reason="lost">τ</supplied>οκρ<supplied cert="high" reason="lost">άτορος</supplied> 

    <lb n="6"/>ἔτους τετάρτου, Ἁθὺρ <num value="22">κβ</num>, <expan>ἰν[δ<ex cert="high">ικτίονος</ex></expan> <app type="BL"><lem resp="9.239"><num value="2">β</num></lem><rdg>δευτέρα?</rdg></app>]. 

    <lb n="7"/>Αὐρήλιος Καλλίνικος <supplied cert="high" reason="lost">υἱὸς</supplied> 

    <lb n="8"/>Σενούθου μητρὸς Τλούλλ<supplied cert="high" reason="lost">ας</supplied> 

    <lb n="9"/><expan>μίσθ<ex cert="high">ιος</ex></expan> ἀπὸ κώμης Θινὸς <supplied cert="high" reason="lost">τοῦ Θινί</supplied>του 

    <lb n="10"/>νομοῦ ἑξῆς <hi rend="diaeresis">ὑ</hi>πογράφων Αὐρηλ<supplied cert="high" reason="lost">ίῳ</supplied> 

    <lb n="11"/>Πα<supplied cert="high" reason="lost">χυμίῳ</supplied> πορφυροπώλῃ ἀπὸ Πανὸς 

    <lb n="12"/>πόλεως, νῦν δὲ οἰκοῦντι ἐν τ<supplied cert="high" reason="lost">ῇ</supplied> 

    <lb n="13"/>αὐτῇ κώμῃ Θινός, <expan>χαίρ<ex cert="high">ειν</ex></expan>. ὁ<supplied cert="high" reason="lost">μολογῶ</supplied> 

    <lb n="14"/>ἐσχηκέναι παρὰ σοῦ καὶ δεδ<supplied cert="high" reason="lost">έχθαι</supplied> 

    <lb n="15"/>ε<supplied cert="high" reason="lost">ἰς</supplied> <hi rend="diaeresis">ἰ</hi><supplied cert="high" reason="lost">δ</supplied>ίαν μου καὶ ἀναγκαία<supplied cert="high" reason="lost">ν χρείαν</supplied> 

    <lb n="16"/>χρυσοῦ <expan>νομισμ<ex cert="high">ατίου</ex></expan> τρίτον πα<supplied cert="high" reason="lost">ρὰ κεράτιον</supplied> 

    <lb n="17"/>τέταρτον ζυγῷ <expan>φύλλω<ex cert="high">ν</ex></expan>, <expan>γ<ex cert="high">ίνεται</ex></expan> <expan>χρ<ex cert="high">υσοῦ</ex></expan> <expan>νο<ex cert="high">μισματίου</ex></expan> <num value="1/3">γ</num> <expan>π<ex cert="high">αρὰ</ex></expan> [<expan>κ<ex cert="high">εράτιον</ex></expan> <num value="1/4">δ</num>], 

    <lb n="18"/>καὶ <supplied cert="high" reason="lost">τ</supplied>οῦτο ἑτοίμως ἔχω παρασχ<supplied cert="high" reason="lost">εῖν</supplied> 

    <lb n="19"/>σοι ἐν οἴνου γλευκοῦ<supplied cert="high" reason="lost">ς</supplied> ἀδόλ<supplied cert="high" reason="lost">ου</supplied> 

    <lb n="20"/>κολόβων εἴκοσι πέντε <num value="25"/>, τῶν 

    <lb n="21"/>ἑπτὰ <num value="7"/> ξεστῶν ἑκάστου κ<supplied cert="high" reason="lost">ο</supplied>λ<supplied cert="high" reason="lost">όβου</supplied>, 

    <lb n="22"/><app type="BL"><lem resp="9.239"><seg cert="low" part="N">ἐν κούφοις</seg></lem><rdg>ἐν κούφαι?</rdg></app> ἐν τῷ καιρῷ <supplied cert="high" reason="lost">τ</supplied>ῆς 

    <lb n="23"/>τρύγης τῆς σὺν <expan>[θ<ex cert="high">εῷ</ex>]</expan> εἰσ<hi rend="diaeresis">ϊ</hi>ούσης <app type="BL"><lem resp="7.184"><num value="3">γ</num></lem><rdg><num value="2">β</num></rdg></app> <expan>ἰ[νδ<ex cert="high">ικτίονος</ex>]</expan> 

    <lb n="24"/>χωρὶς <expan>ὑπερθ<ex cert="high">έσεως</ex></expan>, <expan><hi rend="diaeresis">ὑ</hi>ποκ<ex cert="high">ειμένης</ex></expan> σοι τῆς π<supplied cert="high" reason="lost">αντοί</supplied>ας 

    <lb n="25"/>μου εὐπορίας ἕως πληρώσεως, 

    <lb n="26"/>καὶ <expan>ἐπερ<ex cert="high">ωτη</ex>θ<ex cert="high">εὶς</ex></expan> <expan>ὡμολ<ex cert="high">όγησα</ex></expan>. <handShift new="m2"/> Καλλίνικος 

    <lb n="27"/>ὁ <expan>προκ<ex cert="high">είμενος</ex></expan> στοιχεῖ μοι 

    <lb n="28"/>πάντα, ὡς <expan>πρόκ<ex cert="high">ειται</ex></expan>, καὶ <choice><corr>ὑπογράψας</corr><sic>υπ<supplied cert="high" reason="lost">ο</supplied>γραφας</sic></choice> χειρὶ ἐμ<supplied cert="high" reason="lost">ῇ ἀπ</supplied>έ-

    <lb n="29"/>λυσα. <g type="stauros"/> 

    <lb n="30"/><handShift new="m1"/> <g type="stauros"/> <expan>ἐγρ<ex cert="high">άφη</ex></expan> δι᾽ ἐμ<supplied cert="high" reason="lost">οῦ</supplied> Παύλου. </ab></div></body></text></TEI.2>