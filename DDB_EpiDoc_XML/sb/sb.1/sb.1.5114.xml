<placeName cert="high" full="yes">Apollonopolis Magna</placeName>
<xref evaluate="all" from="ROOT" n="p.lond;2;209,pgxix" targOrder="U" to="DITTO" type="reprint from">cfPLond2,209</xref></head><div lang="grc" org="uniform" part="N" sample="complete" type="edition"><ab part="N">
    <lb n="1"/><gap extent="unknown" reason="lost" unit="line"/> 

    <lb n="1"/><supplied cert="high" reason="lost">κα</supplied>ὶ Ἡρακλείου Νέου Κωνσταν<supplied cert="high" reason="lost">τίνου τῶν αἰωνίων Αὐγούστων</supplied> 

    <lb n="2"/>Αὐτοκρατόρων πεπρακέναι σή<supplied cert="high" reason="lost">μερόν σοι τῷ π</supplied>ρ<supplied cert="high" reason="lost">ογεγραμμένῳ Ζαχαρίᾳ</supplied> 

    <lb n="3"/>κατὰ τήνδε τὴν ἔγγραφον ὠνιακ<supplied cert="high" reason="lost">ὴ</supplied>ν ἀσφάλει<supplied cert="high" reason="lost">αν καὶ καταγεγραφηκέναι</supplied> 

    <lb n="4"/>ὠνιακῷ νόμῳ καὶ πληρεστάτῳ παντὶ δεποτε<supplied cert="high" reason="lost">ίας δικαίῳ ἐπ᾽ αἰωνίᾳ</supplied> 

    <lb n="5"/>κατοχῇ ἀπὸ τοῦ νῦν ἐπὶ τὸν ἑξῆς καὶ ἀε<supplied cert="high" reason="lost">ὶ ἅπαντα διηνεκῆ χρόνον</supplied> 

    <lb n="6"/>τὸ ἀνῆκον καὶ διαφέρον τοῦ αὐτοῦ <expan>ἁγίο<ex cert="high">υ</ex></expan> μοναστηρί<supplied cert="high" reason="lost">ου</supplied> <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">αρὰ</supplied> 

    <lb n="7"/><hi rend="supraline">Ταχυμίας</hi> τῆς μακαρίας θυγατρὸς Σανσνώτου τρίτο<supplied cert="high" reason="lost">ν μέρος ἀπὸ τῆς</supplied> 

    <lb n="8"/>οἰκίας αὐτῆς τῆς καὶ διακειμένης ἐν τῷ ὑψώματι τῆς π<supplied cert="high" reason="lost">όλεως </supplied><gap extent="9" reason="lost" unit="character"/> 

    <lb n="9"/>τοῦ <expan>ἁγίο<ex cert="high">υ</ex></expan> <expan>σεπτο<ex cert="high">ῦ</ex></expan> τόπου <hi rend="supraline">ἀββᾶ Κύρου</hi> ἀπὸ <expan>ἐδάφο<ex cert="high">υς</ex></expan> ἄχρι ἀέρος <supplied cert="high" reason="lost">μετὰ τῆς εἰσόδου</supplied> 

    <lb n="10"/>καὶ <expan>ἐξόδο<ex cert="high">υ</ex></expan> καὶ ἀνόδου καὶ καθόδου καὶ πάντων τῶν <choice><corr>κοινων<supplied cert="high" reason="lost">ιμιαίων</supplied></corr><sic>κοινον<supplied cert="high" reason="lost">ιμιαιων</supplied></sic></choice> <supplied cert="high" reason="lost">τόπων</supplied> 

    <lb n="11"/>κατὰ τὸ αὐτὸ τρίτον μέρος, καὶ ὁμολογῶ τοῦ λοιποῦ μηδὲ ἓν <num value="1"/> <supplied cert="high" reason="lost">μέρος ἢ δίκαιον</supplied> 

    <lb n="12"/><choice><corr>ὑπολείπεσθαι</corr><sic><hi rend="diaeresis">υ</hi>πολιπεισθαι</sic></choice> τοῦ <expan>αὐτο<ex cert="high">ῦ</ex></expan> <expan>ἁγίο<ex cert="high">υ</ex></expan> μοναστηρίου δι᾽ <expan>ἐμο<ex cert="high">ῦ</ex></expan> Πιστώ<hi rend="diaeresis">ϊ</hi>τος <supplied cert="high" reason="lost"><expan>ἐλαχίστ<ex cert="high">ου</ex></expan> <expan>διάκ<ex cert="high">ονος</ex></expan> καὶ</supplied> 

    <lb n="13"/>προεστῶτος τοῦ αὐτοῦ <expan>ἁγίο<ex cert="high">υ</ex></expan> τόπου καὶ τῆς δωρεαστικῆς ς<gap extent="9" reason="lost" unit="character"/> 

    <lb n="14"/>παρὰ τῆς αὐτῆς μακαρίας Ταχυμίας εἰς τὸν αὐτὸν ἅγιον τ<supplied cert="high" reason="lost">όπον, καὶ τὰς</supplied> 

    <lb n="15"/>αὐτῆς προσφορὰς ἀναδέδωκά σοι παρ᾽ ἐμοῦ εἰς μείζονα <gap extent="11" reason="lost" unit="character"/><foreign lang="la">. </foreign>

    <lb n="16"/>εἰσὶ δὲ γείτονες τῆς αὐτῆς οἰκίας νότου κοινὸς τοῖχος μεταξὺ αὐτῆς κ<supplied cert="high" reason="lost">αὶ </supplied><gap extent="5" reason="lost" unit="character"/><supplied cert="high" reason="lost">, βορρᾶ</supplied> 

    <lb n="17"/>ὡσαύτως κοινὸς τοῖχος, ἀπηλιώτου καὶ λιβὸς ῥῦμαι δημοσίαι ἀνέῳκται ἡ <supplied cert="high" reason="lost">θύρα, ἢ οἳ δ᾽ ἂν ὦσι</supplied> 

    <lb n="18"/>γείτονες πάντῃ πάντοθεν, καθὼς ἐγὼ ὁ ἀποδόμενος <supplied cert="high" reason="lost">στόματι ὑπηγόρευσα</supplied>, 

    <lb n="19"/>τιμῆς τῆς πρὸς ἀλλήλους συμπεφωνημένης καὶ συναρεσάσ<supplied cert="high" reason="lost">ης μεταξὺ ἡμῶν</supplied> 

    <lb n="20"/>ἀμφοτέρων ἐπὶ βεβαίῳ καὶ ἀμεταθέτῳ λόγῳ χρυσοῦ 

<app type="editorial"><lem>νομι<supplied cert="high" reason="lost">σματίου ἑνὸς τρίτου</supplied></lem><rdg>νομι<supplied cert="high" reason="lost">σματίου </supplied><gap extent="5" reason="lost" unit="character"/></rdg></app> 

    <lb n="21"/>ζυγῷ Ἀπόλλωνος, <expan>γί<ex cert="high">νεται</ex></expan> <expan>χρ<ex cert="high">υσοῦ</ex></expan> <expan>νο<ex cert="high">μισμάτιον</ex></expan> <num value="1">α</num> <num value="1/3">γ</num> <expan>μόν<ex cert="high">ον</ex></expan>, ἥνπερ τελείαν τιμὴν τ<supplied cert="high" reason="lost">οῦ τρίτου</supplied> 

    <lb n="22"/>μέρους ἀπὸ τῆς οἰκίας Ταχυμίας τῆς μακαρίας αὐτόθι ἀπέ<supplied cert="high" reason="lost">σ</supplied>χ<supplied cert="high" reason="lost">ηκα παρὰ σοῦ</supplied> 

    <lb n="23"/>τοῦ ὠνουμένου ἀπὸ χειρὸς εἰς χεῖράς μου ἐξ οἴκου <unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">ου ἀριθμῷ καὶ</supplied> 

    <lb n="24"/>σταθμῷ πλήρει πρὸς τῷ <hi rend="diaeresis">ὑ</hi>μῖν τοὺς προγεγραμμένους <hi rend="supraline">Ζαχαρίαν</hi> <supplied cert="high" reason="lost">καὶ </supplied><gap extent="9" reason="lost" unit="character"/> 

    <lb n="25"/>ἐντεῦθεν καὶ ἐπὶ τὸ διηνεκὲς αὐτοῦ τοῦ <expan>εἰρημένο<ex cert="high">υ</ex></expan> τρί<supplied cert="high" reason="lost">του μέρους ἀπὸ τῆς</supplied> 

    <lb n="26"/>οἰκίας τῆς μακαρίας <hi rend="supraline">Ταχυμίας</hi> ἀπὸ <app type="editorial"><lem>ἐδάφους</lem><rdg>ἐδάφου</rdg></app> ἄχρι ἀέρος καὶ <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">άντων τῶν</supplied> 

    <lb n="27"/><choice><corr>κοινωνιμιαίων</corr><sic>κοινονιμιαιων</sic></choice> τόπον καὶ τῆς εἰσόδου καὶ ἐξόδου καὶ ἀνόδο<supplied cert="high" reason="lost">υ καὶ καθόδου</supplied> 

    <lb n="28"/>κατὰ τὸ αὐτὸ τρίτον μέρος κρατεῖν καὶ κυριεύειν καὶ δεσπόζει<supplied cert="high" reason="lost">ν κατὰ πᾶν</supplied> 

    <lb n="29"/>δεσποτείας ἀναφαιρέτου δίκαιον καὶ οἰκεῖν καὶ διοικεῖν <supplied cert="high" reason="lost">καὶ μετασχηματίζειν</supplied> 

    <lb n="30"/>καὶ οἰκοδομεῖν καὶ ἀνοικοδομεῖν καὶ ἐκχωρεῖν καὶ παραχωρεῖ<supplied cert="high" reason="lost">ν καὶ ἐκμισθοῦν</supplied> 

    <lb n="31"/>καὶ ἐναλλάττειν καὶ πωλεῖν καὶ χαρίσασθαι καὶ τέκνοις μεταδι<supplied cert="high" reason="lost">δόναι καὶ κληρονόμοις</supplied> 

    <lb n="32"/>καταλιμπάνειν καὶ διαδόχοις καὶ <choice><corr>διακατ<supplied cert="high" reason="omitted">όχ</supplied>οις</corr><sic>διακατοις</sic></choice> καὶ πάντα ἁπλ<supplied cert="high" reason="lost">ῶς εἰπεῖν πράττειν</supplied> 

    <lb n="33"/>ἐπ᾽ αὐτῷ, ὅσα νόμοι ἐπιτρέπουσι τοῖς αὐτοτελέσι δεσπόταις <supplied cert="high" reason="lost">κυρίως καὶ</supplied> 

    <lb n="34"/>ἀνεπικωλύτως. καὶ βεβαιώσω σοι ταύτην τὴν πρᾶσιν <supplied cert="high" reason="lost">καὶ ἀποσοβήσω</supplied> 

    <lb n="35"/>διὰ παντὸς ἀπὸ παντὸς <expan>το<ex cert="high">ῦ</ex></expan> ἐπελευσομένου ἢ ἀντιποιησ<supplied cert="high" reason="lost">ομένου. τὸν δὲ</supplied> 

    <lb n="36"/>ἐπελευσόμενον ἢ ἀντιποιησόμενον παραχρῆμα ἐγὼ ὁ π<supplied cert="high" reason="lost">επρακὼς </supplied><gap extent="6" reason="lost" unit="character"/> 

    <lb n="37"/>ἢ καὶ πάντας τοὺς μετ᾽ ἐμοῦ οἰκονομησάντων ἐκστήσω <supplied cert="high" reason="lost">καὶ ἐκδικήσω</supplied> 

    <lb n="38"/>καὶ ἀποσοβήσω καὶ καθαροποιήσω ταύτην τὴν <supplied cert="high" reason="lost">π</supplied>ρ<supplied cert="high" reason="lost">ᾶσιν ἰδίοις μου</supplied> 

    <lb n="39"/>ἀναλώμασι καὶ κινδύνῳ κατὰ τὸν περὶ πράσεως καὶ βεβα<supplied cert="high" reason="lost">ιώσεως νόμον</supplied>. 

    <lb n="40"/>εἰ δὲ ἀσθενήσοιμι περὶ τὴν ἐκδίκησιν καὶ καθαροποίη<supplied cert="high" reason="lost">σιν ταύτης τῆς</supplied> 

    <lb n="41"/>πράσεως, ἑτοίμως ἔχω παρασχεῖν σοι τὴν ἐγκει<supplied cert="high" reason="lost">μ</supplied>έ<supplied cert="high" reason="lost">ν</supplied>η<supplied cert="high" reason="lost">ν τιμὴν ἐν διπλῷ</supplied>, 

    <lb n="42"/>καὶ εἴ τι δ᾽ ἂν φανείης ἀναλώσας εἴς τε φιλοκαλίαν καὶ <unclear cert="high" reason="undefined">ἑ</unclear><supplied cert="high" reason="lost">τέραν ἐπιμέλειαν</supplied>, 

    <lb n="43"/>καὶ αὐτὰ ἐν διπλῇ ποσότητι, μετὰ καὶ τοῦ μηδὲν ἰσχύειν κατὰ <supplied cert="high" reason="lost">τῶν ἐμφερομένων</supplied> 

    <lb n="44"/>τῇ παρούσῃ πράσει, ἀλλ᾽ ἐφ᾽ ᾧ αὐτὴν καὶ πᾶν μέρος α<supplied cert="high" reason="lost">ὐτ</supplied>ῆ<supplied cert="high" reason="lost">ς</supplied> κ<supplied cert="high" reason="lost">υρίαν εἶναι καὶ</supplied> 

    <lb n="45"/>βεβαίαν καὶ ἔννομον <expan>ἁπανταχο<ex cert="high">ῦ</ex></expan> προφερομένην καὶ <app type="editorial"><lem>ἐμ<unclear cert="high" reason="undefined">φ</unclear><supplied cert="high" reason="lost">ανιζομένην</supplied></lem><rdg>ἐμ<unclear cert="high" reason="undefined">μ</unclear><gap extent="7" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ὧς</supplied></rdg></app> 

    <lb n="46"/>ἐν δημοσίῳ ἀρχείῳ γεγενημένην καὶ κατὰ νόμους τ<supplied cert="high" reason="lost">ετελειωμένην</supplied>, 

    <lb n="47"/>καὶ εἰς τὰ <app type="BL"><lem>προγεγραμμένα πάντα</lem><rdg>προγεγραμμένα</rdg></app> ἐπερωτηθεὶς ὡμ<supplied cert="high" reason="lost">ολόγησα <g type="stauros"/>. <handShift new="m2"/> Πιστῶϊς</supplied> 

    <lb n="48"/>υ<hi rend="diaeresis">ἱ</hi>ὸς Μηνᾶ <expan>ἐλάχ<ex cert="high">ιστος</ex></expan> <expan>διάκ<ex cert="high">ονος</ex></expan> καὶ προεστὼς τοῦ ἁγίου μ<supplied cert="high" reason="lost">οναστηρίου ἀββᾶ</supplied> 

    <lb n="49"/>Κύρου ἀποπληροῦντος τὸ πρόσωπον τοῦ <app type="editorial"><lem>ἁγίο<supplied cert="low" reason="lost">υ μοναστηρίου </supplied></lem><rdg>ἁγίο<supplied cert="high" reason="lost">υ </supplied><gap extent="11" reason="lost" unit="character"/></rdg></app> 

    <lb n="50"/>καὶ στοιχεῖ μοι πάντα τὰ ἐγγεγραμμένα, <supplied cert="high" reason="lost">ὡς πρόκειται </supplied><gap extent="9" reason="lost" unit="character"/> 

    <lb n="51"/>γράψας <choice><corr>χειρὶ</corr><sic>χειρσι</sic></choice> <choice><corr>ἐμῇ</corr><sic>εμε</sic></choice> ἀπέλυσα <g type="stauros"/>. <handShift new="m3"/> Ἡρακλείδη<supplied cert="high" reason="lost">ς </supplied><gap extent="16" reason="lost" unit="character"/> 

    <lb n="52"/>μαρτυρῶ τῇ παρούσῃ πράσει ἀκουσας παρὰ τοῦ θε<supplied cert="high" reason="lost">μένου <g type="stauros"/>. <handShift new="m4"/> </supplied><gap extent="8" reason="lost" unit="character"/><supplied cert="high" reason="lost"> υἱὸς</supplied> 

    <lb n="53"/>Μαρτυρίου <expan>ἐλάχ<ex cert="high">ιστος</ex></expan> πρεσβύτερος μαρτυρῶ τῇ παρ<supplied cert="high" reason="lost">ούσῃ πράσει ἀκούσας</supplied> 

    <lb n="54"/>παρὰ τοῦ θεμένου <g type="stauros"/>. <handShift new="m5"/> Ὀράμπς <hi rend="diaeresis">Ἰ</hi>ωσηφίου <expan>ἐλάχ<ex cert="high">ιτος</ex></expan> <expan>διάκ<ex cert="high">ονος</ex></expan> <supplied cert="high" reason="lost">τοῦ αὐτοῦ</supplied> 

    <lb n="55"/>μοναστηρίου ἀκούσας παρὰ τοῦ θεμένου <g type="stauros"/>. <handShift new="m6"/> Διο<gap extent="9" reason="lost" unit="character"/><supplied cert="high" reason="lost"> μαρτυρῶ</supplied> 

    <lb n="56"/>τῇ παρούσῃ πράσει ἀκούσας παρὰ τοῦ θεμένου <g type="stauros"/>. <handShift new="m7"/> <g type="stauros"/> <unclear cert="high" reason="undefined">Διο</unclear><gap extent="4" reason="illegible" unit="character"/> <supplied cert="high" reason="lost">μαρτυρῶ</supplied> 

    <lb n="57"/>τῆ πράσει ἀκούσας <expan>παρ<ex cert="high">ὰ</ex></expan> <supplied cert="high" reason="omitted">τοῦ θεμένου</supplied>. <handShift new="m8"/> <g type="stauros"/> <expan>Φλ<ex cert="high">άουιος</ex></expan> <unclear cert="high" reason="undefined">Κλ</unclear><gap extent="3" reason="illegible" unit="character"/>ος πα<gap extent="7" reason="lost" unit="character"/><supplied cert="high" reason="lost"> μαρτυρῶ</supplied> 

    <lb n="58"/>τῇ παρούσῃ πράσει ἀκούσας παρὰ <expan>το<ex cert="high">ῦ</ex></expan> <expan>θεμένο<ex cert="high">υ</ex></expan> <g type="stauros"/>. 

    <lb n="59"/><handShift new="m9"/> <g type="stauros"/> δι᾽ ἐμοῦ <expan>Κυρίλλο<ex cert="high">υ</ex></expan> <expan>διακ<ex cert="high">όνου</ex></expan> καὶ συναλλαγματογράφου ἐγένετ<supplied cert="high" reason="lost">ο</supplied> <g type="stauros"/>. 

    <lb n="60"/><handShift new="m10"/> <g type="stauros"/> <expan>Φλ<ex cert="high">άουιος</ex></expan> Παπνοῦθις υἱὸς Βίκτορος ἐκ μητρὸς Σοφία<supplied cert="high" reason="lost">ς </supplied><gap extent="8" reason="lost" unit="character"/>σα πράσει πάντα τὰ ἐγγεγραμμένα, ὡς πρό<supplied cert="high" reason="lost">κειται</supplied>. </ab></div></body></text></TEI.2>