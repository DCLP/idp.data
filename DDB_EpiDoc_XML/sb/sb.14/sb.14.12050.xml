<placeName cert="high" full="yes">Hermoupolis Magna</placeName></head><div lang="grc" org="uniform" part="N" sample="complete" type="edition"><div n="r" org="uniform" part="N" sample="complete" type="textpart"><!--milestone unit="4"--><ab part="N">

    <lb n="1"/><g type="chirho"/> ὑπατεία<supplied cert="high" reason="lost">ς</supplied> <expan>Φλ<ex cert="high">αουίων</ex></expan> Ἰωάνν<unclear cert="high" reason="undefined">ου</unclear> καὶ Παυλίνου τῶν λαμπροτάτων <unclear cert="high" reason="undefined">Χ</unclear>[οιὰκ <gap extent="2" reason="illegible" unit="character"/> <num value="7">ζ</num> <expan>ἰνδ<ex cert="high">ικτίονος</ex></expan> <gap extent="unknown" reason="illegible" unit="character"/> ] 

    <lb n="2"/><expan>Φλ<ex cert="high">αουίᾳ</ex></expan> Ἀφθο<supplied cert="high" reason="lost">ν</supplied>ίᾳ τῇ λαμπροτάτῃ καὶ εὐγενεστάτῃ θυγατρὶ τοῦ τῆς περ<supplied cert="high" reason="lost">ιβλέπτου μνήμης</supplied> 

    <lb n="3"/><gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ι</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ο</supplied>υ ἀπὸ τῆς Ἑρμουπολιτῶν <space dim="horizontal" extent="unknown" unit="character"/> <expan>π<ex cert="high">αρὰ</ex></expan> Αὐρηλίων Ψύρου Π<supplied cert="high" reason="lost">ίνετος</supplied> 

    <lb n="4"/>καὶ Πίνετος καὶ Νωίου υἱῶν τοῦ προγεγραμμένου Ψύρου ἐκ μητρ<unclear cert="high" reason="undefined">ὸ</unclear>[ς <gap extent="unknown" reason="illegible" unit="character"/> ἀπὸ <choice><corr>ἐποι]-

    <lb n="5"/>κίου</corr><sic><supplied cert="high" reason="lost">εποι</supplied>κειου</sic></choice> Μονοὶ τ<supplied cert="high" reason="lost">οῦ</supplied> Ἑρμουπολίτου νομοῦ. ὁμολογοῦμεν <choice><corr>ἀλληλέγγυ<supplied cert="high" reason="omitted">οι</supplied></corr><sic>αλληλεγγ<unclear cert="high" reason="undefined">υ</unclear></sic></choice> ὄντες <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">αὶ ἀλληλανάδοχοι</supplied> 

    <lb n="6"/>ἑκουσίως καὶ <unclear cert="high" reason="undefined">α</unclear>ὐθαιρέτως μεμισθῶ<unclear cert="high" reason="undefined">σ</unclear>θαι παρὰ <choice><corr>τῆ<supplied cert="high" reason="omitted">ς</supplied></corr><sic>τη</sic></choice> σῆς λαμπρότη[τος ἐπὶ <gap extent="unknown" reason="illegible" unit="character"/> ετῆ χρόνον <expan>λογιζ<ex cert="high">όμενον</ex></expan>] 

    <lb n="7"/><supplied cert="high" reason="lost">ἀπὸ</supplied> καρπῶν τῆς εὐτυχοῦς <gap extent="unknown" reason="lost" unit="character"/> ὀγδόης <expan>ἰνδικ<ex cert="high">τίονος</ex></expan> τὸ ὑπ<unclear cert="high" reason="undefined">άρ</unclear><supplied cert="high" reason="lost">χ</supplied><unclear cert="high" reason="undefined">ον</unclear> τῇ σῇ ς<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>ὁλόκληρο<supplied cert="high" reason="lost">ν ἀ</supplied>ρουρῶ<supplied cert="high" reason="lost">ν</supplied> ἑξήκον<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">α</supplied> <unclear cert="high" reason="undefined">ὀ</unclear>κτὼ <num value="68"/> σὺν ἀμπελῶνι ἑτέρων ἀ<supplied cert="high" reason="lost">ρουρῶν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>καὶ λάκκῳ διακεί<unclear cert="high" reason="undefined">μενον</unclear> <unclear cert="high" reason="undefined">ἐ</unclear>ν τῇ <unclear cert="high" reason="undefined">αὐ</unclear>τῇ πεδιάδι πρὸς γεωργικὴν κ<supplied cert="high" reason="lost">αὶ ἀμπελουργικὴν</supplied> 

    <lb n="10"/>ἐπιμέλειαν κ<supplied cert="high" reason="lost">α</supplied>ὶ <choice><corr>φιλοκαλίαν</corr><sic>φιλοκαλειαν</sic></choice> φόρ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">υ</supplied> <unclear cert="high" reason="undefined">τ</unclear>ῶν αὐτῶν ἀρ<unclear cert="high" reason="undefined">ο</unclear>υρῶν <unclear cert="high" reason="undefined">ἑ</unclear>ξ<unclear cert="high" reason="undefined">ή</unclear>κοντα <unclear cert="high" reason="undefined">ὀ</unclear><supplied cert="high" reason="lost">κτὼ</supplied> <num value="68"/> 

    <lb n="11"/>κατ᾽ ἔτος ἔν τε συμβρόχ<unclear cert="high" reason="undefined">ῳ</unclear> καὶ ἀβ<unclear cert="high" reason="undefined">ρόχῳ</unclear> <unclear cert="high" reason="undefined">σίτ</unclear><supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">υ</unclear> <unclear cert="high" reason="undefined">ἀρτα</unclear>βῶν τετρακοσίων τεσσ<unclear cert="high" reason="undefined">α</unclear>ρ<supplied cert="high" reason="lost">άκοντα</supplied> 

    <lb n="12"/>πέντε <num value="445"/>, ὅνπερ φόρον νέον καθαρὸν κεκοσκινευμένον μέτρῳ Ἀθηναίῳ <unclear cert="high" reason="undefined">ἀπ</unclear><supplied cert="high" reason="lost">οδώσομεν</supplied> 

    <lb n="13"/>καὶ μετρήσ<unclear cert="high" reason="undefined">ο</unclear>μεν ἐξ ἀλληλυγγύης τῷ Ἐπεὶφ μηνὶ ἑκάστου ἔτους ἀνυπερθέτως, <supplied cert="high" reason="lost">δώσομεν δὲ</supplied> 

    <lb n="14"/>κατ᾽ ἔτος λόγῳ συνηθείας τοῦ αὐτοῦ γεωργίου καὶ ῥαφανελαίου ξέστας <unclear cert="high" reason="undefined">πεντ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="15"/>ἐν ἐλαίῳ καλλίστῳ καὶ εὐαρέστῳ τῷ σῷ ξέστῃ καὶ μέλιτος ξέσ<unclear cert="high" reason="undefined">τας</unclear> <gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τῷ σῷ</supplied> 

    <lb n="16"/>ξέστῃ καὶ τυροὺς εὐαρέστους δ<supplied cert="high" reason="lost">ια</supplied>κοσίας πεντήκοντα <num value="250"/> καὶ δελφάκια <unclear cert="high" reason="undefined">ἓξ</unclear> <num value="6"/> <supplied cert="high" reason="lost">κάλλιστα</supplied> 

    <lb n="17"/>καὶ εὐάρεστα καὶ ἄρτων ἁπαλῶν ζεύγη ἑκατὸν καὶ σταχύων δέματα ἑκα[τὸν <num value="100"/> καὶ] 

    <lb n="18"/>διδιπλᾶ ὀκτώ <num value="8"/>, δώσομεν δὲ κατ᾽ ἔτος οὐδὲν ἧττον καὶ χόρτου ἀρούρης μιᾶς κ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ὶ ἀχύρου</supplied> 

    <lb n="19"/><choice><corr>σιτίνου</corr><sic>σι<unclear cert="high" reason="undefined">δ</unclear>ινου</sic></choice> <choice><corr>μώια</corr><sic>μουεια</sic></choice> ἑκατὸν καὶ ὑπὲρ <choice><corr>κατανομῆς</corr><sic>καταμονης</sic></choice> τοῦ αὐτοῦ γεωργίου χρυσοῦν ἥμι[συ <num value=" 1/2 "/> <gap extent="unknown" reason="illegible" unit="character"/> ] 

    <lb n="20"/>ἑτοίμως δὲ <unclear cert="high" reason="undefined">ἔχ</unclear>ομεν τὴν <choice><corr>ὑδροπαροχίαν</corr><sic>υδροπαροχειαν</sic></choice> ἐκτελέσαι τοῦ αὐτοῦ χωρίου τῶν <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="21"/>δι᾽ ἡμετέρων βοικῶν ζῴων καὶ τροφῶν αὐτῶν καὶ μισθῶν <gap extent="3" reason="illegible" unit="character"/> <supplied cert="high" reason="lost">ἐν μὲν χειμῶνι</supplied> 

    <lb n="22"/>δεύτερον τοῦ μηνός, τῷ δὲ θέρει δι᾽ ἡμερῶν ὀκτὼ <num value="8"/> ἀμέμπ<supplied cert="high" reason="lost">τως καὶ ἀκατα</supplied>-

    <lb n="23"/>γνώστως καὶ ἀκαταφρονήτως, ἐφ᾽ ᾧτέ σε τὴν γεοῦχον δέξασθαι ἥμ<supplied cert="high" reason="lost">ισυ μέρος</supplied> 

    <lb n="24"/>παντὸς τοῦ ἐκβησομένου οἴνου καὶ ἡμᾶς μετὰ τοῦ ἀμπελουργοῦ <unclear cert="high" reason="undefined">ἔχ</unclear><supplied cert="high" reason="lost">ειν τὸ ἄλλο</supplied> 

    <lb n="25"/>ἥμισυ μέρος ἀνθ᾽ ὧν ποιούμεθα καμάτων, ἐξαιρέτως δέ <unclear cert="high" reason="undefined">σ</unclear>ε <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ὴν γεοῦχον</supplied> 

    <lb n="26"/>λαβεῖν ἀπὸ τοῦ κοινοῦ <choice><corr>πίθου</corr><sic>πειθου</sic></choice> κατ᾽ ἔτος ὑπὲρ πρωτολήνο<unclear cert="high" reason="undefined">υ</unclear> 

    <lb n="27"/><supplied cert="high" reason="lost">οἴνου μέτρα</supplied> 

    <lb n="28"/>ἑκατόν <num value="100"/>. ὑποκειμένων τῇ σῇ λαμπρότητι εἰς ταύτην τὴν μίσθω<supplied cert="high" reason="lost">σιν</supplied> 

    <lb n="29"/>ἡμῶν τῶν ὑπαρχόντων καθάπερ ἐκ δίκης, τὸ γράμμα τῆς μισ<unclear cert="high" reason="undefined">θώ</unclear><supplied cert="high" reason="lost">σεως</supplied> 

    <lb n="30"/>κύριον καὶ βέβαιον καὶ <expan>ἐπερ<ex cert="high">ωτηθέντες</ex></expan> <expan>ὡμολ<ex cert="high">ογήσαμεν</ex></expan>. <handShift new="m2"/> <expan>Αὐρ<ex cert="high">ήλιοι</ex></expan> Ψῦρος Πένητος καὶ Πινᾶς κ<supplied cert="high" reason="lost">αὶ Νώιος</supplied> 

    <lb n="31"/><unclear cert="high" reason="undefined">οἱ</unclear> <expan>προκ<ex cert="high">είμενοι</ex></expan> μεμισθώμεθα ἐξ ἀλληλεγγύης <unclear cert="high" reason="undefined">ὡ</unclear>ς <expan>πρόκ<ex cert="high">ειται</ex></expan>. <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> Μηνᾶς Σερήνου ἀ<unclear cert="high" reason="undefined">πὸ</unclear> <expan><unclear cert="high" reason="undefined">Ἑ</unclear>ρ<ex cert="high">μουπόλεως</ex></expan> ἀξιω<unclear cert="high" reason="undefined">θ</unclear><supplied cert="high" reason="lost">εὶς ἔγραψα ὑπὲρ αὐτῶν</supplied> 

    <lb n="32"/>γράμματα μὴ εἰδότων. <handShift new="m3"/> <g type="chirho"/> Αὐρή<unclear cert="high" reason="undefined">λιος</unclear> Βίκτωρ Ἀλῆτος ἀπ<unclear cert="high" reason="undefined">ὸ</unclear> <expan><unclear cert="high" reason="undefined">Ἑ</unclear>ρ<ex cert="high">μουπόλεως</ex></expan> μαρτυρῶ τῇ <choice><corr>μισθώσει</corr><sic>μισθωσι</sic></choice> ἀκού<unclear cert="high" reason="undefined">σ</unclear>[ας παρὰ τῶν <choice><corr>θε]-

    <lb n="33"/>μένων</corr><sic><supplied cert="high" reason="lost">θε</supplied>μενου</sic></choice>. <handShift new="m4"/> <g type="chirho"/> Αὐρήλιος Φοιβάμμων Πέτρου ἀπὸ <expan><unclear cert="high" reason="undefined">Ἑ</unclear>ρ<ex cert="high">μουπόλεως</ex></expan> <supplied cert="high" reason="lost">μα</supplied><unclear cert="high" reason="undefined">ρτυρῶ</unclear> <unclear cert="high" reason="undefined">τῇ</unclear> <choice><corr><unclear cert="high" reason="undefined">μισθώ</unclear>σει</corr><sic>μισθωσι</sic></choice> ἀκο<supplied cert="high" reason="lost">ύσας παρὰ τῶν</supplied> 

    <lb n="34"/>θ<supplied cert="high" reason="lost">ε</supplied>μένων. <handShift new="m5"/> <g type="chirho"/> Αὐρήλιος Ἰωσῆφις Ἑρμαπόλλων<unclear cert="high" reason="undefined">ος</unclear> ἀπὸ <expan>Ἑρ<ex cert="high">μουπόλεως</ex></expan> μαρτυρῶ τῇ <choice><corr>μισθώ<supplied cert="high" reason="lost">σ</supplied>ει</corr><sic>μι<unclear cert="high" reason="undefined">σ</unclear>θ<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">σ</supplied>ι</sic></choice> <supplied cert="high" reason="lost">ἀκούσας</supplied> 

    <lb n="35"/>παρὰ τῶν θεμέ<unclear cert="high" reason="undefined">νων</unclear>. 

    <lb n="36"/><g type="chirho"/> δι᾽ ἐμοῦ Πινουτίωνος ἐγράφη <g type="chirho"/> <gap extent="4" reason="illegible" unit="character"/><gap extent="2" reason="illegible" unit="character"/> 
