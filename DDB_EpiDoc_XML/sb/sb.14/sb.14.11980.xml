    <lb n="1"/>Σαραπίωνι τῷ καὶ Ἀπολλωνιαν<supplied cert="high" reason="lost">ῷ</supplied> <expan>στρ<ex cert="high">ατηγῷ</ex></expan> Ἀρσ<unclear cert="high" reason="undefined">ινοείτ</unclear><supplied cert="high" reason="lost">ου</supplied> <expan><unclear cert="high" reason="undefined">Θεμ</unclear><ex cert="high">ίστου</ex></expan> <supplied cert="high" reason="lost">καὶ</supplied> <expan><unclear cert="high" reason="undefined">Πολ</unclear><ex cert="high">έμωνος</ex></expan> <expan>[μερίδ<ex cert="high">ων</ex>]</expan> 

    <lb n="2"/>παρὰ Κοπρῆ Ἰσίωνος ἀπὸ τῆς μητροπόλεω<unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">τοῦ</unclear> Ἀρ<unclear cert="high" reason="undefined">σιν</unclear>ο<supplied cert="high" reason="lost">ίτου· ο</supplied><unclear cert="high" reason="undefined">ὗ</unclear> <unclear cert="high" reason="undefined">ἐπιδ</unclear><supplied cert="high" reason="lost">έδω</supplied>-

