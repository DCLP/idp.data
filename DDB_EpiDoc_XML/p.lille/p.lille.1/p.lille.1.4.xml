    <lb n="8"/>Θεογένει. ὑπογεγ<supplied cert="low" reason="lost">ραφα</supplied><gap extent="8" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ηι</unclear> σοι <unclear cert="high" reason="undefined">α</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ν</unclear><gap extent="3" reason="lost" unit="character"/> τοῦ γενομένου 

    <lb n="9"/>ἐπιμελ<supplied cert="high" reason="lost">ητοῦ <expan><ex cert="high">ἔτους</ex></expan></supplied> <num value="5">ε</num> <app type="editorial"><lem>Γορπιαίου Μεχεὶρ σύνταξ<unclear cert="high" reason="undefined">ο</unclear>ν ἀναλαβ<supplied cert="high" reason="lost">εῖν</supplied></lem><rdg>Γορπι<supplied cert="high" reason="lost">α</supplied>ίου <gap extent="1" reason="illegible" unit="character"/><num value="6">ϛ</num> <gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">αν</unclear><gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">α</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/>ν</rdg></app> τοὺς <expan>κλ<ex cert="high">ήρους</ex></expan> 

