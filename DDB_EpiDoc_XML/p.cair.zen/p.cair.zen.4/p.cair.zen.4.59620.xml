    <lb n="1"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ν<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/><gap extent="unknown" reason="lost" unit="character"/>γε<unclear cert="high" reason="undefined">γρ</unclear>αφ<unclear cert="high" reason="undefined">εν</unclear><gap extent="3" reason="lost" unit="character"/>α<gap extent="4" reason="illegible" unit="character"/> 

    <lb n="3"/><gap extent="3" reason="lost" unit="character"/>περ <unclear cert="high" reason="undefined">Εὔτυχος</unclear> περὶ <unclear cert="high" reason="undefined">τούτων</unclear> <gap extent="3" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>εἶναι καὶ δεδαπανηκέναι εἰς τὴν <supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ἰκ</unclear>οδομὴν 

    <lb n="5"/><add place="supralinear"><del rend="erasure" status="unremarkable">καὶ τ</del></add> καὶ ἐπιδείξειν περὶ τ<unclear cert="high" reason="undefined">ού</unclear><supplied cert="high" reason="lost">των</supplied> καὶ πρ<supplied cert="high" reason="lost">οστάξαντος</supplied> 

    <lb n="6"/>Ἀγήνορος ἕως τοῦ ἐπιδεῖξαι Εὔτυχ<supplied cert="high" reason="lost">ον </supplied><gap extent="3" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/><subst><add place="inline">συνέχεσθαι</add><del rend="corrected" status="unremarkable">συνεχειν</del></subst> τοὺς <unclear cert="high" reason="undefined">κρι</unclear>θέντας, φθαρεὶς <unclear cert="high" reason="undefined">Εὔτυ</unclear>χος 

    <lb n="8"/>ὑπὸ τῆς Ἀρσινόη<supplied cert="high" reason="lost">ς</supplied> διὰ τὸ καὶ ἔμπροσθεν αὐτῆι συνεῖναι 

    <lb n="9"/>καὶ τέκνα αὐτῶι ὑπάρχειν ἐξ αὐτῆς <del rend="erasure" status="unremarkable">τά τε</del> 

    <lb n="10"/><del rend="erasure" status="unremarkable">σκεύη τὰ</del> τῶν τε ὑπαρχόντων τινὰ <add place="supralinear">τῶν</add> ἐν τῆι οἰκίαι 

    <lb n="11"/>μετέφερον ἐν τῆι ἐπιούσηι νυκτί, <add place="supralinear">ὧν</add> <sic>ἃ</sic> καὶ οἱ φυλακῖ<unclear cert="high" reason="undefined">τα</unclear>ι 

    <lb n="12"/><del rend="erasure" status="unremarkable">λαβόντες νυκτὶ συνέσχον</del> ἐπελάβοντο, 

    <lb n="13"/>αὐτός τε ἀνεχώρησεν καὶ ἔστιν ἔτι καὶ νῦν ἐν τῶι 

    <lb n="14"/><choice><corr>ἐν</corr><sic>εμ</sic></choice> Μέμφει Σαραπιείωι σιτοδοτούμενος ὑπ<add place="supralinear">ὸ</add> <add place="supralinear">Ἀρσινόης,</add> <choice><corr>τῆς</corr><sic>αυτης</sic></choice> 

    <lb n="15"/><del rend="erasure" status="unremarkable">Ἀρσινόη τε</del> ὕστερόν <add place="supralinear">τε</add> <subst><add place="inline">ἐμβα<add place="supralinear">λούσης</add></add><del rend="corrected" status="unremarkable">εμβα<del rend="erasure" status="unremarkable">λλει</del></del></subst> <add place="supralinear">αὐτῆς</add> <del rend="erasure" status="unremarkable"><add place="supralinear">ἄλλην</add></del> ἔντευξιν φαμ<unclear cert="high" reason="undefined">ένη</unclear>ς 

    <lb n="16"/>ἐμὲ ὡρίσθαι παρέξεσθαι <del rend="erasure" status="unremarkable">δὲ μαρτυροῦντας</del> τ<unclear cert="high" reason="undefined">ούς</unclear> τε 

    <lb n="17"/>οἰκοδόμους καὶ παρ᾽ ὧν τὴν πλίνθον <add place="supralinear"><del rend="erasure" status="unremarkable">τὰ ξύλα καὶ</del> καὶ τὰ ξύλα ἠγ<unclear cert="high" reason="undefined">όρακεν</unclear> <unclear cert="high" reason="undefined">Εὔ</unclear><supplied cert="high" reason="lost">τυχος</supplied></add> καὶ τοὺς ὀνηλ<unclear cert="high" reason="undefined">άτας</unclear> 

    <lb n="18"/><del rend="erasure" status="unremarkable">καὶ παρ᾽ ὧν τὰ ξύλα ἠγόρακεν <add place="supralinear">Εὔτυχος</add></del> χειρογραφοῦντα<unclear cert="high" reason="undefined">ς</unclear>, 

    <lb n="19"/>ἐμοῦ οὐχ ὡρισμένου, διὸ παραλογισθεὶς <app type="editorial"><lem>Ἀγήνωρ</lem><rdg>παραλογισθεὶς <note anchored="yes" lang="en" place="not specified">om</note></rdg></app> καὶ <unclear cert="high" reason="undefined">α</unclear><gap extent="3" reason="illegible" unit="character"/>ου<unclear cert="high" reason="undefined">θα</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="20"/>τῆι Ἀρσινόηι φάσει γράφει Κλειτορίωι <choice><corr>ἀνακαλεσάμενόν</corr><sic>ανακ<unclear cert="high" reason="undefined">αλεσαμ</unclear>ενομ</sic></choice> με 

    <lb n="21"/>συντάξαι ἐφ᾽ οἷς ὥρισμαι παρασχέσθαι χειρο<unclear cert="high" reason="undefined">γρ</unclear>αφοῦντας τοὺς 

    <lb n="22"/>ἀνθρώπους, <del rend="erasure" status="unremarkable">εἰ δὲ μὴ</del> ἐὰν δὲ μὴ χειρογραφῶσιν, μη<unclear cert="high" reason="undefined">θενὶ</unclear> 

    <lb n="23"/>ἐπιτρέπειν ἀποβιάζεσθαι τὴν Ἀρσινόην. δέ<supplied cert="high" reason="lost">ομαι οὖν σου, βασιλεῦ</supplied>, 

    <lb n="24"/><del rend="erasure" status="unremarkable">προστάξαι Ἀγήνορι τῶι στρατηγῶι</del> ἐ<unclear cert="high" reason="undefined">π</unclear>ε<unclear cert="high" reason="undefined">ι</unclear><supplied cert="high" reason="lost">δ</supplied>ὴ ὁ <add place="supralinear">μὲν</add> <del rend="erasure" status="unremarkable">τε Εὔτυ<supplied cert="high" reason="lost">χος</supplied> </del> 

    <lb n="25"/><supplied cert="high" reason="lost">φθαρ</supplied>εὶς ὑπὸ τῆς Ἀρσινόης ἀνακεχώρηκεν ἵνα μὴ ἐνδείξη<unclear cert="high" reason="undefined">τα</unclear>ι 

    <lb n="26"/><supplied cert="high" reason="lost">ὡς ἔ</supplied><unclear cert="high" reason="undefined">στιν</unclear> αὑτοῦ ἡ οἰκία <del rend="erasure" status="unremarkable">αὐτή τε Ἀρσ<supplied cert="high" reason="lost">ι</supplied>ν<supplied cert="high" reason="lost">όη</supplied> γράφε<unclear cert="high" reason="undefined">ι</unclear> <unclear cert="high" reason="undefined">κ</unclear>[ <gap extent="unknown" reason="illegible" unit="character"/> </del> ] 

    <lb n="27"/><gap extent="5" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ένη <app type="editorial"><lem><seg cert="low" part="N">ἀνακαλέσηται</seg></lem><rdg>ἵν<unclear cert="high" reason="undefined">α</unclear> <unclear cert="high" reason="undefined">κ</unclear>αλέσ<unclear cert="high" reason="undefined">ητα</unclear>ι</rdg></app> 
