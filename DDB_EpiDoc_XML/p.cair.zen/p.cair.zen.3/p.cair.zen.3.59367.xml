    <lb n="26"/>Σωστράτωι. Σαμοῆλ<supplied cert="high" reason="lost">ις καὶ Ἀλέξανδρος </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="27"/>κωλυόμενοι <add place="supralinear">ὑπὸ τῶν <unclear cert="high" reason="undefined">πα</unclear><gap extent="unknown" reason="lost" unit="character"/></add> διοδ<unclear cert="high" reason="undefined">εύ</unclear><supplied cert="high" reason="lost">ειν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="28"/><add place="supralinear">καὶ τὸν κάλαμον <gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/></add> <del rend="erasure" status="unremarkable">ἐληλύθασιν ἡ<unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ῖν κ</supplied><unclear cert="high" reason="undefined">ατ</unclear>α<gap extent="4" reason="illegible" unit="character"/>[ <gap extent="unknown" reason="illegible" unit="character"/> </del> ] 

    <lb n="29"/>ὑφ᾽ ἡμῶν ἐν τῶι <num value="2">β</num> <expan>περ<ex cert="high">ιχώματι</ex></expan> παρὰ τ<unclear cert="high" reason="undefined">η</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="30"/><choice><corr>διαμεμαρητυρμένοι</corr><sic>δι<unclear cert="high" reason="undefined">α</unclear>μεμαρτυρμενοι</sic></choice> εἰσὶν <unclear cert="high" reason="undefined">πα</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="31"/>τὸ κτῆμα, ἀλλα᾽ ἀπόρρησιν ποι<unclear cert="high" reason="undefined">ο</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="32"/>δύνασθαι ἑτέρωθεν λαβεῖν, οὐ[κ ἐξὸν διὰ <unclear cert="high" reason="undefined">τῶν</unclear>] 

    <lb n="33"/>κτημάτων διὰ τέλους διοδε<unclear cert="high" reason="undefined">ύ</unclear>[ειν. φασὶν δὲ <unclear cert="high" reason="undefined">τοὺς</unclear>] 

    <lb n="34"/>τούτων κυρίους ἕως μὲν γίν<supplied cert="high" reason="lost">εσθαι </supplied><gap extent="8" reason="lost" unit="character"/>, 

    <lb n="35"/>εῦν δὲ κεκωλῦσθαι καὶ ὑπὸ <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ούτων. φρόντισον</supplied> 

    <lb n="36"/>οὖν <del rend="erasure" status="unremarkable">ὅπως</del> κ<unclear cert="high" reason="undefined">αὶ</unclear> <unclear cert="high" reason="undefined">ἀ</unclear>ξίωσον πε<unclear cert="high" reason="undefined">ρ</unclear><gap extent="1" reason="lost" unit="character"/>τ<unclear cert="high" reason="undefined">ο</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="37"/>ὅπως μὴ <supplied cert="high" reason="lost">ἐγκα</supplied>ταλίπωσι τὸ κ<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ῆμα, ἵνα μὴ διαπέσῃ</supplied> 

    <lb n="38"/>ἀργύριον τοσοῦτο, καὶ νῦν <unclear cert="high" reason="undefined">ην</unclear>αρ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="39"/>αὐτῶν ἐξεργά<unclear cert="high" reason="undefined">σ</unclear>ε<unclear cert="high" reason="undefined">σθ</unclear>αι <expan><ex cert="high">δραχμῶν</ex></expan> <num value="63">ξ<unclear cert="high" reason="undefined">γ</unclear></num>[<gap extent="1" reason="illegible" unit="character"/> <gap extent="unknown" reason="illegible" unit="character"/> ] 

    <lb n="40"/>αὐτοὺς καὶ σοὶ γεγρ<supplied cert="high" reason="lost">αφ</supplied>έναι. 

    <lb n="41"/>Σαμοήλι καὶ Ἀλεξά<supplied cert="high" reason="lost">νδρωι </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="42"/>ὑπολαμβάνοντε[ς ὑμᾶς καίπερ ἐξειληφότας <unclear cert="high" reason="undefined">τὸν</unclear>] 

    <lb n="43"/><expan><ex cert="high">ἑξακοντάρουρον</ex></expan> καὶ τοῦ κα<supplied cert="high" reason="lost">λάμου </supplied><gap extent="unknown" reason="lost" unit="character"/> 

