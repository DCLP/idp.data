    <lb n="35"/><gap extent="4" reason="illegible" unit="character"/><expan><ex cert="high">  </ex></expan> 

    <lb n="36"/>φυγῇ κέχρη<unclear cert="high" reason="undefined">νται</unclear> ἀλλ᾽ ἀδύνοντο <expan><unclear cert="high" reason="undefined">ἀ</unclear>πελθ<ex cert="high">εῖν</ex></expan> 

    <lb n="37"/>αὐ<unclear cert="high" reason="undefined">τῶ</unclear>ν πάλ<supplied cert="high" reason="lost">ι</supplied>ν βιαζομένων 

    <lb n="38"/><choice><corr>ἐκείνους</corr><sic>εκινου<unclear cert="high" reason="undefined">ς</unclear></sic></choice> γ<unclear cert="high" reason="undefined">εω</unclear><supplied cert="high" reason="lost">ργεῖ</supplied><unclear cert="high" reason="undefined">ν</unclear> καὶ <unclear cert="high" reason="undefined">τ</unclear>ούτους 

    <lb n="39"/>βιάζονται. δ<supplied cert="high" reason="lost">ε</supplied>όμεθα <choice><corr>ἐπειδὴ</corr><sic>επιδη</sic></choice> πρό-

    <lb n="40"/>εισιν ἀπογ<supplied cert="high" reason="lost">ρ</supplied>αφαὶ οἵ τε δεσπόται 

    <lb n="41"/>ἑστήκασιν <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ρ</supplied>οστάξαι προνοίᾳ 

