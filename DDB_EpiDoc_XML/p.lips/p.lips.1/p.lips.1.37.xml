    <lb n="15"/>εἰς τὸ ἡμέτερον <app type="BL"><lem resp="1.207"><unclear cert="high" reason="undefined">ὄ</unclear>ργα<unclear cert="high" reason="undefined">νο</unclear><supplied cert="high" reason="lost">ν</supplied> ἐξαφῆ<unclear cert="high" reason="undefined">κα</unclear><supplied cert="high" reason="lost">ν π</supplied>ρὸς <supplied cert="high" reason="lost">πι</supplied>εῖν</lem><rdg><gap extent="1" reason="lost" unit="character"/>ργα<gap extent="2" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐ</supplied>ξα<supplied cert="high" reason="omitted">ί</supplied>φνης <gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ι</unclear>ο<gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ν</rdg></app> τ<gap extent="1" reason="illegible" unit="character"/><gap extent="4" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> 

    <lb n="17"/>ἑαυτῶν ἐν τοῖ<supplied cert="high" reason="lost">ς</supplied> <app type="BL"><lem resp="1.207">αὐτ<supplied cert="high" reason="lost">οῖς πε</supplied>δίοις μου προσεποιησα<gap extent="unknown" reason="lost" unit="character"/></lem><rdg>ε<gap extent="1" reason="illegible" unit="character"/><gap extent="5" reason="lost" unit="character"/>διοις μ<unclear cert="high" reason="undefined">ου</unclear><gap extent="4" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/></rdg></app> 

    <lb n="18"/>ὁ ἡμέτερος <app type="BL"><lem resp="1.207">ποι<supplied cert="high" reason="lost">μὴν </supplied><gap extent="2" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/>βιος τ<unclear cert="high" reason="undefined">ὸ</unclear> <supplied cert="high" reason="lost">ὄνο</supplied>μα</lem><rdg><gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ι</unclear><gap extent="6" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/>βιος τ<unclear cert="high" reason="undefined">ὸ</unclear> <gap extent="5" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ια</unclear></rdg></app> ἐκ <gap extent="3" reason="illegible" unit="character"/><gap extent="9" reason="lost" unit="character"/> 

    <lb n="19"/>οἱ δὲ <app type="BL"><lem resp="1.207">γυμνώ<supplied cert="high" reason="lost">σαντες</supplied></lem><rdg>γυμν<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ῦντες </supplied><gap extent="3" reason="lost" unit="character"/></rdg></app> ε<supplied cert="high" reason="lost">ὐθ</supplied>ὺς <app type="BL"><lem resp="1.207">μετὰ ῥοπαλῶν πρ<gap extent="4" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ν</unclear><gap extent="3" reason="illegible" unit="character"/></lem><rdg>μετ<gap extent="3" reason="illegible" unit="character"/>παλων <gap extent="2" reason="illegible" unit="character"/>ρ<gap extent="4" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ν</unclear><gap extent="3" reason="illegible" unit="character"/></rdg></app> 

    <lb n="20"/>τὴν ἐπικιμένην α<supplied cert="high" reason="lost">ὐτοῦ ἐ</supplied>σθῆτα διαρ<supplied cert="high" reason="lost">ή</supplied>ξαντες ἀφ<supplied cert="high" reason="lost">εί</supplied>λαντ<supplied cert="high" reason="lost">ο</supplied>,

    <lb n="21"/>ἔπειτα κατέκοψα<supplied cert="high" reason="lost">ν</supplied> <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ληγ</supplied>αῖς αὐτὸν κατά τ<supplied cert="high" reason="lost">ε</supplied> τῶν σκελῶν καὶ 

    <lb n="22"/>κατὰ τῶν ἄλλων μελ<unclear cert="high" reason="undefined">ῶ</unclear><supplied cert="high" reason="lost">ν</supplied> τοῦ σώματος, <app type="BL"><lem resp="1.207">ἡμιθανὴν</lem><rdg>ἡμιθανῆ</rdg></app> αὐτὸν <app type="BL"><lem resp="1.207">κατα-

    <lb n="23"/>στήσαντες</lem><rdg><supplied cert="high" reason="lost">πο</supplied><unclear cert="high" reason="undefined">ι</unclear>ήσαντεσ</rdg></app> ὡς κα<supplied cert="high" reason="lost">ὶ</supplied> φα<unclear cert="high" reason="undefined">νε</unclear><supplied cert="high" reason="lost">ρ</supplied>ά ἐστιν τὰ προσφωνηθέντα <unclear cert="high" reason="undefined">ὑπὸ</unclear> 

    <lb n="24"/>τῶν ἐπιθεωρησάντων τὰ πλήγματα, ἀδήλου ο<supplied cert="high" reason="lost">ὔ</supplied>σης τῆς 

    <lb n="25"/>ἑαυτοῦ σωτηρίας. διὰ το<supplied cert="high" reason="lost">ῦτ</supplied>ο καὶ νῦν ἐπιδίδωμι τῇ σῇ λογιότητι 

    <lb n="26"/>τούσδε μου τοὺς λιβέλλου<supplied cert="high" reason="lost">ς</supplied> μαρτυρόμενος καὶ <app type="BL"><lem resp="1.207">ἀξιῶν</lem><rdg>ἀξιῶ</rdg></app> τούτο<supplied cert="high" reason="lost">υς</supplied> 

    <lb n="27"/>ἐν ἀσφαλεῖ εἶναι μέχρις τῆς εὐτυχοῦς ἐπιδημίας τοῦ κυρίου μου 

    <lb n="28"/>τοῦ ἄρχοντος· ἑτοιμότατα γὰρ ἔχω ἀπελ<unclear cert="high" reason="undefined">έν</unclear>ξαι ἐν τῷ <app type="BL"><lem resp="1.207"><choice><corr>ἀχράν<supplied cert="high" reason="omitted">τῳ</supplied></corr><sic>αχραν</sic></choice></lem><rdg>ἀχρ<supplied cert="high" reason="lost">ά</supplied>ντῳ</rdg></app> αὐτοῦ 

    <lb n="29"/>δικαστηρίῳ τούτους λῃστὰς ὁμολό<supplied cert="high" reason="lost">γ</supplied>ους καὶ ζῶα <choice><corr>ἀπελ<supplied cert="high" reason="omitted">ηλ</supplied>ακότας</corr><sic>απελακοτας</sic></choice> 

    <lb n="30"/>πολλάκις καὶ Ἰωνᾶν τὸν προκίμενον φονέα ἀποδεῖξαι. 

    <lb n="31"/>μετὰ τὴν <expan>προκ<ex cert="high">ειμένην</ex></expan>, Παχὼν <app type="BL"><lem resp="1.207"><num value="10">ι</num></lem><rdg><num value="1">α</num><supplied cert="low" reason="lost"><num value="4">δ</num> </supplied></rdg></app>. <expan>Φλ<ex cert="high">άυιος</ex></expan> Ἰσίδωρος ὁ <expan>προκ<ex cert="high">είμενος</ex></expan> ἐπιδέδωκα. 
