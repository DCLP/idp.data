    <lb n="1"/><handShift new="m2"/> ἔτους <num value="32">λβ</num> Θω<supplied cert="high" reason="lost">ὺθ</supplied> <num value="25">κε</num>. τέτακται <supplied cert="high" reason="lost">ἐ</supplied>πὶ τὴ<supplied cert="high" reason="lost">ν ἐν </supplied><app type="BL"><lem resp="1.185"><supplied cert="high" reason="lost">Λ</supplied>άτ<supplied cert="high" reason="lost">ων πό</supplied>λει</lem><rdg><supplied cert="high" reason="lost">Λ</supplied>ατ<supplied cert="high" reason="lost">ῶ</supplied>νος π<supplied cert="high" reason="lost">όλει</supplied></rdg></app> <expan>τρά<ex cert="high">πεζαν</ex></expan> ἐφ᾽ ἧς] <app type="BL"><lem resp="9.96">Χαιρ<supplied cert="high" reason="lost">ήμων</supplied></lem><rdg>Χατρ<supplied cert="high" reason="lost">εοῦς</supplied></rdg></app><supplied cert="high" reason="lost"> <expan><ex cert="high">εἰκοστῆς</ex></expan> <expan>ἐγκυ<ex cert="high">κλίου</ex></expan> κατὰ <expan>διαγρ<ex cert="high">αφὴν</ex></expan></supplied> 

    <lb n="2"/>Ἀγαθίνου τε<supplied cert="high" reason="lost">λώ</supplied>νου, ὑφ᾽ ἣν ὑπογράφει Ἀμ<supplied cert="high" reason="lost">μώ</supplied>νιος ὁ ἀντιγρ<supplied cert="high" reason="lost">αφεύς</supplied>, Ψενθώτη<supplied cert="high" reason="lost">ς Κελῆτος ὠνῆς <expan><ex cert="high">ἕκτου</ex></expan></supplied> 

    <lb n="3"/>μέρους γῆς <supplied cert="high" reason="lost">νη</supplied>σιωτίδος ἐν τῆι κάτω τοπαρχίαι τοῦ <expan>Λατ<supplied cert="high" reason="lost">οπο<ex cert="high">λίτου</ex></supplied></expan> ἀπὸ <expan><ex cert="high">ἀρουρῶν</ex></expan> <num value="10">ι</num>, ὧν α<supplied cert="high" reason="lost">ἱ γειτνίαι <expan>δεδήλ<ex cert="high">ωνται</ex></expan></supplied> 

    <lb n="4"/>διὰ τῆς συ<supplied cert="high" reason="lost">γγραφ</supplied>ῆς, ἧς ἐώνηται παρὰ <app type="BL"><lem resp="1.185">Ταγῶτ<supplied cert="high" reason="lost">ο</supplied>ς</lem><rdg>Ταγῶν<supplied cert="high" reason="lost">ο</supplied>σ</rdg></app> τῆς Ἀχιλλέως κα<supplied cert="high" reason="lost">ὶ</supplied> Ἀμ<supplied cert="high" reason="lost">μω</supplied>νίου <supplied cert="high" reason="lost">Ἀχιλλέως</supplied> 

    <lb n="5"/>ἔτους <num value="32">λβ</num> Θ<supplied cert="high" reason="lost">ωὺθ</supplied> <num value="25">κε</num> χαλκοῦ <expan><ex cert="high">ταλάντων</ex></expan> <supplied cert="high" reason="lost">δ</supplied>ύο τρι<supplied cert="high" reason="lost">σχι</supplied>λίων, <expan><ex cert="high">γίνονται</ex></expan> <expan><ex cert="high">τάλαντα</ex></expan> <num value="2">β</num>  <num value="3000">Γ</num>, <app type="BL"><lem resp="1.185"><expan><supplied cert="high" reason="lost">τέ</supplied>λ<ex cert="high">ος</ex></expan> οὗ <expan>ἀλ<ex cert="high">λαγὴ</ex></expan></lem><rdg><expan>τέλ<ex cert="high">οσ</ex></expan></rdg></app> ἑπτα<supplied cert="high" reason="lost">κοσίας</supplied> <expan>πεντή<ex cert="high">κοντα</ex></expan>, <expan><ex cert="high">γίνονται</ex></expan> <num value="7">ζ</num><supplied cert="high" reason="lost"><num value="50">ν</num></supplied>. </ab></div></div></body></text></TEI.2>