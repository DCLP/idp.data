    <lb n="1"/>καὶ ἐπειδ<supplied cert="high" reason="lost">ὴ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>παρ᾽ αὐτοῦ δέομαι <gap extent="4" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>ὥστε κελεύειν τὸν παρά<supplied cert="high" reason="lost">νομον τοῦτον</supplied> 

    <lb n="4"/>πρωτοτύπως μὲν ἀποδοῦ<supplied cert="high" reason="lost">ναι ὅσα ἔχων</supplied> 

    <lb n="5"/>τυγχάνει ἡμέτερα σκεύη <supplied cert="high" reason="lost">καὶ μηκέτι ἐκ</supplied> 

