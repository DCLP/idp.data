    <lb n="1"/>βασιλεῖ Πτολεμαίωι χαίρειν Λύκος. ἀδικο<supplied cert="high" reason="lost">ῦμαι ὑπὸ Πετεσίου </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> μισθω</supplied>-

    <lb n="2"/>σάμενος γὰρ τὸν Θεοκλ<supplied cert="high" reason="lost">έους κλῆρον </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>τὴν συγγραφὴν πρὸς αὐτ<supplied cert="high" reason="lost">ὸν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>εργώμεθα, τοῦ δὲ σπό<supplied cert="high" reason="lost">ρου </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>ἀλλὰ κατεσπείραμεν <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/>μενον ἀνάλωμα καὶ συν<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/>πλεονέκτης ὢν προεστ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>διασαφῆσαι ἀμφοτερ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>τοῦ κλήρου, ἐν δὲ τῶι <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/>ὑπὸ Λυκομήδους εἰς κ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/>μετροῦν αὐτῶι σπέρμα <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="12"/>διον κατῆγον, ὁ δὲ αἰς<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/>σπέρμα οὐδὲ τὰ ζ<unclear cert="high" reason="undefined">εύ</unclear>γ<supplied cert="high" reason="lost">η </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="14"/>μεμέτρηκεν εἰς τὸ βασ<supplied cert="high" reason="lost">ιλικὸν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="15"/>τοῦ <expan><ex cert="low">ἑκατονταρούρου</ex></expan>. δέομαι οὖ<supplied cert="high" reason="lost">ν σου, βασιλεῦ, </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="16"/>γράψαι Μοσχίωνι τῶι <supplied cert="high" reason="lost">ἐπιστάτηι </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐπι</supplied>-

    <lb n="17"/>σκεψάμενον τὴν συγ<unclear cert="high" reason="undefined">γ</unclear><supplied cert="high" reason="lost">ραφὴν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="18"/>Πετέσιον τὸν τελ<gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="19"/>σησάμου καὶ ἐὰν ἦι <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="20"/>δοῦναί με αὐτῶι τ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="21"/>τοῦ σησάμου τὸ ἐπιβ<supplied cert="high" reason="lost">άλλον </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> εἰς τὸν λοι</supplied>-

    <lb n="22"/>πὸν χ<unclear cert="high" reason="undefined">ρ</unclear>όνον τῆς μισθώ<supplied cert="high" reason="lost">σεως. </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="23"/>τεύξομαι τοῦ δικαίου. <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="24"/><space dim="horizontal" extent="unknown" unit="character"/> 

    <lb n="24"/><handShift new="m2"/> Μοσχίωνι. <supplied cert="high" reason="lost">μ</supplied>άλιστα μὲν διάλυσον αὐτούς· εἰ δὲ μή, <supplied cert="high" reason="lost">πρὸς ἡμᾶς <expan>ἀπό<ex cert="high">στειλον</ex></expan> <expan>ὅπ<ex cert="high">ως</ex></expan> <expan>ἐπι<ex cert="high">σκεψώμεθα</ex></expan></supplied>. 

