    <lb n="10"/>σ<unclear cert="high" reason="undefined">φρά</unclear>γι<supplied cert="high" reason="lost">σ</supplied>ον, καὶ <choice><corr>δώσεις</corr><sic>δωσις</sic></choice> <unclear cert="high" reason="undefined">τ</unclear>ῷ ἀδελφῷ <unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ου</supplied>

