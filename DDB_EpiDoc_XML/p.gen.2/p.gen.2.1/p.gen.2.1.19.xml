    <lb n="1"/>Τυράνν<supplied cert="high" reason="lost">ῳ</supplied> τῷ καὶ Ἰ<unclear cert="high" reason="undefined">σι</unclear>δώρῳ καὶ Νίννωι τ<unclear cert="high" reason="undefined">ῷ</unclear> καὶ <expan>Χρυσίπ<ex cert="high">πῳ</ex></expan> <expan>γεγυ<ex cert="high">μνασιαρχηκόσιν</ex></expan> <expan>ἐπικ<ex cert="high">ριταῖς</ex></expan>

    <lb n="2"/>παρὰ Ἥρωνος Σουχίωνος τοῦ Διοδώρου, <expan>μη<ex cert="high">τρὸς</ex></expan> Ἑρμιόνης, καὶ τῆς

    <lb n="3"/>γενομένη<unclear cert="high" reason="undefined">ς</unclear> καὶ ἀποπεπληγμένης αὐτοῦ γυναικὸς Θαυβαρίου τῆς Ἥρωνος

    <lb n="4"/>τοῦ Ἥρ<unclear cert="high" reason="undefined">ω</unclear>νος, ἀμφοτέρων <expan>ἀναγρ<ex cert="high">αφομένων</ex></expan> τοῦ μὲν Ἥρωνος ἐπ᾽ ἀμφόδου <choice><corr>Ἑλληνείου</corr><sic>ελληνιου</sic></choice>,

    <lb n="5"/>τῆς δὲ <unclear cert="high" reason="undefined">Θα</unclear>υβαρίου ἐπὶ <expan>Βιθ<ex cert="high">υνῶν</ex></expan> <expan>Ἄ<ex cert="high">λλων</ex></expan> <expan>Τό<ex cert="high">πων</ex></expan> μετὰ κυρίου αὐτοῦ Ἥρωνος. τοῦ ἐξ <expan>ἀλλήλ<ex cert="high">ων</ex></expan>

    <lb n="6"/>υἱοῦ Ἰσιδ<unclear cert="high" reason="undefined">ώ</unclear><supplied cert="high" reason="lost">ρ</supplied><unclear cert="high" reason="undefined">ο</unclear>υ <expan>προσβ<ex cert="high">άντος</ex></expan> εἰς <expan><ex cert="high">τρεισκαιδεκαετεῖς</ex></expan> τῷ ἐνεστῶτι <num value="11">ια</num> <expan><ex cert="high">ἔτει</ex></expan> Ἀντωνίνου Καίσαρος τοῦ <expan>κυρίο<ex cert="high">υ</ex></expan>

    <lb n="7"/>κα<unclear cert="high" reason="undefined">ὶ</unclear> <expan>ὀφε<supplied cert="high" reason="lost">ί</supplied><unclear cert="high" reason="undefined">λ</unclear><ex cert="high">οντος</ex></expan> <expan>ἐπικριθ<ex cert="high">ῆναι</ex></expan>, ὑ<unclear cert="high" reason="undefined">π</unclear>ετάξαμεν τὰ δίκαια. ἐγὼ μὲν οὖν ὁ Ἥρων

    <lb n="8"/><expan>ἀπεγ<unclear cert="high" reason="undefined">ρ</unclear>[<ex cert="high">αψάμην</ex></expan> τῇ] τοῦ <num value="2">β</num> <expan><ex cert="high">ἔτους</ex></expan> θεοῦ Ἁδριανοῦ <expan>ἀπογρ<ex cert="high">αφῇ</ex></expan> ἐπὶ Φανησίου, τῇ δὲ τοῦ <num value="16">ιϛ</num> <expan><ex cert="high">ἔτους</ex></expan>

    <lb n="9"/>θ<unclear cert="high" reason="undefined">εοῦ</unclear> Ἁδρ<supplied cert="high" reason="lost">ιαν</supplied><unclear cert="high" reason="undefined">οῦ</unclear> καὶ τῇ <unclear cert="high" reason="undefined">τ</unclear>οῦ <num value="9">θ</num> <expan><ex cert="high">ἔτους</ex></expan> Ἀντωνίνου Καίσαρος τοῦ κυρίου κατ᾽ <expan>οἰκ<ex cert="high">ίαν</ex></expan>

    <lb n="10"/><expan>ἀπογρ<ex cert="high">αφῇ</ex></expan> ἐ<supplied cert="high" reason="lost">π</supplied><unclear cert="high" reason="undefined">ὶ</unclear> <choice><corr>Ἑλληνείου</corr><sic>ελληνιου</sic></choice> <expan>συναπογρ<ex cert="high">αψάμενος</ex></expan> ἐν τῇ τοῦ <num value="9">θ</num> <expan><ex cert="high">ἔτους</ex></expan> <expan>ἀπογρ<ex cert="high">αφῇ</ex></expan> καὶ τὸν <expan>ἐπικρινό<ex cert="high">μενον</ex></expan>

    <lb n="11"/>υἱὸν Ἰσί<unclear cert="high" reason="undefined">δ</unclear>ωρον. <choice><corr>καὶ ἐγὼ</corr><sic>καγω</sic></choice> δὲ ἡ Θαυβάριον <expan>ἀπεγρ<ex cert="high">αψάμην</ex></expan> τῇ τοῦ <num value="16">ιϛ</num> <expan><ex cert="high">ἔτους</ex></expan> καὶ τῇ τοῦ <num value="9">θ</num> <expan><ex cert="high">ἔτους</ex></expan> ἐπὶ <expan>Βιθ<ex cert="high">υνῶν</ex></expan> <expan>Ἄ<ex cert="high">λλων</ex></expan> <expan>Τό<ex cert="high">πων</ex></expan>.

    <lb n="12"/>τὸν δὲ <expan>ἐπικρινό<ex cert="high">μενον</ex></expan> υἱὸν <unclear cert="high" reason="undefined">Ἰ</unclear>σίδωρον, ἀμφότεροι <expan>ἀπεγρ<ex cert="high">αψάμεθα</ex></expan> ἐν <expan>ἐπιγεγεννη<ex cert="high">μένοις</ex></expan> τῶι

    <lb n="13"/><num value="6">ϛ</num> <expan><ex cert="high">ἔτει</ex></expan> Ἀν<unclear cert="high" reason="undefined">τ</unclear>ωνίνου Καίσαρο<unclear cert="high" reason="undefined">ς</unclear> τοῦ κυρίου ἐπὶ <choice><corr>Ἑλληνείου</corr><sic>ελληνιου</sic></choice>. οἱ δὲ ἐμοῦ τῆς <expan>Θαυβαρί<ex cert="high">ου</ex></expan>

    <lb n="14"/>γον<unclear cert="high" reason="undefined">ε</unclear>ῖς <expan>ἀ[πεγρά<ex cert="high">ψαντο</ex>]</expan> τῇ τοῦ <num value="2">β</num> <expan><ex cert="high">ἔτους</ex></expan> <expan>ἀπογρ<ex cert="high">αφῇ</ex></expan> μηδ<unclear cert="high" reason="undefined">έπ</unclear>ω μου <expan>γεννηθ<ex cert="high">είσης</ex></expan> ἐπὶ <expan>Βιθ<ex cert="high">υνῶν</ex></expan> <expan>Ἄ<ex cert="high">λλων</ex></expan> <expan><unclear cert="high" reason="undefined">Τ</unclear>ό<ex cert="high">πων</ex></expan>. <handShift new="m2"/>Νίννος

    <lb n="15"/>ὁ καὶ Χ<unclear cert="high" reason="undefined">ρύ</unclear><supplied cert="high" reason="lost">σιπ</supplied>πος <expan>γεγυ<ex cert="high">μνασιαρχηκὼς</ex></expan> δι᾽ Ἀμμωνίο[υ <expan>γ]ρ<ex cert="high">αμματέως</ex></expan> <expan>σεση<ex cert="high">μείωμαι</ex></expan> Ἰσίδωρον Ἥ<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ω</supplied>νος τοῦ

    <lb n="16"/>Σουχί<supplied cert="high" reason="lost">ωνος μ</supplied>ητρὸς Θαυβαρίου. <expan><ex cert="high">ἔτους</ex></expan> <num value="11">ι<supplied cert="high" reason="lost">α</supplied></num><supplied cert="high" reason="lost"> Ἀ</supplied>ντωνείνου Καίσαρο<supplied cert="high" reason="lost">ς το</supplied><unclear cert="high" reason="undefined">ῦ</unclear> κυρίου Μεσορὴ <num value="30">λ</num>.
