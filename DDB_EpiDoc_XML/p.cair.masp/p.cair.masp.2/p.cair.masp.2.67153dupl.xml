    <lb n="32"/>δ<supplied cert="high" reason="lost">ί</supplied><unclear cert="high" reason="undefined">χ</unclear><supplied cert="high" reason="lost">α κ</supplied><unclear cert="high" reason="undefined">ρ</unclear>ίσεω<supplied cert="high" reason="lost">ς κ</supplied>αὶ δίκης καὶ οἵας δήποτ<supplied cert="high" reason="lost">ε</supplied> <choice><corr>εὑρησιλογί<supplied cert="high" reason="lost">ας</supplied></corr><sic>ευρεσιλογι<supplied cert="high" reason="lost">ας</supplied></sic></choice> <supplied cert="high" reason="lost">καὶ τὸ</supplied> 

