    <lb n="1"/>[<g type="stauros"/> βα]σιλείας καὶ ὑπ<supplied cert="high" reason="lost">ατ</supplied>είας τοῦ θειοτάτο[υ ἡμῶν <expan>δ]εσπότο<ex cert="high">υ</ex></expan> Φλαυ<hi rend="diaeresis"><unclear cert="high" reason="undefined">ΐ</unclear></hi><supplied cert="high" reason="lost">ου</supplied> 

    <lb n="2"/>[Ἰουστίνου τοῦ <expan>αἰωνίο<ex cert="high">υ</ex></expan>] <expan>αὐγούστο<ex cert="high">υ</ex></expan> Α<supplied cert="high" reason="lost">ὐτοκρ</supplied><unclear cert="high" reason="undefined">ά</unclear><supplied cert="high" reason="lost">τ</supplied>ορος ἔτους τρί<supplied cert="high" reason="lost">τ</supplied>ο<unclear cert="high" reason="undefined">υ</unclear>, 

    <lb n="3"/><supplied cert="high" reason="lost">Πα</supplied>χὼν δωδεκ<supplied cert="high" reason="lost">άτῃ, ἀ</supplied>ρχῆς δευτ[έρας <expan>ἰν]δ<ex cert="high">ικτίονος</ex></expan>. ἐν <expan>Ἀντι<ex cert="high">νόου</ex></expan> <expan>πόλ<ex cert="high">ει</ex></expan> τῇ <expan>λα<unclear cert="high" reason="undefined">μ</unclear>προ<ex cert="high">τάτῃ</ex></expan>. 

    <lb n="4"/><g type="chirho"/> Αὐρήλιος <unclear cert="high" reason="undefined">Μη</unclear><supplied cert="high" reason="lost">νᾶς υ</supplied>ἱὸς <expan>Ὡρουωγχίο<ex cert="high">υ</ex></expan>, ἐ<supplied cert="high" reason="lost">κ μη</supplied>τρὸς Τσίας <app type="editorial"><lem>κραμπι<unclear cert="high" reason="undefined">τᾶς</unclear></lem><rdg>Κραμπι<unclear cert="high" reason="undefined">τος</unclear></rdg></app>, 

