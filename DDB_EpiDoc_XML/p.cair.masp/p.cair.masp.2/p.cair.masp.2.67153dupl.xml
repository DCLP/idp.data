    <lb n="25"/>ἢ <expan>μ<unclear cert="high" reason="undefined">ε</unclear>[γάλο<ex cert="high">υ</ex>]</expan>, περὶ οὐδέ<supplied cert="high" reason="lost">νος τ</supplied><unclear cert="high" reason="undefined">ὸ</unclear> <unclear cert="high" reason="undefined">σ</unclear>ύνο<unclear cert="high" reason="undefined">λ</unclear><supplied cert="high" reason="lost">ον</supplied>, καὶ ἐξ<unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">ῖναι ἑκατέρῳ</supplied> 

    <lb n="26"/>μέρ<supplied cert="high" reason="lost">ει ἡμ</supplied>ῶν ἑτέρῳ γ<supplied cert="high" reason="lost">άμῳ</supplied> προσο<unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ιλ</supplied>ῆσαι, δι<unclear cert="high" reason="undefined">ὰ</unclear> <supplied cert="high" reason="lost">τὸ ἀμφοτέρους</supplied> 

    <lb n="27"/>ἡμ<supplied cert="high" reason="lost">ᾶς τοῦ</supplied>το τὸ ῥεπ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ύδιο</supplied>ν πρὸς <supplied cert="high" reason="lost">ἀλ</supplied>λή<unclear cert="high" reason="undefined">λ</unclear><supplied cert="high" reason="lost">ους διαπέμψασθαι</supplied>. 

    <lb n="28"/>κ<unclear cert="high" reason="undefined">αὶ</unclear> <supplied cert="high" reason="lost">εἰ συμβ</supplied><unclear cert="high" reason="undefined">α</unclear>ίη τινα ἐξ ἡ<supplied cert="high" reason="lost">μῶν πα</supplied>ραβ<supplied cert="high" reason="lost">ῆ</supplied>ναι τα[ῦτα τὰ <expan>προγεγραμμ<ex cert="high">ένα</ex></expan> καὶ] 

    <lb n="29"/>ἐπε<unclear cert="high" reason="undefined">ξ</unclear><supplied cert="high" reason="lost">ελθ</supplied>εῖν κατὰ τοῦ ἑτ<unclear cert="high" reason="undefined">έ</unclear><supplied cert="high" reason="lost">ρου π</supplied>ερὶ οἵ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">υ δ</supplied>ήποτε <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ράγμ</supplied><unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">τος, παρέξει</supplied> 

    <lb n="30"/>τὸ παραβαῖνον μέρος τῷ ἐμμ<supplied cert="high" reason="lost">ένο</supplied>ντι <expan>χρ[υσο<ex cert="high">ῦ</ex></expan> <expan>ν]ομίσμ<ex cert="high">ατα</ex></expan> <unclear cert="low" reason="undefined">ἓ</unclear><supplied cert="low" reason="lost">ξ</supplied>[ <num value="6"/> <expan>ζυγ<ex cert="high">ῷ</ex></expan>] 

