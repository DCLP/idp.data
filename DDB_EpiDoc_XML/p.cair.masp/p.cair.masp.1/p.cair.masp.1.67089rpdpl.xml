    <lb n="21" rend="inverse"/> φύλακας <add place="supralinear"><unclear>ἀ</unclear>κρι<supplied reason="lost">β</supplied>εῖς</add> καταστήσας <g type="chirho"/> <g type="chirho"/> <g type="chirho"/> 
