    <lb n="2"/>Βασιλείου τ<supplied cert="high" reason="lost">οῦ</supplied> ἐνδοξοτ<supplied cert="high" reason="lost">ά</supplied>του 

    <lb n="3"/>ἔτου<supplied cert="high" reason="lost">ς</supplied> εἰκοστ<supplied cert="high" reason="lost">οῦ τ</supplied>ετάρτο<supplied cert="high" reason="lost">υ</supplied>, 

