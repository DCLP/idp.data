    <lb n="19"/>χάρ<supplied cert="high" reason="lost">ι</supplied><unclear cert="high" reason="undefined">τά</unclear> <supplied cert="high" reason="lost">σ</supplied><unclear cert="high" reason="undefined">οι</unclear> <unclear cert="high" reason="undefined">ὁ</unclear><supplied cert="high" reason="lost">μο</supplied><unclear cert="high" reason="undefined">λογή</unclear>σω, κύριε. διευτύχει. 

    <lb n="20"/>Αὐρήλιος <supplied cert="high" reason="lost">Ἀβ</supplied><unclear cert="high" reason="undefined">ο</unclear>ῦλ ἐπιδέδωκα. 

    <lb n="21"/>ὑπατείας τῶν δεσπ<supplied cert="high" reason="lost">οτ</supplied>ῶν ἡμῶν <choice><corr>Κωνσταντίου</corr><sic>κωνσταντινου</sic></choice> 

    <lb n="22"/>τὸ <num value="4">δ</num> καὶ <choice><corr>Κώνσταντος</corr><sic>κωσταντος</sic></choice> τὸ <num value="3">γ</num> <choice><corr>Αὐγούστων</corr><sic>αγουστων</sic></choice>, 

    <lb n="23"/>Ἐπεὶφ <num value="5">ε</num>. 

