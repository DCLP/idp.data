    <lb n="18"/>ἀννών<supplied cert="high" reason="lost">α</supplied>ς τοῦ ἐνιαυτοῦ <choice><corr>ἐγκλεῖσαι</corr><sic><subst><add place="inline">ενκλισαι</add><del rend="corrected" status="unremarkable">ννκλισαι</del></subst></sic></choice> 

    <lb n="19"/>ἐπὶ τὰ κά<supplied cert="high" reason="lost">σ</supplied>τρα ἐξ ἐντυχίας τῶ<hi rend="supraline">ν</hi> 

    <lb n="20"/>ἀκτου<unclear cert="high" reason="undefined">α</unclear>ρίων τῆς ἄνω Θηβα<hi rend="diaeresis">ί</hi>δος, 

