    <lb n="16" type="inWord"/>ρῶς βουλόμενο<supplied reason="lost">ι</supplied> <app type="editorial"><lem resp="BGU 2 p.355">μ<supplied reason="lost">οι</supplied></lem><rdg>μ<gap reason="lost" quantity="2" unit="character"/></rdg></app> ἐργολα

    <lb n="17" type="inWord"/>βεῖν κα<supplied reason="lost">ὶ</supplied> <choice><corr>πράσσειν</corr><sic>πρασιν</sic></choice> <app type="editorial"><lem resp="BGU 2 p.355">ἃ <expan>ο<ex>ὐ</ex></expan>κ</lem><rdg><unclear>α</unclear><gap reason="lost" quantity="1" unit="character"/><unclear>ο</unclear></rdg></app> <choice><corr>ὀφείλω</corr><sic><app type="editorial"><lem resp="BGU 2 p.355">οφιλω</lem><rdg>ωφιλω</rdg></app></sic></choice> 

    <lb n="18"/>πρὸς καταλοχισμοὺς τέλη 

    <lb n="19"/>τοῦ συμβόλου <app type="editorial"><lem resp="BGU 2 p.355"><unclear>παρ</unclear>ʼ<certainty match=".." locus="value"/></lem><rdg><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><unclear>ε</unclear><gap reason="lost" quantity="2" unit="character"/></rdg></app> ἐμοὶ ὄν

