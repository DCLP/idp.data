    <lb n="1"/>λήμφθη τοῖς ὑπομνήμα<unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">ι </supplied><gap extent="2" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>αντα<gap extent="3" reason="illegible" unit="character"/> 

    <lb n="2"/>οἱ διαδικαζόμενοι περὶ ὧ[ν <app type="BL"><lem resp="cf. 1.41">ἔ<unclear cert="high" reason="undefined">χ</unclear>ουσι</lem><rdg><gap extent="2" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/>τι</rdg></app> π<supplied cert="high" reason="lost">ρὸ</supplied>ς 

    <lb n="3"/>ἀλλήλους εἰπεῖν ἐπὶ τοῦ <supplied cert="high" reason="lost">κρα</supplied><unclear cert="high" reason="undefined">τίσ</unclear><supplied cert="high" reason="lost">του</supplied> δι-

    <lb n="4"/>καιοδότου τοῦ πράγματο<supplied cert="high" reason="lost">ς</supplied> <app type="BL"><lem resp="cf. 1.41">τοιούτου</lem><rdg><unclear cert="high" reason="undefined">ἀλύ</unclear>του</rdg></app> 

    <lb n="5"/>ὄν<unclear cert="high" reason="undefined">τος</unclear> ὁποῖον ἦλ<unclear cert="high" reason="undefined">θ</unclear>εν <app type="BL"><lem resp="cf. 1.41">ἐπ᾽ ἐμὲ</lem><rdg>ἐ<gap extent="4" reason="illegible" unit="character"/><gap extent="2" reason="illegible" unit="character"/></rdg></app>. ἀμφο<supplied cert="high" reason="lost">τ</supplied>έ-

    <lb n="6"/>ρων ἀξιούντων προθεσμίαν λαβεῖν 

    <lb n="7"/>ὁ στρατηγὸς εἶπεν· μετὰ τὴν κατασπο-

