         <head n="8958" xml:lang="en">
            <date>AD 17</date>
            <placeName>Arsinoite</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>ἔτους τετάρτου Τιβερίου Καίσαρος Σεβαστοῦ μηνὸς Ἀπελλ<supplied reason="lost">αίου ἑπτακαι</supplied>

    <lb n="2" type="inWord"/>δεκάτῃ, Φ<supplied reason="lost">α</supplied>ῶφι ἑπτακαιδεκάτῃ  ἐν Διονυσιάδι τῇ πρὸς τοῖ<supplied reason="lost">ς χαλκωρυχίοις</supplied> 

    <lb n="3"/>τῆς Θεμίστου μερίδος τοῦ Ἀρσινοείτου ν<supplied reason="lost">ο</supplied>μοῦ. ἐμίσθωσ<supplied reason="lost">ε Θερμοῦθις</supplied> 

    <lb n="4"/>Στοτοήτιος ὡς ἐτῶν πεντήκοντα <num value="50"/> οὐλὴ <choice><corr>ὀφρύι</corr><sic>οφρυει</sic></choice> δεξιᾷ μ<supplied reason="lost">ετὰ κυρίου τοῦ</supplied> 

    <lb n="5"/><choice><corr>ἑαυτῆς</corr><sic>εατης</sic></choice> υἱοῦ Πολυδεύκου τοῦ Εὐφροσύνου ὡς ἐτῶν τριάκ<supplied reason="lost">οντα <num value="30"/> </supplied><gap reason="lost" quantity="5" unit="character"/> 

    <lb n="6"/>οὐλὴ μήλῳ ἀριστερῶι Σωτηρίχωι Πετεσούχου ὡς ἐτῶ<supplied reason="lost">ν </supplied><gap reason="lost" quantity="9" unit="character"/> 

    <lb n="7"/>οὐλὴ ῥινὶ μέσῃ καὶ τῷ τούτου <choice><corr>υἱῷ</corr><sic>υιου</sic></choice> Πετεσούχωι ὡς ἐτῶν <app type="BL"><lem resp="1.25"><unclear>τ</unclear><gap reason="lost" quantity="9" unit="character"/></lem><rdg><gap reason="lost" quantity="8" unit="character"/></rdg></app> <supplied reason="lost">οὐ</supplied>

    <lb n="8" type="inWord"/>λὴ μετώπῳ ἐξ ἀριστερῶν, τοῖς δυσὶ <num value="2"/> Πέρσαις τῆς <app type="editorial"><lem resp="BGU 1 p.396">ἐπιγο<supplied reason="lost">νῆς ἀλλήλων</supplied></lem><rdg>ἐπιγο<supplied reason="lost">νῆς </supplied><gap reason="lost" quantity="7" unit="character"/></rdg></app>

    <lb n="9"/>ἐγγύοις <app type="editorial"><lem resp="bgu 2 p.354">εἰς</lem><rdg>ἐπ</rdg></app> ἔκτισιν <app type="editorial"><lem resp="bgu 2 p.354">τὸν</lem><rdg>τὰ</rdg></app> ὑπάρχοντα αὐτῇ 

    <lb n="10"/>περὶ τὰ <app type="BL"><lem resp="1.25">χαλκω<supplied reason="lost">ρύχια <expan>πρό<ex>τερον</ex></expan></supplied></lem><rdg>χαλκω<supplied reason="lost">ρύχια </supplied><gap reason="lost" quantity="3" unit="character"/></rdg></app> 

    <lb n="11"/>Κεφάλωνος κλῆρον ἀρουρῶν δύο <num value="2"/> ἢ ὅσων ἐὰν <app type="BL"><lem resp="1.25">ᾖ <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="10" unit="character"/></lem><rdg>αἱρ<supplied reason="lost">ῆται ἡ Θερμοῦθις</supplied></rdg></app> 

    <lb n="12"/><app type="editorial"><lem resp="BGU 1 p.396">εἰς</lem><rdg>ἐπὶ</rdg></app> ἔτη δύο <num value="2"/> ἀπὸ τοῦ ἐνεστῶτος τετάρτου ἔτους Τιβερίου <app type="BL"><lem resp="1.25"><supplied reason="lost">Καίσαρος</supplied></lem><rdg><gap reason="lost" quantity="9" unit="character"/></rdg></app> 

    <lb n="13"/>ἐπὶ τρίτωι <choice><corr>μέρει</corr><sic>μερι</sic></choice> τοῖς μεμισθωμένοις τῶν <app type="BL"><lem resp="1.25">ἐγβη<supplied reason="lost">σομένων</supplied></lem><rdg>ἐγβαλ<supplied reason="lost">λομένων</supplied></rdg></app> 

    <lb n="14"/><choice><corr>κατʼ</corr><sic>καθ</sic></choice> ἔτος ἐκ τοῦ κλήρου γενημάτων καὶ ἐπιγενημάτω<supplied reason="lost">ν ὄντι</supplied> 

    <lb n="15"/>καθαρῷ ἀπὸ δημοσίων καὶ παντὸς εἴδους· μὴ <app type="BL"><lem resp="1.25"><choice><corr>ἐξέστω</corr><sic>εξεστωι</sic></choice></lem><rdg>ἐξέσται</rdg></app> <supplied reason="lost">τοῖς μεμισ</supplied>

    <lb n="16" type="inWord"/>θωμένοις προλιπεῖν τὴν μίσθωσιν ἐ<supplied reason="lost">ν</supplied>τὸς τοῦ χρόν<supplied reason="lost">ου, <app type="editorial"><lem>τὰ δὲ</lem><rdg>μήτε</rdg></app> </supplied><choice><corr><supplied reason="lost">κα</supplied>

    <lb n="17" type="inWord"/>τʼ</corr><sic><supplied reason="lost">κα</supplied>θ</sic></choice> ἔτος ἔργα πάντα τοῦ κλήρου <app type="editorial"><lem resp="BGU 1 p.396; 2 p.354">τούς τε χωματισμοὺς <supplied reason="lost">καὶ ποτισμοὺς</supplied></lem><rdg>τοῦ ἀναχωματισμοῦ <gap reason="lost" quantity="7" unit="character"/></rdg></app>

    <lb n="18"/>καὶ βοτανισμοὺς καὶ τὴν ἄλλην γεωργικὴν <app type="BL"><lem resp="1.25">ὑπουργ<supplied reason="lost">ίαν</supplied></lem><rdg>ὑπηρε<supplied reason="lost">σίαν</supplied></rdg></app> <app type="editorial"><lem resp="BGU 1 p.396"><supplied reason="lost">ἐπι</supplied>

    <lb n="19" type="inWord"/>τελείτωσαν</lem><rdg><supplied reason="lost">προ</supplied>στελείτωσαν</rdg></app> οἱ μεμισθωμένοι κατὰ καιρὸν <app type="BL"><lem resp="6.11"><unclear>ἐκ</unclear> <supplied reason="lost">τοῦ ἰδίου</supplied></lem><rdg><unclear>ἐκ</unclear><gap reason="lost" quantity="7" unit="character"/></rdg></app> 

    <lb n="20"/>καὶ σπείροντες <choice><corr>κατʼ</corr><sic>καθ</sic></choice> ἔτος, οἷς ἐὰν αἱρῆται ἡ Θερμοῦθις <app type="BL"><lem resp="1.25">τ<supplied reason="lost">ῆς</supplied></lem><rdg><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="4" unit="character"/></rdg></app> <supplied reason="lost">αὐτῆς</supplied> 

    <lb n="21"/>χορηγούσης τὸ δίμοιρον μέρος τῶν εἰς τὴν γῆν <app type="BL"><lem resp="1.25"><supplied reason="lost">σπειρομένων</supplied></lem><rdg><gap reason="lost" quantity="9" unit="character"/></rdg></app> 

    <lb n="22"/>τῶν μεμισθωμένων χορηγούντων τὸ λοιπὸν <gap reason="lost" quantity="8" unit="character"/> 

    <lb n="23"/><choice><corr>ἑαυτοῖς</corr><sic>εατοις</sic></choice> παρεχόμενοι βο<gap reason="illegible" quantity="3" unit="character"/>ο<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/>εε<gap reason="illegible" quantity="1" unit="character"/>πε<gap reason="illegible" quantity="2" unit="character"/> <gap reason="lost" quantity="8" unit="character"/> 

    <lb n="24"/>πρὸς τοὺς μεμισθω<supplied reason="lost">μένους </supplied><gap reason="lost" quantity="26" unit="character"/> 

    <lb n="25"/><unclear>τ</unclear>αν αἱρουμένων <unclear>π</unclear>ρ<gap reason="lost" quantity="30" unit="character"/> 

    <lb n="26"/>ἔσται <choice><corr>κατʼ</corr><sic>καθ</sic></choice> ἔτος ἐφαμ<gap reason="lost" quantity="25" unit="character"/><supplied reason="lost"> κλή</supplied>

    <lb n="27" type="inWord"/>ρου μέρος καὶ οἱ <app type="editorial"><lem resp="BGU 1 p.396; 2 p.354">μεμισ<supplied reason="lost">θωμένοι </supplied><gap reason="lost" quantity="16" unit="character"/><supplied reason="lost"> καὶ μετὰ</supplied></lem><rdg>μεμισ<supplied reason="lost">θωμένοι </supplied><gap reason="lost" quantity="23" unit="character"/></rdg></app> 

    <lb n="28"/>τὸν χρόνον παραδότωσαν οἱ μεμισθωμένοι <gap reason="lost" quantity="12" unit="character"/> 

    <lb n="29"/>χέρσου <app type="editorial"><lem resp="bgu 2 p.354; 3 p.2">ἀγρώστεως <choice><corr>δείσης</corr><sic>δισης</sic></choice> πάσης</lem><rdg>ἀγρῶν <gap reason="illegible" quantity="2" unit="character"/>εδι<gap reason="illegible" quantity="2" unit="character"/>πα<unclear>λ</unclear>ησ</rdg></app> θρύου δὲ καὶ καλάμου <gap reason="lost" quantity="6" unit="character"/> 

    <lb n="30"/>ἐὰν δέ τι τῶν προκιμένων παραβῇ ὁ μεμισθω<supplied reason="lost">μένος </supplied><gap reason="lost" quantity="4" unit="character"/> 

    <lb n="31"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> ἡ Θερμοῦθις <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="7" unit="character"/> 

    <lb n="32"/><gap reason="lost" extent="unknown" unit="line"/> </ab></div>
