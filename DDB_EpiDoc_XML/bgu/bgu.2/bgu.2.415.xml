    <lb n="3"/>Αὐτοκράτορος Κ<supplied cert="high" reason="lost">αί</supplied>σαρος Νέρουα <supplied cert="high" reason="lost">Τραιανοῦ Σεβαστοῦ</supplied> 

    <lb n="4"/>Γέρμ<supplied cert="high" reason="lost">ανικοῦ</supplied> Δακικοῦ μηνὸς Νέου Σεβ<supplied cert="high" reason="lost">αστοῦ </supplied><gap extent="2" reason="lost" unit="character"/> 

    <lb n="5"/>Ὡρίων Σαταβοῦτος καὶ Σαραπί<supplied cert="high" reason="lost">ων</supplied> 

    <lb n="6"/>Ἀμμωνίου καὶ Πόπλιος Φάβιος Δ<supplied cert="high" reason="lost">ιονύσι</supplied>ος 

    <lb n="7"/>καὶ Πα<supplied cert="high" reason="lost">νο</supplied>μγεὺς <supplied cert="high" reason="lost">Σ</supplied>τοτοήτιος καὶ Στ<supplied cert="high" reason="lost">οτοῆτις</supplied> 

    <lb n="8"/>Ἐριέως καὶ Πακῦσις Ἐριέως, Στοτοῆ<supplied cert="high" reason="lost">τις</supplied> 

    <lb n="9"/>Πετεσούχου καὶ Στοτοῆτις Ψενή<supplied cert="high" reason="lost">σεως</supplied>, 

    <lb n="10"/>οἱ ὀκτὼ <num value="8"/> Ἀμμωνίῳ Ἥρωνος ἀπέχειν 

    <lb n="11"/>αὐτὸν τὰς ἴσας <unclear cert="high" reason="undefined">ὧν</unclear> ὠφειλή<supplied cert="high" reason="lost">κ</supplied>α<supplied cert="high" reason="lost">σι</supplied> 

    <lb n="12"/>κατὰ διαγραφ<supplied cert="high" reason="lost">ὴ</supplied>ν τῆς Φίλου τρα<supplied cert="high" reason="lost">πέζης</supplied>, 

    <lb n="13"/>ἣν καὶ ἀναδέδωκεν αὐτοῖς εἰς <supplied cert="high" reason="lost">ἀκύ</supplied>-

    <lb n="14"/>ρωσιν, ἀργυρίου δραχμὰς τετρακο-

    <lb n="15"/>σίας τεσσαράκοντα <expan><ex cert="high">γίνονται</ex></expan> <expan><ex cert="high">δραχμαὶ</ex></expan> <num value="440">υμ</num>, 

    <lb n="16"/>καὶ μηδὲν α<supplied cert="high" reason="lost">ὐ</supplied>τοῖς <supplied cert="high" reason="lost">ἐνκαλεῖν περὶ</supplied> 

    <lb n="17"/>μηδ<supplied cert="high" reason="lost">ενὸ</supplied>ς ἁπ<supplied cert="high" reason="lost">λ</supplied>ῶς πράγ<supplied cert="high" reason="lost">ματο</supplied>ς <unclear cert="high" reason="undefined">ἐγγρά</unclear>-

    <lb n="18"/>πτου ἀγράφου μέχρι τῆ<supplied cert="high" reason="lost">ς ἐ</supplied>νεστώ-

    <lb n="19"/>σης ἡμέρας τρόπῳ μηδε<supplied cert="high" reason="lost">νί</supplied>, 

    <lb n="20"/>ἀκύρου οὔσης, ἧς ἐξέδοτο <supplied cert="high" reason="lost">αὐτοῖς</supplied> 

    <lb n="21"/>ὁ προγεγραμμένος τοῦ Ἀμμωνίου 

    <lb n="22"/>πατὴρ Ἥρων ἑτέρας ἀποχ<supplied cert="high" reason="lost">ῆς</supplied>. 

    <lb n="23"/><handShift new="m2"/> Ἀμμώ<supplied cert="high" reason="lost">νι</supplied>ος Ἥρωνος ἀπέχω παρὰ <supplied cert="high" reason="lost">τῶ</supplied>ν 

    <lb n="24"/>προγεγρ<supplied cert="high" reason="lost">α</supplied>μμέν<supplied cert="high" reason="lost">ω</supplied>ν τὰς τοῦ ἀργυρίου 

    <lb n="25"/>δραχμὰς τετρακοσίας τεσσαράκοντα <num value="440"/>, 

    <lb n="26"/>ἃς ἔδωκάν μοι κατὰ <expan>διαγρα<ex cert="high">φὴν</ex></expan> τ<supplied cert="high" reason="lost">ῆς Φί</supplied>λου 

    <lb n="27"/>τραπέζης Ἀγορᾶς Ἱματίων, ἣν κ<supplied cert="high" reason="lost">αὶ ἀ</supplied>να-

    <lb n="28"/>δέδωκα <choice><corr>εἰς</corr><sic>ις</sic></choice> ἀκύρωσιν καὶ οὐδὲν αὐτοῖς 

    <lb n="29"/>ἐνκαλῶ περὶ μ<supplied cert="high" reason="lost">ηδ</supplied>ενὸς ἁπλῶ<supplied cert="high" reason="lost">ς πράγ</supplied>-

    <lb n="30"/>ματος μέχρι τῆς <supplied cert="high" reason="lost">ἐ</supplied>νεστώσης ἡμέρας 

    <lb n="31"/>ἀκύρου οὔσης, ἧς ἐξέδοτο αὐτοῖς 

    <lb n="32"/>ὁ προγεγραμμ<supplied cert="high" reason="lost">ένος</supplied> μου πατὴρ ἐν-

    <lb n="33"/>γ<supplied cert="high" reason="lost">ρά</supplied>φου ἀποχῆς κ<supplied cert="high" reason="lost">αθὼς</supplied> π<supplied cert="high" reason="lost">ρόκ</supplied>ιται. </ab></div></body></text></TEI.2>