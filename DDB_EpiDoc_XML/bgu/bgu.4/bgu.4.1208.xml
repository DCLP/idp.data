    <lb n="30"/>τῶν ἕως <unclear cert="high" reason="undefined">τοῦ</unclear> συνκεχωρισμένο<unclear cert="high" reason="undefined">υ</unclear> <unclear cert="high" reason="undefined">τ</unclear><gap extent="2" reason="illegible" unit="character"/>τ<unclear cert="high" reason="undefined">ασ</unclear>ν<unclear cert="high" reason="undefined">ωε</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/> 

    <lb n="31"/>ὠνῶν ἐπιβαλομένου Ἀν<unclear cert="high" reason="undefined">τιλό</unclear>χου καθάπερ <unclear cert="high" reason="undefined">ἐπὶ</unclear> 

    <lb n="32"/>τῶν ἄλλων διαμισθ<unclear cert="high" reason="undefined">οῦ</unclear>ν, καὶ <unclear cert="high" reason="undefined">σοῦ</unclear> <unclear cert="high" reason="undefined">μνησ</unclear>θέ<unclear cert="high" reason="undefined">ντος</unclear> <unclear cert="high" reason="undefined">δαν</unclear> 

