         <head n="18612" xml:lang="en">
            <date>17/16BC</date>
            <placeName>Alexandria</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><supplied reason="lost">Πρωτάρχωι παρὰ Σωσιπάτρου τοῦ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> καὶ παρὰ Ἀριστοκλήας τῆς </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Περσίνης μετὰ κυρίου τοῦ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> καὶ Τρυφαίνης τῆς </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Περσίνης μετὰ κυρίου 
τοῦ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"><foreign xml:lang="la">.</foreign> συνχωροῦσιν ἡ Ἀριστοκλήα καὶ Τρύφαινα ἔχειν παρὰ τοῦ</supplied> 

    <lb n="1"/><supplied reason="lost">Σωσιπάτρου</supplied> <choice><corr>δάνειον</corr><sic><unclear>δανηον</unclear></sic></choice> διὰ <choice><corr>χειρ<supplied reason="lost">ὸς</supplied></corr><sic>χιρ<supplied reason="lost">ος</supplied></sic></choice> <supplied reason="lost">ἐξ οἴκου <expan>α᾿ργυ<ex>ρίου</ex></expan> Πτολε</supplied>

    <lb n="2" type="inWord"/>μαικοῦ δραχμὰς χιλίας ὀγδοήκοντα <num value="1080"/> <unclear>ἄτ</unclear><supplied reason="lost">οκον,</supplied> 

    <lb n="3"/>ὃ καὶ <choice><corr>ἀποδώσειν</corr><sic>αποδωσιν</sic></choice> ἐν μησὶν δέκα <num value="10"/> οὖσιν ἀ<unclear>πὸ</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>τοῦ ἐνεστῶτος <choice><corr>τεσσαρεσκαιδεκ<supplied reason="omitted">άτ</supplied>ου</corr><sic>τεσσαρεσκαιδεκου</sic></choice> ἔτους <supplied reason="lost">Καίσαρος</supplied> 

    <lb n="5"/>ἄνευ πάσης ὑπερθέσεως, εἰ δὲ <supplied reason="omitted">μὴ</supplied> εἶναι αὐ<supplied reason="lost">τὰς ἀγω</supplied>

    <lb n="6" type="inWord"/>γίμας καὶ <choice><corr>συνέχ<supplied reason="omitted">εσ</supplied>θαι</corr><sic>συνεχθαι</sic></choice> μέχρι τοῦ <choice><corr>ἐ<supplied reason="omitted">κ</supplied>τεῖσα<supplied reason="lost">ι</supplied></corr><sic>ετισα<supplied reason="lost">ι</supplied></sic></choice> <supplied reason="lost">τὸ μὲν </supplied><choice><corr><supplied reason="lost">δά</supplied>

    <lb n="7" type="inWord"/>νειον</corr><sic><supplied reason="lost">δα</supplied>νηον</sic></choice> <surplus>συ</surplus> τὰς τοῦ <expan>ἀργυ<ex>ρίου</ex></expan> δραχμὰς χιλίας ὀ<supplied reason="lost">γδοήκον</supplied>

    <lb n="8" type="inWord"/>τα <num value="1080"/> σὺν ἡμιολίᾳ παραχρῆμα, τοῦ δʼ ὑπερ<supplied reason="lost">πεσόντος</supplied> 

    <lb n="9"/>χρόνου τοὺς κατὰ τὸ <choice><corr>διάγραμ<supplied reason="omitted">μα</supplied></corr><sic>διαγραμ</sic></choice> τόκου<supplied reason="lost">ς<expan><ex>διδράχμους</ex></expan>τῆς</supplied> 

    <lb n="10"/>πράξεως γεινομένης τῷ Σωσι<unclear>πάτρ</unclear><supplied reason="lost">ῳ ἔκ τε</supplied> 

    <lb n="11"/>τῶν <choice><corr>ὑποχρέ<supplied reason="omitted">ω</supplied>ν</corr><sic>υποχρεν</sic></choice> <choice><corr><expan>ἀλλη<supplied reason="omitted">λ</supplied>εγ<ex>γύων</ex></expan></corr><sic>αλληεν</sic></choice> οὐσῶν <unclear>εἰς</unclear> <choice><corr>ἔ<supplied reason="omitted">κ</supplied>τι<supplied reason="lost">σιν</supplied></corr><sic><unclear>ε</unclear>τ<unclear>ι</unclear><supplied reason="lost">σιν</supplied></sic></choice> <supplied reason="lost">καὶ </supplied><choice><corr><supplied reason="lost">ἐ</supplied>

    <lb n="12" type="inWord"/>κ</corr><sic><supplied reason="lost">ε</supplied>γ</sic></choice> μιᾶς <num value="1"/> ἧς ἐὰν αὐτῶν <choice><corr>α<supplied reason="omitted">ἱ</supplied>ρῆται</corr><sic>αρ<unclear>ηται</unclear></sic></choice> καὶ ἐκ <unclear>τῶν</unclear> <supplied reason="lost">ὑπαρχόντων</supplied> 

    <lb n="13"/>αὐταῖς <expan>πάντω<ex>ν</ex></expan> <choice><corr>κα<supplied reason="omitted">θά</supplied>περ</corr><sic>καπερ</sic></choice> <choice><corr>ἐκ</corr><sic>εγ</sic></choice> <choice><corr>δί<supplied reason="omitted">κ</supplied>ης</corr><sic>διης</sic></choice> <choice><corr><expan>ἀκ<supplied reason="omitted">ύ</supplied>ρω<ex>ν</ex></expan></corr><sic>ακρω</sic></choice> οὐσῶ<supplied reason="lost">ν καὶ ὧν</supplied> 

    <lb n="14"/>ἐὰν <choice><corr><expan>ἐπενέγκω<ex>σι</ex></expan></corr><sic><unclear>επεν</unclear>ενκω</sic></choice> <expan>πίστε<ex>ων</ex></expan> <expan>πασῶ<ex>ν</ex></expan> σκέπης <supplied reason="lost">πάσης.</supplied> 

    <lb n="15"/>τοῦτο δʼ <unclear>ἔστιν</unclear> τὸ <choice><corr>δάνειον</corr><sic>δανηον</sic></choice> ἐκτὸς οὗ <supplied reason="lost">ὀφείλου</supplied>

    <lb n="16" type="inWord"/>σιν αἱ αὐταὶ Ἀριστοκλήα καὶ Τρύφαινα <supplied reason="lost">Σωσιπά</supplied>

    <lb n="17" type="inWord"/>τρῳ καθʼ ἑτέραν συνχώρησιν διὰ τοῦ <supplied reason="lost">αὐτοῦ</supplied> 

    <lb n="18"/>κριτηρίου ἄλλων ἀργυρίου ταλάντων τριῶ<supplied reason="lost">ν</supplied> <num value="3"/>. 
<milestone rend="paragraphos" unit="undefined"/>
