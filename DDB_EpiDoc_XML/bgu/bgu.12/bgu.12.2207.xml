    <lb n="1"/><g type="stauros"/> ἐν ὀνόματι τῆς ἁγίας καὶ ζωοποιοῦ τριάδος πατρὸς καὶ υ<hi rend="diaeresis">ἱ</hi>οῦ καὶ ἁγίου 

    <lb n="2"/>πνεύματο<add place="supralinear">ς</add>, βασιλείας τοῦ θειοτάτου ἡμῶν δεσπότου <expan>Φλ<ex cert="high">αυίου</ex></expan> Φωκᾶ τοῦ αἰωνίο<add place="supralinear">υ</add> 

    <lb n="3"/>Αὐγού<supplied cert="high" reason="lost">σ</supplied>του καὶ <expan>Αὐ<unclear cert="high" reason="undefined">το</unclear>κ<ex cert="high">ράτορος</ex></expan> <unclear cert="high" reason="undefined">ἔ</unclear>τους τετάρ<supplied cert="high" reason="lost">του</supplied>, Φαῶφι πεντεκαιδεκάτῃ, δεκάτη<unclear cert="high" reason="undefined">ς</unclear> <expan><unclear cert="high" reason="undefined">ἰν</unclear>δ<ex cert="high">ικτίονος</ex></expan>. 

    <lb n="4"/><g type="stauros"/> Αὐρήλιος Ταυρῖνος υ<hi rend="diaeresis">ἱ</hi><unclear cert="high" reason="undefined">ὸ</unclear><supplied cert="high" reason="lost">ς </supplied><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>αλωτος ἐκ μητρὸς Θέκλ<supplied cert="high" reason="lost">ας</supplied> 

    <lb n="5"/>ἀμπελουργὸς ἀπὸ <choice><corr>ἐποικίου</corr><sic>εποικειου</sic></choice> Ταν<unclear cert="high" reason="undefined">α</unclear>μη<unclear cert="high" reason="undefined">υ</unclear> τοῦ <expan>Ἑρμ<ex cert="high">ουπολίτου</ex></expan> νομ<unclear cert="high" reason="undefined">οῦ</unclear> 

    <lb n="6"/>δι᾽ ἐμοῦ ἐγγυωμένο<unclear cert="high" reason="undefined">υ</unclear> αὐτὸ<unclear cert="high" reason="undefined">ν</unclear> <unclear cert="high" reason="undefined">καὶ</unclear> ἀ<supplied cert="high" reason="lost">να</supplied>δ<unclear cert="high" reason="undefined">εχο</unclear>μένου εἰς <unclear cert="high" reason="undefined">τὴ</unclear><supplied cert="high" reason="lost">ν</supplied> 

    <lb n="7"/>ἀπόδοσιν τοῦ <gap extent="1" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ου</unclear> <hi rend="diaeresis">ἰ</hi><unclear cert="high" reason="undefined">δί</unclear><supplied cert="high" reason="lost">ῳ</supplied> <unclear cert="high" reason="undefined">μου</unclear> κινδύνῳ καὶ <supplied cert="high" reason="omitted">πόρῳ</supplied> τῆς <unclear cert="high" reason="undefined">ἐμ</unclear><supplied cert="high" reason="lost">ῆς</supplied> 

    <lb n="8"/>ὑποστάσεως <g type="stauros"/> <choice><corr>Αὐρηλίου</corr><sic><unclear cert="high" reason="undefined">αυρη</unclear>λ<unclear cert="high" reason="undefined">ιος</unclear></sic></choice> <choice><corr>Πέτρου</corr><sic><unclear cert="high" reason="undefined">πετρ</unclear>ο<unclear cert="high" reason="undefined">ς</unclear></sic></choice> <choice><corr>υἱοῦ</corr><sic>υιος</sic></choice> Παύ<unclear cert="high" reason="undefined">λου</unclear> ἐκ μητρ<unclear cert="high" reason="undefined">ὸς</unclear> 

    <lb n="9"/>Ἀγνοίας ἀπὸ τ<unclear cert="high" reason="undefined">οῦ</unclear> <unclear cert="high" reason="undefined">αυ</unclear><gap extent="1" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="3" reason="illegible" unit="character"/> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/>Φ<unclear cert="high" reason="undefined">οι</unclear>βάμμων<supplied cert="high" reason="lost">ος ἀπ</supplied>ὸ τῆς <expan>Ἑρμ<ex cert="high">ουπολιτῶν</ex></expan> χαίρ<supplied cert="high" reason="lost">ειν. ὁμολογῶ ἐσχηκέναι</supplied> 

    <lb n="11"/>καὶ πεπληρῶσ<unclear cert="high" reason="undefined">θαι</unclear> παρά σου τῆς τελ<supplied cert="high" reason="lost">είας καὶ ἀξίας τιμῆς</supplied> 

    <lb n="12"/>οἴνου μέτρων διακοσίων <supplied cert="high" reason="lost">τεσσαράκοντα </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/><expan>γί<ex cert="high">νεται</ex></expan> <expan>οἴ<ex cert="high">νου</ex></expan> <expan>μέτρ<ex cert="high">α</ex></expan> <num value="24">κδ</num><gap extent="1" reason="illegible" unit="character"/>, ἅπερ παρέ<supplied cert="high" reason="lost">ξω σοι ἐν τῷ Μεσορὴ μηνὶ</supplied> 

    <lb n="14"/>τῆς σὺν <expan>θ<ex cert="high">εῷ</ex></expan> ἑνδεκάτης <expan><unclear cert="high" reason="undefined">ἰνδ</unclear><ex cert="high">ικτίονος</ex></expan> <supplied cert="high" reason="lost">ἐν οἴνῳ νέῳ καλλίστῳ καὶ</supplied> 

    <lb n="15"/>εὐαρέ<supplied cert="high" reason="omitted">σ</supplied>τῳ, μέτρῳ τοῦ <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="16"/>διοικητοῦ· καὶ εἰ <choice><corr>εὑρε<supplied cert="high" reason="lost">θείη</supplied></corr><sic>ευρη<supplied cert="high" reason="lost">θειη</supplied></sic></choice><supplied cert="high" reason="lost"> ὄξος ἢ ἀποίητος ἢ ὀζόμενος</supplied> 

    <lb n="17"/>ἐξ αὐτῶν μέ<supplied cert="high" reason="lost">χρι τοῦ Τῦβι μηνὸς τῆς αὐτῆς <expan>ἰνδ<ex cert="high">ικτίονος</ex></expan>, ὁμολογῶ</supplied> 

