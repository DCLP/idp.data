    <lb n="6"/>ταῦτά μοι ἐκείνου ἀπαγγείλα<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">τ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/>ἀρίστου καὶ οὐδενὸς ἑτέρου ἐδυν<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>ἐν πολλῇ εἰμι ἀθυμίᾳ τούτου χάρι<supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>ἀδελφικὴν λαμπρότητα εὐθέ<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">ς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/>καὶ τὸν τρόπον τῆς ἀηδίας καὶ ὁ π<gap extent="unknown" reason="lost" unit="character"/> 

