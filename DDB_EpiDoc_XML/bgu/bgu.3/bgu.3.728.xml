    <lb n="1"/><g type="stauros"/> καταλαβὼν Νεφερᾶς ὁ Μαῦρος ἀπ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>αὐτὸν ἐκεῖσε διὰ χρείαν τινὰ ἀπη<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>δεσπότης ὁ θ<supplied cert="high" reason="lost">εο</supplied>φ<supplied cert="high" reason="lost">ύ</supplied>λ<supplied cert="high" reason="lost">α</supplied>κτος <choice><corr>κύριος</corr><sic>κυρις</sic></choice> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>καὶ οὐκ ηὗρε<unclear cert="high" reason="undefined">ν</unclear> <unclear cert="high" reason="undefined">πα</unclear>ραστῆσαί μοι τὸν <unclear cert="high" reason="undefined">γρ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

