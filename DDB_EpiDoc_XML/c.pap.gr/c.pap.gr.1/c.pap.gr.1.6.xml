    <lb n="2"/>Πρωτάρχ<supplied reason="lost">ω</supplied>ι 

    <lb n="3"/>παρὰ Ἰσιδώρας τῆς <unclear reason="undefined">Κομ</unclear><gap reason="lost" extent="11" unit="character"/> <unclear reason="undefined">μετὰ</unclear> <unclear reason="undefined">κυρίου</unclear> <unclear reason="undefined">τοῦ</unclear> <unclear reason="undefined">ἀ</unclear><supplied reason="lost">δελφοῦ Εὐτυχίδου</supplied> 

