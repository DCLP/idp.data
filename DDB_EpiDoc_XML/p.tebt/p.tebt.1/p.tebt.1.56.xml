    <lb n="1"/>Πετεσοῦχος Μαρρήους <unclear cert="high" reason="undefined">γ</unclear><supplied cert="high" reason="lost">εωρ</supplied><unclear cert="high" reason="undefined">γὸς</unclear> 

    <lb n="2"/>τῶν ἐκ Κερκεσήφεως <sic><supplied cert="high" reason="lost">εω</supplied>ς</sic> 

    <lb n="3"/>Μαρρῆτι Πετοσείριος τῶ<gap extent="1" reason="lost" unit="character"/> 

    <lb n="4"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>καὶ ἀδελφῷ χαίρειν. <choice><corr>γίν<supplied cert="high" reason="lost">ωσ</supplied>κε</corr><sic><unclear cert="high" reason="undefined">γειν</unclear><supplied cert="high" reason="lost">ωσ</supplied>κε</sic></choice> δ<unclear cert="high" reason="undefined">ὲ</unclear> 

    <lb n="6"/>περὶ τοῦ κατακεκλῦσθαι τὸ πεδίον 

    <lb n="7"/><choice><corr>ἡμῶν</corr><sic>υμων</sic></choice> καὶ οὐκ ἔχομεν ἕως τῆς 

    <lb n="8"/>τροφῆς <unclear cert="high" reason="undefined">τῶ</unclear>ν <subst><add cert="low" place="inline"/><del rend="corrected" status="unremarkable">υμων</del></subst> κτηνῶν ἡμῶν. 

    <lb n="9"/>καλῶς οὖν <choice><corr>ποιήσῃς</corr><sic>ποησης</sic></choice> εὐχαριστῆσαι 

    <lb n="10"/>πρῶτον μὲν τοῖς θεοῖς δεύτερον 

    <lb n="11"/>δὲ σῶσαι ψυχὰς πολλὰς ζητή<supplied cert="high" reason="lost">σ</supplied><unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ς</supplied> 

    <lb n="12"/>μοι περὶ τὴν κώμην σου εἰς τὴν 

    <lb n="13"/>τροφὴν ἡμῶν γῆς ἀρούρας πέ<supplied cert="high" reason="lost">ν</supplied>-

    <lb n="14"/>τε <num value="5"/> ὡς ἕξομεν ἐξ αὐτῶν τὴ<supplied cert="high" reason="lost">ν</supplied> 

    <lb n="15"/>τροφὴν ἡμῶν.  τοῦτο δὲ <choice><corr>ποιήσας</corr><sic>ποησας</sic></choice> 

    <lb n="16"/>ἔσῃ μοι κεχαρισμένος εἰς τὸν 

    <lb n="17"/>ἅπαντα χρόν<supplied cert="high" reason="lost">ον</supplied>. 

    <lb n="18"/>ἔρρωσο. 
