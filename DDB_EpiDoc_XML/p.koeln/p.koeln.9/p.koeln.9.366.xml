    <lb n="15"/>συγγραφῶν ἐπέλθῃ ἐπι<unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">αλῶ</supplied><unclear cert="high" reason="undefined">ν</unclear> <supplied cert="high" reason="lost">αὐτῷ</supplied> <unclear cert="high" reason="undefined">πε</unclear>ρ<unclear cert="high" reason="undefined">ὶ</unclear> <supplied cert="high" reason="lost">τῶν</supplied>

    <lb n="16"/>προγεγραμμένων ἢ <unclear cert="high" reason="undefined">ἄλ</unclear><supplied cert="high" reason="lost">λος</supplied> <unclear cert="high" reason="undefined">τις</unclear> <supplied cert="high" reason="lost">τῶν</supplied> <unclear cert="high" reason="undefined">παρ</unclear>᾽ <supplied cert="high" reason="lost">αὐτοῦ</supplied>,

    <lb n="17"/>ἡ μὲν ἔφοδος αὐτῷ τε καὶ π<unclear cert="high" reason="undefined">α</unclear>ντὶ τῷ <supplied cert="high" reason="lost">περὶ τούτου</supplied>

    <lb n="18"/>ἐπελθ<supplied cert="high" reason="lost">ό</supplied>ντι ἄκυρος ἔστω καὶ προσαποτ<supplied cert="high" reason="lost">εισάτω</supplied>

    <lb n="19"/>Δημήτριος Ἀγαθίνῳ ἐπίτιμον χαλκοῦ ν<supplied cert="high" reason="lost">ο</supplied>-

