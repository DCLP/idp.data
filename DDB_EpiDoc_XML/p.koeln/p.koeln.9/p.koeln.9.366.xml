    <lb n="6"/>αὐτ<unclear cert="high" reason="undefined">ῷ</unclear> τε καὶ Θέωνι Θέωνος τοῦ ὀγδόου καὶ τρι<unclear cert="high" reason="undefined">α</unclear>κ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">στοῦ</supplied>

    <lb n="7"/>ἔτους Τῦβι πέμπτηι διάγων ἐν Ἀλεξανδρε<supplied cert="high" reason="lost">ίᾳ ἐπὶ τῶν</supplied>

    <lb n="8"/>μνημόνων χαλκοῦ ταλάντων δεκαπέντ<supplied cert="high" reason="lost">ε <num value="15"/></supplied>,

    <lb n="9"/>καὶ μηθὲν ἐνκαλεῖ<unclear cert="high" reason="undefined">ν</unclear> μηδ᾽ ἐνκαλέσειν μ<supplied cert="high" reason="lost">ηδὲ</supplied>

    <lb n="10"/><supplied cert="high" reason="lost">ἐ</supplied>πελεύσεσθαι Δημήτριον Ἀγαθίνωι μη<unclear cert="high" reason="undefined">δ</unclear><supplied cert="high" reason="lost">ὲ τοῖς</supplied>

    <lb n="11"/>παρ᾽ αὐτοῦ περὶ τοῦ δανείου τ<unclear cert="high" reason="undefined">ῶ</unclear>ν <unclear cert="high" reason="undefined">δ</unclear>εκαπέντ<supplied cert="high" reason="lost">ε <num value="15"/></supplied>

    <lb n="12"/>ταλάντων μηδὲ <unclear cert="high" reason="undefined">π</unclear>ερὶ μέρους αὐτῶν μ<unclear cert="high" reason="undefined">ηδ</unclear><supplied cert="high" reason="lost">ὲ</supplied>

