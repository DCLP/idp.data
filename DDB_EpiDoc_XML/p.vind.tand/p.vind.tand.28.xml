    <lb n="1"/><g type="stauros"/> βασιλείας καὶ ὑπατεία<supplied reason="lost">ς τοῦ θειοτάτου ἡμῶν δεσπότου <expan>Φλ<ex>αουίου</ex></expan> Ἰουστίνου τοῦ αἰωνίου</supplied> 

    <lb n="2"/>Αὐγούστου Αὐτοκρ<unclear>ά</unclear>τορο<supplied reason="lost">ς ἔτους <gap extent="unknown" reason="illegible" unit="character"/> δεκάτου δεκάτης ἰνδικτίονος καὶ <expan>Φλ<ex>αουίου</ex></expan> Τιβερίου</supplied> 

    <lb n="3"/>τοῦ <supplied reason="omitted">καὶ</supplied> Νέου Κ<unclear>ων</unclear>σταντίνου <supplied reason="lost">Καίσαρος ἔτους </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied reason="lost"> δεκάτης ἰνδικτίονος</supplied>. 

    <lb n="4"/><g type="stauros"/> Αὐρηλίῳ Δ<unclear>α</unclear>νιηλίῳ <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/><expan>π<ex>αρὰ</ex></expan> Αὐρηλί<supplied reason="lost">ου Πκύλιος Ἰωσηφίου    </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied reason="lost">         ἀμπελουργοῦ ἀπὸ</supplied> 

    <lb n="6"/><choice><corr>πεδίων</corr><sic>πεδηον</sic></choice> κώμης Ἐνσ<unclear>ε</unclear><supplied reason="lost">ῦ τοῦ Ἑρμοπολίτου νομοῦ.  ὁμολογῶ ἑκουσίως καὶ αὐθαιρέτως</supplied> 

    <lb n="7"/>μεμισθῶσθαι παρὰ σοῦ <supplied reason="lost">ἐπὶ χρόνον λογιζόμενον ἀπὸ καρπῶν τῆς σὺν <expan>θ<ex>εῷ</ex></expan> εἰσιούσης</supplied> 

    <lb n="8"/>ἑνδεκάτης <expan>ἰνδ<ex>ικτίονος</ex></expan> κ<unclear>αὶ</unclear> <unclear>α</unclear><supplied reason="lost">ὐ</supplied><unclear>τ</unclear><supplied reason="lost">ῆς τὰς διαφερούσας σοι ἀρούρας </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied reason="lost"> ἐν ἀμπέλῳ πλέον ἔλαττον</supplied> 

    <lb n="9"/>σὺν φύτοις διαφόρ<unclear>ο</unclear><supplied reason="lost">ις καὶ φοίνιξι <gap extent="unknown" reason="illegible" unit="character"/> καὶ  <gap extent="unknown" reason="illegible" unit="character"/> καὶ πάσῃ ἐξαρτίᾳ</supplied> 

    <lb n="10"/>ἐξηρτισμένῳ καὶ λη<unclear>ν</unclear><supplied reason="lost">οπίθῳ καὶ ἡλιαστηρίῳ καὶ πύργῳ καὶ εἰσόδῳ καὶ ἐξόδῳ</supplied> 

    <lb n="11"/>καὶ παντὶ ἐκεῖσε δικαίῳ <unclear>δ</unclear><supplied reason="lost">ιακειμένας ἐν τῷ σῷ κτήματι καλουμένῳ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied reason="lost"> καὶ ὑπὸ τὴν</supplied> 

    <lb n="12"/>παραφυλακὴν τῶν <unclear>ἀ</unclear><supplied reason="lost">πὸ κώμης </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied reason="lost"> τοῦ Ἑρμοπολίτου νομοῦ πρὸς χειρικὴν</supplied> 

    <lb n="13"/>καὶ ἀμπελουργικὴν ἐμ<unclear>ὴ</unclear><supplied reason="lost">ν ἐργασίαν καὶ φιλοκαλίαν καὶ καλλιεργίαν καὶ περίσκαψιν</supplied> 

    <lb n="14"/>καὶ θρυοκοπίαν καὶ <unclear>κ</unclear><supplied reason="lost">αλαμοστασίαν </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied reason="lost"> κατ᾽ ἔτος ἀμέμπτως καὶ ἀκατα</supplied>


    <lb n="15" type="worddiv"/>φρονήτ<unclear>ω</unclear><supplied reason="lost">ς</supplied>, <unclear>ἐ</unclear>φ᾽ ᾧτέ σε τὸν γ<supplied reason="lost">εοῦχον λαβεῖν τὸ ἥμισυ μέρος τοῦ ἐκβησομένου οἴνου</supplied> 

    <lb n="16"/>καὶ τοῦ καρποῦ τῶν εκ<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/><supplied reason="lost">                   καὶ ἀπὸ τοῦ κοινοῦ</supplied> 

    <lb n="17"/>πίθου τὰ πρωτόληνα κ<unclear>αὶ</unclear> <gap extent="unknown" reason="lost" unit="character"/><supplied reason="lost">  καὶ ἐμὲ μετὰ τοῦ γεωργοῦ ἔχειν τὸ ἄλλο ἥμισυ</supplied> 

    <lb n="18"/>μέρος, ἀνθ᾽ ὧν πο<unclear>ι</unclear>ούμεθα κ<unclear>α</unclear><supplied reason="lost">μάτων, παρέξω δὲ κατ᾽ ἔτος οὐδὲν ἧττον λόγῳ συνηθείας</supplied> 

    <lb n="19"/><choice><corr>θηλυκοῦ</corr><sic>θηλικου</sic></choice> φοίνικος φοινίκ<unclear>ω</unclear><supplied reason="lost">ν ἀρτάβας      </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="20"/>των τούτων <choice><corr>χοιράδα</corr><sic>χ<unclear>ο</unclear>ιραδαν</sic></choice> μία<unclear>ν</unclear> <supplied reason="lost">τιμῆς κερατίων </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied reason="lost">  καὶ ἐλαίου</supplied> 

    <lb n="21"/>ξέστην ἕνα καὶ οἴνου παλαιοῦ μέ<unclear>τ</unclear><supplied reason="lost">ρα     </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="22"/>φυτεῦσαι καὶ ἀνηνεγκεῖν πλ<unclear>η</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="23"/>καὶ τὴν πᾶσαν ἀμπελουργικὴν <unclear>ἐργ</unclear><supplied reason="lost">ασίαν   </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied reason="lost">          ἀμέμπτως καὶ</supplied> 

    <lb n="24"/>ἀκαταφρονήτως <supplied reason="lost">ἐ</supplied><unclear>π</unclear>ὶ τῆς παρούσης <unclear>δε</unclear><supplied reason="lost">κάτης ἰνδικτίονος   </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="25"/>ἀπὸ τοῦ καρ<unclear>π</unclear><supplied reason="lost">ο</supplied><unclear>ῦ</unclear> <unclear>αυτ</unclear>ου <unclear>ν</unclear><supplied reason="lost">ε</supplied><unclear>ο</unclear>φύτου <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="26"/>μισθῶν καὶ <choice><corr>φιλοκαλίαν</corr><sic>φιλοκαλει<unclear>αν</unclear></sic></choice> <gap extent="4" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="27"/>καὶ ποιῆσαι ἡμέραν μίαν ε<gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/><supplied reason="lost"> ἡ παροῦσα μίσθωσις κυρία καὶ</supplied> 

    <lb n="28"/>βεβαία καὶ <expan>ἐπερ<ex>ωτηθεὶς</ex></expan> <expan>ὡμολό<ex>γησα</ex></expan>. <handShift new="m2"/> <g type="stauros"/> <expan>Αὐρ<ex>ήλιος</ex></expan> Πκῦλις Ἰω<supplied reason="lost">σηφίου ὁ προκείμενος μεμίσθωμαι ὡς <expan>πρόκ<ex>ειται</ex></expan>. ἀξιωθεὶς ἔγραψα</supplied> 

    <lb n="29"/><expan><ex>ὑπὲρ</ex></expan> αὐτοῦ <expan>γράμ<ex>ματα</ex></expan> μὴ εἰδότος. <handShift new="m3"/> <g type="stauros"/> Ἰωάννης <gap extent="2" reason="illegible" unit="character"/><supplied reason="lost"> <gap extent="unknown" reason="illegible" unit="character"/> ἀπὸ <expan>Ἑρμ<ex>οῦ πόλεως</ex></expan> μαρτυρῶ τῇ μισθώσει ἀκούσας παρὰ τοῦ θεμένου</supplied>. 

