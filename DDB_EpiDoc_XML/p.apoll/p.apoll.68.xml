    <lb n="1"/><gap extent="10" reason="lost" unit="character"/> τοῦ δεσπότου μου ὡς <unclear cert="high" reason="undefined">κε</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>τί γὰρ ἔγραψα τῷ δεσπότῃ μου; <choice><corr>ἀληθινὰ</corr><sic>αληθεινα</sic></choice> γὰρ ἐ<supplied cert="high" reason="lost">στι </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>σήμερον διὰ τὸν δεσπότην μου μη<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>καὶ π<supplied cert="high" reason="lost">ά</supplied>λιν ὡς ὑπὲρ ἐμοῦ βλήσκεται τὰ λημφθη<supplied cert="high" reason="lost">σόμενα </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>τοῦ ἀνθρώπου ἐστὶ μετὰ Θεὸν ἀναγκαιότερο<supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/><supplied cert="high" reason="lost">γ</supplied>νῶναι ὡς οὐκ ἔχει ἄλλον τι νῦν φροντίζοντ<supplied cert="high" reason="lost">α </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/>τῷ δεσπότῃ αὐτοῦ· καὶ περὶ Βίκτορος τοῦ διακ<supplied cert="high" reason="lost">όνου </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>καὶ Μακαρία εἰ καλῶς ἔχ<supplied cert="high" reason="lost">ει </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>ου ἀπῆλθεν εἰς Τοετὸ κα<unclear cert="high" reason="undefined">λ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/><supplied cert="high" reason="lost">τὰ</supplied> τετιμημένα γράμματα τοῦ ἀγαθοῦ καὶ θε<supplied cert="high" reason="lost">οστηρίκτου </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/>καὶ ἰδοὺ ἔπεμψεν στρατιώτην ὁ ἐνδοξότατο<supplied cert="high" reason="lost">ς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="12"/><gap extent="3" reason="lost" unit="character"/> πρὸς τὴν κέλευσιν τοῦ ἀγαθοῦ καὶ θεοστηρ<supplied cert="high" reason="lost">ίκτου </supplied><gap extent="unknown" reason="lost" unit="character"/> 
