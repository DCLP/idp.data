    <lb n="1"/><gap reason="lost" extent="10" unit="character"/> τοῦ δεσπότου μου ὡς <unclear reason="undefined">κε</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>τί γὰρ ἔγραψα τῷ δεσπότῃ μου; <choice><corr>ἀληθινὰ</corr><sic>αληθεινα</sic></choice> γὰρ ἐ<supplied reason="lost">στι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>σήμερον διὰ τὸν δεσπότην μου μη<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>καὶ π<supplied reason="lost">ά</supplied>λιν ὡς ὑπὲρ ἐμοῦ βλήσκεται τὰ λημφθη<supplied reason="lost">σόμενα </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>τοῦ ἀνθρώπου ἐστὶ μετὰ Θεὸν ἀναγκαιότερο<supplied reason="lost">ν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><supplied reason="lost">γ</supplied>νῶναι ὡς οὐκ ἔχει ἄλλον τι νῦν φροντίζοντ<supplied reason="lost">α </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>τῷ δεσπότῃ αὐτοῦ· καὶ περὶ Βίκτορος τοῦ διακ<supplied reason="lost">όνου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>καὶ Μακαρία εἰ καλῶς ἔχ<supplied reason="lost">ει </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>ου ἀπῆλθεν εἰς Τοετὸ κα<unclear reason="undefined">λ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/><supplied reason="lost">τὰ</supplied> τετιμημένα γράμματα τοῦ ἀγαθοῦ καὶ θε<supplied reason="lost">οστηρίκτου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>καὶ ἰδοὺ ἔπεμψεν στρατιώτην ὁ ἐνδοξότατο<supplied reason="lost">ς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><gap reason="lost" extent="3" unit="character"/> πρὸς τὴν κέλευσιν τοῦ ἀγαθοῦ καὶ θεοστηρ<supplied reason="lost">ίκτου </supplied><gap reason="lost" extent="unknown" unit="character"/> 
