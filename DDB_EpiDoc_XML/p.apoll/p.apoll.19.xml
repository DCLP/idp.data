    <lb n="1"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost">  ἡ ὑμετέρα θεοφύλακτος</supplied> 

    <lb n="2"/>ἀδελφότης εὐτρεπεί<supplied cert="high" reason="lost">σῃ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τῶ</supplied>ν γενημάτων καὶ ἀποκλείσατε. 

    <lb n="3"/>ἀλλὰ πάντως ἔργον ὑμῖ<supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">η</unclear>τε σιαινομένῳ χάριν <expan>αὐτ<ex cert="high">οῦ</ex></expan> <foreign lang="la">?. </foreign>

    <lb n="4"/>λοιπὸν ἀντιγράψατέ μοι <unclear cert="high" reason="undefined">ε</unclear><gap extent="unknown" reason="lost" unit="character"/><supplied cert="low" reason="lost">προσ</supplied>ῆλθαν χθὲς οἱ ἀντίδικοι 

    <lb n="5"/>Σαβίνου, τοῦτ᾽ ἐστὶν Μαρτ<gap extent="unknown" reason="lost" unit="character"/><supplied cert="low" reason="lost">περιβ</supplied><unclear cert="low" reason="undefined">λ</unclear>έψητε τοὺς ἀνθρώπους 

    <lb n="6"/>αὐτῶν ἄχρις οὗ γράψω ὑμῖν π<supplied cert="high" reason="lost">ερὶ τούτου </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> καὶ ἀσπάζομαι τοὺ</supplied>ς <expan>θεοφυλ<ex cert="high">άκτους</ex></expan> καὶ <expan>μεγαλο<ex cert="high">πρεπεστάτους</ex></expan> ὑμᾶς 

    <lb n="7"/>διὰ τοῦ γράμματος <g type="stauros"/> <expan>Π<ex cert="high">α</ex>χ<ex cert="high">ὼν</ex></expan> <gap extent="unknown" reason="lost" unit="character"/> </ab></div></body></text></TEI.2>