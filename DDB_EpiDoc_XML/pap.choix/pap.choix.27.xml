    <lb n="1"/><supplied cert="high" reason="lost">ὑπατείας τῶν κυρίων ἡμῶν Κωνσταντίου καὶ Μαξιμιανοῦ</supplied> 

    <lb n="2"/><supplied cert="high" reason="lost">τῶ</supplied><unclear cert="high" reason="undefined">ν</unclear> ἐπιφα<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">εστά</supplied>των Κα<unclear cert="high" reason="undefined">ισ</unclear>άρων τ<unclear cert="high" reason="undefined">ὸ</unclear> <supplied cert="high" reason="lost"><num value="5">ε</num></supplied>. 

    <lb n="3"/>Α<unclear cert="high" reason="undefined">ὐ</unclear>ρηλίωι Ἀπί<unclear cert="high" reason="undefined">ω</unclear>νι στρατηγῶι Ἀρσιν<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">είτο</supplied><unclear cert="high" reason="undefined">υ</unclear> 

    <lb n="4"/>παρὰ Αὐρηλίω<unclear cert="high" reason="undefined">ν</unclear> <unclear cert="high" reason="undefined">Π</unclear>αμουτίου <subst><add place="inline"><unclear cert="high" reason="undefined">Ἀ</unclear>θιόει</add><del rend="corrected" status="unremarkable"><unclear cert="high" reason="undefined">α</unclear>θιουι</del></subst> κα<unclear cert="high" reason="undefined">ὶ</unclear> Ἀρριανο<unclear cert="high" reason="undefined">ῦ</unclear> Ἀ<supplied cert="high" reason="lost">π</supplied>ύγχεως ἀμφο-

    <lb n="5"/>τέρων κωμαρχ<supplied cert="high" reason="lost">ῶν</supplied> κώμης Φι<unclear cert="high" reason="undefined">λ</unclear>αδελφία<unclear cert="high" reason="undefined">ς</unclear>. ὑπὲρ <choice><corr>λυσιτελείας</corr><sic>λυ<unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">ιτε</supplied>λιας</sic></choice> τοῦ ἱερω-

    <lb n="6"/>τάτου <choice><corr>ταμιείου</corr><sic>ταμιου</sic></choice> κα<unclear cert="high" reason="undefined">ὶ</unclear> τοῦ συστῆναι τὴν ἡμέτεραν <supplied cert="high" reason="lost">κώ</supplied>μην ποιού-

    <lb n="7"/>με<unclear cert="high" reason="undefined">θ</unclear>α. στρατηγῶ<unclear cert="high" reason="undefined">ν</unclear> ἄριστε, μακρ<unclear cert="high" reason="undefined">ό</unclear>θεν τυγχά<supplied cert="high" reason="lost">νο</supplied>μεν κατοικοῦν-

    <lb n="8"/>τε<unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">τ</unclear>ὴν ἡμέτερ<unclear cert="high" reason="undefined">α</unclear>ν κώμην, <choice><corr>πάμπολλα</corr><sic>πανπολλα</sic></choice> δὲ χρέως <supplied cert="high" reason="lost">ἔχο</supplied><unclear cert="high" reason="undefined">ντες</unclear> τῷ ἱερω-

    <lb n="9"/>τάτῳ <choice><corr>ταμιείῳ</corr><sic>ταμιω</sic></choice> ὑπὲρ τῆς ἡμετέρας κώμης οὐ μὴ<unclear cert="high" reason="undefined">ν</unclear> <supplied cert="high" reason="lost">ἀλλ</supplied><unclear cert="high" reason="undefined">ὰ</unclear> <unclear cert="high" reason="undefined">κ</unclear>αὶ ὑπὲρ 

    <lb n="10"/>μέρους Τάνεως. <expan>ὑ<ex cert="high">πὲρ</ex></expan> τοῦ οὖν εὐθέως δύνασθαι τὰ ὕ<unclear cert="high" reason="undefined">δ</unclear><supplied cert="high" reason="lost">ατα</supplied> <unclear cert="high" reason="undefined">κ</unclear>αὶ ἡμᾶς 

    <lb n="11"/><choice><corr>καταλαμβάνειν</corr><sic>καταλαμβανιν</sic></choice> καὶ ὄνησιν ἔχειν καὶ τοῦ πᾶσαν <unclear cert="high" reason="undefined">τὴν</unclear> γῆν ἡμῶν 

    <lb n="12"/><choice><corr>γεωργεῖν</corr><sic>γεωργιν</sic></choice> καὶ δύνασθαι <choice><corr>τελεῖν</corr><sic>τελιν</sic></choice> τὰ ὑπὲρ αὐτῶν <unclear cert="high" reason="undefined">δη</unclear>μ<unclear cert="high" reason="undefined">ό</unclear>σια τελέσ-τελέσ-

    <lb n="13"/>ματα καὶ <choice><corr>ἀννωνιακὴν</corr><sic>αννωνιακη</sic></choice> καὶ παντοίας ἐπιβολὰς, ἐπιδίδ<unclear cert="high" reason="undefined">ομ</unclear>έν σοι τάδε 

    <lb n="14"/>τὰ ἔνγραφα ἀξιοῦντες <choice><corr>ἐπιστεῖλέ</corr><sic>επιστιλε</sic></choice> <choice><corr>σε</corr><sic>σαι</sic></choice> δι᾽ <unclear cert="high" reason="undefined">ἑ</unclear>νὸς τῶν πε<supplied cert="high" reason="lost">ρ</supplied>ὶ σὲ ὑ<unclear cert="high" reason="undefined">π</unclear>ηρετῶν 

    <lb n="15"/>τῇ κρατίστῃ βου<unclear cert="high" reason="undefined">λ</unclear>ῇ διὰ τοῦ ἐνάρχου πρυτάνεως Αὐ<supplied cert="high" reason="lost">ρ</supplied>ηλί<supplied cert="high" reason="lost">ο</supplied>υ Καστορίω-

    <lb n="16"/>νος, ὅπως <choice><corr>ἡρή<supplied cert="high" reason="lost">σ</supplied>ηται</corr><sic>ερη<supplied cert="high" reason="lost">σ</supplied>ηται</sic></choice> συνοψιστὴν τὸν τὴν σύν<unclear cert="high" reason="undefined">ο</unclear>ψιν <unclear cert="high" reason="undefined">τ</unclear>ῶν <choice><corr>ῥιθρῶν</corr><sic>ριθου</sic></choice> 

    <lb n="17"/>καὶ λ<unclear cert="high" reason="undefined">ι</unclear>θικῶν <choice><corr>ποιούμεν<supplied cert="high" reason="omitted">ον</supplied></corr><sic>ποιουμεν</sic></choice> τῶν ἐν κώμῃ Τάνει τοῦ <expan><supplied cert="high" reason="lost">ὑμ</supplied>ετ<supplied cert="high" reason="lost">έρ<ex cert="high">ου</ex></supplied></expan> <unclear cert="high" reason="undefined">ν</unclear>ομοῦ, 

    <lb n="18"/>ἵνα δυνηθῶμεν καὶ <choice><corr>ἡμεῖς</corr><sic>ημις</sic></choice> τῆς τοῦ <choice><corr><supplied cert="high" reason="omitted">Νίλ</supplied>ου</corr><sic>ου</sic></choice> παροχ<unclear cert="high" reason="undefined">ῆ</unclear>ς ἀπ<unclear cert="high" reason="undefined">ολ</unclear>αύειν 

    <lb n="19"/>καὶ πότιμον ὕδωρ ἔχειν καὶ τὸ πεδίον τῆς ἡμ<unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">έ</unclear>ρα<unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">κώ</unclear>μης 

    <lb n="20"/><choice><corr>κατασπείρειν</corr><sic>κατασπιριν</sic></choice> καὶ ἐν τῇ ἰδίᾳ στῆναι καὶ ὄνησιν ἔ<unclear cert="high" reason="undefined">χ</unclear><supplied cert="high" reason="lost">ει</supplied>ν <unclear cert="high" reason="undefined">τῶ</unclear>ν ἰδίων. <expan>διευτύ<unclear cert="high" reason="undefined">χ</unclear><ex cert="high">ει</ex></expan>. 

    <lb n="21"/><expan><ex cert="high">ἔτους</ex></expan> <num value="21">κα</num> καὶ <num value="13">ιγ</num> τῶν κυρίων ἡμῶν Διο<unclear cert="high" reason="undefined">κλ</unclear>ητιανοῦ καὶ Μαξιμιανοῦ Σ<supplied cert="high" reason="lost">εβ</supplied>α<unclear cert="high" reason="undefined">στ</unclear>ῶν καὶ <expan>Κωνσταντί<ex cert="high">ου</ex></expan> 

    <lb n="22"/>καὶ Μαξιμιανοῦ τῶν <unclear cert="high" reason="undefined">ἐ</unclear>πιφανεσ<unclear cert="high" reason="undefined">τ</unclear>ά<unclear cert="high" reason="undefined">των</unclear> Καισάρων Παχὼν <num value="1"><unclear cert="high" reason="undefined">α</unclear></num>. <handShift new="m2"/> <unclear cert="high" reason="undefined">Α</unclear>ὐρήλιοι 

    <lb n="23"/>Παμ<unclear cert="high" reason="undefined">ο</unclear>ῦτις καὶ Ἀρριανὸς ἐπιδε<supplied cert="high" reason="lost">δ</supplied><unclear cert="high" reason="undefined">ώ</unclear>καμεν. Αὐρήλιος Κο<unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ρ</supplied><unclear cert="high" reason="undefined">ῆ</unclear>ς Εὐπόρου 

    <lb n="24"/>ἔγ<unclear cert="high" reason="undefined">ρα</unclear>ψα <expan>ὑ<ex cert="high">πὲρ</ex></expan> αὐτῶν ἀγραμμ<unclear cert="high" reason="undefined">ά</unclear><supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ω</unclear>ν. </ab></div></body></text></TEI.2>