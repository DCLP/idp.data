    <lb n="1"/>Σεπτίμι<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ς Ζένιος λογι</supplied><unclear cert="high" reason="undefined">σ</unclear>τῇ Ὀξυρυγχ<supplied cert="high" reason="lost">ίτου</supplied> <unclear cert="high" reason="undefined">χ</unclear><supplied cert="high" reason="lost">α</supplied>ίρ<unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">ι</supplied><unclear cert="high" reason="undefined">ν</unclear>· α<unclear cert="high" reason="undefined">ν</unclear><gap extent="10" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>τῇ ἐμῇ καθ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">σιώσ</supplied>ει καὶ φρόντισον εἰ μὴ περὶ τῆς κοι<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">ωνίας τοῦ</supplied> <choice><corr>οἰκοπέδου</corr><sic>οικοπαιδου</sic></choice> μηδ<gap extent="1" reason="illegible" unit="character"/><gap extent="7" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>τὴν δι<unclear cert="high" reason="undefined">α</unclear>ίρεσιν τού<supplied cert="high" reason="lost">το</supplied>υ κατὰ τὸ δίκαι<unclear cert="high" reason="undefined">ο</unclear>ν ποιήσασθαι. ἔρρωσο. κ<gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> <num value="4">δ</num>. τῶν δὲ <choice><corr>ἀνενεχθέντων</corr><sic>ανεν<unclear cert="high" reason="undefined">εγ</unclear>χ<unclear cert="high" reason="undefined">θ</unclear>εντω<unclear cert="high" reason="undefined">ν</unclear></sic></choice> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>Σεπτιμίῳ Ζενίῳ τῷ διασημοτάτῳ ἐπάρχῳ Αἰγύπτ<supplied cert="high" reason="lost">ο</supplied>υ παρὰ Αὐρηλίου <choice><corr>Κάστορος</corr><sic>κασστορος</sic></choice> Παράμμων<supplied cert="high" reason="lost">ος ἀπὸ τῆς Ἀμμωνιακῆς</supplied> 

    <lb n="5"/>Ὀάσεως καταγινομένου ἐν τῇ λαμπρᾷ καὶ λαμπροτάτῃ Ὀξυρυγχιτῶν πόλει. ἐπριάμην, κύριε, παρὰ Θ<supplied cert="high" reason="lost">ωνίου Πααντᾶ μητρὸς</supplied> 

    <lb n="6"/>Κοπρίας ἀπὸ τῆς αὐτῆς Ὀξυρυγχιτῶν πόλεως τὸ ὑπάρχον αὐτῷ ἐ<supplied cert="high" reason="lost">ν τῇ</supplied> <unclear cert="high" reason="undefined">αὐ</unclear><supplied cert="high" reason="lost">τῇ</supplied> πόλει ἐπ᾽ ἀμφόδου <supplied cert="high" reason="lost">Ἑρμαίου τέταρτον μέρος</supplied>

    <lb n="7"/>ὁλοκλήρου οἰκίας καὶ τῶν ταύ<supplied cert="high" reason="lost">της ὑ</supplied><unclear cert="high" reason="undefined">π</unclear>οστελλόντων πάντων παραχρ<gap extent="1" reason="illegible" unit="character"/><gap extent="5" reason="lost" unit="character"/> <choice><corr>συμφωνηθεις<gap extent="unknown" reason="lost" unit="character"/></corr><sic>συμφωνηθις<gap extent="unknown" reason="lost" unit="character"/></sic></choice> 

    <lb n="8"/>τῷ <choice><corr><supplied cert="high" reason="omitted">ἐ</supplied>μοὶ</corr><sic>μη</sic></choice> <choice><corr>ἀποδομένῳ</corr><sic cert="low">αποδω<gap extent="2" reason="illegible" unit="character"/>νω</sic></choice> Θωνίῳ <choice><corr>ἀκολούθως</corr><sic>ακ<unclear cert="high" reason="undefined">ω</unclear>λουθως</sic></choice> ᾗ πεποίηταί μου πράσεως <gap extent="1" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> ὁμολογια<unclear cert="high" reason="undefined">σ</unclear>ο<unclear cert="high" reason="undefined">δ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/><choice><corr>κοινωνῶν</corr><sic>κοινονων</sic></choice> τῆς οἰκίας <choice><corr>τό</corr><sic>τω</sic></choice> μ<unclear cert="high" reason="undefined">οι</unclear> <choice><corr>ὑποστέλλον</corr><sic>υπο<unclear cert="high" reason="undefined">στελ</unclear>λων</sic></choice> τέταρτον μέρος ἐπικρατουντ<gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/><choice><corr>μετριότητος</corr><sic>μετριωτητος</sic></choice> <choice><corr>καταφρονοῦντας</corr><sic>κ<unclear cert="high" reason="undefined">ατα</unclear>φρωνουντας</sic></choice>, ἐκ το<unclear cert="high" reason="undefined">ύ</unclear>του καταφεύγω πρὸς τοὺς σοὺς πόδας του<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/>ἀξιῶν ἐλεήσαντά μου τὸ μέτριον μετὰ <choice><corr>χεῖρας</corr><sic>χιρας</sic></choice> ἔχοντ<unclear cert="high" reason="undefined">ό</unclear>ς μου τὴν εἰς <choice><corr>ἐμὲ</corr><sic>εμαι</sic></choice> γεγενη<supplied cert="high" reason="lost">μένην </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="12"/>ἐπικρίνῃς ἐπα<unclear cert="high" reason="undefined">ναγ</unclear>κασθῆναι τοὺς τῆς ο<supplied cert="high" reason="lost">ἰ</supplied>κίας <choice><corr>κοινωνοὺς</corr><sic>κοινουνους</sic></choice> Κοπ<supplied cert="high" reason="lost">ρί</supplied>αν καὶ Ταωρίωνα του<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/>τὸ <choice><corr>καὶ ἐμὲ</corr><sic>καμαι</sic></choice> δύνασθαι ἀπολαβόντα <choice><corr>τε ἀμὰ</corr><sic>ταμ<unclear cert="high" reason="undefined">α</unclear></sic></choice> <supplied cert="high" reason="lost">χά</supplied><unclear cert="high" reason="undefined">ρι</unclear>ν ὁμοίως <choice><corr>ἔχειν</corr><sic>εχιν</sic></choice> διὰ τὴν σὴν <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 
