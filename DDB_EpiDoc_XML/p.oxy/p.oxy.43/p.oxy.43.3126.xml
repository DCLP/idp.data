    <lb n="24"/><unclear cert="high" reason="undefined">Δ</unclear>ιδύμου <supplied cert="high" reason="lost">ἔγραψ</supplied><unclear cert="high" reason="undefined">α</unclear> ὑπὲρ <supplied cert="high" reason="lost">αὐ</supplied><unclear cert="high" reason="undefined">τοῦ</unclear> γράμ<supplied cert="high" reason="lost">ματα μὴ εἰ</supplied><unclear cert="high" reason="undefined">δ</unclear>ότος. 
