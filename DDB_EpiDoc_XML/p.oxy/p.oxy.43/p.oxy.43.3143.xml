    <lb n="29"/>δεκαπέντ<unclear>ε</unclear> <num value="15"/> <gap extent="3" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/><gap extent="5" reason="lost" unit="character"/><unclear>βε</unclear><gap extent="2" reason="lost" unit="character"/> 

