    <lb n="15"/><supplied cert="high" reason="lost">τ</supplied>ηναρ<supplied cert="high" reason="lost">ί</supplied>ου ἑνὸς <num value="1"/> χωροῦντος εἰς 

    <lb n="16"/>δημόσια πολιτικὰ ἔργα 

    <lb n="17"/>ἀργυρί<supplied cert="high" reason="lost">ου</supplied> <expan><ex cert="high">τάλαντα</ex></expan> <num value="6">ϛ</num> πλήρη. κυρία ἡ ἀποχή, 

    <lb n="18"/>καὶ <choice><corr>ἐπερωτηθεὶς</corr><sic>επ<supplied cert="high" reason="lost">ε</supplied>ρωτητεις</sic></choice> ὡμ<supplied cert="high" reason="lost">ολ</supplied>όγησα. 

    <lb n="19"/>ὑπατε<supplied cert="high" reason="lost">ία</supplied>ς Καικινίου Σαβίνου 

    <lb n="20"/>καὶ Οὐετ<g type="apostrophe"/>τ<supplied cert="high" reason="lost">ίου</supplied> Ῥουφίνου τῶν λαμπροτάτων, 

    <lb n="21"/>Ἁθὺρ <num value="5">ε</num>. 

    <lb n="22"/><handShift new="m2"/> Αὐρήλι<supplied cert="high" reason="lost">ο</supplied>ς Σεουῆρος <choice><corr>ἠρί<supplied cert="high" reason="omitted">θ</supplied>μημαι</corr><sic>εριμη<supplied cert="high" reason="lost">μ</supplied>α<supplied cert="high" reason="lost">ι</supplied></sic></choice> 

