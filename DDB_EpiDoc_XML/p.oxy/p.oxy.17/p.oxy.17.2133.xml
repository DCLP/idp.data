    <lb n="1"/><unclear reason="undefined">Α</unclear><gap reason="illegible" extent="3" unit="character"/>ῳ <gap reason="illegible" extent="4" unit="character"/><unclear reason="undefined">ω</unclear><gap reason="lost" extent="unknown" unit="character"/> τῷ διασημοτάτῳ ἐ<supplied reason="lost">πάρχῳ Αἰγύπτου</supplied> 

    <lb n="2"/>παρὰ Αὐρηλίας Ἠοῦτος Ἡρακλείδου μητρὸς <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀπὸ κώμης </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><choice><corr>το<supplied reason="omitted">ῦ</supplied></corr><sic><unclear reason="undefined">τ</unclear>ο</sic></choice> Ὀξυρυγχίτου νομοῦ. δέησιν δικαιοτάτ<unclear reason="undefined">η</unclear><supplied reason="lost">ν τῇ </supplied><gap reason="lost" extent="18" unit="character"/> 

    <lb n="4"/>σου ἐπιεικίᾳ, δέσποτα ἡγεμών, προσφέρου<supplied reason="lost">σα </supplied><gap reason="lost" extent="3" unit="character"/><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="10" unit="character"/><supplied reason="lost"> δέομαι</supplied> 

    <lb n="5"/>τῆς ἀπὸ τοῦ σ<supplied reason="lost">ο</supplied>ῦ μεγαλείου ε<supplied reason="lost">ὐερ</supplied>γεσίας καὶ α<gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="1" unit="character"/>χ<gap reason="lost" extent="10" unit="character"/><supplied reason="lost"> ἀδικου</supplied>-

    <lb n="6"/>μένη <hi rend="diaeresis">ὑ</hi>πὸ τοῦ οὐκ ἂν εἴποι<supplied reason="lost">μ</supplied>ι θείου πρὸς πατρὸς <gap reason="lost" extent="10" unit="character"/><gap reason="illegible" extent="1" unit="character"/>θεμ<gap reason="illegible" extent="1" unit="character"/> 

    <lb n="7"/>παρατιθεμένη, εἰς τὸ τῶν δι<supplied reason="lost">κ</supplied>αίων τυχεῖν <hi rend="diaeresis">ὑ</hi>πὸ τῆς σῆς <choice><corr>κα<supplied reason="lost">λοκα</supplied>γαθίας</corr><sic><unclear reason="undefined">κα</unclear><supplied reason="lost">λοκα</supplied><unclear reason="undefined">γ</unclear>αθειας</sic></choice>. ὁ προ-

