         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/><hi rend="diaeresis">ὑ</hi>πατείας Ο<supplied reason="lost">ὐιβίου</supplied> Νεπωτιανοῦ καὶ Τετ<g type="apostrophe"/><unclear>τ</unclear>ίου Φακούνδου 

    <lb n="2"/>τῶν λαμπροτάτων Π<supplied reason="lost">α</supplied>χὼν <num value="6">ϛ</num>. 

    <lb n="3"/>Φλαου<hi rend="diaeresis">ί</hi><supplied reason="lost">ῳ</supplied> <hi rend="diaeresis">Ἰ</hi>ουλιανῷ διοικοῦντι <app type="BL"><lem resp="9.181; cf. 3.133"><unclear>συ</unclear>νδικίαν</lem><rdg><unclear>ἐκ</unclear>δικίαν</rdg></app> Ὀξυρυγχίτου 

    <lb n="4"/>παρὰ Αὐρηλίας Ἀλλοῦτος Θωνίου ἀπὸ κώμης Τααμπέμο<supplied reason="lost">υ</supplied> 

    <lb n="5"/><num value="5">ε</num> πάγου. <choice><corr>ἑσπεριναῖς</corr><sic>εσπερινες</sic></choice> <choice><corr>ὥραις</corr><sic>ωρες</sic></choice> τῇ χθὲς ἡμέρᾳ ἡμέτεροι <choice><corr>χοῖροι</corr><sic>χυροι</sic></choice> 

    <lb n="6"/>δύο <num value="2"/> τὴν ὁρμὴν ποιούμενοι ἐπὶ ἡμέτερον ἔδαφος 

    <lb n="7"/><choice><corr>γενόμενοι</corr><sic>γενωμενοι</sic></choice> ἐν ὑδραγωγίῳ μηχανῆς τῶν ἡμετέρων 

    <lb n="8"/><app type="BL"><lem resp="9.181"><choice><corr>πεδίων</corr><sic>πεδων</sic></choice></lem><rdg>παίδων</rdg></app> καὶ <unclear>Π</unclear>αβάνου τινὸς ἀπὸ τῆς αὐτῆς <app type="BL"><lem resp="9.181">κώμης</lem><rdg>κώμης <supplied reason="lost">καὶ</supplied></rdg></app>. 

    <lb n="9"/>ὁ <choice><corr>προειρημ<supplied reason="lost">ένος</supplied></corr><sic>προ<hi rend="diaeresis">ι</hi>ρημ<supplied reason="lost">ενος</supplied></sic></choice><supplied reason="lost"> Π</supplied><unclear>α</unclear>βᾶνος παρατυχὼν ἔχων μετὰ 

    <lb n="10"/><choice><corr>χεῖρας</corr><sic>χιρας</sic></choice> <app type="BL"><lem resp="9.181">ξοίδιον ἀνελεῖν</lem><rdg><choice><corr>ξύλιον</corr><sic>ξοι<unclear>λ</unclear>ιο<unclear>ν</unclear></sic></choice> <gap reason="illegible" quantity="4" unit="character"/>ιν</rdg></app> τοὺς <choice><corr>χοίρους</corr><sic>χυρους</sic></choice> βουλόμεν<supplied reason="lost">ο</supplied>ς 

    <lb n="11"/>καὶ τοῦ<unclear>τ</unclear>ο <unclear>μ</unclear><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="5" unit="character"/><unclear>ω</unclear>ς μηδαμῶς ἀδικηθεὶ<supplied reason="lost">ς</supplied> 

    <lb n="12"/>ὑπὸ τῶν <choice><corr>χοίρ<supplied reason="lost">ων</supplied></corr><sic>χ<unclear>υρ</unclear><supplied reason="lost">ων</supplied></sic></choice> <app type="BL"><lem resp="9.181">ἀποσχόμενος</lem><rdg><supplied reason="lost">ἀλ</supplied><unclear>λὰ</unclear> ἐχόμενος</rdg></app> τῆς πρὸς τοὺς 

    <lb n="13"/><choice><corr>χοίρους</corr><sic>χυρους</sic></choice> ε<gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="2" unit="character"/><supplied reason="lost"> ὅτι</supplied> <unclear>ἐμοὶ</unclear> ἐπελήλυθαν βουλόμενος 

    <lb n="14"/><app type="BL"><lem resp="9.182"><unclear>μο</unclear>ι κατα<certainty match=".." locus="value"/></lem><rdg><unclear>ο</unclear>ικατα</rdg></app> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="15" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ως <choice><corr>εἰ</corr><sic><hi rend="diaeresis">ι</hi></sic></choice> μὴ ἔκ<g type="apostrophe"/> τινος 

    <lb n="15"/><app type="BL"><lem resp="9.182"><choice><corr><supplied reason="lost">προ</supplied>νοίας</corr><sic><supplied reason="lost">προ</supplied>νυας</sic></choice></lem><rdg><choice><corr><supplied reason="lost">ἀπο</supplied>νοίας</corr><sic><supplied reason="lost">απο</supplied>νυας</sic></choice></rdg></app> τὴν <gap reason="lost" quantity="8" unit="character"/><unclear>π</unclear><gap reason="illegible" quantity="1" unit="character"/> <supplied reason="lost">ἀ</supplied><unclear>λ</unclear>λʼ οὖν τῶν <choice><corr>βοῶν</corr><sic>βωων</sic></choice> 

    <lb n="16"/><gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>ον</unclear> <unclear>α</unclear>νκα<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="2" unit="character"/><unclear>η</unclear>μην <hi rend="diaeresis">ὑ</hi>πʼ αὐτοῦ 

    <lb n="17"/><gap reason="lost" quantity="23" unit="character"/>κόψας ὡς ἐκ τῶν 

