    <lb n="1"/><g type="stauros"/> 

    <lb n="1"/><g type="stauros"/> τῷ τύπῳ τῶν <choice><corr>ἀξιοπίστων</corr><sic>αξιω<gap extent="1" reason="lost" unit="character"/>πιστων</sic></choice> ἀνδρῶν Παμουθίου <expan>μείζ<ex>ονος</ex></expan> <expan><supplied reason="lost">υ</supplied>ἱ<ex>οῦ</ex></expan> <gap extent="1" reason="illegible" unit="character"/><unclear>σ</unclear>ω<gap extent="1" reason="lost" unit="character"/><supplied reason="lost">ο</supplied>υ καὶ Πανῖρεν ἀπὸ <expan>μειζ<ex>όνων</ex></expan> 

  <lb n="2"/><expan><supplied reason="lost">υ</supplied>ἱ<ex>οῦ</ex></expan> <supplied reason="lost"><hi rend="diaeresis">Ἰ</hi></supplied>ωάννου καὶ Ἀπολλὼ ἀπὸ <expan>μειζ<ex>όνων</ex></expan> <expan>υἱ<ex>οῦ</ex></expan> Φοιβάμμωνος ἀπὸ κώμης Ἀπόλλωνος ὑπὲρ Μάρκου 

    <lb n="3"/>ἀπὸ <expan>μειζ<ex>όνων</ex></expan> καὶ Μαρίνου <choice><corr>στιπποπραγματευτοῦ</corr><sic>στιπποπραγματευτη</sic></choice>, <subst><add place="inline">ὥστε</add><del rend="corrected">ω<del rend="erasure"><gap extent="3" reason="illegible" unit="character"/></del></del></subst> <choice><corr>Μαρίνῳ</corr><sic>μαρινου</sic></choice> ἐξουσίαν <choice><corr>αὐτῷ</corr><sic>αυτου</sic></choice> <supplied reason="omitted">εἶναι</supplied> 

    <lb n="4"/>ζητῆσαι <supplied reason="omitted">διὰ</supplied> τοῦ <choice><corr>θε<supplied reason="omitted">ί</supplied>ου</corr><sic>θεου</sic></choice> ὅρκου διὰ <choice><corr>Σοφίας</corr><sic>σοφια</sic></choice> <choice><corr>θυγατρὸς</corr><sic>θυγατρι</sic></choice> τοῦ αὐτοῦ Μάρκου ἀπὸ <expan>μειζ<ex>όνων</ex></expan> 

    <lb n="5"/>καὶ μετὰ <app type="BL"><lem resp="3.132">τὴν <choice><corr>ζητουμένην</corr><sic>ζητουμενον</sic></choice> <supplied reason="omitted">ἐξουσίαν</supplied></lem><rdg><choice><corr>τὸ</corr><sic>την</sic></choice> ζητούμενον</rdg></app> τοῦ θε<supplied reason="omitted">ί</supplied>ου ὅρκου διὰ <choice><corr>τῆς</corr><sic>τη</sic></choice> <choice><corr>αὐτῆς</corr><sic>αυτη</sic></choice> <choice><corr>Σοφίας</corr><sic>σοφια</sic></choice> <choice><corr>αὐτῷ</corr><sic>α<supplied reason="lost">υ</supplied>του</sic></choice> <sic>δε</sic> <choice><corr>Μαρίνῳ</corr><sic>μαρινου</sic></choice> <supplied reason="omitted">ἔσεσθαι</supplied> 

    <lb n="6"/>οὐδέν<supplied reason="lost">α</supplied> λόγον ὑπὲρ <choice><corr>οἱουδήποτε</corr><sic>οιασδηποτε</sic></choice> ὅλον τὸ σύνολον πράγματος. <expan>ἐγράφ<ex>η</ex></expan> <expan>μη<ex>νὸς</ex></expan> 

    <lb n="7"/>Παῦνι <num value="29"><supplied reason="lost">κ</supplied>θ</num> <choice><corr>ὥρᾳ</corr><sic>ορα</sic></choice> ἕκτῃ <choice><corr>τῆς</corr><sic>του</sic></choice> ἡμέρας. δῆλα δὲ πάλιν <unclear>ἢ</unclear> εἰ δὲ μὴ <choice><corr>θελήσει</corr><sic>θελησαι</sic></choice> <choice><corr>ὁ</corr><sic>του</sic></choice> <choice><corr>αὐτὸς</corr><sic>αυτου</sic></choice> 

