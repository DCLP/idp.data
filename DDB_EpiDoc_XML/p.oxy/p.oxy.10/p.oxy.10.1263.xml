    <lb n="6"/>ἀπ᾽ Ὀξυρύγχων π<supplied cert="high" reason="lost">όλεως</supplied> 

    <lb n="7"/>ἀμφόδου Ἑρμαί<supplied cert="high" reason="lost">ου</supplied>. 

