    <lb n="13"/>ὡς τοῦ <num value="1">α</num> <expan><ex cert="high">δραχμῶν</ex></expan> <num value="9">θ</num> <expan><ex cert="high">ὀβολοῦ 1</ex></expan><num value="1"/> <expan><ex cert="high">δραχμὰς</ex></expan> <num value="27">κζ</num> <expan><ex cert="high">ὀβολοὺς 3</ex></expan><num value="3"/>. 

