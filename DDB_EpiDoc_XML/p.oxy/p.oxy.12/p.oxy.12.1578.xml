    <lb n="11"/>καὶ <subst><add place="inline">ἀντελλόγησ<add place="supralinear">ον</add></add><del rend="corrected" status="unremarkable">αντελλογησαι</del></subst> <hi rend="diaeresis">ὑ</hi>πὲρ <choice><corr><expan>τιμ<ex cert="high">ῆς</ex></expan></corr><sic>τειμ</sic></choice> <expan>κερα<ex cert="high">μίων</ex></expan> <num value="3">γ</num> 

