    <lb n="6"/>παρὰ Αὐρηλίου Ἀκούειτος Ἄπι<unclear cert="high" reason="undefined">τ</unclear>ος <expan>μη<ex cert="high">τρὸς</ex></expan> Τσεν-

    <lb n="7"/>θώνιος ἀπὸ τῆς αὐτῆς πόλεως καταγεινομένου 

    <lb n="8"/>ἐν ἐποικίῳ Ψενεῦαρ περὶ Σενέπτα. ἑ<supplied cert="high" reason="lost">κουσίως</supplied> 

    <lb n="9"/>ἐπιδέχομαι μισθώσασθαι πρὸς μόν<supplied cert="high" reason="lost">ον τὸ</supplied> 

    <lb n="10"/>ἐνεστὸς <num value="13">ιγ</num> <orig><g type="s-etous"/></orig> καὶ <num value="12">ιβ</num> <orig><g type="s-etous"/></orig> καὶ <num value="5">ε</num> <expan><ex cert="high">ἔτος</ex></expan> ἀπὸ <unclear cert="high" reason="undefined">τῶ</unclear><supplied cert="high" reason="lost">ν ὑπαρχόντων</supplied> 

    <lb n="11"/>σοι περὶ Σενέπτα <gap extent="3" reason="illegible" unit="character"/><gap extent="15" extentmax="20" reason="lost" unit="character"/> 

    <lb n="12"/>βεν ἀπὸ ἀρουρῶν ε<gap extent="1" reason="illegible" unit="character"/><gap extent="2" extentmax="4" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἄρουραν μίαν</supplied> 

    <lb n="13"/>ἥμισυ <num value="1  1/2 "/> ὥστε ταύτην ξ<supplied cert="high" reason="lost">υλαμῆσα</supplied>ι χόρτ<unclear cert="high" reason="undefined">ῳ</unclear> <sic><unclear cert="high" reason="undefined">φόρο</unclear><supplied cert="high" reason="lost">υ</supplied></sic> 

