    <lb n="1"/><expan>π<ex cert="high">αρὰ</ex></expan> 

    <lb n="2"/>καταξιώσῃ <supplied cert="high" reason="omitted">ἡ σὴ</supplied> διαθέσις <choice><corr>συνήθως</corr><sic>συνηθος</sic></choice> <choice><corr>καταξιοῦσα</corr><sic>καταξυουσα</sic></choice> <choice><corr>φιλεῖν</corr><sic>φιλιν</sic></choice> <choice><corr>συγκροτῆσαι</corr><sic>συνκροτησε</sic></choice> τὸν 

    <lb n="3"/>ε<unclear cert="high" reason="undefined">ὐγ</unclear>ε<supplied cert="high" reason="lost">ν</supplied>ῆ Παῦλον ἀποσταλέντα παρ᾽ ἐμοῦ <choice><corr>συνωνήσασθαι</corr><sic>συνονησασθαι</sic></choice> οἶνον ἐμοί, καὶ <choice><corr>μὴ</corr><sic>μι</sic></choice> <choice><corr>συγ-

    <lb n="4"/>χωρησάτω</corr><sic>συνχορησατωι</sic></choice> <choice><corr>βαρεθῆναι</corr><sic>βαρεθεναι</sic></choice> τὸ πλοῖον <choice><corr>ο<supplied cert="high" reason="omitted">ἴ</supplied>νου</corr><sic>ονου</sic></choice> εἰς τὸ <choice><corr>τελώνιον</corr><sic>τελονιν</sic></choice>. <choice><corr>θαρρῶν</corr><sic>θαρον</sic></choice> γὰρ τῇ <choice><corr>σῇ</corr><sic>σε</sic></choice> ἀρετῇ καὶ 

    <lb n="5"/>τῇ <choice><corr>με<supplied cert="high" reason="omitted">τα</supplied>ξὺ</corr><sic>μεξυ</sic></choice> φιλίᾳ <choice><corr>γεγράφηκα</corr><sic>γεγραφεκα</sic></choice>. θεὸς γὰρ <choice><corr>οἶδεν</corr><sic>υδεν</sic></choice> ἅγιος οὐ προστασίας ἕνεκεν 

    <lb n="6"/><choice><corr><supplied cert="high" reason="lost">γ</supplied>ε<supplied cert="high" reason="lost">γ</supplied>ράφηκα</corr><sic><supplied cert="high" reason="lost">γ</supplied>ε<supplied cert="high" reason="lost">γ</supplied>ραφεκα</sic></choice>, ἀλλὰ ἐμ<unclear cert="high" reason="undefined">ο</unclear>ὶ <choice><corr>φέρεται</corr><sic>φερετε</sic></choice> ὁ <choice><corr>οἶνος</corr><sic>υνος</sic></choice>. τὰ δὲ ἀποσταλέντα παρ᾽ ἐμοῦ 

    <lb n="7"/>εὐτελέστατα <choice><corr>καταξιώσατε</corr><sic>καταξιοσατη</sic></choice> <choice><corr>ὡς</corr><sic>ος</sic></choice> πολλὰ δέξασθαι. <choice><corr>τὴ<supplied cert="high" reason="omitted">ν</supplied></corr><sic>τη</sic></choice> δὲ <choice><corr>σεμνοπρεπεστάτη<supplied cert="high" reason="omitted">ν</supplied></corr><sic>σεμνοπρεπεστατη</sic></choice> 

    <lb n="8"/>ἐλευθέραν καὶ τὰ φίλτατα ὑμῶν <choice><corr>παιδία</corr><sic>πεδια</sic></choice> ἐξ ἐμοῦ <choice><corr>προσειπεῖν</corr><sic>προσειπιν</sic></choice> <choice><corr>καταξίωσον</corr><sic>καταξιοσον</sic></choice>, 

    <lb n="9"/>καὶ ἐν οἷς <choice><corr>χρεία</corr><sic>χρια</sic></choice> <choice><corr>τῶν</corr><sic>τον</sic></choice> ἐνταῦθα <subst><add place="inline"><choice><corr>καταξιώσατε</corr><sic>καταξιοσατη</sic></choice></add><del rend="corrected" status="unremarkable"><gap extent="1" reason="illegible" unit="character"/>αταξιοσατη</del></subst> κελεύειν. 
