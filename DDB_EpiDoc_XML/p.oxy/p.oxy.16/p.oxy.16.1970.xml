    <lb n="1"/><g type="stauros"/> βασ<supplied cert="high" reason="lost">ιλεία</supplied>ς τοῦ θειοτάτ<supplied cert="high" reason="lost">ου</supplied> καὶ <expan>εὐσεβ<ex cert="high">εστάτου</ex></expan> 

    <lb n="2"/>ἡμ<supplied cert="high" reason="lost">ῶν</supplied> <expan>δεσπ<ex cert="high">ότου</ex></expan> <expan>Φλ<ex cert="high">αουίου</ex></expan> <hi rend="diaeresis">Ἰ</hi>ουστινιανοῦ 

