    <lb n="6"/>τῷ <supplied cert="high" reason="lost">α</supplied>ἰδεσίμῳ Ἐπιμάχῳ ἐ<unclear cert="high" reason="undefined">πι</unclear>κ<unclear cert="high" reason="undefined">ειμ</unclear><supplied cert="high" reason="lost">ένῳ τοῦ</supplied> 

    <lb n="7"/>θείου οἴκου υἱῷ τοῦ τῆς μακαρία<supplied cert="high" reason="lost">ς μνήμης</supplied> 

    <lb n="8"/>Ἰούστου ἀπὸ τα<supplied cert="high" reason="lost">ύ</supplied>της τῆς λαμπρᾶς <expan>Ὀξ<unclear cert="high" reason="undefined">υ</unclear>[ρυγχ<ex cert="high">ιτῶν</ex></expan> πόλεως,] 

    <lb n="9"/>Ἁρεώτης πρεσβύτερος τῆς ἁγίας ἐκ<supplied cert="high" reason="lost">κλησίας</supplied> 

    <lb n="10"/>υἱὸς Φὶβ μητρὸς Ἐρεβέκκας ὁρμώ<supplied cert="high" reason="lost">μενος</supplied> 

    <lb n="11"/>ἀπὸ κτήματος <unclear cert="high" reason="undefined">Ἀπ</unclear>ὲλ τοῦ Ὀξυρυγχίτο<supplied cert="high" reason="lost">υ νομοῦ</supplied> 

    <lb n="12"/>χαίρειν. ὁμολογῶ ἐσχηκέναι παρὰ τ<supplied cert="high" reason="lost">ῆς σῆς</supplied> 

    <lb n="13"/>αἰδεσιμότητος ἐν χρήσε<supplied cert="high" reason="lost">ι δι</supplied><unclear cert="high" reason="undefined">ὰ</unclear> χειρὸ<supplied cert="high" reason="lost">ς ἐξ οἴκου</supplied> 

