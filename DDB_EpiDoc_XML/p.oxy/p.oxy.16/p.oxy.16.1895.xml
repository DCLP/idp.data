    <lb n="3"/><supplied cert="low" reason="lost">Αὐρηλία</supplied><supplied cert="high" reason="lost"> Ἡρα</supplied>ῒς θυγάτηρ <hi rend="diaeresis">Ἰ</hi>ωάννου μητρὸς Σωσάννας ἀπὸ <gap extent="25" reason="lost" unit="character"/> 

    <lb n="4"/><gap extent="10" reason="lost" unit="character"/><unclear cert="high" reason="undefined">α</unclear>πώλῃ υ<hi rend="diaeresis">ἱ</hi>ῷ Μηνᾶ καὶ <subst><add place="inline">Μαξίμᾳ</add><del rend="corrected" status="unremarkable">μαξιμ<gap extent="1" reason="illegible" unit="character"/></del></subst> <subst><add place="inline">τῇ</add><del rend="corrected" status="unremarkable"><gap extent="2" reason="illegible" unit="character"/></del></subst> σῇ γαμετῇ, <unclear cert="high" reason="undefined">θ</unclear>[υγατρὶ <gap extent="4" reason="illegible" unit="character"/> ἀπὸ <gap extent="5" reason="illegible" unit="character"/> χαίρειν.] 

    <lb n="5"/><supplied cert="high" reason="lost">πρὸ </supplied><gap extent="4" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐνι</supplied>αυτῶν πλέον ἔλατ<g type="apostrophe"/>τον ὁ ἐμὸς σύμβιος ἐτελ<supplied cert="high" reason="lost">εύτησεν, ἐγὼ δὲ ἀπελείφθην</supplied> 

    <lb n="6"/><supplied cert="high" reason="lost">ὑπὲρ τῆς ἐμῆ</supplied><unclear cert="high" reason="undefined">ς</unclear> ἐξ αὐτοῦ θυγατρὸς κάμνουσα καὶ <choice><corr>δυστυχο<supplied cert="high" reason="lost">ῦσα</supplied></corr><sic>δυστηχο<supplied cert="high" reason="lost">υσα</supplied></sic></choice><supplied cert="high" reason="lost"> εἰς τὸ χορηγεῖν αὐτῇ τὰς</supplied> 

    <lb n="7"/><supplied cert="high" reason="lost">δεούσας τρο</supplied>φάς, καὶ τανῦν μὴ εὐποροῦσα ἀποθρέψαι <supplied cert="high" reason="lost">αὐτὴν </supplied><gap extent="18" reason="lost" unit="character"/> 

    <lb n="8"/><gap extent="6" reason="lost" unit="character"/><supplied cert="high" reason="lost"> οὖσα</supplied><unclear cert="high" reason="undefined">ν</unclear> ἐτῶν ἐννέα <num value="9"/> πλέον ἔλατ<g type="apostrophe"/>τον ἠξίωσα <unclear cert="high" reason="undefined">ὑ</unclear><supplied cert="high" reason="lost">μᾶς </supplied><gap extent="16" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ταύτην</supplied> 

