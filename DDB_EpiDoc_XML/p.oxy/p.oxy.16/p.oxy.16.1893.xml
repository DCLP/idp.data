    <lb n="1"/><g type="stauros"/> <choice><corr>ὑπατεία<supplied cert="high" reason="omitted">ς</supplied></corr><sic><hi rend="diaeresis">υ</hi>πατεια</sic></choice> <expan>Φλ<ex cert="high">αουίου</ex></expan> <expan>Βελισ<ex cert="high">αρίου</ex></expan> τοῦ <expan>ἐνδοξ<ex cert="high">οτάτου</ex></expan> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>Ἐπεὶφ <num value="24">κδ</num> <expan>ἰνδικ<ex cert="high">τίονος</ex></expan> <num value="13">ιγ</num>. 

    <lb n="3"/>Αὐρήλιος Μηνᾶς ναυπηγὸς υ<hi rend="diaeresis">ἱ</hi>ὸς <hi rend="diaeresis">Ἰ</hi>ο<supplied cert="high" reason="lost">ύστου</supplied> 

    <lb n="4"/>μητρὸς Ἡρα<hi rend="diaeresis">ϊ</hi>δος ἀπὸ τῆς Ὀξυρυγχι<supplied cert="high" reason="lost">τῶν</supplied> 

    <lb n="5"/>Αὐρηλίοις Ἀνοῦπ υ<hi rend="diaeresis">ἱ</hi>ῷ Φοιβάμμωνος 

    <lb n="6"/>καὶ <choice><corr>Βίκτωρι</corr><sic>βικτωρ</sic></choice> υ<hi rend="diaeresis">ἱ</hi>ῷ <hi rend="diaeresis">Ἰ</hi>ωάννου ἀπὸ τῆς Πτολεμα<hi rend="diaeresis">ί</hi><unclear cert="high" reason="undefined">δο</unclear>ς 

    <lb n="7"/>τῆς Θηβαίων ἐπαρχίας καὶ <choice><corr>αὐτοῖ<supplied cert="high" reason="omitted">ς</supplied></corr><sic>αυτοι</sic></choice> <choice><corr>ναυπηγοῖ<supplied cert="high" reason="omitted">ς</supplied></corr><sic>ναυπηγοι</sic></choice>. 

    <lb n="8"/>ἐπειδὴ <choice><corr>ἤρεσα</corr><sic>ηρησα</sic></choice> ὑμῖν πρὸς ἐργασίαν τῆς 

    <lb n="9"/>τέχνης τῶν ναυπηγῶν εἰς τὸ πλοῖον, 

    <lb n="10"/>ὥστε ὑμᾶς <choice><corr>ποιήσασθαί</corr><sic>ποιησατε</sic></choice> μοι τεσσεράκοντα 

    <lb n="11"/>πέντε <num value="45"/> ἡμέρας ἐν ἔργοις ἑκάστου νομίσ-

    <lb n="12"/>ματος καὶ <choice><corr>ψωμίων</corr><sic>ψουμιων</sic></choice> ἀρταβῶν δύο <num value="2"/> καὶ κρέως 

    <lb n="13"/>λίτρας εἴκοσι <num value="20"/> καὶ ἐλαίου ξέστας ἓξ  καὶ <unclear cert="high" reason="undefined">οἴ</unclear><supplied cert="high" reason="lost">νο</supplied>υ 

    <lb n="14"/>διπλᾶ ἓξ <num value="6"/> καὶ <choice><corr>ἀνενοχλήτους</corr><sic>ανενοχλητον</sic></choice> ὑμᾶς <unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">ἶναι</supplied> 

    <lb n="15"/>ἐκ τοῦ δημοσίου τῆς <supplied cert="high" reason="lost">τ</supplied>έχνης, καὶ μ<supplied cert="high" reason="lost">ετὰ τὸ</supplied> 

    <lb n="16"/>τὸ <supplied cert="high" reason="lost">π</supplied><unclear cert="high" reason="undefined">ᾶ</unclear>ν <unclear cert="high" reason="undefined">πλοῖ</unclear><supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ν</unclear> <choice><corr>τετελέσθ<supplied cert="high" reason="lost">α</supplied>ι</corr><sic><unclear cert="high" reason="undefined">ετελ</unclear>ε<unclear cert="high" reason="undefined">σ</unclear>τ<supplied cert="high" reason="lost">α</supplied>ι</sic></choice>, κοινωνῆσ<supplied cert="high" reason="lost">αι </supplied><gap extent="3" reason="lost" unit="character"/> 

