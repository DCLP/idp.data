    <lb n="1"/><g type="stauros"/> γνῶσις ὄξους <expan>ἀποκλεισθ<ex cert="high">έντος</ex></expan> ἐκ τῆς <expan>ὑποδοχ<ex cert="high">ῆς</ex></expan> Φὶβ <expan>οἰνοχειρ<ex cert="high">ιστοῦ</ex></expan> 

    <lb n="2"/>ἐπὶ <expan>μη<ex cert="high">νὸς</ex></expan> Μεσορὴ <expan>ἰνδ<ex cert="high">ικτίονος</ex></expan> <num value="12">ιβ</num>, <expan>οὕτ<ex cert="high">ως</ex></expan>· 

