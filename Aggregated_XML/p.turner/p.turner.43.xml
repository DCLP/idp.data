    <lb n="5"/>τον μὲν <unclear reason="undefined" cert="high">τ</unclear>ὸ τῆς ἐ<unclear reason="undefined" cert="high">πινε</unclear><gap reason="illegible" extent="2" unit="character"/><unclear reason="undefined" cert="high">ας</unclear> 

    <lb n="6"/>πίσσης δεύτερον τὸ τῶν <unclear reason="undefined" cert="high">χιτω</unclear>-

    <lb n="7"/><unclear reason="undefined" cert="high">ν</unclear>ίων, πλὴν <choice><corr>ἐκπέπλεκται</corr><sic>εξεπλεκται</sic></choice> τὸ <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ῆς</supplied> 

    <lb n="8"/>πίσσης. καὶ <choice><corr>ἐὰν</corr><sic>καν</sic></choice> νῦν ὅρα <unclear reason="undefined" cert="high">μὴ</unclear> ἀ<supplied reason="lost" cert="high">μ</supplied><unclear reason="undefined" cert="high">ε</unclear>-

    <lb n="9"/>λήσῃς πέμψαι μοι τὰ ὀ<unclear reason="undefined" cert="high">θόνια</unclear>. 

    <lb n="10"/><choice><corr>ᾔδειν</corr><sic><hi rend="diaeresis">ι</hi>δην</sic></choice> γὰρ ὅτι οὐκ ἔστιν σε ου<gap reason="lost" extent="1" unit="character"/><unclear reason="undefined" cert="high">αι</unclear><gap reason="illegible" extent="2" unit="character"/>-

    <lb n="11"/>ον <choice><corr>ὑποβαλέσθαι</corr><sic><hi rend="diaeresis">υ</hi>πεβαλα<unclear reason="undefined" cert="high">σ</unclear>ται</sic></choice> μοι λινό<hi rend="diaeresis">υ</hi><unclear reason="undefined" cert="high">φ</unclear><supplied reason="lost" cert="high">ον</supplied> 

    <lb n="12"/>ὄντα, ὡς ὅπου οἱ κόρακες ἀ<supplied reason="lost" cert="high">πέ</supplied>-

    <lb n="13"/>χονται ἔρις <choice><corr>σπάνιος</corr><sic><unclear reason="undefined" cert="high">σ</unclear>πανις</sic></choice> ἐστὶν τ<supplied reason="lost" cert="high">ῶν</supplied> 

    <lb n="14"/>λινο<hi rend="diaeresis">ύ</hi>φων.  περὶ γὰρ τῶν χιτ<supplied reason="lost" cert="high">ω</supplied>-

    <lb n="15"/>νίων <choice><corr>εἰ</corr><sic><hi rend="diaeresis">ι</hi></sic></choice> <choice><corr>οἶδας</corr><sic><unclear reason="undefined" cert="high">ο</unclear>ιδες</sic></choice> <unclear reason="undefined" cert="high">ὅ</unclear>τι πλε<supplied reason="lost" cert="high">ί</supplied>ω τῶ<unclear reason="undefined" cert="high">ν</unclear> 

    <lb n="16"/><expan><ex cert="high">δραχμῶν</ex></expan> <num value="240">σμ</num> οὐκ ἐδόθη, ὡς ἔφη <unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">οι</supplied> 

    <lb n="17"/>Κοπρεύς, πέμψον μοι αὐτ<supplied reason="lost" cert="high">ὰ</supplied> 

    <lb n="18"/>ἄρτι. <choice><corr>εἴ</corr><sic>ι</sic></choice> τι δὲ ἐδαπάνησας ε<unclear reason="undefined" cert="high">ἰς</unclear> 

    <lb n="19"/>τὰ ὀθόνια δήλωσόν μοι. <unclear reason="undefined" cert="high">λα</unclear>-

    <lb n="20"/>βὼν μέντοι γε τὸ πρόλ<supplied reason="lost" cert="high">οι</supplied>π<supplied reason="lost" cert="high">ον</supplied> 

    <lb n="21"/>παρὰ Κοπρέως <supplied reason="omitted" cert="high">   </supplied>, ἐὰν δὲ μή, 

    <lb n="22"/>πάλιν δήλωσόν μοι καὶ π<supplied reason="lost" cert="high">έμ</supplied>-

    <lb n="23"/>πω σοι δι᾽ ἑτέρου καταγ<g type="apostrophe"/>γί<supplied reason="lost" cert="high">σας</supplied> 

    <lb n="24"/>αὐτὰ εἰς τὸ <choice><corr>ἡμιτύλιον</corr><sic>ημιτυλ<supplied reason="lost" cert="high">ι</supplied>ν</sic></choice>.  οὐ δ<supplied reason="lost" cert="high">έ τι</supplied> 

    <lb n="25"/>ἐκομισάμην παρὰ Πετ<supplied reason="lost" cert="high">ε</supplied>-

    <lb n="26"/>χῶντος, οὐδὲ ὄψιν <unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">ὐ</supplied><unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">οῦ εἶ</supplied>-

    <lb n="27"/>δον.  ἀναζήτησον αὐ<unclear reason="undefined" cert="high">τὸ</unclear><supplied reason="lost" cert="high">ν καὶ</supplied> 

