    <lb n="1"/><gap unit="line" extent="1" reason="illegible"/> 

    <lb n="2"/><unclear reason="undefined" cert="high">Εὐ</unclear>σεβίου καὶ Σαραπίωνος <gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="7" unit="character"/><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="40" unit="character"/> 

    <lb n="3"/>παρὰ Αὐρηλίου Παήσιος Ὥρου ἀπὸ το<supplied reason="lost" cert="high">     κυβερνήτου πλοίου     ἀπὸ</supplied> 

    <lb n="4"/>λογιστῶν μετ᾽<g type="apostrophe"/> ἐγγύου <unclear reason="undefined" cert="high">τῆς</unclear> τοῦ γόμου π<supplied reason="lost" cert="high">α</supplied>ρ<supplied reason="lost" cert="high">αδόσεως </supplied><gap reason="lost" extent="15" unit="character"/><supplied reason="lost" cert="high"> Αὐρηλίου Διογένους</supplied> 

    <lb n="5"/>Διογέν<supplied reason="lost" cert="high">ου</supplied>ς τοῦ προκε<supplied reason="lost" cert="high">ι</supplied><unclear reason="undefined" cert="high">μ</unclear>ένου αὐτο<supplied reason="lost" cert="high">ῦ </supplied><gap reason="lost" extent="20" unit="character"/><supplied reason="lost" cert="high"><foreign lang="la">.</foreign> ὁμολογοῦμεν ὀμνύντες</supplied> 

    <lb n="6"/>τὸν σεβάσμιον θεῖον ὅρκ<unclear reason="undefined" cert="high">ον</unclear> τῶν δεσποτ<supplied reason="lost" cert="high">ῶν ἡμῶν Αὐτοκρατορ</supplied><gap reason="lost" extent="2" unit="character"/><supplied reason="lost" cert="high"> τε καὶ Καισάρων       </supplied> 

    <lb n="7"/>ἐγὼ μὲν ὁ <expan>κυβερ<ex cert="high">νήτης</ex></expan> παρειλη<supplied reason="lost" cert="high">φ</supplied><unclear reason="undefined" cert="high">έ</unclear>ναι παρὰ τῶν <gap reason="lost" extent="10" unit="character"/><supplied reason="lost" cert="high"> καὶ ἐμβεβλῆσθαι εἰς τὸ <expan>προκ<ex cert="high">είμενον</ex></expan> πλοῖον</supplied> 

    <lb n="8"/>ἀπὸ γενήματος τῆς εὐτυχοῦς <num value="9">θ</num> νέας ἰ<supplied reason="lost" cert="high">νδικτίονος σίτου καθαροῦ ἀκρίθου ἐκτὸς πάσης</supplied> 

    <lb n="9"/>αἰτίας καθεστηκότος μέτρῳ δημοσίῳ με<unclear reason="undefined" cert="high">γ</unclear><supplied reason="lost" cert="high">άλῳ ἀρτάβας ἑξακοσίας δεκαοκτώ, <expan><ex cert="high">γίνονται</ex></expan>  <expan><ex cert="high">ἀρτάβαι</ex></expan> <num value="618">χιη</num>, ἅσπερ ἀπενέγ</supplied>-

    <lb n="10"/>κω ἐπὶ τὴν <expan>λαμ<ex cert="high">προτάτην</ex></expan> Ἀλεξάνδρι<unclear reason="undefined" cert="high">αν</unclear> καὶ παραδώσω <supplied reason="lost" cert="high">οἷς ἐὰν κελευσθῶ σῶα καὶ ἀκακούργητα καὶ τῆς</supplied> 

    <lb n="11"/>τούτων παραδόσεως <supplied reason="lost" cert="high">ἄποχ</supplied>α γράμματα ἐπ<supplied reason="lost" cert="high">οίσω      ἐγὼ δὲ Αὐρηλίος Διογένης    </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>ἐγγυᾶσθαι τὸν αὐ<supplied reason="lost" cert="high">τὸ</supplied>ν <expan>κυβερ<ex cert="high">νήτην</ex></expan> παραδιδόντα <supplied reason="lost" cert="high">τὰς τοῦ σίτου ἀρτάβας οἷς ἐὰν κελευσθῇ ὑπὸ</supplied> 

    <lb n="13"/>ὑμῶν τῶν ἐπιμελητῶν ἢ ἐμὲ τὸν ἔγγ<supplied reason="lost" cert="high">υον ἐπάναγκες ὑποστῆναι πάντα τὰ πρὸς αὐτὸν ζη</supplied>-

    <lb n="14"/>τούμενα περὶ <unclear reason="undefined" cert="high">τού</unclear>του τοῦ γόμου εἰς τὸ ἐ<unclear reason="undefined" cert="high">ν</unclear> <supplied reason="lost" cert="high">μηδένι μεμφθῆναι ἢ ἔνοχος εἴην τῷ θείῳ ὅρκῳ</supplied>. 

    <lb n="15"/><handShift new="m2"/> Αὐρήλιος Διογένης Διογένους ἐ<supplied reason="lost" cert="high">γγυῶμαι τὸν Αὐρήλιον Παῆσιν Ὥρου <expan>κυβερ<ex cert="high">νήτην</ex></expan> πα</supplied>-

    <lb n="16"/>ραδιδόντα τὸν γόμον τοῦ σίτου καὶ <supplied reason="lost" cert="high">ὤμοσα τὸν θεῖον ὅρκον ὡς <expan>πρόκ<ex cert="high">ειται</ex></expan> καὶ <expan>ἐπερωτη<ex cert="high">θεὶς</ex></expan> <expan>ὡμολ<ex cert="high">όγησα</ex></expan></supplied>. 

    <lb n="17"/><handShift new="m3"/> Αὐρήλιος <supplied reason="lost" cert="high">Πα</supplied><unclear reason="undefined" cert="high">ῆ</unclear>σις Ὥρου ὑπεδε<supplied reason="lost" cert="high">χ</supplied>όμην καὶ ἐνεβ<supplied reason="lost" cert="high">αλόμην τὰς τοῦ σίτου ἀρτάβας ἑξακοσίας δεκα</supplied>-

    <lb n="18"/>οκτώ, <expan><ex cert="high">γίνονται</ex></expan>  <expan><ex cert="high">ἀρτάβαι</ex></expan> <num value="618">χιη</num> καὶ παραδώσω ὡς <expan>πρόκ<ex cert="high">ειται</ex></expan>. Αυρή<supplied reason="lost" cert="high">λιος       ἔγραψα ὑπὲρ αὐτοῦ μὴ εἰδότος γράμματα</supplied>. </ab></div><div n="meta.21390" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.21390" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate">365 (?)</date></p></div><div n="meta.21390" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Zu Ort und Datierung (335/6) vgl. ZPE 89, 1991, S. 95. Zur Datierung (365) vgl. ZPE 143, 2003, S. 165 mit Anm. 8.</p></div><div n="meta.21390" type="commentary" subtype="mentionedDates" org="uniform" part="N" sample="complete"><head>Erwähnte Daten</head><p>Z. 8: 9. Indiktion</p></div><div n="meta.21390" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">P.Mich.</title><biblScope type="volume">15</biblScope><biblScope type="numbers">724</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">21390</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0163</series>; <biblScope type="volume">15</biblScope>; <biblScope type="numbers">724</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0163;15;724" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.21390" type="bibliography" subtype="corrections" org="uniform" part="N" sample="complete"><head>BL-Einträge nach BL-Konkordanz</head><listBibl default="NO"><bibl type="BL" default="NO"><biblScope type="volume">VIII</biblScope>, S. <biblScope type="pages">219</biblScope></bibl><bibl type="BL