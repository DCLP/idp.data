    <lb n="1"/><supplied reason="lost" cert="high">ἔ</supplied><unclear reason="undefined" cert="high">τ</unclear>ους ἕκτου Αὐτοκράτορος Καίσαρος <unclear reason="undefined" cert="high">Τραιανοῦ</unclear> 

    <lb n="2"/><unclear reason="undefined" cert="high">Ἁδ</unclear>ριανοῦ Σεβαστοῦ, μηνὸς Γερμανικεί<unclear reason="undefined" cert="high">ου</unclear> <unclear reason="undefined" cert="high">τρίτῃ</unclear> 

    <lb n="3"/><unclear reason="undefined" cert="high">καὶ</unclear> <unclear reason="undefined" cert="high">εἰκά</unclear>δι, Παχὼν <num value="23">κγ</num>, ἐν Βακχιάδι τῆς <unclear reason="undefined" cert="high">Ἡρ</unclear>α<unclear reason="undefined" cert="high">κλείδου</unclear> 

    <lb n="4"/><unclear reason="undefined" cert="high">μερ</unclear><supplied reason="lost" cert="high">ίδο</supplied>ς τοῦ Ἀρσινοείτου νομοῦ. ὁμολογεῖ Ἁ<supplied reason="lost" cert="high">ρ</supplied><unclear reason="undefined" cert="high">υ</unclear><supplied reason="lost" cert="high">ώτης</supplied> 

    <lb n="5"/><unclear reason="undefined" cert="high">Ἀλε</unclear>ξᾶ ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <num value="60">ξ</num>, οὐλὴ ποδὶ ἀριστερῶι, Ὥρωι Ὥ<unclear reason="undefined" cert="high">ρου</unclear> <unclear reason="undefined" cert="high">ἐπι</unclear>-

    <lb n="6"/><unclear reason="undefined" cert="high">λε</unclear>γ<unclear reason="undefined" cert="high">ομ</unclear>ένου Ἰμεσβαί<unclear reason="undefined" cert="high">λ</unclear>ιος γεωργῶι ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <num value="48">μη</num>, οὐ<unclear reason="undefined" cert="high">λὴ</unclear> 

    <lb n="7"/><choice><corr>ὀφρύι</corr><sic><unclear reason="undefined" cert="high">οφρυει</unclear></sic></choice> ἀριστερᾷ, ἀπέχειν παρ᾽ αὐτοῦ τὸ<unclear reason="undefined" cert="high">ν</unclear> <unclear reason="undefined" cert="high">ὁμολο</unclear>-

    <lb n="8"/>γοῦντα τὸ ἐκφόριον τοῦ ἐνεστῶτος ἕκ<unclear reason="undefined" cert="high">το</unclear>υ <supplied reason="lost" cert="high">ἔτους</supplied> 

    <lb n="9"/><unclear reason="undefined" cert="high">Ἁδ</unclear>ριανοῦ Καίσαρος τοῦ κυρίου ὧν γεωργεῖ <unclear reason="undefined" cert="high">αὐτοῦ</unclear> 

    <lb n="10"/><supplied reason="lost" cert="high">Ἁρυ</supplied><unclear reason="undefined" cert="high">ώ</unclear>του ὁ Ὧρος περὶ τὴν προγεγραμμέν<unclear reason="undefined" cert="high">ην</unclear> 

    <lb n="11"/>κώμην κλήρου ἀρουρῶν τεσσάρων ἡμί-

    <lb n="12"/>σους <num value="4  1/2 "/> πυροῦ ἀρτάβας δέκα ἓξ <num value="16"/> μέτρωι <choice><corr>ἑξαχ<supplied reason="lost" cert="high">οι</supplied>-

    <lb n="13"/>νίκωι</corr><sic>ε<unclear reason="undefined" cert="high">ξαχ</unclear><supplied reason="lost" cert="high">οι</supplied>νεικωι</sic></choice> αὐτοῦ τοῦ Ὥρου ὑπολελογηκ<unclear reason="undefined" cert="high">ό</unclear>τ<unclear reason="undefined" cert="high">ος</unclear> 

    <lb n="14"/>παρ᾽ ἑαυτῶι τὰ <unclear reason="undefined" cert="high">σ</unclear>πέρματα τῆς τοῦ <choice><corr><expan>εἰσιόντ<ex cert="high">ος</ex></expan></corr><sic>ισιοντ</sic></choice> <num value="7">ζ</num> <expan><ex cert="high">ἔτους</ex></expan> 

    <lb n="15"/>κατασπορᾶς πυροῦ <expan>ἀρτάβ<ex cert="high">ας</ex></expan> τέσσαρας <num value="4"/>, μενούσης κυρίας 

    <lb n="16"/>τῆς μισθώσεως. ὑπογραφεῖς τοῦ μὲν ὁμο-

    <lb n="17"/>λογοῦντος Ἡλιόδωρος Ἡλιοδώρου ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <num value="49">μθ</num>, 

    <lb n="18"/>οὐλὴ μήλωι δεξιῶι, τοῦ δὲ ἄλλου Ἀμμώνιος 

    <lb n="19"/>Διοσκόρου ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <num value="45">με</num>, <expan>οὐλ<ex cert="high">ὴ</ex></expan> μετώπωι <choice><corr>ἐκ</corr><sic>εγ</sic></choice> δεξιῶν. 

    <lb n="20"/><handShift new="m2"/> Ἁρυώτης Ἀλεξᾶτος <choice><corr>ὁμολογῶ</corr><sic>ομολογωι</sic></choice> <choice><corr>ἀπέχειν</corr><sic>απεχιν</sic></choice> παρὰ 

    <lb n="21"/><unclear reason="undefined" cert="high">το</unclear>ῦ Ὥρου τὸ <choice><corr>ἐκφόριον</corr><sic>εκφοριου</sic></choice> τ<unclear reason="undefined" cert="high">ο</unclear>ῦ <choice><corr>ἐνεστῶτος</corr><sic>ενεστοτο</sic></choice> ἔτους 

    <lb n="22"/>ὧν <choice><corr>γεωργεῖ</corr><sic>γεωργι</sic></choice> ἐμοῦ περὶ τὴν <choice><corr>κώ<supplied reason="omitted" cert="high">μ</supplied>ην</corr><sic>κ<unclear reason="undefined" cert="high">ω</unclear>ην</sic></choice> ἀρουρῶν 

    <lb n="23"/><choice><corr>τεσσάρων</corr><sic>τεσσαρον</sic></choice> <choice><corr>ἡμίσους</corr><sic>ημισου</sic></choice> <num value="4  1/2 "/> πυρ<unclear reason="undefined" cert="high">ο</unclear>ῦ ἀρτάβας δέκα ἕξ <num value="16"/>, αὐ-

    <lb n="24"/><unclear reason="undefined" cert="high">το</unclear>ῦ ἔχοντος τὰ σπέρματα τοῦ <choice><corr>εἰσιόντος</corr><sic>ισιοντος</sic></choice> 

    <lb n="25"/>ἑβδόμου ἔτους πυροῦ ἀρτάβας τέσσ<unclear reason="undefined" cert="high">αρας</unclear> <num value="4"/>, 

    <lb n="26"/><choice><corr>κυρίας</corr><sic>κυρεας</sic></choice> τῆς μισθώσεως κλήρου. <unclear reason="undefined" cert="high">ἔγραψεν</unclear> <unclear reason="undefined" cert="high">ὑπὲρ</unclear> 

    <lb n="27"/>αὐτοῦ Ἡλιόδωρ<unclear reason="undefined" cert="high">ο</unclear>ς Ἡλιοδώρου μη` εἰδό<unclear reason="undefined" cert="high">τος</unclear> <unclear reason="undefined" cert="high">γράμματα</unclear>. 

    <lb n="28"/><handShift new="m3"/> Ὧρος Ὥρου γέγονε εἴς με ἡ ἀποχὴ καθ<unclear reason="undefined" cert="high">ὼ</unclear>ς 

    <lb n="29"/>πρόκιται. Ἀμμόνιος Διοσκόρου ἔγραψεν <unclear reason="undefined" cert="high">ὑπ</unclear>ὲρ 

    <lb n="30"/>αὐτοῦ μὴ εἰδότος <choice><corr>γράμμα<supplied reason="omitted" cert="high">τα</supplied></corr><sic>γραμμα</sic></choice>. <handShift new="m1"/> <expan>ἀναγέγρα<ex cert="high">πται</ex></expan> 

    <lb n="31"/>διὰ <supplied reason="lost" cert="high">το</supplied>ῦ ἐν <expan>Βακχ<ex cert="high">ιάδι</ex></expan> γραφείου. </ab></div><div n="meta.11994" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.11994" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" value="0122-05-18" notBefore="122" notAfter="122">18. Mai 122</date></p></div><div n="meta.11994" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Gehört zum Horos - Archiv (?). </p></div><div n="meta.11994" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">P.Mich.</title><biblScope type="volume">3</biblScope><biblScope type="numbers">196</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">11994</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0163</series>; <biblScope type="volume">3</biblScope>; <biblScope type="numbers">196</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0163;3;196" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.11994" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p>keine</p></div><div n="meta.11994" type="history" subtype="locations" org="uniform" part="N" sample="complete"><p><placeName type="ancientFindspot" cert="high" full="yes">Bakchias</placeName> (<geogName type="nome" cert="high" full="yes">Arsinoites</geogName>), <geogName type="ancientRegion" key="Aegyptus" cert="high" full="yes">Egypt</geogName></p></div><div n="meta.11994" type="figure" org="uniform" part="N" sample="complete"><p><figure href="http://wwwapp.cc.columbia.edu/ldpd/app/apis/item?mode=item&amp;key=michigan.apis.1322"><figDesc>Abbildung im Inter