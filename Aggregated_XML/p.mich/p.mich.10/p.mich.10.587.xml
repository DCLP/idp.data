    <lb n="2"/><unclear reason="undefined" cert="high">Πα</unclear><supplied reason="lost" cert="high">βελλ</supplied>ηοῦ<unclear reason="undefined" cert="high">ς</unclear> Ὀ<supplied reason="lost" cert="high">νν</supplied>ώ<unclear reason="undefined" cert="high">φ</unclear><supplied reason="lost" cert="high">ρ</supplied><unclear reason="undefined" cert="high">ι</unclear><supplied reason="lost" cert="high">ος</supplied> Π<supplied reason="lost" cert="high">έρσης τῆ</supplied>ς <unclear reason="undefined" cert="high">ἐπ</unclear><supplied reason="lost" cert="high">ιγο</supplied>ν<supplied reason="lost" cert="high">ῆς</supplied> 

    <lb n="3"/>ὡς ἐτῶν τεσσαρ<unclear reason="undefined" cert="high">ά</unclear>κοντα <num value="40"/> <choice><corr>οὐλὴ</corr><sic>ουληι</sic></choice> <choice><corr>μετώπῳ</corr><sic>μετοπωι</sic></choice> ἐξ ἀρισ-

    <lb n="4"/>τερῶν Ἁρμιύ<unclear reason="undefined" cert="high">σ</unclear>ι Ὀννώφριος ὡς ἐτῶν εἴκοσι ἓξ <num value="26"/> 

    <lb n="5"/><choice><corr>οὐλὴ</corr><sic>ουληι</sic></choice> γαστροκνημίᾳ ἀριστερᾷ <choice><corr>ἔχειν</corr><sic>εχιν</sic></choice> παρ᾽ αὐτοῦ 

    <lb n="6"/>παραχρῆμα διὰ <choice><corr>χειρὸς</corr><sic>χιρος</sic></choice> ἐξ οἴκου ἀργυρίου ἐπισήμου 

    <lb n="7"/>δραχμὰς τεσσαράκοντα <choice><corr>ὀκτὼ</corr><sic>οκτωι</sic></choice> <num value="48"/>, καὶ ἀντὶ τῶν τού-

    <lb n="8"/>των τόκων καὶ δεόντων καὶ <unclear reason="undefined" cert="high">ἱ</unclear>ματισμοῦ παρ-

    <lb n="9"/>έξεσθαι τὸν ὁμολογοῦντα τὴν ἑαυτοῦ <choice><corr>θυγατέρα</corr><sic>θυγατεραν</sic></choice> 

    <lb n="10"/>Κολλεῦθιν <choice><corr>παραμ<unclear reason="undefined" cert="high">έ</unclear>νουσα<supplied reason="omitted" cert="high">ν</supplied></corr><sic>παραμενουσα</sic></choice> τῶι Ἁρμιύσει καὶ τοῖς 

    <lb n="11"/>παρ᾽ αὐτοῦ ἐπ᾽ ἐνιαυτὸν ἕνα <num value="1"/> ἀπὸ τοῦ προκιμένου 

    <lb n="12"/>χρόνου διατρίβ<supplied reason="lost" cert="high">ου</supplied>σαν καὶ ποιοῦσαν τὰ <choice><corr>ἐπιστ<unclear reason="undefined" cert="high">ε</unclear>λ<supplied reason="omitted" cert="high">λ</supplied>όμενα</corr><sic>επιστ<unclear reason="undefined" cert="high">ε</unclear>λομενα</sic></choice> 

    <lb n="13"/>αὐτῆ<unclear reason="undefined" cert="high">ι</unclear> πάν<unclear reason="undefined" cert="high">τα</unclear>, <supplied reason="lost" cert="high">οὐ</supplied> <choice><corr>γινομένην</corr><sic>γεινομενηι</sic></choice> <choice><corr>ἀπόκοιτον</corr><sic>αποκοιτος</sic></choice> οὐδ᾽ <choice><corr>ἀφή-

    <lb n="14"/><supplied reason="lost" cert="high">με</supplied>ρον</corr><sic>αφημ<supplied reason="lost" cert="high">ερ</supplied>ος</sic></choice> ἀπὸ τῆς τοῦ Ἁρμιύσιος οἰκίας ἄνευ τῆς 

    <lb n="15"/>αὐτοῦ <choice><corr>γνώμης</corr><sic>γνομη<unclear reason="undefined" cert="high">ς</unclear></sic></choice>, ἀλλὰ καὶ <choice><corr>συνακο<supplied reason="lost" cert="high">λ</supplied>ουθοῦσα<supplied reason="omitted" cert="high">ν</supplied></corr><sic>συνακ<supplied reason="lost" cert="high">λ</supplied>ουθουσα</sic></choice> αὐτῶι 

    <lb n="16"/>π<unclear reason="undefined" cert="high">αν</unclear>ταχῆι κα<unclear reason="undefined" cert="high">τ</unclear>᾽ <supplied reason="lost" cert="high">Α</supplied>ἴγυπτον, καὶ μὴ <choice><corr>ἐξέστω</corr><sic>εξεστωι</sic></choice> αὐτῶι 

    <lb n="17"/>Παβελληοῦ ἐ<unclear reason="undefined" cert="high">ν</unclear><supplied reason="lost" cert="high">τ</supplied>ὸς τοῦ χρόνου ἀποσπάσαι τὴν θυ-

    <lb n="18"/>γατέραν, ἐὰν δὲ ἀ<unclear reason="undefined" cert="high">π</unclear>οσπάσηι ἢ καὶ <choice><corr>αὐτὴ</corr><sic>αυτηι</sic></choice> ἑκοῦσα 

    <lb n="19"/>ἀπαλλαγῆι ἢ ἄλλ<supplied reason="lost" cert="high">ως</supplied> παραβῶσί τι τῶν <expan>προγεγραμμέν<ex cert="high">ων</ex></expan> 

    <lb n="20"/>ἢ <unclear reason="undefined" cert="high">α</unclear><gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">η</unclear><gap reason="illegible" extent="4" unit="character"/>ου<supplied reason="lost" cert="high">σ</supplied><unclear reason="undefined" cert="high">α</unclear> ἢ <choice><corr>καταβλάπτουσα</corr><sic><unclear reason="undefined" cert="high">κατ</unclear>αβ<unclear reason="undefined" cert="high">λα</unclear>πτουσι</sic></choice> ἢ <choice><corr>νοσφιζ<supplied reason="omitted" cert="high">ομ</supplied>ένη</corr><sic>νοσφιζενος</sic></choice> 

    <lb n="21"/>ἁλίσκητα<unclear reason="undefined" cert="high">ι</unclear> τῶ<unclear reason="undefined" cert="high">ν</unclear> <supplied reason="lost" cert="high">Ἁ</supplied>ρμιύσιος ἢ τῶν παρ᾽ αὐτοῦ, ἢ λα-

    <lb n="22"/>βοῦσά τι φυλάσσ<supplied reason="lost" cert="high">ει</supplied><unclear reason="undefined" cert="high">ν</unclear> μὴ παράσχηται σῶον, ἐὰν 

    <lb n="23"/>μή τι μετὰ βία<unclear reason="undefined" cert="high">ς</unclear> φαιρεθῆι, <choice><corr>ἀποτισάτω</corr><sic>αποτισατωι</sic></choice> <supplied reason="omitted" cert="high">ὁ</supplied> <expan>ὁμολογῶ<ex cert="high">ν</ex></expan> 

    <lb n="24"/>Ἁρμιύσι παραχ<supplied reason="lost" cert="high">ρ</supplied><unclear reason="undefined" cert="high">ῆ</unclear>μα ἅς τ᾽ εἴληφεν παρ᾽ αὐτοῦ καθότι 

    <lb n="25"/>πρόκιται <expan>ἀργ<ex cert="high">υρίου</ex></expan> <supplied reason="lost" cert="high"><expan><ex cert="high">δραχμὰς</ex></expan> τεσ</supplied>σαράκοντα <choice><corr>ὀκτὼ</corr><sic>οκτωι</sic></choice> <num value="48"/> μεθ᾽ ἡμιολίας 

    <lb n="26"/>καὶ τόκων καὶ ἐπ<supplied reason="lost" cert="high">ί</supplied>τιμον <expan>ἀργ<ex cert="high">υρίου</ex></expan> <expan><ex cert="high">δραχμὰς</ex></expan> ἑκατὸν <num value="100"/> καὶ εἰς τὸ <expan>δημό<ex cert="high">σιον</ex></expan> <supplied reason="omitted" cert="high">τὰς ἴσας</supplied> 

    <lb n="27"/>καὶ τὸ βλάβος, κ<supplied reason="lost" cert="high">αὶ ἑ</supplied>κάστης <unclear reason="undefined" cert="high">ἡ</unclear>μέρ<unclear reason="undefined" cert="high">α</unclear>ς ἧς ἐὰν <expan>ἀτ<unclear reason="undefined" cert="high">ακτ</unclear>ή<ex cert="high">σῃ</ex></expan> 

    <lb n="28"/>τῆς <gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><unclear reason="undefined" cert="high">ς</unclear> χαλκοῦ ὀβολοὺς τρεῖς <num value="3"/> καὶ τὸ <expan>κεφ<ex cert="high">άλαιον</ex></expan> 

    <lb n="29"/>καὶ τὸ ἀπόκ<unclear reason="undefined" cert="high">λεμμ</unclear>α πενταπλοῦν, τὸ δὲ νοσφισμὸν 

    <lb n="30"/>ἡμιόλιον ἡμιόλιον τοὺς δὲ τόκους ἁπλοῦς. ἐὰν δὲ μὴ 

    <lb n="31"/>παραδῷ ἢ μὴ παράσχηται σῶον, τὴν ἀξίαν τὴν <expan><ex cert="high">κατὰ</ex></expan> 

    <lb n="32"/>τὴν <choice><corr>ἐπι<supplied reason="lost" cert="high">γ</supplied>νώμην</corr><sic>επι<supplied reason="lost" cert="high">γ</supplied><unclear reason="undefined" cert="high">ν</unclear>ομην</sic></choice> Ἁρμιύσιος, οὔσης τῶι Ἁρμιύσει 

    <lb n="33"/>τῆς πράξεως ἔκ τε τοῦ <expan>ὑποχ<ex cert="high">ρέου</ex></expan> καὶ ἐκ τῶν <expan>ὑπαρχ<ex cert="high">όντων</ex></expan> καθά-

    <lb n="34"/>περ <choice><corr>ἐκ</corr><sic>εγ</sic></choice> δίκης. τοῦ δὲ χρόνου <choice><corr>πληρωθέντος</corr><sic>πληροθεντος</sic></choice> <choice><corr><expan>ἀποδό<ex cert="high">τω</ex></expan></corr><sic>αποδω</sic></choice> 

    <lb n="35"/><supplied reason="omitted" cert="high">ὁ</supplied> ὁμολογῶν τὰς <expan>προκιμ<ex cert="high">ένας</ex></expan> <expan>ἀργ<ex cert="high">υρίου</ex></expan> <expan><ex cert="high">δραχμὰς</ex></expan> τεσσαράκοντα <choice><corr>ὀκτὼ</corr><sic>οκτωι</sic></choice> <num value="48"/>, 

    <lb n="36"/>ἐὰν δὲ μὴ ἀποδῷ, ἀ<unclear reason="undefined" cert="high">π</unclear>οτισάτω μεθ᾽ ἡμιολίας καὶ <expan>τόκω<ex cert="high">ν</ex></expan> 

    <lb n="37"/>καὶ ἐπίτιμον ἄλλας ἀργυρίου δραχμὰς ἑκατὸν <num value="100"/> καὶ εἰς τὸ <expan>δημό<ex cert="high">σιον</ex></expan> <supplied reason="omitted" cert="high">τὰς ἴσας</supplied>. 

    <lb n="38"/><expan>ὑπογρ<ex cert="high">αφεὺς</ex></expan> τοῦ μὲν <expan>ὁμο<ex cert="high">λογοῦντος</ex></expan> <expan>Κολλοῦθο<ex cert="high">ς</ex></expan> <expan>Μιεῦτο<ex cert="high">ς</ex></expan> <expan><ex cert="high">ὡς ἐτῶν</ex></expan> <num value="20">κ</num> <expan>οὐ<ex cert="high">λὴ</ex></expan> <expan>καρ<ex cert="high">πῷ</ex></expan> <expan>δεξ<ex cert="high">ιῷ</ex></expan> 

    <lb n="39"/><handShift new="m2"/> Παβελληοῦς <unclear reason="undefined" cert="high">Ὀννώ</unclear>φριος Πέρσης τῆς ἐπιγονῆς 

    <lb n="40"/><choice><corr>ὁμολογῶ</corr><sic>ομολογωι</sic></choice> ἔχειν <supplied reason="lost" cert="high">π</supplied>αρ᾽ Ἁρμ<unclear reason="undefined" cert="high">ιύσιο</unclear>ς <unclear reason="undefined" cert="high">ἀργυ</unclear>ρίου δρα-

