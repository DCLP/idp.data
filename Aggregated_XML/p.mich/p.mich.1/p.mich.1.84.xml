    <lb n="1"/><supplied reason="lost" cert="high">ὑπόμν</supplied>ημα Ζήνωνι παρὰ Ἡρακλείδου <gap reason="illegible" extent="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="lost" extent="1" unit="character"/><unclear reason="undefined" cert="high">ι</unclear>α<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="1" unit="character"/><unclear reason="undefined" cert="high">ε</unclear>ον τοῦ ὕδατος παραγινομένου <unclear reason="undefined" cert="high">ωσπ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>ο<gap reason="illegible" extent="2" unit="character"/><unclear reason="undefined" cert="high">ι</unclear>α, δι᾽ ἣν αἰτίαν ἱερεῖά τε οὐκ ὀλίγα κα<unclear reason="undefined" cert="high">τα</unclear>να<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="2" unit="character"/> πάντων τῶν ἐν τῆι πόλει καὶ ἀνθρώπου<supplied reason="lost" cert="high">ς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>ἐ<unclear reason="undefined" cert="high">π</unclear>ιὸν <unclear reason="undefined" cert="high">ὕδω</unclear><supplied reason="lost" cert="high">ρ</supplied> <unclear reason="undefined" cert="high">μ</unclear>ηθὲν τῶν ἐν τῆι πόλει ἁλώσε<supplied reason="lost" cert="high">ται </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>χρ<unclear reason="undefined" cert="high">α</unclear>ς <unclear reason="undefined" cert="high">ἐν</unclear> τ<unclear reason="undefined" cert="high">ῆ</unclear>ι διώρυγι. δοθήσεται δὲ <choice><corr>ἐκ</corr><sic>εγ</sic></choice> βασιλικ<unclear reason="undefined" cert="high">οῦ</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>ξύλω<unclear reason="undefined" cert="high">ν</unclear> <expan>μ<ex cert="high">υριάδες</ex></expan> <num value="4">δ</num> <num value="40000"/> καὶ πλίνθου <num value="3000">Γ</num> καὶ σελίων δε<unclear reason="undefined" cert="high">ρ</unclear><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="3" unit="character"/> ἐν ἑπταμήνωι ἀπὸ τοῦ Ἐπεὶφ <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><gap reason="illegible" extent="4" unit="character"/><unclear reason="undefined" cert="high">εσω</unclear> δὲ ἑκάστου οἰκήματος συνοικισμένου <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9a"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="2" unit="character"/> ἡμιωβέλιον <num value=" 1/2 "/> 

    <lb n="10"/>καταστήσω δὲ τηρητὴν καὶ ἐρευνητήν, ὃς ἐπ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>κοντων ἑκάστης ἡμέρας ἐπισκεπόμενος τ<unclear reason="undefined" cert="high">ὴ</unclear>ν <unclear reason="undefined" cert="high">στ</unclear><supplied reason="lost" cert="high">άσιν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>ἐ<unclear reason="undefined" cert="high">ρευν</unclear>ήσει τὰ περιφράγματα. εἰ δέ σοι δοκεῖ δοῦν<supplied reason="lost" cert="high">αι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>κατασκευασθήσεται. δὸς δέ μοι τῆς λογείας τῆ<supplied reason="lost" cert="high">ς  </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/><unclear reason="undefined" cert="high">ἐν</unclear>  πογεγραμμένωι χρόνωι τὸ ἀνήλωμα <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><unclear reason="undefined" cert="high">εἰς</unclear> ν συναγωγὴν τῶν ξύλων μὴ πρότερον δῶι<unclear reason="undefined" cert="high">ς</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>π<unclear reason="undefined" cert="high">αραλάβηις</unclear> ἑκάστης ἡμέρας ἢ ἐγγύους σοι δω<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/><unclear reason="undefined" cert="high">ἀργυρ</unclear>ίου. ἔστιν δὲ ἡ διαγραφὴ ἡ προγεγραμμέ<supplied reason="lost" cert="high">νη </supplied><gap reason="lost" extent="unknown" unit="character"/> 

