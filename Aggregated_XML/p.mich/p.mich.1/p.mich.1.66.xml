    <lb n="21"/>ου τοῦ Δαμ<unclear cert="high" reason="undefined">ᾶ</unclear> <unclear cert="high" reason="undefined">Ἀ</unclear>λεξάνδρου καὶ θεῶν Ἀδελφῶν, κανηφόρ<unclear cert="high" reason="undefined">ου</unclear> 

    <lb n="22"/>Ἀρσινόης Φι<unclear cert="high" reason="undefined">λα</unclear>δέλφου Ἀρσινόη<unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">τῆς</unclear> <unclear cert="high" reason="undefined">Π</unclear>ολε<unclear cert="high" reason="undefined">μο</unclear>κράτους, 


