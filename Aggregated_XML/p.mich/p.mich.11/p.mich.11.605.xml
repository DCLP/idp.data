<placeName cert="high" full="yes">Bacchias</placeName></head><div type="edition" lang="grc" org="uniform" part="N" sample="complete"><div type="textpart" n="r" org="uniform" part="N" sample="complete"><ab part="N">

    <lb n="1"/><gap unit="line" extent="1" reason="illegible"/> 

    <lb n="2"/><gap reason="lost" extent="30" unit="character"/><supplied reason="lost" cert="high"> οὐ</supplied><unclear reason="undefined" cert="high">λὴ</unclear> <gap reason="lost" extent="6" unit="character"/> Ἑρμᾷ Πτολεμ<unclear reason="undefined" cert="high">αίου</unclear> 

    <lb n="3"/><gap reason="lost" extent="10" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="4" unit="character"/><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="8" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="2" unit="character"/><choice><corr>τοὺς</corr><sic>τον</sic></choice> μὲ<unclear reason="undefined" cert="high">ν</unclear> Κατοίτην καὶ Πετεσοῦχον <choice><corr>ἔχειν</corr><sic>εχιν</sic></choice> 

    <lb n="4"/>παρ<unclear reason="undefined" cert="high">ὰ</unclear> <supplied reason="lost" cert="high">το</supplied>ῦ <unclear reason="undefined" cert="high">Ἑ</unclear>ρμᾶ ἀργυρίου ἐ<unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ι</supplied><unclear reason="undefined" cert="high">σ</unclear>ήμου Σεβαστῶν νομίσματος κεφαλαίου δραχμὰ<supplied reason="lost" cert="high">ς</supplied> 

    <lb n="5"/>ἑξήκοντα <num value="60"/> παραχρῆμα διὰ <unclear reason="undefined" cert="high">χειρὸ</unclear>ς ἐξ οἴκου καὶ ἀντὶ τῶν τούτων τόκων 

    <lb n="6"/>συνκεχωρηκέναι τοὺς ὁμολ<unclear reason="undefined" cert="high">ογοῦ</unclear>ντας τῶι Ἑρμᾷ <choice><corr>ἐνοικεῖν</corr><sic>ενοικιν</sic></choice> αὐτὸν καὶ τοὺς 

    <lb n="7"/>παρ᾽ αὐτοῦ καὶ οὓς ἐὰν βού<unclear reason="undefined" cert="high">λη</unclear>ται ἐν τῶι ὑπάρχοντι αὐτοῖς τοῖς ὁμολογοῦσι 

    <lb n="8"/>ἐν τῇ προγεγραμμένῃ κ<unclear reason="undefined" cert="high">ώμ</unclear>ῃ π<unclear reason="undefined" cert="high">α</unclear>τ<unclear reason="undefined" cert="high">ρ</unclear>ικῶι ἐνάτῳ καὶ <subst><add place="inline"><choice><corr>εἰκοσιεβδόμῳ</corr><sic>εικοσεβδομωι</sic></choice></add><del rend="corrected" status="unremarkable">ιικοσεβδομωι</del></subst> μέρ-

    <lb n="9"/>ει κοινῶι καὶ ἀδιαιρέτ<unclear reason="undefined" cert="high">ῳ</unclear> ρὸς αὐτὸν Ἑρμᾶν τῶν λοιπῶν μερῶν οἰκοπέ-

    <lb n="10"/>δων καὶ τῶν συνκυρόντω<unclear reason="undefined" cert="high">ν</unclear> πάντων καὶ <choice><corr>βεβαιώσειν</corr><sic>βεβαιωσιν</sic></choice> τοὺς ὁμολογοῦντας καὶ 

    <lb n="11"/>τοὺς παρ᾽ αὐτῶν τῷ Ἑρμᾷ <supplied reason="lost" cert="high">κ</supplied><unclear reason="undefined" cert="high">α</unclear>ὶ τοῖς παρ᾽ αὐτοῦ τὰ κατὰ τὴν ἐνοίκησιν <expan>ταύτη<ex cert="high">ν</ex></expan> 

    <lb n="12"/>πάσῃ <choice><corr>βεβαιώσει</corr><sic>βεβαιωσι</sic></choice> καὶ παρεχ<supplied reason="lost" cert="high">έ</supplied><unclear reason="undefined" cert="high">σ</unclear>θωισαν καθαρὰ ἀπὸ παντὸς <choice><corr>ὀφειλήματος</corr><sic>οφιληματος</sic></choice> <expan>δημοσί<ex cert="high">ου</ex></expan> 

    <lb n="13"/>τε καὶ ἰδιωτικοῦ καὶ μηδέ<unclear reason="undefined" cert="high">να</unclear> ἐνποιούμενον αὐτῶν μηδὲ μέρους μηδὲ <choice><corr><expan>κωλύοντ<ex cert="high">α</ex></expan></corr><sic>κολυοντ</sic></choice> <supplied reason="omitted" cert="high"> </supplied> 

    <lb n="14"/>ἐὰν δὲ φανῇ ὁ Ἑ<unclear reason="undefined" cert="high">ρ</unclear>μᾶς ἀνηλ<unclear reason="undefined" cert="high">ω</unclear>κὼς δαπάνην ἐπισκευῆς ἢ ἀνοικοδομῆς καὶ 

    <lb n="15"/>βούλωνται οἱ ὁμολογοῦντες ἀ<unclear reason="undefined" cert="high">πο</unclear>δῶναι ἀποδότωσαν σὺν καὶ τῷ προκιμένῳ <expan>ἀργυρί<ex cert="high">ῳ</ex></expan>, 

    <lb n="16"/>τῆς πίστεως περὶ αὐτὸν <unclear reason="undefined" cert="high">Ἑ</unclear><supplied reason="lost" cert="high">ρ</supplied>μᾶν οὔσης. <expan>ὑπο<ex cert="high">γραφεὺς</ex></expan> <expan>τῶ<ex cert="high">ν</ex></expan> <expan>ὁμολ<ex cert="high">ογούντων</ex></expan> <handShift new="m2"/> <expan>Ἡλειόδω<ex cert="high">ρος</ex></expan> <expan>Ἡλει<unclear reason="undefined" cert="high">ο</unclear>δ<ex cert="high">ώρου</ex></expan> <unclear reason="undefined" cert="high">ὡς</unclear> <expan><ex cert="low">ἐτῶν</ex></expan> <num value="44">μδ</num> <expan><unclear reason="undefined" cert="high">ο</unclear>ὐ<ex cert="high">λὴ</ex></expan> <expan><unclear reason="undefined" cert="high">μή</unclear><ex cert="high">λῳ</ex></expan> <expan><unclear reason="undefined" cert="high">δ</unclear><ex cert="high">εξιῷ</ex></expan>. 

    <lb n="17"/>Κα<unclear reason="undefined" cert="high">το</unclear>ίτης καὶ <choice><corr>Πε<supplied reason="omitted" cert="high">τεσοῦ</supplied>χος</corr><sic>πεχου</sic></choice> ἀ<unclear reason="undefined" cert="high">μφό</unclear>τεροι Ὥρου καὶ ἡ μήτηρ τούτων <gap reason="lost" extent="4" unit="character"/><unclear reason="undefined" cert="high">ι</unclear>ος 

    <lb n="18"/>Κατοίτου μετὰ κυρίου τοῦ τῆς θυγατρὸς αὐτῆς Θα<unclear reason="undefined" cert="high">υ</unclear>ήρ<unclear reason="undefined" cert="high">ιος</unclear> ἀνδρὸς 

    <lb n="19"/>Ἀπολλωνίου τοῦ Ἀπολλων<supplied reason="lost" cert="high">ί</supplied>ου ὁμολογοῦμεν ἔχ<unclear reason="undefined" cert="high">ε</unclear>ιν παρὰ τοῦ Ἑρμᾶ 

    <lb n="20"/>τὰς τοῦ ἀργυρίου δραχμ<unclear reason="undefined" cert="high">ὰς</unclear> <choice><corr>ἑξήκον<supplied reason="omitted" cert="high">τα</supplied></corr><sic>εξηκον</sic></choice> <num value="60"/> διὰ χειρὸς καὶ ἀντὶ τῶν τούτων τόκων 

    <lb n="21"/>συνχωροῦμεν <choice><corr>ἐνοικεῖν</corr><sic>ενοικιν</sic></choice> αὐτὸν ἐν τῶι ἐπιβάλλοντι ἡμῖν 

    <lb n="22"/>ἐνάτωι καὶ <choice><corr>εἰκοσιεβδόμῳ</corr><sic>εικοσιε<unclear reason="undefined" cert="high">β</unclear>δομου</sic></choice> <choice><corr>μέρει</corr><sic>μερι</sic></choice> <choice><corr>τότων</corr><sic>τοπον</sic></choice> οἰκοπέδων 

    <lb n="23"/>καὶ βεβαιώσο<unclear reason="undefined" cert="high">μ</unclear>εν <supplied reason="lost" cert="high">καὶ ἐ</supplied>ὰν φανῇ <choice><corr>ἀνηλωκὼς</corr><sic>ανηλοκος</sic></choice> δαπάνην <choice><corr>ἐπισ-

    <lb n="24"/>κ<supplied reason="omitted" cert="high">ευ</supplied>ῆς</corr><sic>επισκης</sic></choice> <supplied reason="omitted" cert="high">ἢ</supplied> <choice><corr>ἀνοικοδ<supplied reason="omitted" cert="high">ομ</supplied>ῆς</corr><sic>ανοικοδης</sic></choice> ἀπο<supplied reason="lost" cert="high">δώ</supplied>σομεν, τῆς πίστεως περὶ αὐτὸν 

    <lb n="25"/>οὔσης καθὰ πρόκιται. <supplied reason="lost" cert="high">ἔγρ</supplied>αψεν <expan>ὑπ<ex cert="high">ὲρ</ex></expan> <choice><corr>αὐτῶν</corr><sic>αυτον</sic></choice> <choice><corr>Ἡλιόδωρος</corr><sic>ηλιοδορος</sic></choice> Ἡλιοδώρου 

    <lb n="26"/>μὴ <choice><corr>εἰδότων</corr><sic>ειδοτον</sic></choice> γράμ<supplied reason="lost" cert="high">μα</supplied>τα. 

    <lb n="27"/><handShift new="m3"/> Ἑρμᾶς Πτολεμαί<supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">υ</unclear> <choice><corr>γέγονε</corr><sic>γεγωνε</sic></choice> <supplied reason="omitted" cert="high">εἴς με</supplied> ἡ ἀποχὴ καθὼς πρόκιται. 

    <lb n="28"/><handShift new="m4"/> <expan>ἐντέτακ<ex cert="high">ται</ex></expan> <expan>δι<ex cert="high">ὰ</ex></expan> <expan>το<ex cert="high">ῦ</ex></expan> ἐν <expan>Βακχ<ex cert="high">ιάδι</ex></expan> <expan>γρ<ex cert="high">αφείου</ex></expan>. 
