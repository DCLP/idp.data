    <lb n="2"/><gap extent="30" reason="lost" unit="character"/><supplied cert="high" reason="lost"> οὐ</supplied><unclear cert="high" reason="undefined">λὴ</unclear> <unclear cert="high" reason="undefined">ὑπ</unclear><gap extent="6" reason="lost" unit="character"/> Ἑρμᾷ Πτολεμ<unclear cert="high" reason="undefined">αίου</unclear> 

    <lb n="3"/><gap extent="10" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="4" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="8" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="2" reason="illegible" unit="character"/><choice><corr>τοὺς</corr><sic>τον</sic></choice> μὲ<unclear cert="high" reason="undefined">ν</unclear> Κατοίτην καὶ Πετεσοῦχον <choice><corr>ἔχειν</corr><sic>εχιν</sic></choice> 

    <lb n="4"/>παρ<unclear cert="high" reason="undefined">ὰ</unclear> <supplied cert="high" reason="lost">το</supplied>ῦ <unclear cert="high" reason="undefined">Ἑ</unclear>ρμᾶ ἀργυρίου ἐ<unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ι</supplied><unclear cert="high" reason="undefined">σ</unclear>ήμου Σεβαστῶν νομίσματος κεφαλαίου δραχμὰ<supplied cert="high" reason="lost">ς</supplied> 

    <lb n="5"/>ἑξήκοντα <num value="60"/> παραχρῆμα διὰ <unclear cert="high" reason="undefined">χειρὸ</unclear>ς ἐξ οἴκου καὶ ἀντὶ τῶν τούτων τόκων 

    <lb n="6"/>συνκεχωρηκέναι τοὺς ὁμολ<unclear cert="high" reason="undefined">ογοῦ</unclear>ντας τῶι Ἑρμᾷ <choice><corr>ἐνοικεῖν</corr><sic>ενοικιν</sic></choice> αὐτὸν καὶ τοὺς 

    <lb n="7"/>παρ᾽ αὐτοῦ καὶ οὓς ἐὰν βού<unclear cert="high" reason="undefined">λη</unclear>ται ἐν τῶι ὑπάρχοντι αὐτοῖς τοῖς ὁμολογοῦσι 

    <lb n="8"/>ἐν τῇ προγεγραμμένῃ κ<unclear cert="high" reason="undefined">ώμ</unclear>ῃ π<unclear cert="high" reason="undefined">α</unclear>τ<unclear cert="high" reason="undefined">ρ</unclear>ικῶι ἐνάτῳ καὶ <subst><add place="inline"><choice><corr>εἰκοσιεβδόμῳ</corr><sic>εικοσεβδομωι</sic></choice></add><del rend="corrected" status="unremarkable">ιικοσεβδομωι</del></subst> μέρ-

    <lb n="9"/>ει κοινῶι καὶ ἀδιαιρέτ<unclear cert="high" reason="undefined">ῳ</unclear> <unclear cert="high" reason="undefined">π</unclear>ρὸς αὐτὸν Ἑρμᾶν τῶν λοιπῶν μερῶν οἰκοπέ-

    <lb n="10"/>δων καὶ τῶν συνκυρόντω<unclear cert="high" reason="undefined">ν</unclear> πάντων καὶ <choice><corr>βεβαιώσειν</corr><sic>βεβαιωσιν</sic></choice> τοὺς ὁμολογοῦντας καὶ 

    <lb n="11"/>τοὺς παρ᾽ αὐτῶν τῷ Ἑρμᾷ <supplied cert="high" reason="lost">κ</supplied><unclear cert="high" reason="undefined">α</unclear>ὶ τοῖς παρ᾽ αὐτοῦ τὰ κατὰ τὴν ἐνοίκησιν <expan>ταύτη<ex cert="high">ν</ex></expan> 

    <lb n="12"/>πάσῃ <choice><corr>βεβαιώσει</corr><sic>βεβαιωσι</sic></choice> καὶ παρεχ<supplied cert="high" reason="lost">έ</supplied><unclear cert="high" reason="undefined">σ</unclear>θωισαν καθαρὰ ἀπὸ παντὸς <choice><corr>ὀφειλήματος</corr><sic>οφιληματος</sic></choice> <expan>δημοσί<ex cert="high">ου</ex></expan> 

    <lb n="13"/>τε καὶ ἰδιωτικοῦ καὶ μηδέ<unclear cert="high" reason="undefined">να</unclear> ἐνποιούμενον αὐτῶν μηδὲ μέρους μηδὲ <choice><corr><expan>κωλύοντ<ex cert="high">α</ex></expan></corr><sic>κολυοντ</sic></choice> <supplied cert="high" reason="omitted"> </supplied> 

    <lb n="14"/>ἐὰν δὲ φανῇ ὁ Ἑ<unclear cert="high" reason="undefined">ρ</unclear>μᾶς ἀνηλ<unclear cert="high" reason="undefined">ω</unclear>κὼς δαπάνην ἐπισκευῆς ἢ ἀνοικοδομῆς καὶ 

    <lb n="15"/>βούλωνται οἱ ὁμολογοῦντες ἀ<unclear cert="high" reason="undefined">πο</unclear>δῶναι ἀποδότωσαν σὺν καὶ τῷ προκιμένῳ <expan>ἀργυρί<ex cert="high">ῳ</ex></expan>, 

    <lb n="16"/>τῆς πίστεως περὶ αὐτὸν <unclear cert="high" reason="undefined">Ἑ</unclear><supplied cert="high" reason="lost">ρ</supplied>μᾶν οὔσης. <expan>ὑπο<ex cert="high">γραφεὺς</ex></expan> <expan>τῶ<ex cert="high">ν</ex></expan> <expan>ὁμολ<ex cert="high">ογούντων</ex></expan> <handShift new="m2"/> <expan>Ἡλειόδω<ex cert="high">ρος</ex></expan> <expan>Ἡλει<unclear cert="high" reason="undefined">ο</unclear>δ<ex cert="high">ώρου</ex></expan> <unclear cert="high" reason="undefined">ὡς</unclear> <expan><ex cert="low">ἐτῶν</ex></expan> <num value="44">μδ</num> <expan><unclear cert="high" reason="undefined">ο</unclear>ὐ<ex cert="high">λὴ</ex></expan> <expan><unclear cert="high" reason="undefined">μή</unclear><ex cert="high">λῳ</ex></expan> <expan><unclear cert="high" reason="undefined">δ</unclear><ex cert="high">εξιῷ</ex></expan>. 

    <lb n="17"/>Κα<unclear cert="high" reason="undefined">το</unclear>ίτης καὶ <choice><corr>Πε<supplied cert="high" reason="omitted">τεσοῦ</supplied>χος</corr><sic>πεχου</sic></choice> ἀ<unclear cert="high" reason="undefined">μφό</unclear>τεροι Ὥρου καὶ ἡ μήτηρ τούτων <gap extent="4" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ι</unclear>ος 

    <lb n="18"/>Κατοίτου μετὰ κυρίου τοῦ τῆς θυγατρὸς αὐτῆς Θα<unclear cert="high" reason="undefined">υ</unclear>ήρ<unclear cert="high" reason="undefined">ιος</unclear> ἀνδρὸς 

    <lb n="19"/>Ἀπολλωνίου τοῦ Ἀπολλων<supplied cert="high" reason="lost">ί</supplied>ου ὁμολογοῦμεν ἔχ<unclear cert="high" reason="undefined">ε</unclear>ιν παρὰ τοῦ Ἑρμᾶ 

    <lb n="20"/>τὰς τοῦ ἀργυρίου δραχμ<unclear cert="high" reason="undefined">ὰς</unclear> <choice><corr>ἑξήκον<supplied cert="high" reason="omitted">τα</supplied></corr><sic>εξηκον</sic></choice> <num value="60"/> διὰ χειρὸς καὶ ἀντὶ τῶν τούτων τόκων 

    <lb n="21"/>συνχωροῦμεν <choice><corr>ἐνοικεῖν</corr><sic>ενοικιν</sic></choice> αὐτὸν ἐν τῶι ἐπιβάλλοντι ἡμῖν 

    <lb n="22"/>ἐνάτωι καὶ <choice><corr>εἰκοσιεβδόμῳ</corr><sic>εικοσιε<unclear cert="high" reason="undefined">β</unclear>δομου</sic></choice> <choice><corr>μέρει</corr><sic>μερι</sic></choice> <choice><corr>τότων</corr><sic>τοπον</sic></choice> οἰκοπέδων 

    <lb n="23"/>καὶ βεβαιώσο<unclear cert="high" reason="undefined">μ</unclear>εν <supplied cert="high" reason="lost">καὶ ἐ</supplied>ὰν φανῇ <choice><corr>ἀνηλωκὼς</corr><sic>ανηλοκος</sic></choice> δαπάνην <choice><corr>ἐπισ-

    <lb n="24"/>κ<supplied cert="high" reason="omitted">ευ</supplied>ῆς</corr><sic>επισκης</sic></choice> <supplied cert="high" reason="omitted">ἢ</supplied> <choice><corr>ἀνοικοδ<supplied cert="high" reason="omitted">ομ</supplied>ῆς</corr><sic>ανοικοδης</sic></choice> ἀπο<supplied cert="high" reason="lost">δώ</supplied>σομεν, τῆς πίστεως περὶ αὐτὸν 

    <lb n="25"/>οὔσης καθὰ πρόκιται. <supplied cert="high" reason="lost">ἔγρ</supplied>αψεν <expan>ὑπ<ex cert="high">ὲρ</ex></expan> <choice><corr>αὐτῶν</corr><sic>αυτον</sic></choice> <choice><corr>Ἡλιόδωρος</corr><sic>ηλιοδορος</sic></choice> Ἡλιοδώρου 

    <lb n="26"/>μὴ <choice><corr>εἰδότων</corr><sic>ειδοτον</sic></choice> γράμ<supplied cert="high" reason="lost">μα</supplied>τα. 

    <lb n="27"/><handShift new="m3"/> Ἑρμᾶς Πτολεμαί<supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">υ</unclear> <choice><corr>γέγονε</corr><sic>γεγωνε</sic></choice> <supplied cert="high" reason="omitted">εἴς με</supplied> ἡ ἀποχὴ καθὼς πρόκιται. 

    <lb n="28"/><handShift new="m4"/> <expan>ἐντέτακ<ex cert="high">ται</ex></expan> <expan>δι<ex cert="high">ὰ</ex></expan> <expan>το<ex cert="high">ῦ</ex></expan> ἐν <expan>Βακχ<ex cert="high">ιάδι</ex></expan> <expan>γρ<ex cert="high">αφείου</ex></expan>. 
