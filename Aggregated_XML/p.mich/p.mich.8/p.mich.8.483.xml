    <lb n="7"/><handShift new="m2"/> <unclear reason="undefined" cert="high">ἐρρῶσ</unclear>θαί σε εὔχομαι, 

    <lb n="8"/><unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ιμι</supplied><unclear reason="undefined" cert="high">ώ</unclear>τατε. 

