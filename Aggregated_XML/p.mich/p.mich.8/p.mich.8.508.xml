    <lb n="10"/><space dim="horizontal" extent="unknown" unit="character"/> 

    <lb n="10"/>Θα<hi rend="diaeresis">ι</hi>σάριον Σεραποῦτι τῇ <unclear cert="high" reason="undefined">ἀ</unclear><supplied cert="high" reason="lost">δελφῇ καὶ τοῖ</supplied><unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">ἀδ</unclear><supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">λ</unclear>φο<unclear cert="high" reason="undefined">ῖς</unclear> πλεῖστα <choice><corr>χαίρειν</corr><sic>χαιριν</sic></choice>. 

    <lb n="11"/>πρὸ μὲν πάντων <choice><corr>εὔχομαι</corr><sic>ε<supplied cert="high" reason="lost">υχ</supplied>ομε</sic></choice> ὑμᾶ<supplied cert="high" reason="lost">ς ὑγιαίνει</supplied><unclear cert="high" reason="undefined">ν</unclear> καὶ τὸ προσκύν<supplied cert="high" reason="lost">η</supplied>μα <choice><corr>ὑμῶν</corr><sic>ημω<add place="supralinear">ν</add></sic></choice> 

    <lb n="12"/>ποιῶ παρὰ τῷ κυρίῳ Σ<supplied cert="high" reason="lost">αρά</supplied>πιδι. <unclear cert="high" reason="undefined">γ</unclear><supplied cert="high" reason="lost">ι</supplied><unclear cert="high" reason="undefined">ν</unclear>ώ<supplied cert="high" reason="lost">σκει</supplied><unclear cert="high" reason="undefined">ν</unclear> ὑμᾶς θέ<supplied cert="high" reason="lost">λω ὅ</supplied>τι ὁ ἀδελφὸς 

    <lb n="13"/>ἡμῶν Πτολεμαῖ<supplied cert="high" reason="lost">ος ἀ</supplied><unclear cert="high" reason="undefined">ν</unclear>έβη τὸ <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ρ</supplied><unclear cert="high" reason="undefined">ωὶ</unclear> <supplied cert="high" reason="lost">τῇ </supplied><gap extent="1" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ὥρ</supplied>ᾳ Ἐπὶφ <num value="9">θ</num>. καὶ ἐδαπά-

    <lb n="14"/>νησα εἰς τὸ <choice><corr>δεῖπνον</corr><sic>διπνον</sic></choice> α<supplied cert="high" reason="lost">ὐτ</supplied>οῦ <unclear cert="high" reason="undefined">ὅ</unclear> <choice><corr>τι</corr><sic><supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ει</unclear></sic></choice> <supplied cert="high" reason="lost">ἐ</supplied><unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">έ</supplied>μ<unclear cert="high" reason="undefined">ψ</unclear><supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">τέ</unclear> μοι. καὶ <choice><corr>ὑμεῖς</corr><sic>ημις</sic></choice> τὴν ἡμίσι-

    <lb n="15"/>αν πέμψα<supplied cert="high" reason="lost">τέ μ</supplied><unclear cert="high" reason="undefined">οι</unclear> ῥαφαν<supplied cert="high" reason="lost">ελαίο</supplied>υ κεράμια δύο <num value="2"/> ὡς τῆς τιμῆς οὗ δε-

    <lb n="16"/>δαπάνηκα. καὶ γὰρ ἐγὼ <choice><corr>χρείαν</corr><sic>χριαν</sic></choice> ἔχω <supplied cert="high" reason="lost">α</supplied>ὐτῶν ὅταν τίκτω. <del rend="erasure"><gap extent="1" reason="illegible" unit="character"/></del> καὶ 

    <lb n="17"/>ὑμῶν γὰρ ἀδ<supplied cert="high" reason="lost">ε</supplied>λφός ἐστιν. Ἀμμών<supplied cert="high" reason="lost">ιο</supplied>ς γὰρ ἀνέπ<supplied cert="high" reason="lost">λ</supplied>ευσεν με<supplied cert="high" reason="lost">τ</supplied>ὰ 

    <lb n="18"/>τοῦ ἐπάρχου, <choice><corr>α<supplied cert="high" reason="omitted">ὐ</supplied>τοῦ</corr><sic>ατου</sic></choice> <unclear cert="high" reason="undefined">ὢν</unclear> <unclear cert="high" reason="undefined">ἐ</unclear>κ τῆς <choice><corr>συσσιτί<supplied cert="high" reason="lost">ας</supplied></corr><sic><supplied cert="high" reason="lost">σ</supplied>υσιτ<unclear cert="high" reason="undefined">ι</unclear><supplied cert="high" reason="lost">ας</supplied></sic></choice><supplied cert="high" reason="lost">, κ</supplied><unclear cert="high" reason="undefined">α</unclear>ὶ ὅπου ἐὰ<unclear cert="high" reason="undefined">ν</unclear> <supplied cert="high" reason="lost">ᾖ</supplied> κατέλαβεν 

