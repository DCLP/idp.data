    <lb n="1"/>Χαιρήμων Σεραπί<supplied cert="high" reason="lost">ω</supplied>νι <gap extent="19" reason="lost" unit="character"/><supplied cert="high" reason="lost"> χαίρειν</supplied>. 

    <lb n="2"/>πρὸ μὲν πάντων εὔχομ<supplied cert="high" reason="lost">αί σε ὑγιαίνειν καὶ τὸ προσκύνη</supplied>-

    <lb n="3"/>μά σου ποιῶ παρὰ <choice><corr>τῷ</corr><sic>του</sic></choice> κυ<supplied cert="high" reason="lost">ρίῳ Σαράπιδι καθ᾽ ἡμέραν. γινώσκειν</supplied> 

    <lb n="4"/>σε θέλω ὅτι καὶ παρ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>Λίβυν οὐδεμίαν φά<supplied cert="high" reason="lost">σιν μοι ἀπέστειλας ἀφ᾽ ἧς ἦλθον εἰς</supplied> 

    <lb n="6"/>Ἀλεξάνδρειαν. ἐὰν <supplied cert="high" reason="lost">οὖν με φιλῇς, μὴ ἀμελήσῃς γράψαι</supplied> 

    <lb n="7"/>μοι. ἀσπάζομαι τὴν <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>καὶ <choice><corr>Σαραπιάδα</corr><sic>σαραπιαδαν</sic></choice> καὶ τ<unclear cert="high" reason="undefined">ὰ</unclear> <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">έκνα αὐτῆς. ἀσπάζεται ὑμᾶς Πασό</supplied>-

    <lb n="9"/><unclear cert="high" reason="undefined">ξις</unclear> ὁ τέκτων καὶ τὰ τ<supplied cert="high" reason="lost">έκνα ὑμῶν καὶ </supplied><gap extent="8" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τὴν σύμ</supplied>-

    <lb n="10"/>βιον αὐτοῦ <unclear cert="high" reason="undefined">ο</unclear>ναμ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/>ὑμ<unclear cert="high" reason="undefined">ᾶς</unclear> κατ᾽ <unclear cert="high" reason="undefined">ὄ</unclear>νο<supplied cert="high" reason="lost">μα </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="12"/><gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 
