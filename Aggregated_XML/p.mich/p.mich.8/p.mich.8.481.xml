    <lb n="17"/>ράμια <choice><corr>δύο</corr><sic>δυω</sic></choice> <num value="2"/> <unclear cert="high" reason="undefined">τὰ</unclear> <unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">έ</supplied><unclear cert="high" reason="undefined">γ</unclear>ιστα <unclear cert="high" reason="undefined">ὀ</unclear><supplied cert="high" reason="lost">λυ</supplied>-

    <lb n="18"/>ρῶν κ<supplied cert="high" reason="lost">α</supplied>ὶ ἀρτάβην ἐλαίου ῥαφα-

    <lb n="19"/>νίνου. ἔπεμ<supplied cert="high" reason="lost">ψ</supplied>ά σοι ὀριγάνιν 

    <lb n="20"/>τὸ συνπαρὰ τῷ ἐλαίῳ. καὶ εἰ 

    <lb n="21"/>ἔλαβ<supplied cert="high" reason="lost">ες</supplied> παρὰ Πτολεμαίου 

    <lb n="22"/>ἃ ἔγραψα <gap extent="2" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>δ<unclear cert="high" reason="undefined">η</unclear><gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">οι</unclear> γράψον 

    <lb n="23"/>μοι <gap extent="1" reason="illegible" unit="character"/><gap extent="7" reason="lost" unit="character"/> τὰς δύο <num value="2"/> ἀρτά-

    <lb n="24"/>βας <gap extent="1" reason="illegible" unit="character"/><gap extent="7" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">αν</unclear> καὶ ἔχῃ 

    <lb n="25"/>ἵνα <gap extent="7" reason="lost" unit="character"/> ἐνθάδε ἀναβ<unclear cert="high" reason="undefined">άς</unclear>. 

    <lb n="26"/>ἀσπ<supplied cert="high" reason="lost">άζεταί σ</supplied><unclear cert="high" reason="undefined">ε</unclear> τὰ τέ<supplied cert="high" reason="lost">κν</supplied>α ἡμῶν 

    <lb n="27"/><gap extent="11" reason="lost" unit="character"/>α<gap extent="1" reason="illegible" unit="character"/><gap extent="6" reason="lost" unit="character"/> κ<unclear cert="high" reason="undefined">αὶ</unclear>. 

    <lb n="28"/><gap extent="11" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="9" reason="lost" unit="character"/> 
