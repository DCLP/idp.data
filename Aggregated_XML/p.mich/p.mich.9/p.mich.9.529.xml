    <lb n="3"/><supplied reason="lost" cert="high">τῆς Ἡρακλ</supplied>είδου μερίδος τοῦ Ἀρσινοείτου νομοῦ. <unclear reason="undefined" cert="high">α</unclear>ἱ <unclear reason="undefined" cert="high">θ</unclear><supplied reason="lost" cert="high">εῖαι καὶ</supplied> 

    <lb n="4"/><supplied reason="lost" cert="high">βασιλικαὶ</supplied> διατάξεις διαρρήδην κελεύουσιν μὴ κατ<supplied reason="lost" cert="low">ὰ τὸ ἑξῆς</supplied> 

    <lb n="5"/><supplied reason="lost" cert="low"> ἔτος ἑτέρα</supplied><unclear reason="undefined" cert="high">ν</unclear> λειτουργίαν τινὰ ἐκτε<supplied reason="lost" cert="high">λ</supplied>εῖν ἀλλὰ παραλ<unclear reason="undefined" cert="high">είπεσ</unclear><supplied reason="lost" cert="high">θαι ἀνα</supplied>-

    <lb n="6"/><supplied reason="lost" cert="high">παυόμενον <foreign lang="la">?.</foreign></supplied> ἐπεὶ οὖν, λαμπρότ<supplied reason="lost" cert="high">α</supplied>τ<unclear reason="undefined" cert="high">ε</unclear> ἡγεμών, οἱ νῦν <unclear reason="undefined" cert="high">ἀ</unclear>π<supplied reason="lost" cert="high">ὸ κώμης</supplied> 


    <lb n="7"/><supplied reason="lost" cert="high">Καρανίδ</supplied>ος πραγματικοὶ παρη<unclear reason="undefined" cert="high">γ</unclear>ούμενοι τὰ νενόμ<supplied reason="lost" cert="high">ισμενα</supplied> 

    <lb n="8"/><gap reason="lost" extent="4" unit="character"/><supplied reason="lost" cert="high"> ἐπὶ σ</supplied><unclear reason="undefined" cert="high">ω</unclear>τήριᾳ τοῦ ἔθνους καὶ ἡμῶν τῶν γεωρ<supplied reason="lost" cert="high">γῶν </supplied><gap reason="lost" extent="3" unit="character"/> 

    <lb n="9"/><gap reason="lost" extent="8" unit="character"/> <choice><corr>σιτολόγον</corr><sic>σειτολογον</sic></choice> με ὄντα γενήματος τοῦ διεληλ<supplied reason="lost" cert="high">υθότος</supplied> 

    <lb n="10"/><supplied reason="lost" cert="high"><num value="2">β</num> <expan><ex cert="high">ἔτους</ex></expan> εἰς φυλα</supplied><unclear reason="undefined" cert="high">κ</unclear>ὴν τοῦ πυροῦ ἐν τῷ θησαυρῷ τῆς κώμ<supplied reason="lost" cert="high">ης Κα</supplied>-

    <lb n="11"/><supplied reason="lost" cert="high">ρανίδος ἀν</supplied>αδεδώκασι, καὶ νῦν <unclear reason="undefined" cert="high">πα</unclear>ρὰ πάντας τοὺς νόμ<supplied reason="lost" cert="high">ους ἐδό</supplied>-

    <lb n="12"/><supplied reason="lost" cert="low">θην</supplied><supplied reason="lost" cert="high"> εἰς λειτ</supplied>ουργίαν τὴν προκειμένην σιτολογίαν γε<supplied reason="lost" cert="high">νήματος</supplied> 

    <lb n="13"/><supplied reason="lost" cert="high">τοῦ ἐνεστῶ</supplied><unclear reason="undefined" cert="high">τος</unclear> <num value="3">γ</num> <expan><ex cert="high">ἔτους</ex></expan>, κατέφυγον ἐπὶ τοὺς πόδας σου δεό<supplied reason="lost" cert="high">μενός σου</supplied> 

