    <lb n="15"/><supplied reason="lost" cert="low">ὥστε</supplied><supplied reason="lost" cert="low"> τῆς σῆς</supplied> <unclear reason="undefined" cert="high">τ</unclear>οῦ κυρίου μου <choice><corr>βοηθείας</corr><sic>βοη<unclear reason="undefined" cert="high">θ</unclear>ιας</sic></choice> δυνηθῆναί μ<supplied reason="lost" cert="high">ε τύχειν</supplied> 

    <lb n="16"/><supplied reason="lost" cert="high">ὅπως ἀπὸ τῆς</supplied> παρανόμου ταύτης ἀναδόσεως οὕτω<supplied reason="lost" cert="low">ς ἀφεθεὶς</supplied> 

    <lb n="17"/><supplied reason="lost" cert="high">δυνηθῶ ἀμέμ</supplied>πτως ἀποπληρῶσαι τὸ τῆς προενχει<unclear reason="undefined" cert="high">ρ</unclear><supplied reason="lost" cert="high">ισθείσης</supplied> 

