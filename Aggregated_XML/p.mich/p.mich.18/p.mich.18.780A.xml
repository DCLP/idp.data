    <lb n="9"/><del rend="erasure" status="unremarkable">καὶ</del> Ἀφροδίτης δὲ <expan><ex cert="high">τάλαντα</ex></expan> <num value="12">ιβ</num>

    <lb n="10"/>ὥστ᾽ εἶναι <expan><ex cert="high">τάλαντα</ex></expan> <num value="39">λθ</num>. ἀφ᾽ ὧν

    <lb n="11"/>πέπτωκεν <supplied cert="high" reason="omitted">εἰς</supplied> Μοῦχιν <expan><ex cert="high">τάλαντα</ex></expan> <num value="8">η</num>

    <lb n="12"/><supplied cert="high" reason="omitted">καὶ εἰς</supplied> Ἀφροδίτης <expan><ex cert="high">τάλαντα</ex></expan> <num value="6">ϛ</num>, <expan><ex cert="high">γίνεται</ex></expan> <expan><ex cert="high">τάλαντα</ex></expan> <num value="14">ιδ</num>

    <lb n="13"/><expan>λο<ex cert="high">ιπὰ</ex></expan> ἐν αὐτῶι <expan><ex cert="high">τάλαντα</ex></expan> <num value="25">κε</num>

    <lb n="14"/>ἀφ᾽ ὧν ὑπάρχει αὐτῶι

    <lb n="15"/>εἰς τὴν διεγγύησιν οἰκία<sic>ν</sic>

    <lb n="16"/>ἐμ Μούχει καὶ ἄλλ<supplied cert="high" reason="lost">ο</supplied>ς ψιλὸς

    <lb n="17"/>τόπος.
