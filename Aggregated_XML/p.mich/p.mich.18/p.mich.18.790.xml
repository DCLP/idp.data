    <lb n="1"/>χαίρο<unclear cert="high" reason="undefined">ις</unclear> <supplied cert="high" reason="lost">ἄ</supplied><unclear cert="high" reason="undefined">δ</unclear><supplied cert="high" reason="lost">ελφ</supplied>ε Πετεῦρ<unclear cert="high" reason="undefined">ι</unclear><supplied cert="high" reason="lost">ς</supplied>

    <lb n="2"/>τ<unclear cert="high" reason="undefined">ὴ</unclear>ν ἐξ<unclear cert="high" reason="undefined">ω</unclear>χ<unclear cert="high" reason="undefined">ην</unclear> σου <gap extent="1" reason="lost" unit="character"/> φι<unclear cert="high" reason="undefined">λα</unclear>νθρωπία<supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="3"/>ἔχω κ<unclear cert="high" reason="undefined">αὶ</unclear> <unclear cert="high" reason="undefined">ο</unclear>ὐ <unclear cert="high" reason="undefined">δ</unclear>ύν<unclear cert="high" reason="undefined">αμα</unclear>ι πώποτε τούτου <supplied cert="high" reason="lost">ἀμείψασθαι. κύριέ</supplied>

    <lb n="4"/>μου ἄδ<unclear cert="high" reason="undefined">ελ</unclear>φε, λείαν <unclear cert="high" reason="undefined">οἶ</unclear>δ́α ὅτι ἐβάρησά <unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">ε </supplied><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="5"/>ὑπέμεινα μὴ ἄλλῳ ἐπιθέσθαι μ<unclear cert="high" reason="undefined">η</unclear><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="6"/>ὅτι περ<unclear cert="high" reason="undefined">ὶ</unclear> <supplied cert="high" reason="lost">τ</supplied>ούτου ἄλλῳ <choice><corr>ἐπιτιμησ<unclear cert="high" reason="undefined">α</unclear><gap extent="unknown" reason="lost" unit="character"/></corr><sic>επε<unclear cert="high" reason="undefined">ι</unclear>τειμησ<unclear cert="high" reason="undefined">α</unclear><gap extent="unknown" reason="lost" unit="character"/></sic></choice>

    <lb n="7"/>ἀρκεῖ μο<unclear cert="high" reason="undefined">ι</unclear> οὖν ὅσου ἐὰν παραιτήσῃ ἔχε<supplied cert="high" reason="lost">ιν </supplied><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="8"/>μὴ πρὸς τοῦτό σε βαρεῖσθαι. ἐὰν οὖν π<gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> πεί-</supplied>

    <lb n="9"/>θομαι δὲ ὅτι ἡδέως παρέχεις γλ<gap extent="unknown" reason="lost" unit="character"/>

    <lb n="10"/>τῷ ἐρχομένῳ τί δῶ, ἵνα καὶ περὶ <unclear cert="high" reason="undefined">ὧ</unclear><supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="11"/>καλέσω. κεῖται δὲ ἐπὶ <choice><corr>σφραγίδ<supplied cert="high" reason="lost">ων</supplied></corr><sic>σφραγειδ<supplied cert="high" reason="lost">ων</supplied></sic></choice><supplied cert="high" reason="lost">, ἕως περὶ πάντων</supplied>

    <lb n="12"/>τῆς ἀπὸ σοῦ ἀποκρίσεως ἐπιτύχω. <supplied cert="high" reason="lost">ἄσπασαι τοὺς</supplied>

    <lb n="13"/>σ<unclear cert="high" reason="undefined">οὺς</unclear> π<unclear cert="high" reason="undefined">ά</unclear>ντας, μεθ᾽ ὧν ἔρρωσθαί σε εὔ<supplied cert="high" reason="lost">χομαι </supplied><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="14"/><supplied cert="high" reason="lost">κύ</supplied><unclear cert="high" reason="undefined">ρ</unclear>ιέ μου.
