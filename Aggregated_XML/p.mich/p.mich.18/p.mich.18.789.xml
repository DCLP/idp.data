    <lb n="1"/><supplied cert="high" reason="lost">Καλ</supplied>πουρνία καὶ <supplied cert="high" reason="lost">Ἀπία ἡ καὶ Θεοδώρα</supplied> ἀμφότεραι

    <lb n="2"/><supplied cert="high" reason="lost">θυγα</supplied>τέρες Γα<unclear cert="high" reason="undefined">ι</unclear><gap extent="15" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ερη<gap extent="3" reason="illegible" unit="character"/>

    <lb n="3"/><gap extent="3" reason="lost" unit="character"/><supplied cert="high" reason="lost"> μ</supplied>ητρὸς Θεο<supplied cert="high" reason="lost">δώ</supplied><unclear cert="high" reason="undefined">ρ</unclear>ας ἀπ<supplied cert="high" reason="lost">ὸ Ὀξυρύ</supplied>γχων π<supplied cert="high" reason="lost">ό</supplied>λεως

    <lb n="4"/><supplied cert="high" reason="lost">ἐκατ</supplied>έρα μετὰ κυρίου τοῦ ἀ<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">δ</supplied>ρός, ἡ μὲν Καλ-

    <lb n="5"/><supplied cert="high" reason="lost">πο</supplied><unclear cert="high" reason="undefined">υ</unclear>ρνία Ἀπίωνος Ἀπολλωνίου τοῦ Σ<unclear cert="high" reason="undefined">α</unclear>ραπίω-

    <lb n="6"/><supplied cert="high" reason="lost">νο</supplied>ς μητρὸς Σαραεῦτος θέ<unclear cert="high" reason="undefined">σε</unclear>ι Διογένους Σαρα-

    <lb n="7"/>πίωνος ἀπὸ τῆς αὐτῆς πόλεως, ἡ δὲ Ἀπία ἡ

    <lb n="8"/>καὶ Θε<supplied cert="high" reason="lost">ο</supplied>δώρα Χωσίων<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ς </supplied><gap extent="2" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ω</unclear>νος τοῦ καὶ Χω-

    <lb n="9"/>σίωνος μητρὸς Θαϊσοῦτ<supplied cert="high" reason="lost">ος ἀ</supplied>πὸ τῆς αὐτῆς πό-

    <lb n="10"/>λεως τῷ πρὸς μητρὸς <unclear cert="high" reason="undefined">ἡ</unclear><supplied cert="high" reason="lost">μ</supplied><unclear cert="high" reason="undefined">ῶ</unclear>ν θείῳ Διοσ-

    <lb n="11"/>κουρίδῃ Ζωΐλου τοῦ <unclear cert="high" reason="undefined">Δ</unclear><supplied cert="high" reason="lost">ιοσκουρ</supplied>ίδου Ἁδριανείῳ

    <lb n="12"/>τῷ καὶ Ὀλυμπείῳ τῶν <supplied cert="high" reason="lost">ἱερονι</supplied>κῶν καὶ ἀτελῶν

    <lb n="13"/>γυμνασιαρχήσαντι καὶ ἀρχ<supplied cert="high" reason="lost">ιερα</supplied>τεύσαντι τῶν

    <lb n="14"/>κυρίων Σεβαστῶν καὶ θεῶ<unclear cert="high" reason="undefined">ν</unclear> <supplied cert="high" reason="lost">ἁπ</supplied><unclear cert="high" reason="undefined">ά</unclear>ντων τῆς αὐ-

    <lb n="15"/>τῆς <choice><corr>Ὀξυρυγχιτῶν</corr><sic>οξυρυγχειτων</sic></choice> πόλεως <supplied cert="high" reason="lost">νεω</supplied>κόρῳ τοῦ μεγά-

    <lb n="16"/>λου Σαράπιδος τοῦ ἐν τῇ <supplied cert="high" reason="lost">αὐτῇ π</supplied>όλει Ἑλληνικοῦ

    <lb n="17"/>Σαραπ<unclear cert="high" reason="undefined">ε</unclear>ίου χαίρειν. <choice><corr>ἐπεί</corr><sic>επι</sic></choice> δι᾽ ἧς ἔθετο ὁ σοῦ

    <lb n="18"/>μὲν πατήρ, ἡμῶν δὲ κατὰ <unclear cert="high" reason="undefined">μη</unclear>τέρα πάππος,

    <lb n="19"/>Ζωΐλος Διοσκουρίδου τοῦ Ἑρμο<unclear cert="high" reason="undefined">ῦ</unclear> <unclear cert="high" reason="undefined">μη</unclear><supplied cert="high" reason="lost">τρ</supplied><unclear cert="high" reason="undefined">ὸς</unclear> Θεοδώρας

    <lb n="20"/>ἀπὸ τῆς αὐτῆς Ὀξυρύγχων πόλεως <expan>δι<ex cert="high">ὰ</ex></expan> το<supplied cert="high" reason="lost">ῦ</supplied> ἐν

    <lb n="21"/>τῇ αὐτῇ πόλει ἀγορ<unclear cert="high" reason="undefined">ανο</unclear>μείου τῷ <num value="27">κζ</num> <expan><ex cert="high">ἔτει</ex></expan> Τῦβι

    <lb n="22"/>ἐπὶ σφραγίδων διαθήκης ἐφ᾽ ᾗ ἀμεταθέτῳ

    <lb n="23"/>ἐτελεύτησεν τῇ καὶ λυθείσῃ μεθ᾽ ἃ ἄλλα διέτα-

    <lb n="24"/>ξεν <choice><corr>ἡμῖν</corr><sic>ημειν</sic></choice> ἠθέλησ<unclear cert="high" reason="undefined">ε</unclear>ν ἐπὶ τῷ <supplied cert="high" reason="lost">σ</supplied><unclear cert="high" reason="undefined">ὲ</unclear> <choice><corr>ἐκλέξασθαι</corr><sic>εγλεξασθαι</sic></choice>

    <lb n="25"/>τὰς προσόδους τῶν δι<unclear cert="high" reason="undefined">ὰ</unclear> <supplied cert="high" reason="lost">ταύτης</supplied> <unclear cert="high" reason="undefined">τ</unclear>ῆς διαθή-

    <lb n="26"/>κης δηλουμένων <choice><corr>ἡμῖ<supplied cert="high" reason="lost">ν</supplied></corr><sic>ημ<unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">ιν</supplied></sic></choice><supplied cert="high" reason="lost"> ὑπ</supplied>αρχόντων

    <lb n="27"/>ἄχρις οὗ γαμηθῶμεν ἀν<supplied cert="high" reason="lost">δράσ</supplied><unclear cert="high" reason="undefined">ι</unclear> καὶ μετὰ τὸν

    <lb n="28"/>γάμον ἡμῶν ἐπὶ ἄλλα <unclear cert="high" reason="undefined">ἔ</unclear><supplied cert="high" reason="lost">τη </supplied><gap extent="2" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> δοῦναι <choice><corr>ἡμῖν</corr><sic>ημειν</sic></choice>,

