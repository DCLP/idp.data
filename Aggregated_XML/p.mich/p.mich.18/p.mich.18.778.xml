    <lb n="1"/>Ἀθηνοδώρωι δοικητῆ<supplied cert="high" reason="lost">ι π</supplied>αρὰ Πετεμίνιος τῶν <choice><corr>ἐκ</corr><sic><unclear cert="high" reason="undefined">εγ</unclear></sic></choice> <unclear cert="high" reason="undefined">Μούχεως</unclear>

    <lb n="2"/>τῆς Πολέμωνος μερίδος· <unclear cert="high" reason="undefined">ἀ</unclear>δικοῦμαι ὑπὸ Πυθίωνος τοῦ οἰκονο-

    <lb n="3"/>μοῦντος τὴν αὐτὴ<supplied cert="high" reason="lost">ν</supplied> μερίδα. Ὀννώφριος γὰρ <unclear cert="high" reason="undefined">τοῦ</unclear> <choice><corr>ἐκλαβόντος</corr><sic><unclear cert="high" reason="undefined">εγ</unclear>λαβοντος</sic></choice>

    <lb n="4"/>τὴν ζυτηρὰν Μούχεως εἰς τὸ <num value="13">ιγ</num> <expan><ex cert="high">ἔτος</ex></expan> <gap extent="5" reason="illegible" unit="character"/>

    <lb n="5"/><unclear cert="high" reason="undefined">σ</unclear>αντος ἐν τῶι Φαῶφι δ<gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> τὰς τοῦ Πυθί<unclear cert="high" reason="undefined">ωνος</unclear> παρα<gap extent="8" reason="illegible" unit="character"/>

    <lb n="6"/>προσεβάλετο Πυθίων Πε<supplied cert="high" reason="lost">τ</supplied>εσούχωι τῶι ἐξειληφότι

    <lb n="7"/>τὴν ζυτηρὰν Ἀφροδίτης πόλεως καὶ Ἐλευσ<unclear cert="high" reason="undefined">ῖνος</unclear> τὴν

    <lb n="8"/>ζυτηρὰν Μούχεως ἀνὰ <expan><ex cert="high">τάλαντα</ex></expan> <num value="42">μβ</num> <expan><ex cert="high">δραχμὰς</ex></expan> <num value="2000">Β</num> <expan><ex cert="high">τάλαντα</ex></expan> <num value="20"><unclear cert="high" reason="undefined">κ</unclear></num> <gap extent="4" reason="illegible" unit="character"/> εἰς δ<unclear cert="high" reason="undefined">ε</unclear><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="9"/>προστῆναι τῶι ἐμ Μούχ<supplied cert="high" reason="lost">ει ζυ</supplied>τοπωλίωι καὶ ἅμα τέλει <gap extent="2" reason="illegible" unit="character"/>

    <lb n="10"/>γραμματ<gap extent="3" reason="illegible" unit="character"/>δι<gap extent="4" reason="illegible" unit="character"/>τ<supplied cert="high" reason="lost">ὸ</supplied>ν αὐτὸν Πετεσοῦχον <gap extent="11" reason="illegible" unit="character"/>τὰ ἐγ-

    <lb n="11"/>γραπτὰ γεγραμμένα ζυτοπώλια <unclear cert="high" reason="undefined">καὶ</unclear><gap extent="4" reason="illegible" unit="character"/> <gap desc="vestiges" extent="unknown" reason="illegible" unit="character"/>

    <lb n="12"/>οὐκ ἐμβλεψα<gap extent="4" reason="illegible" unit="character"/>κ<gap extent="3" reason="illegible" unit="character"/><gap extent="6" reason="lost" unit="character"/> Μούχεως <gap desc="vestiges" extent="unknown" reason="illegible" unit="character"/>

    <lb n="13"/>εἰς τὸ ἴδιον λ<unclear cert="high" reason="undefined">όγ</unclear><gap extent="6" reason="lost" unit="character"/> <gap desc="vestiges" extent="unknown" reason="illegible" unit="character"/>

    <lb n="14"/>τὰ γράμματ<supplied cert="high" reason="lost">α</supplied><gap extent="4" reason="illegible" unit="character"/><gap extent="6" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/> καὶ <gap extent="5" reason="illegible" unit="character"/> Ἀφροδίτης <unclear cert="high" reason="undefined">π</unclear>όλ<unclear cert="high" reason="undefined">εως</unclear>

