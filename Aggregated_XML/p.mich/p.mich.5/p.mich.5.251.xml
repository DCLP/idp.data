    <lb n="1"/><supplied reason="lost" cert="high">ἀντίγραφον χαράγματος. <expan><ex cert="high">ἔτους</ex></expan></supplied> πέμπτου Τιβ<supplied reason="lost" cert="high">ερίου Καίσαρος Σεβαστοῦ</supplied> 

    <lb n="2"/><supplied reason="lost" cert="high">μηνὸς Μεσορὴ ἐπαγομέν</supplied><unclear reason="undefined" cert="high">ων</unclear> <choice><corr>πέμπτῃ</corr><sic>πεμπτης</sic></choice> κ<supplied reason="lost" cert="high">ατακεχώρισται διὰ τοῦ ἐν</supplied> 

    <lb n="3"/><supplied reason="lost" cert="high">Ταλεὶ γραφείου</supplied>. 

    <lb n="4"/><supplied reason="lost" cert="high">ἀντίγραφον ὑπογραφῆς</supplied>. Κρονίων Πατύ<unclear reason="undefined" cert="high">ν</unclear><supplied reason="lost" cert="high">εως μητρὸς Διδύμης</supplied>

    <lb n="5"/><supplied reason="lost" cert="high">ὁμολογῶ πεπρακέν</supplied><unclear reason="undefined" cert="high">α</unclear>ι Πιμῶτι Ὀρσεν<supplied reason="lost" cert="high">ούφιος μητρὸς Τελεμάσιος</supplied> 

    <lb n="6"/><supplied reason="lost" cert="high">τοὺς ὑπάρχον</supplied><unclear reason="undefined" cert="high">τά</unclear>ς μοι ψιλοὺς τό<unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ους σὺν τοῖς ἐνοῦσι φορ</supplied>-

    <lb n="7"/><unclear reason="undefined" cert="high">τί</unclear><supplied reason="lost" cert="high">οις ἐν Ταλ</supplied><unclear reason="undefined" cert="high">εὶ</unclear>  Πολέμωνος μερίδ<supplied reason="lost" cert="high">ος, ὧν μέτρα νότου</supplied> 

    <lb n="8"/><unclear reason="undefined" cert="high">ἐπὶ</unclear> <supplied reason="lost" cert="high">ορρᾶ πήχ</supplied><unclear reason="undefined" cert="high">ει</unclear>ς εἴκοσι <num value="20"/>, λιβὸς ἐπ᾽ ἀπηλιώ<unclear reason="undefined" cert="high">την</unclear> <supplied reason="lost" cert="high">πήχεις δέκα ἓξ <num value="16"/> </supplied><gap reason="lost" extent="3" unit="character"/> 


    <lb n="9"/>φ<unclear reason="undefined" cert="high">οτ</unclear><gap reason="lost" extent="5" unit="character"/> ἢ ὅσων ἐὰν ὦσιν καὶ τῶν συνκυρ<supplied reason="lost" cert="high">όντων πάντων,</supplied> 

    <lb n="10"/>ὧν <choice><corr>γείτονες</corr><sic>γ<unclear reason="undefined" cert="high">ι</unclear><supplied reason="lost" cert="high">τονε</supplied>ς</sic></choice> νότου ῥύμη βασιλική, βορρᾶ Πετερμο<unclear reason="undefined" cert="high">ύ</unclear><supplied reason="lost" cert="high">θεως Ὀρ</supplied><unclear reason="undefined" cert="high">σεῦ</unclear>-

    <lb n="11"/>τος ψι<supplied reason="lost" cert="high">λοὶ</supplied> <unclear reason="undefined" cert="high">τ</unclear>όποι, λιβὸς Ὥρου τοῦ Ἡρακλήους ἔπαυλις, ἀ<unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ηλι</supplied><unclear reason="undefined" cert="high">ώ</unclear>του 

    <lb n="12"/><choice><corr>Ἁρμιύσιος</corr><sic>αρμιυ<unclear reason="undefined" cert="high">σις</unclear></sic></choice> Σοκονεῦτος ψιλὸς τόπος. καὶ ἀπέχω παρὰ <supplied reason="lost" cert="high">τοῦ πρ</supplied>ιαμένου 

    <lb n="13"/>τὴν <unclear reason="undefined" cert="high">συν</unclear>κεχωρημένην τιμὴν πᾶσαν ἐκ πλήρους παρ<supplied reason="lost" cert="high">αχ</supplied>ρῆμα 

    <lb n="14"/>διὰ <choice><corr>χειρὸς</corr><sic>χι<unclear reason="undefined" cert="high">ρ</unclear>ος</sic></choice> ἐξ οἴκου καὶ βεβαιώσω πάσῃ <choice><corr>βεβαιώσει</corr><sic>βεβαιωσι</sic></choice> ἐπ<unclear reason="undefined" cert="high">ὶ</unclear> ὸν ἀεὶ 

    <lb n="15"/>χρό<unclear reason="undefined" cert="high">νο</unclear>ν, καὶ τὰ ἄλλα ποιήσω καθὼς πρόκιται. ἔγραψεν ὑπὲρ 

    <lb n="16"/>αὐτοῦ <unclear reason="undefined" cert="high">Ἡ</unclear>λιόδωρος Ἀπολλωνίδου διὰ τὸ μὴ εἰδέναι αὐτὸν γράμματα. 

    <lb n="17"/>ἀντί<supplied reason="lost" cert="high">γ</supplied>ραφ<unclear reason="undefined" cert="high">ον</unclear> ἄλλης ὑπογραφῆς. Πιμῶς Ὀρσενούφιος μητρὸς Τελεμάσιος 

    <lb n="18"/>γέγονε εἴς με ἡ προκιμένη πρᾶσις τῶν προκιμ<unclear reason="undefined" cert="high">ένω</unclear>ν τόπων 

    <lb n="19"/><supplied reason="lost" cert="high">ψιλῶν καὶ</supplied> ἔδωκα τὴν συνκεχωρημένην τιμὴν κα<unclear reason="undefined" cert="high">θ</unclear>ὼς πρόκι-

    <lb n="20"/><supplied reason="lost" cert="high">ται. ἔγ</supplied>ραψεν ὑπὲρ αὐτοῦ Θέων Διδύμου διὰ τὸ μὴ εἰδέναι αὐτὸν 

    <lb n="21"/><supplied reason="lost" cert="high">γράμ</supplied>ματα. 

    <lb n="22"/><supplied reason="lost" cert="high">ἀντίγρα</supplied>φον σώματος. ἔτους πέμπτου Τιβερίου Καίσαρος Σεβαστοῦ 

    <lb n="23"/><supplied reason="lost" cert="high">μην</supplied><unclear reason="undefined" cert="high">ὸς</unclear> Μεσορὴ ἐπαγομένων <choice><corr>πέμπτῃ</corr><sic>πεμπτης</sic></choice> ἀναγέγραπται διὰ Μαλχί-

    <lb n="24"/><supplied reason="lost" cert="high">ων</supplied><unclear reason="undefined" cert="high">ο</unclear>ς τοῦ πρὸς τὸ <choice><corr>γραφεῖον</corr><sic>γραφιωι</sic></choice> Ταλεὶ καὶ ἄλλων κωμῶν τῆς <expan>Πολέμωνο<ex cert="high">ς</ex></expan> 

    <lb n="25"/><supplied reason="lost" cert="high">μερ</supplied>ίδος τοῦ Ἀρσινοείτου νομοῦ. συνγραφὴ πράσεως ψιλοῦ τόπου 

    <lb n="26"/><supplied reason="lost" cert="high">σὺν</supplied> <unclear reason="undefined" cert="high">το</unclear>ῖς ἐνοῦσι φορτίοις πᾶσι ἐν Ταλεὶ τῆς Πολέμωνος μερίδος, 

    <lb n="27"/><supplied reason="lost" cert="high">ὧν μ</supplied>έτρα <choice><corr>νότου</corr><sic>νοτον</sic></choice> ἐπὶ βορρᾶ <choice><corr><expan>πήχει<ex cert="high">ς</ex></expan></corr><sic>πηχι</sic></choice> εἴκοσι <num value="20"/>, <expan>λι<ex cert="high">βὸς</ex></expan> ἐπ᾽ <expan>ἀπηλιώτη<ex cert="high">ν</ex></expan> πήχεις 

    <lb n="28"/><supplied reason="lost" cert="high">δέκ</supplied>α <supplied reason="lost" cert="high">ἓ</supplied><unclear reason="undefined" cert="high">ξ</unclear> <num value="16"/> ἐπὶ παντὸς τοῦ πηχισμοῦ, ἢ <choice><corr>ὅσαι</corr><sic>οσε</sic></choice> ἐὰν ὦσι, καὶ τῶν συν-

    <lb n="29"/><supplied reason="lost" cert="high">κυ</supplied><unclear reason="undefined" cert="high">ρόν</unclear>των πάντων, ὧν <choice><corr>γείτονες</corr><sic>γιτωνος</sic></choice> νότου ῥύμη βασιλική, 

    <lb n="30"/><supplied reason="lost" cert="high">βορρ</supplied><unclear reason="undefined" cert="high">ᾶ</unclear> ετερμούθεως Ὀρσεῦτος ψιλοὶ τόποι, <expan>λι<ex cert="high">βὸς</ex></expan> Ὥρου τοῦ <expan>Ἡρακλή<ex cert="high">ους</ex></expan> 

    <lb n="31"/><supplied reason="lost" cert="high">ἔπαυλ</supplied><unclear reason="undefined" cert="high">ις</unclear>, ἀπηλιώτου Ἁρμιύσιος τοῦ Σοκέως ψιλοὶ <expan>τόπο<ex cert="high">ι</ex></expan>, ἣν ποι-

    <lb n="32"/><supplied reason="lost" cert="high">εῖται</supplied> <unclear reason="undefined" cert="high">Κ</unclear>ρονίων Πατύνεως μητρὸς Διδύμης πε<unclear reason="undefined" cert="high">πρ</unclear>ακέναι 

    <lb n="33"/><supplied reason="lost" cert="high">Πιμῶ</supplied>τι Ὀρσενούφιος μητρὸς Τελεμάσιος καὶ ἀπέ<supplied reason="lost" cert="high">χει</supplied><unclear reason="undefined" cert="high">ν</unclear> ν συν-

    <lb n="34"/><supplied reason="lost" cert="high">κεχω</supplied><unclear reason="undefined" cert="high">ρ</unclear>ημένην τιμὴν πᾶσαν ἐκ πλήρους παραχ<unclear reason="undefined" cert="high">ρ</unclear>ῆμα διὰ 

    <lb n="35"/><supplied reason="lost" cert="high">χειρὸς ἐξ</supplied> οἴκου καὶ <choice><corr>βεβαιώσ<supplied reason="omitted" cert="high">ε</supplied>ι<supplied reason="omitted" cert="high">ν</supplied></corr><sic>βεβαιωσι</sic></choice> αὐτὸν τὴν πρᾶ<unclear reason="undefined" cert="high">σι</unclear>ν ἐ<unclear reason="undefined" cert="high">π</unclear>ὶ τὸν ἅπαν-

    <lb n="36"/><supplied reason="lost" cert="high">τα χρόνον. ὑ</supplied>πογραφεὺς τοῦ ὁμολογοῦντος Ἡλιόδωρ<unclear reason="undefined" cert="high">ος</unclear> Ἀπολλω-

    <lb n="37"/><supplied reason="lost" cert="high">νίδου ὡς <expan><ex cert="high">ἐτῶν</ex></expan></supplied> <num value="23"><unclear reason="undefined" cert="high">κ</unclear>γ</num>, <expan>οὐλ<ex cert="high">ὴ</ex></expan> παρὰ <expan>γαστροκνη<ex cert="high">μίᾳ</ex></expan> <expan>ἀρ<ex cert="high">ιστερᾷ</ex></expan>, τοῦ δὲ <unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ριαμέ</supplied><unclear reason="undefined" cert="high">νου</unclear> Θέων 

    <lb n="38"/><supplied reason="lost" cert="high">Διδύμου</supplied> ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <num value="39">λθ</num>, <expan>οὐλ<ex cert="high">ὴ</ex></expan> <choice><corr>μετώπῳ</corr><sic>μετοπο</sic></choice> ἐκ δεξι<supplied reason="lost" cert="high">ῶν. τὰ ἄλ</supplied><unclear reason="undefined" cert="high">λα</unclear> <choice><corr>ἀκ<supplied reason="omitted" cert="high">ο</supplied>λούθως</corr><sic>ακλου<unclear reason="undefined" cert="high">θω</unclear>ς</sic></choice> 

