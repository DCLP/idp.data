    <lb n="2"/>παρὰ Πτολε<unclear cert="high" reason="undefined">μαία</unclear>ς. ἐ<unclear cert="high" reason="undefined">γδο</unclear>ύσης

    <lb n="3"/><unclear cert="high" reason="undefined">ἐμ</unclear>οῦ <unclear cert="high" reason="undefined">Τ</unclear>ετώυ<unclear cert="high" reason="undefined">τ</unclear>ι καπηλίδι

    <lb n="4"/>τῶν ἀπὸ τοῦ ὅρμου ἐρίου <app type="alternative"><lem><expan>στα<ex cert="high">θμίον</ex></expan> <num value="1"><unclear cert="high" reason="undefined">α</unclear></num></lem><rdg><expan>στα<ex cert="high">θμία</ex></expan> <num value="4"><unclear cert="high" reason="undefined">δ</unclear></num></rdg></app>

    <lb n="5"/>ὥστε νῆσαι καὶ ἀπ<unclear cert="high" reason="undefined">ὸ</unclear> <unclear cert="high" reason="undefined">τ</unclear>οῦ

    <lb n="6"/>συγχωρηθέντος μισθοῦ

    <lb n="7"/><choice><corr>κομισαμένης</corr><sic>κομισομενης</sic></choice> τὸ <expan><ex cert="high">ἥμισυ</ex></expan>, <choice><corr>νυνὶ</corr><sic>νυνει</sic></choice>

    <lb n="8"/>δὲ μὴ ὑπομενούσης αὐτῆς

    <lb n="9"/>ἀποδιδόναι τὸ ἔργον, ἀλλὰ

    <lb n="10"/>διαπλανώσης, <choice><corr>ἀξιῶ</corr><sic>αξιω<unclear cert="high" reason="undefined">ι</unclear></sic></choice>

    <lb n="11"/>ἀνακαλεσαμένους αὐτὴ<unclear cert="high" reason="undefined">ν</unclear>

    <lb n="12"/>ἐπαναγκάσαι κομισαμένην

    <lb n="13"/>τὸ λοιπ<unclear cert="high" reason="undefined">ὸν</unclear> τοῦ μισθοῦ ἀπο-

    <lb n="14"/>δοῦναί μοι ἵν᾽ οὕτω τύχ<unclear cert="high" reason="undefined">ω</unclear>

    <lb n="15"/>τοῦ δικαίου.

    <lb n="16"/><unclear cert="high" reason="undefined">εὐτυχ</unclear><supplied cert="high" reason="lost">εῖτε</supplied>
