    <lb n="4"/><gap reason="lost" extent="2" unit="character"/>ου<gap reason="lost" extent="1" unit="character"/>τ<gap reason="lost" extent="1" unit="character"/> καρπῶν τῆς εὐτυχοῦς τρισκαιδεκ<supplied reason="lost" cert="high">ά</supplied>της <expan>ἰνδικτ<ex cert="high">ίονος</ex></expan> 

