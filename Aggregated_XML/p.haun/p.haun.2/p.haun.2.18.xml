    <lb n="25"/><choice><corr>ζυμίτας</corr><sic>ζυμια</sic></choice> <choice><corr>ἄρτους</corr><sic>αρτοι</sic></choice> <num value="4">δ</num>. ἄσπασ<unclear reason="undefined" cert="high">ον</unclear> 

    <lb n="25"/><gap reason="illegible" extent="unknown" unit="character" desc="vestiges"/> 

    <lb n="26"/>τὴν ἀδελφήν μοι κ<unclear reason="undefined" cert="high">αὶ</unclear> 

    <lb n="26"/><gap reason="illegible" extent="unknown" unit="character" desc="vestiges"/> 

    <lb n="27"/><gap reason="illegible" extent="unknown" unit="character" desc="vestiges"/> 

    <lb n="27"/>δήλωσόν μοι ἐὰν τέτο-

    <lb n="28"/><choice><corr>κε</corr><sic>δωκαι</sic></choice> καὶ τί ἐστιν τὸ γε<unclear reason="undefined" cert="high">ν</unclear>-

    <lb n="29"/>νώμε<unclear reason="undefined" cert="high">ν</unclear>ον. ἐρρῶσθα<unclear reason="undefined" cert="high">ί</unclear> <unclear reason="undefined" cert="high">σε</unclear> 

    <lb n="30"/><expan>εὔχομ<ex cert="high">αι</ex></expan>. Τῦβ<unclear reason="undefined" cert="high">ι</unclear> <num value="21">κ<unclear reason="undefined" cert="high">α</unclear></num>. 
