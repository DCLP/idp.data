    <lb n="2"/>ἀπέδοτο Ἁκετε-

    <lb n="3"/>φνάχθης <expan><ex cert="high">ἥμισυ</ex></expan> 

    <lb n="4"/>οἰκία<unclear reason="undefined" cert="high">ς</unclear> κα<unclear reason="undefined" cert="high">ὶ</unclear> <unclear reason="undefined" cert="high">α</unclear>ὐ<unclear reason="undefined" cert="high">λ</unclear>η῀ς. 

    <lb n="5"/>ἐπρίατο <expan>Σενεν<unclear reason="undefined" cert="high">ο</unclear>ῦ<ex cert="high">φις</ex></expan> 

    <lb n="6"/>καὶ Σεμπ<unclear reason="undefined" cert="high">ε</unclear>λαία<unclear reason="undefined" cert="high">ς</unclear> 

    <lb n="7"/><expan>χα<ex cert="high">λκοῦ</ex></expan> <num value="3000">Γ</num>. 
