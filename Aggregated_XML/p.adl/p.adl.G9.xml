    <lb n="5"/>προσού<unclear reason="undefined" cert="high">σ</unclear><supplied reason="lost" cert="high">η</supplied>ς αὐλῆς τὸ <num value="1/2">𐅵</num>, <supplied reason="omitted" cert="high">ἣν</supplied> <choice><corr>ἠγόρασαν</corr><sic>ηγ<unclear reason="undefined" cert="high">ο</unclear>ρασεν</sic></choice> παρ<unclear reason="undefined" cert="high">ὰ</unclear> 

    <lb n="6"/><sic>παρὰ</sic> Ἁκ<supplied reason="lost" cert="high">ε</supplied>τεπνάχθ<unclear reason="undefined" cert="high">ου</unclear> τοῦ Ψεμμώνθ<unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">υ</supplied> 

    <lb n="7"/>χαλκοῦ <num value="3000">Γ</num> τέ<unclear reason="undefined" cert="high">λος</unclear> <num value="300">τ</num>. 

