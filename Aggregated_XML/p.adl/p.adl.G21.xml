    <lb n="1"/>βασιλευόντων Πτολεμαί<unclear cert="high" reason="undefined">ου</unclear> τοῦ ἐπικαλουμένου Ἀλεξάνδ<supplied cert="high" reason="lost">ρο</supplied><unclear cert="high" reason="undefined">υ</unclear> καὶ Βερενί<unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">ης τῆς ἀδελφῆς θεῶν Φιλομη</supplied>-

    <lb n="2"/>τόρων ἔτους <num value="17">ιζ</num> Ἁθὺρ <num value="5">ε</num> ἐφ᾽ <choice><corr>ἱερέων</corr><sic>ι<unclear cert="high" reason="undefined">ερ</unclear>ειων</sic></choice> καὶ ἱ<unclear cert="high" reason="undefined">ε</unclear>ρειῶν κ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ὶ</supplied> <unclear cert="high" reason="undefined">κα</unclear>νηφόρο<unclear cert="high" reason="undefined">υ</unclear> <supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ῶ</unclear>ν ὄντ<unclear cert="high" reason="undefined">ω</unclear>[ν καὶ οὐσῶν, ἐν Κροκοδίλων <unclear cert="high" reason="undefined">πόλει</unclear>] 

    <lb n="3"/>ἐπ᾽ Ἀμμωνίου τοῦ παρὰ Πανίσ<unclear cert="high" reason="undefined">κ</unclear>ου ἀγορανόμου. ἀπέδοτο <unclear cert="high" reason="undefined">Ἑ</unclear>τπ<unclear cert="high" reason="undefined">εῆ</unclear>σις Πατῆτο[ς <unclear cert="high" reason="undefined">Περσίνη</unclear> ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <gap extent="2" reason="illegible" unit="character"/> μέση <foreign lang="la">?,</foreign> <unclear cert="high" reason="undefined">μελίχρως</unclear>] 

    <lb n="4"/>τετανὸ<unclear cert="high" reason="undefined">ς</unclear> στρ<unclear cert="high" reason="undefined">ο</unclear>γγυλοπρόσω<unclear cert="high" reason="undefined">π</unclear>ος <unclear cert="high" reason="undefined">ε</unclear>ὐθύριν μετὰ κυρίου τοῦ ἑαυτ<unclear cert="high" reason="undefined">ῆ</unclear>ς ἀνδρὸς Π[<gap extent="8" reason="illegible" unit="character"/> τοῦ <gap extent="8" reason="illegible" unit="character"/> <unclear cert="high" reason="undefined">Πέρσου</unclear>] 

    <lb n="5"/>ὡς <expan><ex cert="low">ἐτῶν</ex></expan> <num value="30">λ</num> μέσου <expan>μελι<ex cert="high">χρόου</ex></expan> <expan>τετα<ex cert="high">νοῦ</ex></expan> <expan>μα<unclear cert="high" reason="undefined">κ</unclear>ροπρ<ex cert="high">οσώπου</ex></expan> εὐθυρίνου τὴν ὑπάρχουσαν αὐτῆι γῆν <expan><unclear cert="high" reason="undefined">ἤπ</unclear><ex cert="high">ειρον</ex></expan> [σιτοφόρον ἐν τῷ περὶ <expan>Παθῦ<ex cert="high">ριν</ex></expan> <unclear cert="high" reason="undefined">πεδίῳ</unclear>] 

    <lb n="6"/>ἣν ἠγόρασεν παρὰ Νεχθανούπιος, ἧς μέτεστι τοῖ[ς <expan>ἀ]<unclear cert="high" reason="undefined">δ</unclear>ελ<ex cert="high">φοῖς</ex></expan> ἀδιαιρέ<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ω</supplied><unclear cert="high" reason="undefined">ς</unclear>, ὡ<unclear cert="high" reason="undefined">σ</unclear>εὶ <expan><unclear cert="high" reason="undefined">ἀρ</unclear>ού<ex cert="high">ρας</ex></expan> <gap extent="11" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἧς γείτονες νότου</supplied> 

    <lb n="7"/><unclear cert="high" reason="undefined">γ</unclear>ῆ Κοτ<unclear cert="high" reason="undefined">τ</unclear>άβου, βορρᾶ γῆ Π<unclear cert="high" reason="undefined">α</unclear>οῦ<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ος τοῦ</supplied> <unclear cert="high" reason="undefined">Ὡ</unclear>ρπ<unclear cert="high" reason="undefined">α</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/>ς καὶ τῶν <supplied cert="high" reason="lost">μετόχων<foreign lang="la">?,</foreign>ἀ</supplied>π<unclear cert="high" reason="undefined">η</unclear>λιώ<unclear cert="high" reason="undefined">τ</unclear>ου γῆ τῶν [αὐτῶν <foreign lang="la">?,</foreign> λιβὸς ψιλοὶ] <unclear cert="high" reason="undefined">τόπο</unclear><supplied cert="high" reason="lost">ι τοῦ </supplied><gap extent="6" reason="lost" unit="character"/> 

    <lb n="8"/>καὶ ἀπ᾽ ἄλλης σφραγῖδος ἐν κώ<unclear cert="high" reason="undefined">μῃ</unclear> Τμόνοβιτ ἐν <expan>ἀρού<ex cert="high">ραις</ex></expan> <num value="8">η</num> <expan><ex cert="high">τὸ</ex></expan> <unclear cert="high" reason="undefined">ἐ</unclear><supplied cert="high" reason="lost">πι</supplied>βάλλον αὐτῆι ἐν τ<unclear cert="high" reason="undefined">ῶ</unclear><supplied cert="high" reason="lost">ι </supplied><supplied cert="low" reason="lost">κάτ</supplied><unclear cert="low" reason="undefined">ω</unclear> π<supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">δίωι</unclear> μέρος <expan><unclear cert="high" reason="undefined">σ</unclear>[χοιν<ex cert="high">ίων</ex></expan> ἐν]νέ<supplied cert="high" reason="lost">α</supplied> <num value="9"/> 

    <lb n="9"/>οἳ ἂν ὦσι γείτονες πάντοθεν. ἐπρίατο Ὧρος Νεχούτου Πέ<unclear cert="high" reason="undefined">ρ</unclear>σης τῆς ἐ<unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ιγο</supplied><unclear cert="high" reason="undefined">ν</unclear>ῆς ὡ<unclear cert="high" reason="undefined">ς</unclear> <expan><ex cert="high">ἐτῶν</ex></expan> <num value="40">μ</num> μέσος <supplied cert="high" reason="lost">μελί</supplied><unclear cert="high" reason="undefined">χ</unclear>ρως 

    <lb n="10"/>ὑποκλαστὸς στρογγυλοπρόσωπο<unclear cert="high" reason="undefined">ς</unclear> εὐθύριν χαλκοῦ νομίσματος ταλάν<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ου</supplied> ἑνός <num value="1"/>. <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ροπ</supplied>ωλητ<unclear cert="high" reason="undefined">ὴ</unclear>ς <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">αὶ βεβαι</supplied><unclear cert="high" reason="undefined">ώ</unclear>τ<unclear cert="high" reason="undefined">η</unclear><supplied cert="high" reason="lost">ς</supplied> 

    <lb n="11"/>τῶν κατὰ τὴν ὠνὴν ταύτ<unclear cert="high" reason="undefined">ην</unclear> πάντων Ἑτπεῆ<supplied cert="high" reason="lost">σ</supplied><unclear cert="high" reason="undefined">ι</unclear>ς ἡ ἀποδομένη, <choice><corr>ἣν</corr><sic>ον</sic></choice> ἐδ<supplied cert="high" reason="lost">έξ</supplied><unclear cert="high" reason="undefined">α</unclear>το Ὧρος ὁ πριάμεν<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ς</supplied>. 
