    <lb n="1"/>βασιλευόντων Πτολεμαί<unclear reason="undefined" cert="high">ου</unclear> τοῦ ἐπικαλουμένου Ἀλεξάνδ<supplied reason="lost" cert="high">ρο</supplied><unclear reason="undefined" cert="high">υ</unclear> καὶ Βερενί<unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">ης τῆς ἀδελφῆς θεῶν Φιλομη</supplied>-

    <lb n="2"/>τόρων ἔτους <num value="17">ιζ</num> Ἁθὺρ <num value="5">ε</num> ἐφ᾽ <choice><corr>ἱερέων</corr><sic>ι<unclear reason="undefined" cert="high">ερ</unclear>ειων</sic></choice> καὶ ἱ<unclear reason="undefined" cert="high">ε</unclear>ρειῶν κ<unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">ὶ</supplied> <unclear reason="undefined" cert="high">κα</unclear>νηφόρο<unclear reason="undefined" cert="high">υ</unclear> <supplied reason="lost" cert="high">τ</supplied><unclear reason="undefined" cert="high">ῶ</unclear>ν ὄντ<unclear reason="undefined" cert="high">ω</unclear>[ν καὶ οὐσῶν, ἐν Κροκοδίλων <unclear cert="high" reason="undefined">πόλει</unclear>] 

    <lb n="3"/>ἐπ᾽ Ἀμμωνίου τοῦ παρὰ Πανίσ<unclear reason="undefined" cert="high">κ</unclear>ου ἀγορανόμου. ἀπέδοτο <unclear reason="undefined" cert="high">Ἑ</unclear>τπ<unclear reason="undefined" cert="high">εῆ</unclear>σις Πατῆτο[ς <unclear cert="high" reason="undefined">Περσίνη</unclear> ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <gap reason="illegible" extent="2" unit="character"/> μέση <foreign lang="la">?,</foreign> <unclear cert="high" reason="undefined">μελίχρως</unclear>] 

    <lb n="4"/>τετανὸ<unclear reason="undefined" cert="high">ς</unclear> στρ<unclear reason="undefined" cert="high">ο</unclear>γγυλοπρόσω<unclear reason="undefined" cert="high">π</unclear>ος <unclear reason="undefined" cert="high">ε</unclear>ὐθύριν μετὰ κυρίου τοῦ ἑαυτ<unclear reason="undefined" cert="high">ῆ</unclear>ς ἀνδρὸς Π[<gap reason="illegible" extent="8" unit="character"/> τοῦ <gap reason="illegible" extent="8" unit="character"/> <unclear cert="high" reason="undefined">Πέρσου</unclear>] 

    <lb n="5"/>ὡς <expan><ex cert="low">ἐτῶν</ex></expan> <num value="30">λ</num> μέσου <expan>μελι<ex cert="high">χρόου</ex></expan> <expan>τετα<ex cert="high">νοῦ</ex></expan> <expan>μα<unclear reason="undefined" cert="high">κ</unclear>ροπρ<ex cert="high">οσώπου</ex></expan> εὐθυρίνου τὴν ὑπάρχουσαν αὐτῆι γῆν <expan><unclear reason="undefined" cert="high">ἤπ</unclear><ex cert="high">ειρον</ex></expan> [σιτοφόρον ἐν τῷ περὶ <expan>Παθῦ<ex cert="high">ριν</ex></expan> <unclear cert="high" reason="undefined">πεδίῳ</unclear>] 

    <lb n="6"/>ἣν ἠγόρασεν παρὰ Νεχθανούπιος, ἧς μέτεστι τοῖ[ς <expan>ἀ]<unclear reason="undefined" cert="high">δ</unclear>ελ<ex cert="high">φοῖς</ex></expan> ἀδιαιρέ<unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ω</supplied><unclear reason="undefined" cert="high">ς</unclear>, ὡ<unclear reason="undefined" cert="high">σ</unclear>εὶ <expan><unclear reason="undefined" cert="high">ἀρ</unclear>ού<ex cert="high">ρας</ex></expan> <gap reason="lost" extent="11" unit="character"/><supplied reason="lost" cert="high"> ἧς γείτονες νότου</supplied> 

    <lb n="7"/><unclear reason="undefined" cert="high">γ</unclear>ῆ Κοτ<unclear reason="undefined" cert="high">τ</unclear>άβου, βορρᾶ γῆ Π<unclear reason="undefined" cert="high">α</unclear>οῦ<unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ος τοῦ</supplied> <unclear reason="undefined" cert="high">Ὡ</unclear>ρπ<unclear reason="undefined" cert="high">α</unclear><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="2" unit="character"/>ς καὶ τῶν <supplied reason="lost" cert="high">μετόχων<foreign lang="la">?,</foreign>ἀ</supplied>π<unclear reason="undefined" cert="high">η</unclear>λιώ<unclear reason="undefined" cert="high">τ</unclear>ου γῆ τῶν [αὐτῶν <foreign lang="la">?,</foreign> λιβὸς ψιλοὶ] <unclear reason="undefined" cert="high">τόπο</unclear><supplied reason="lost" cert="high">ι τοῦ </supplied><gap reason="lost" extent="6" unit="character"/> 

    <lb n="8"/>καὶ ἀπ᾽ ἄλλης σφραγῖδος ἐν κώ<unclear reason="undefined" cert="high">μῃ</unclear> Τμόνοβιτ ἐν <expan>ἀρού<ex cert="high">ραις</ex></expan> <num value="8">η</num> <expan><ex cert="high">τὸ</ex></expan> <unclear reason="undefined" cert="high">ἐ</unclear><supplied reason="lost" cert="high">πι</supplied>βάλλον αὐτῆι ἐν τ<unclear reason="undefined" cert="high">ῶ</unclear><supplied reason="lost" cert="high">ι </supplied><supplied reason="lost" cert="low">κάτ</supplied><unclear reason="undefined" cert="low">ω</unclear> π<supplied reason="lost" cert="high">ε</supplied><unclear reason="undefined" cert="high">δίωι</unclear> μέρος <expan><unclear reason="undefined" cert="high">σ</unclear>[χοιν<ex cert="high">ίων</ex></expan> ἐν]νέ<supplied reason="lost" cert="high">α</supplied> <num value="9"/> 

