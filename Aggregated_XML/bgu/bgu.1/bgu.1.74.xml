    <lb n="1"/>Αὐτοκράτωρ Καῖσαρ Μάρκος Αὐρήλιος <app type="BL"><lem resp="9.15; cf.1.15">Ἀντ<supplied cert="high" reason="lost">ωνῖνος Σεβαστὸς Ἀρμενιακὸς Μηδικὸς Παρθικὸς Μέγιστος ἀρχιερεὺς μέγιστος δημαρχικῆς ἐξουσίας</supplied></lem><rdg>Ἀντ<supplied cert="high" reason="lost">ωνῖνος </supplied><gap extent="unknown" reason="lost" unit="character"/></rdg></app> 

    <lb n="2"/>τὸ <num value="21">κα</num>, αὐτοκράτωρ τὸ <num value="5">ε</num>, ὕπατος τὸ <num value="3">γ</num>, πατὴ<supplied cert="high" reason="lost">ρ πατρίδος καὶ Αὐτοκράτωρ Καῖσαρ Λούκιος Αὐρήλιος Οὐῆρος Σεβαστὸς Ἀρμενιακὸς Μηδικὸς Παρθικὸς</supplied> 

    <lb n="3"/>Μέγιστος δημαρχικῆς ἐξουσίας <app type="BL"><lem resp="9.15; cf.1.15">τὸ <num value="7">ζ</num>, <supplied cert="high" reason="lost">αὐτοκράτωρ τὸ <num value="5">ε</num> ὕπατος τὸ <num value="3">γ</num>, πατὴρ πατρίδος, Θεοῦ Ἀντωνίνου υἱοί, Θεοῦ Ἁδριανοῦ υἱωνοί,</supplied></lem><rdg>τὸ <num value="7">ζ</num> <gap extent="unknown" reason="lost" unit="character"/></rdg></app> 

    <lb n="4"/>Θεοῦ Τραιανοῦ Παρτικοῦ ἔγγονοι <app type="BL"><lem resp="9.15-6"><choice><corr>θεοῦ</corr><sic>θειο<supplied cert="high" reason="lost">υ</supplied></sic></choice> <supplied cert="high" reason="lost">Νέρουα ἀπόγονοι, Ἀντινοέων Νέων Ἑλλήνων τοῖς ἄρχουσι καὶ τῇ βουλῇ καὶ τῷ δήμῳ</supplied></lem><rdg>θειο<gap extent="unknown" reason="lost" unit="character"/></rdg></app> 

    <lb n="5"/>χαίρει<supplied cert="high" reason="lost">ν· ὅ</supplied>τι μὲν τὰς δωρεάς, ὧν ἔτ<supplied cert="high" reason="lost">ι</supplied> δ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/><choice><corr>ὑμῖν</corr><sic>υμειν</sic></choice> καὶ αὐτοὶ δύνεσθ᾽ ἂν τοῦτο <app type="BL"><lem resp="9.16">ε<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τὰ τοῖς πρὸ ἡμῶν</supplied></lem><rdg>ε<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/></rdg></app> <supplied cert="high" reason="lost">αὐτο</supplied>-

    <lb n="7"/>κράτορσιν δόξαντα <app type="BL"><lem resp="1.15"><unclear cert="high" reason="undefined">τι</unclear>δυ</lem><rdg>ἤδη</rdg></app> κεχρημ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>καὶ γὰρ ἂν ἄλογον εἴη ὁπόσων μὲν <app type="editorial"><lem resp="bgu 1 p.355">υ<gap extent="unknown" reason="lost" unit="character"/></lem><rdg><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/></rdg></app> 

    <lb n="9"/><choice><corr>ἀφαιρεθείητε</corr><sic>αφερετειητε</sic></choice> ὅτε ἐπὶ τῶν ἔργων α<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/>πρὸς τοὺς εὐεργέτας χάριτι ταῦτα μ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/>σαφῆ <choice><corr>δηλώσειν</corr><sic>δηλωσιν</sic></choice> <choice><corr>ἐκ</corr><sic>εγ</sic></choice> μόνων τῶν <app type="editorial"><lem resp="bgu 1 p.355">γε<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/></lem><rdg><gap extent="1" reason="lost" unit="character"/>ε<gap extent="unknown" reason="lost" unit="character"/></rdg></app> 

    <lb n="12"/>μενον εἴπερ τι <app type="editorial"><lem resp="bgu 1 p.355">ζη<unclear cert="high" reason="undefined">τ</unclear>εῖται</lem><rdg>ζητῆται</rdg></app> οὔτε παρα<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/>ἐνῆν ἀλλ᾽ ὅμως καὶ τούτων οὕτως ε<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="14"/>ταῦτα ἐπιστέλλομεν <supplied cert="high" reason="lost">ὅ</supplied>τι τὰ μὲν <app type="BL"><lem resp="9.16">δ<gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> εἴ τινι</supplied></lem><rdg>δ<gap extent="unknown" reason="lost" unit="character"/></rdg></app> 

    <lb n="15"/>συμβαίνοι <app type="BL"><lem resp="9.16">παραλι<unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">εῖ</supplied>ν <hi rend="diaeresis">ὑ</hi>μέτ<supplied cert="high" reason="lost">ερα δίκαια </supplied><gap extent="unknown" reason="lost" unit="character"/></lem><rdg>παρὰ λιτ<supplied cert="high" reason="lost">ῶ</supplied>ν <hi rend="diaeresis">ὑ</hi>μετ<supplied cert="high" reason="lost">έρων </supplied><gap extent="unknown" reason="lost" unit="character"/></rdg></app> 

