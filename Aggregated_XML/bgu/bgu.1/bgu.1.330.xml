    <lb n="1"/><expan>ἀντίγρ<ex cert="high">αφον</ex></expan> προσφωνήσεως. 

    <lb n="2"/>Σαραπίωνι τῷ καὶ <gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="1" unit="character"/>σᾷ καὶ <expan>μετόχ<ex cert="high">οις</ex></expan> 

    <lb n="3"/>πράκτορσι ἀργυρικῶν Καρανίδος 

    <lb n="4"/>παρὰ Ἀμμωνί<supplied reason="lost" cert="high">ου</supplied> <expan>κωμογρ<ex cert="high">αμματέως</ex></expan> τῆς 

    <lb n="5"/>αὐτῆς κώμης. ἐπὶ κατα<unclear reason="undefined" cert="high">νομ</unclear>ὴν 

    <lb n="6"/>ἐπανῄτησας <choice><corr>ὑμῖν</corr><sic>υμει<supplied reason="lost" cert="high">ν</supplied></sic></choice> <app type="BL"><lem resp="1.38"><gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">ε</unclear>κ <expan>ἡμιτελ<ex cert="high">είου</ex></expan></lem><rdg><gap reason="illegible" extent="1" unit="character"/><abbr><unclear reason="undefined" cert="high">ε</unclear>κημ<gap reason="illegible" extent="2" unit="character"/>ελ</abbr></rdg></app> 

    <lb n="7"/><unclear reason="undefined" cert="high">ἀρ</unclear>ιθμητικοῦ ἐκκειμένου εἰς 

    <lb n="8"/>Σαταβοῦν Φαν<gap reason="illegible" extent="4" unit="character"/>εως παρὰ τῶν 

    <lb n="9"/>ὤφειλεν πράσσεσθαι καὶ <expan>ἐδη<unclear reason="undefined" cert="high">λ</unclear><supplied reason="lost" cert="high">ώ</supplied>θ<ex cert="low">η</ex></expan> 

    <lb n="10"/>παρὰ Πτολεμαίου ἰχθυοπώλ<supplied reason="lost" cert="high">ο</supplied>υ 

    <lb n="11"/><gap reason="lost" extent="1" unit="character"/>κενδε ἐγνώσθη μοι ὀφείλει<supplied reason="lost" cert="high">ν ἀπαι</supplied>-

    <lb n="12"/>τεῖσθαι παρὰ Δείου καὶ τῶν αὐτοῦ 

    <lb n="13"/>τέκνων Πετεῶτος Ἁτρείους <supplied reason="lost" cert="low">ἀπὸ</supplied> 

    <lb n="14"/>Καρανίδος ὅ<unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ε</supplied><unclear reason="undefined" cert="high">ρ</unclear> αὐτὸ τοῦτο ὑπο<supplied reason="lost" cert="high">π</supplied>-

    <lb n="15"/>τεύω. Ἀμμώνιος <expan>κωμογρ<ex cert="high">αμματεὺς</ex></expan> <expan>ἐπ<supplied reason="lost" cert="high">ιδ<ex cert="high">έδωκα</ex></supplied></expan>. 

    <lb n="16"/><expan><ex cert="high">ἔτους</ex></expan> <num value="16">ιϛ</num> Αὐτοκράτορος Καίσαρος 

    <lb n="17"/>Τίτου Αἰλίου Ἁδριανοῦ Ἀντωνείνου 

    <lb n="18"/>Σεβαστοῦ Εὐσεβοῦς Μεχεὶρ <num value="6">ϛ</num>. 

