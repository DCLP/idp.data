    <lb n="1"/><expan>σπερμ<ex cert="high">άτων</ex></expan> <num value="22">κβ</num> <expan><ex cert="high">ἔτους</ex></expan> 

    <lb n="2"/>Ἀντωνείνου Καίσαρος 

    <lb n="3"/>τοῦ κυρίου <num value="51">να</num> <expan>κλη<ex cert="high">ρου</ex>χ<ex cert="high">ίας</ex></expan> 

    <lb n="4"/>Φᾶσις Πακύσεως 

    <lb n="5"/><expan>Ψεναρ<ex cert="high">ψενήσεως</ex></expan> <expan>βασι<ex cert="high">λικῆς</ex></expan> <expan><ex cert="high">ἀρούρας</ex></expan> <num value="12">ιβ</num>. 

