    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/>ἀνοικοδ<supplied reason="lost" cert="high">ο</supplied>μεῖν ἕως <gap reason="lost" extent="3" unit="character"/>το<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>ἀδιακωλύτως καὶ οὐκ ἐπελευ<supplied reason="lost" cert="high">σόμεθα </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>ἡμῶν <choice><corr>οὐχ</corr><sic>ουκ</sic></choice> οἱ μετ᾽ ἐμὲ τὴν <hi rend="diaeresis">Ἰ</hi>ωάν<supplied reason="lost" cert="high">ναν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>κατ᾽ οὐδένα τρόπον ἀλλὰ καὶ τὸν ἐπελ<supplied reason="lost" cert="high">ευσόμενον </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>χάριν ἀποστήσειν ἡμᾶς καὶ κληρον<supplied reason="lost" cert="high">όμους </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>καὶ τὴν νομὴν καθαροποιήσωμεν <app type="editorial"><lem resp="bgu 3 p.2"><supplied reason="lost" cert="high">ἢ ἐκτίσειν σοι τὴν προγε</supplied>-

    <lb n="7"/>γραμμένην</lem><rdg><gap reason="lost" extent="unknown" unit="character"/>γραμμένην</rdg></app> τιμὴν διπλῆν καὶ ποι<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>μετὰ τὴν τούτων καταβολὴν πάλιν <hi rend="diaeresis">ἰ</hi>σχυρ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>καὶ ὑπεθέμεθά σοι ἐπ᾽ ἀσφαλείᾳ αὐτῆ<supplied reason="lost" cert="high">ς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>ἐν <expan>ἐνεχύρο<ex cert="high">υ</ex></expan> λόγῳ καὶ ὑποθήκης δικαίῳ καθ<supplied reason="lost" cert="high">άπερ ἐκ δίκης </supplied><gap reason="lost" extent="unknown" unit="character"/> 

