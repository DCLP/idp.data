    <lb n="1"/><gap extent="unknown" reason="lost" unit="line"/> 

    <lb n="1"/>ἀνοικοδ<supplied cert="high" reason="lost">ο</supplied>μεῖν ἕως <gap extent="3" reason="lost" unit="character"/>το<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>ἀδιακωλύτως καὶ οὐκ ἐπελευ<supplied cert="high" reason="lost">σόμεθα </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>ἡμῶν <choice><corr>οὐχ</corr><sic>ουκ</sic></choice> οἱ μετ᾽ ἐμὲ τὴν <hi rend="diaeresis">Ἰ</hi>ωάν<supplied cert="high" reason="lost">ναν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>κατ᾽ οὐδένα τρόπον ἀλλὰ καὶ τὸν ἐπελ<supplied cert="high" reason="lost">ευσόμενον </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>χάριν ἀποστήσειν ἡμᾶς καὶ κληρον<supplied cert="high" reason="lost">όμους </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/>καὶ τὴν νομὴν καθαροποιήσωμεν <app type="editorial"><lem resp="bgu 3 p.2"><supplied cert="high" reason="lost">ἢ ἐκτίσειν σοι τὴν προγε</supplied>-

    <lb n="7"/>γραμμένην</lem><rdg><gap extent="unknown" reason="lost" unit="character"/>γραμμένην</rdg></app> τιμὴν διπλῆν καὶ ποι<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>μετὰ τὴν τούτων καταβολὴν πάλιν <hi rend="diaeresis">ἰ</hi>σχυρ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>καὶ ὑπεθέμεθά σοι ἐπ᾽ ἀσφαλείᾳ αὐτῆ<supplied cert="high" reason="lost">ς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/>ἐν <expan>ἐνεχύρο<ex cert="high">υ</ex></expan> λόγῳ καὶ ὑποθήκης δικαίῳ καθ<supplied cert="high" reason="lost">άπερ ἐκ δίκης </supplied><gap extent="unknown" reason="lost" unit="character"/> 

