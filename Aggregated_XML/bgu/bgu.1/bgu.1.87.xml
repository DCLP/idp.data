    <lb n="30"/>πεντακοσίας <num value="500"/> καὶ βεβαιώσω πά-

    <lb n="31"/><supplied cert="high" reason="lost">σῃ </supplied><choice><corr><supplied cert="high" reason="lost">βε</supplied>βαιώσει</corr><sic><supplied cert="high" reason="lost">βε</supplied>βαιωσι</sic></choice> καθὼς πρόκιτ<supplied cert="high" reason="lost">αι</supplied> 

    <lb n="32"/><gap extent="3" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/> ἔγραψα ὑπὲρ αὐτῆς 

    <lb n="33"/><handShift new="m4"/> <expan>ἐντέτακ<ex cert="high">ται</ex></expan> διὰ <app type="editorial"><lem resp="bgu 1 p.356"><expan>γρ<ex cert="high">αφείου</ex></expan></lem><rdg><expan>γρ<ex cert="high">αμματέωσ</ex></expan></rdg></app> <expan>Σοκνοπ<ex cert="high">αίου Νήσου</ex></expan>. 
