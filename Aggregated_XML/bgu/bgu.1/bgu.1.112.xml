    <lb n="1"/>ἀ<supplied reason="lost" cert="high">ντ</supplied>ί<supplied reason="lost" cert="high">γ</supplied>ραφον <supplied reason="lost" cert="high">ἀπ</supplied>ογρα<supplied reason="lost" cert="high">φ</supplied>ῆς. 

    <lb n="2"/>Ἀμμωνίῳ <supplied reason="lost" cert="high">καὶ </supplied><app type="BL"><lem resp="1.21"><supplied reason="lost" cert="high">Σαραπ</supplied><unclear reason="undefined" cert="high">ί</unclear>ωνι</lem><rdg><gap reason="lost" extent="4" unit="character"/><unclear reason="undefined" cert="high">ί</unclear>ωι</rdg></app> <app type="editorial"><lem resp="bgu 3 p.1"><expan>γ<supplied reason="lost" cert="high">υμ</supplied>να<supplied reason="lost" cert="high">σιαρ</supplied>χ<ex cert="high">ήσασι</ex></expan></lem><rdg><expan>γ<supplied reason="lost" cert="high">υμ</supplied>να<supplied reason="lost" cert="high">σιαρ</supplied>χ<ex cert="high">ήσαντι</ex></expan></rdg></app> 

    <lb n="3"/>βιβλιοφύλαξι <supplied reason="lost" cert="high">τῆς ἐ</supplied>ν <expan>Ἀρσινοει[τ<ex cert="high">ῶν</ex></expan> <expan>πόλ<ex cert="high">ει</ex></expan>] 

    <lb n="4"/>δημοσίας βιβ<supplied reason="lost" cert="high">λιοθήκ</supplied>ης 

    <lb n="5"/>παρὰ <app type="editorial"><lem resp="bgu 2 p.354">Πασόξεως το<supplied reason="lost" cert="high">ῦ</supplied></lem><rdg>Πα<gap reason="illegible" extent="2" unit="character"/>ξε<unclear reason="undefined" cert="high">νει</unclear><gap reason="lost" extent="3" unit="character"/></rdg></app> Παήσιος τοῦ <expan>Μυὸ<ex cert="high">ς</ex></expan> 

    <lb n="6"/>ἱερέως τῶν ἀ<supplied reason="lost" cert="high">πὸ</supplied> Καρανίδος τῆς <expan>Ἡρ<ex cert="high">ακλείδου</ex></expan> 

    <lb n="7"/>μερίδος. κατὰ τὰ ὑπὸ τοῦ κρατίστου 

    <lb n="8"/>ἡγεμόνος Λευκίου Ἰουλίου Οὐηστείνου 

    <lb n="9"/>προστεταγμένα ἀπογράφομαι εἰς 

    <lb n="10"/>τὴν ἐνεστῶσαν ἡμέραν τὰ ὑπάρχοντά 

    <lb n="11"/>μοι ὄντα καθαρὰ ἀπό τε <choice><corr>ὀφείλης</corr><sic>οφιλης</sic></choice> καὶ 

    <lb n="12"/>ὑ<supplied reason="lost" cert="high">π</supplied>οθήκης καὶ παντὸς διεγγυήματος 

    <lb n="13"/>ἐν τῇ προκειμένηι κώμηι πατρικὸν 

    <lb n="14"/>τρίτον μέρος οἰκίας καὶ αὐλῆς καὶ <choice><corr>ψιλοὺς</corr><sic>ψειλους</sic></choice> 

    <lb n="15"/>τόπους βίκων δύο ἡμίσους <num value="2  1/2 "/>, οὓς ἠγόρα-

    <lb n="16"/>σα παρὰ Μεσοήριος τοῦ Νεκφερῶ<unclear reason="undefined" cert="high">τ</unclear>ος 

    <lb n="17"/>τῶι <num value="5">ε</num> <expan><ex cert="high">ἔτει</ex></expan> Νέρωνος Κλαυδίου Καίσαρος 

    <lb n="18"/>Σεβαστοῦ Γερμανικοῦ Αὐτοκράτορος 

    <lb n="19"/>καὶ ἐν τῇ κώμηι οἰκίαν, ἣν ἠγόρασα 

    <lb n="20"/>παρὰ Ὀννώφ<supplied reason="lost" cert="high">ρ</supplied>ιος τοῦ Πετεοραίπιος 

    <lb n="21"/>τῶι <num value="6">ϛ</num> <expan><ex cert="high">ἔτει</ex></expan> Νέρωνος Κλαυδίου Καίσαρος 

    <lb n="22"/>Σεβαστοῦ Γερμανικοῦ Αὐτοκράτορος 

    <lb n="23"/>ὅτι δ᾽ ἂν ἀπὸ το<supplied reason="lost" cert="high">ύτ</supplied>ων ἐξοικονομήσω 

    <lb n="24"/>ἢ καὶ προσαγο<supplied reason="lost" cert="high">ρ</supplied>άσωι, πρότερ<unclear reason="undefined" cert="high">ο</unclear>ν <g type="filler" rend="extension"/> 

    <lb n="25"/>προσαγγελῶ<unclear reason="undefined" cert="high">ν</unclear>, ὡς <choice><corr>ἐκελεύσθη</corr><sic>εκελευσθηι</sic></choice>. 
