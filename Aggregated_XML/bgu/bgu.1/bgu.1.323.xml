    <lb n="1"/><gap extent="unknown" reason="lost" unit="line"/> 

    <lb n="1"/><gap extent="8" reason="lost" unit="character"/><supplied cert="high" reason="lost"> δεκά</supplied>της <expan>ἰν<ex cert="high">δικτίωνος</ex></expan> ἐπ᾽ Ἀρ<supplied cert="high" reason="lost">σινόης <g type="stauros"/></supplied> 

    <lb n="2"/><expan>Φλ<ex cert="high">αουίῳ</ex></expan> <app type="BL"><lem resp="8.23"><supplied cert="high" reason="lost">Σεν</supplied>ουθίῳ</lem><rdg><supplied cert="high" reason="lost">Παπν</supplied>ουθίῳ</rdg></app> τῷ εὐκλεεστάτῳ δοῦκ<supplied cert="high" reason="lost">ι </supplied><gap extent="9" reason="lost" unit="character"/> 

    <lb n="3"/>τοῦ ἐνδοξοτάτου ἰλλουστρίου καὶ <unclear cert="high" reason="undefined">κτ</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="8" reason="lost" unit="character"/> 


    <lb n="4"/>τῆς Ἀρσινοιτῶν πόλεως καὶ Ἰούστου τοῦ <expan>μεγαλο<ex cert="high">πρεπεστάτου</ex></expan> 

    <lb n="5"/>διοικητοῦ οὐσίας Θεοδοσίου τοῦ τῆς ἐνδόξου μνήμης 

    <lb n="6"/>Αὐρήλιος Μηνᾶς μείζων υἱὸς <expan>Γεωργίο<ex cert="high">υ</ex></expan> 

    <lb n="7"/>ἀπὸ <expan>ἐποικίο<ex cert="high">υ</ex></expan> Κο<gap extent="4" reason="illegible" unit="character"/> τοῦ Ἀρσινοίτου νομοῦ <expan>χ<ex cert="high">αίρειν</ex></expan>. ὁμολογ<add place="supralinear">ῶ</add> 

    <lb n="8"/>ἑκουσίᾳ γνώμῃ ἐπομνύμενος θεὸν παντοκράτορ<add place="supralinear">α</add> 

    <lb n="9"/>ἑτοίμως ἔχειν ἕως εἰκάδος τοῦ παρόντος μηνὸς 

    <lb n="10"/>Πα<hi rend="diaeresis">ῦ</hi>νι ἀρχῇ τῆς παρούσης δεκάτης <expan>ἰν<ex cert="high">δικτίωνος</ex></expan> <gap extent="3" reason="lost" unit="character"/><supplied cert="high" reason="lost"> </supplied> 

    <lb n="11"/><supplied cert="high" reason="lost">εἰ</supplied>ς κατοχὴν ποιήσω πάντα τὰ ὄντ<supplied cert="high" reason="lost">α ἐν τῷ</supplied> 

    <lb n="12"/>μου χωρίῳ ξένα πρόσωπα καὶ ταῦτα ἐπι<supplied cert="high" reason="lost">ζητήσω</supplied> 

    <lb n="13"/>καὶ παραδώσω τῇ ὑμετέρᾳ εὐκλε<supplied cert="high" reason="lost">ΐᾳ. εἰ δ᾽ ἐπὶ τῆς</supplied> 

    <lb n="14"/>προθεσμίας μὴ τοῦτο πράξω, ἐνέ<supplied cert="high" reason="lost">χεσθαί με</supplied> 

    <lb n="15"/>αὐτῇ ὑπὲρ ἑκάστου προσώπου <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">αρασχεῖν</supplied> 

    <lb n="16"/>χρυσοῦ λίτρα μίαν <num value="1"/> <unclear cert="high" reason="undefined">μειτο</unclear> καὶ ὑποκ<supplied cert="high" reason="lost">εῖσθαι τῇ</supplied> 

    <lb n="17"/>κεφαλητικῇ τιμωρίᾳ <gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ω</unclear><gap extent="11" reason="lost" unit="character"/> 

    <lb n="18"/>στοιχεῖ μοι πάντα <gap extent="1" reason="illegible" unit="character"/><gap extent="14" reason="lost" unit="character"/> 

