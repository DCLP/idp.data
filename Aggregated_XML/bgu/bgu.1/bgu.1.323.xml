    <lb n="7"/>ἀπὸ <expan>ἐποικίο<ex cert="high">υ</ex></expan> Κο<gap reason="illegible" extent="4" unit="character"/> τοῦ Ἀρσινοίτου νομοῦ <expan>χ<ex cert="high">αίρειν</ex></expan>. ὁμολογ<add place="supralinear">ῶ</add> 

