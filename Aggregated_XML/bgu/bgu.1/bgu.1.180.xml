    <lb n="1"/>παρὰ <supplied reason="lost" cert="high">Γα</supplied>ίου Ἰ<supplied reason="lost" cert="high">ου</supplied>λ<supplied reason="lost" cert="high">ίου Ἀπολ</supplied>ινα<supplied reason="lost" cert="high">ρίο</supplied>υ οὐ<supplied reason="lost" cert="high">ε</supplied>-

    <lb n="2"/>τρανοῦ γε<supplied reason="lost" cert="high">ο</supplied>υχ<supplied reason="lost" cert="high">ο</supplied>ῦ<supplied reason="lost" cert="high">ντος ἐν</supplied> κώμῃ Κα<supplied reason="lost" cert="high">ρα</supplied>-

    <lb n="3"/>νίδι. <supplied reason="lost" cert="high">δ</supplied>ιατέτακ<supplied reason="lost" cert="high">ται, κ</supplied>ύριε, <choice><corr>τοὺ<supplied reason="omitted" cert="high">ς</supplied></corr><sic>του</sic></choice> οὐετρα-

    <lb n="4"/>νοὺς ἔχειν μετὰ τ<supplied reason="lost" cert="high">ὴν ἀπό</supplied>λυσιν πέντ<supplied reason="lost" cert="high">α</supplied>-

    <lb n="5"/>ετῆ χρό<supplied reason="lost" cert="high">ν</supplied>ον ἀνα<supplied reason="lost" cert="high">παύσε</supplied>ως. παρὰ δὲ ταύ-

    <lb n="6"/>την τὴν <supplied reason="lost" cert="high">δι</supplied>άτ<supplied reason="lost" cert="high">α</supplied>ξιν ἐ<supplied reason="lost" cert="high">γὼ</supplied> ἐπηρεάσθην 

    <lb n="7"/>μ<supplied reason="lost" cert="high">ε</supplied>τὰ διετίαν τῆς <supplied reason="lost" cert="high">ἀπο</supplied>λύσεως κα<supplied reason="lost" cert="high">ὶ</supplied> 

    <lb n="8"/>ἀ<supplied reason="lost" cert="high">ν</supplied>εδόθην κατ᾽ <app type="BL"><lem resp="cf. 1.24">ἐπή<supplied reason="lost" cert="high">ρειαν</supplied></lem><rdg>ἐ<unclear reason="undefined" cert="high">τή</unclear><supplied reason="lost" cert="high">σιο</supplied>ν</rdg></app> εἰς λειτουργίαν 

    <lb n="9"/>καὶ μέχρι τοῦ <app type="BL"><lem resp="cf. 1.24">δεῦρο <supplied reason="lost" cert="high">κ</supplied>ατὰ τὸ</lem><rdg><choice><corr>δευρὶ</corr><sic>δευρε<supplied reason="lost" cert="high">ι</supplied></sic></choice> <supplied reason="lost" cert="high">κα</supplied><unclear reason="undefined" cert="high">τ</unclear>᾽ <unclear reason="undefined" cert="high">ἔ</unclear>τ<unclear reason="undefined" cert="high">ο</unclear>σ</rdg></app> ἑξῆ<supplied reason="lost" cert="high">ς</supplied> 

    <lb n="10"/>ἐν λειτουργίᾳ εἰμ<supplied reason="lost" cert="high">ὶ</supplied> ἀδιαλεί<supplied reason="lost" cert="high">πτ</supplied>ως. 

    <lb n="11"/>τοῦ τοιούτου <app type="BL"><lem resp="3.9">πάντ<supplied reason="lost" cert="high">ῇ</supplied></lem><rdg>παν<supplied reason="lost" cert="high">τὶ</supplied></rdg></app> ἀπηγορευ-

