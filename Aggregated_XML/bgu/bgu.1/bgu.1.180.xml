    <lb n="1"/><gap extent="unknown" reason="lost" unit="line"/> 

    <lb n="1"/>παρὰ <supplied cert="high" reason="lost">Γα</supplied>ίου Ἰ<supplied cert="high" reason="lost">ου</supplied>λ<supplied cert="high" reason="lost">ίου Ἀπολ</supplied>ινα<supplied cert="high" reason="lost">ρίο</supplied>υ οὐ<supplied cert="high" reason="lost">ε</supplied>-

    <lb n="2"/>τρανοῦ γε<supplied cert="high" reason="lost">ο</supplied>υχ<supplied cert="high" reason="lost">ο</supplied>ῦ<supplied cert="high" reason="lost">ντος ἐν</supplied> κώμῃ Κα<supplied cert="high" reason="lost">ρα</supplied>-

    <lb n="3"/>νίδι. <supplied cert="high" reason="lost">δ</supplied>ιατέτακ<supplied cert="high" reason="lost">ται, κ</supplied>ύριε, <choice><corr>τοὺ<supplied cert="high" reason="omitted">ς</supplied></corr><sic>του</sic></choice> οὐετρα-

    <lb n="4"/>νοὺς ἔχειν μετὰ τ<supplied cert="high" reason="lost">ὴν ἀπό</supplied>λυσιν πέντ<supplied cert="high" reason="lost">α</supplied>-

    <lb n="5"/>ετῆ χρό<supplied cert="high" reason="lost">ν</supplied>ον ἀνα<supplied cert="high" reason="lost">παύσε</supplied>ως. παρὰ δὲ ταύ-

    <lb n="6"/>την τὴν <supplied cert="high" reason="lost">δι</supplied>άτ<supplied cert="high" reason="lost">α</supplied>ξιν ἐ<supplied cert="high" reason="lost">γὼ</supplied> ἐπηρεάσθην 

    <lb n="7"/>μ<supplied cert="high" reason="lost">ε</supplied>τὰ διετίαν τῆς <supplied cert="high" reason="lost">ἀπο</supplied>λύσεως κα<supplied cert="high" reason="lost">ὶ</supplied> 

    <lb n="8"/>ἀ<supplied cert="high" reason="lost">ν</supplied>εδόθην κατ᾽ <app type="BL"><lem resp="cf. 1.24">ἐπή<supplied cert="high" reason="lost">ρειαν</supplied></lem><rdg>ἐ<unclear cert="high" reason="undefined">τή</unclear><supplied cert="high" reason="lost">σιο</supplied>ν</rdg></app> εἰς λειτουργίαν 

    <lb n="9"/>καὶ μέχρι τοῦ <app type="BL"><lem resp="cf. 1.24">δεῦρο <supplied cert="high" reason="lost">κ</supplied>ατὰ τὸ</lem><rdg><choice><corr>δευρὶ</corr><sic>δευρε<supplied cert="high" reason="lost">ι</supplied></sic></choice> <supplied cert="high" reason="lost">κα</supplied><unclear cert="high" reason="undefined">τ</unclear>᾽ <unclear cert="high" reason="undefined">ἔ</unclear>τ<unclear cert="high" reason="undefined">ο</unclear>σ</rdg></app> ἑξῆ<supplied cert="high" reason="lost">ς</supplied> 

    <lb n="10"/>ἐν λειτουργίᾳ εἰμ<supplied cert="high" reason="lost">ὶ</supplied> ἀδιαλεί<supplied cert="high" reason="lost">πτ</supplied>ως. 

    <lb n="11"/>τοῦ τοιούτου <app type="BL"><lem resp="3.9">πάντ<supplied cert="high" reason="lost">ῇ</supplied></lem><rdg>παν<supplied cert="high" reason="lost">τὶ</supplied></rdg></app> ἀπηγορευ-

    <lb n="12"/>μένου <supplied cert="high" reason="lost">ἐ</supplied>πὶ τῶν ἐ<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">χ</supplied>ωρίων πολλῷ 

    <lb n="13"/>πλεῖον ἐπ᾽ ἐμοῦ συντηρεῖσθαι 

    <lb n="14"/><choice><corr>ὀφείλει</corr><sic>οφειλι</sic></choice> τοῦ ὑπηρετήσαντος τὸν 

    <lb n="15"/>τοσοῦ<supplied cert="high" reason="lost">το</supplied>ν τῆς στρατείας χρόνον. 

    <lb n="16"/>διόπερ προσφεύγειν σοι <choice><corr>ἠναγκάσ-

    <lb n="17"/>θην</corr><sic>ηναγκησθην</sic></choice> δικαίαν δέ<supplied cert="high" reason="lost">ησ</supplied>ιν ποιούμενος 

    <lb n="18"/>καὶ ἀξιῶ συντηρῆσαί μοι τὸν τῆς 

    <lb n="19"/>ἀναπαύσεως ἴσον χρόνον κατὰ <app type="editorial"><lem><supplied cert="high" reason="omitted">τὰ</supplied></lem><rdg/></app> 

    <lb n="20"/>περὶ τούτου διατεταγμένα, ἵνα δυνηθῶ 

    <lb n="21"/><choice><corr>καὶ ἐγὼ</corr><sic>κααγω</sic></choice> <supplied cert="high" reason="lost">τὴ</supplied>ν ἐπιμέλειαν τῶν ἰδίων 

    <lb n="22"/>ποιεῖσθαι, ἄ<supplied cert="high" reason="lost">ν</supplied>θρ<supplied cert="high" reason="lost">ω</supplied>πος <app type="BL"><lem resp="cf. 1.24">πρεσβύ<supplied cert="high" reason="lost">τη</supplied>ς καὶ</lem><rdg>πρεσβύτεροσ</rdg></app> 

    <lb n="23"/>μόνος τυγχ<supplied cert="high" reason="lost">άν</supplied>ων, <app type="BL"><lem resp="cf. 1.24"><supplied cert="high" reason="lost">κ</supplied>αὶ</lem><rdg><supplied cert="high" reason="lost">ε</supplied>ἰ</rdg></app> τῇ τύχῃ σου 

    <lb n="24"/>εἰς ἀεὶ <app type="editorial"><lem>εὐχαριστῶ</lem><rdg>σὺ<supplied cert="high" reason="lost">ν</supplied> ἀρίστῳ</rdg></app>.  διευτύχει 

    <lb n="25"/><handShift new="m2"/> Γάιος Ἰούλιος Ἀπολινάριος <choice><corr>ἐπιδέδωκ-

    <lb n="26"/>α</corr><sic>επιδεδωκκα</sic></choice>. 

    <lb n="27"/><handShift new="m3"/> <expan><ex cert="high">ἔτους</ex></expan> <num value="12">ιβ</num> Μεχεὶρ <num value="29">κθ</num> 

    <lb n="28"/><handShift new="m4"/> τῷ <unclear cert="low" reason="undefined">σ</unclear><supplied cert="low" reason="lost">τρατη</supplied>γ<supplied cert="low" reason="lost">ῷ</supplied> ἔντυ<unclear cert="high" reason="undefined">χ</unclear><supplied cert="high" reason="lost">ε</supplied> καὶ τὰ 

    <lb n="29"/>πρ<supplied cert="high" reason="lost">οσήκο</supplied>ντα ποιήσει. 

    <lb n="30"/><g type="unintelligible"/> <handShift new="m5"/> <app type="BL"><lem resp="cf. 1.24">ἀ<supplied cert="high" reason="lost">πόδος</supplied></lem><rdg>α<gap extent="5" reason="lost" unit="character"/></rdg></app>. </ab></div><div n="meta.28231" org="uniform" part="N" sample="complete" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.28231" org="uniform" part="N" sample="complete" subtype="textDate" type="commentary"><p><date notAfter="172" notBefore="172" type="textDate" value="0172-02-24">24. Febr. 172 (Jahr unsicher)</date></p><p><date notAfter="204" notBefore="204" type="textDate" value="0204-02-24">24. Febr. 204 (Jahr unsicher)</date></p></div><div n="meta.28231" org="uniform" part="N" sample="complete" subtype="general" type="commentary"><p>Alternativdatierung: 24. Febr. 204</p></div><div n="meta.28231" org="uniform" part="N" sample="complete" subtype="principalEdition" type="bibliography"><listBibl default="NO"><bibl default="NO" subtype="principal" type="publication"><title level="s" type="abbreviated">BGU</title><biblScope type="volume">1</biblScope><biblScope type="numbers">180</biblScope></bibl><bibl default="NO" type="Trismegistos"><biblScope type="numbers">28231</biblScope></bibl><bibl default="NO" type="DDbDP"><series>0001</series>; <biblScope type="volume">1</biblScope>; <biblScope type="numbers">180</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0001;1;180" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.28231" org="uniform" part="N" sample="complete" subtype="corrections" type="bibliography"><head>BL-Einträge nach BL-Konkordanz</head><bibl default="NO" type="BL"><biblScope type="volume">IX</biblScope>, S. <biblScope type="pages">17</biblScope></bibl></div><div n="meta.28231" org="uniform" part="N" sample="complete" subtype="illustrations" type="bibliography"><p>keine</p></div><div n="meta.28231" org="uniform" part="N" samp