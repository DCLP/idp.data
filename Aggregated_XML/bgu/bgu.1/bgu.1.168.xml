    <lb n="1"/><expan><supplied reason="lost" cert="high">ἀν</supplied>τίγρ<ex cert="high">αφον</ex></expan> <supplied reason="lost" cert="high">ἀν</supplied>αφορίο<supplied reason="lost" cert="high">υ.  </supplied><app type="BL"><lem resp="5.10"><supplied reason="lost" cert="high">Ἀκ</supplied>υλιῳ</lem><rdg><supplied reason="lost" cert="high">Ἰο</supplied>υλίῳ</rdg></app> Καπιτω<supplied reason="lost" cert="high">λεί</supplied>νῳ τῷ <expan>κρα<ex cert="high">τίστῳ</ex></expan> <expan><supplied reason="lost" cert="high">ἐπ</supplied>ιστρα<ex cert="high">τήγῳ</ex></expan> 

    <lb n="2"/>παρὰ Ἰου<supplied reason="lost" cert="high">λίο</supplied>υ Ἀπολι<supplied reason="lost" cert="high">να</supplied>ρίου οὐετρανοῦ προδίκου Ἀπολιναρίου καὶ 

