    <lb n="1"/><supplied cert="high" reason="lost">ἔτους τρίτου Αὐτοκράτορος</supplied> 

    <lb n="2"/>Κ<supplied cert="high" reason="lost">αίσαρος</supplied> Μάρκο<supplied cert="high" reason="lost">υ Αὐρήλιου Ἀντωνίνου</supplied> 

