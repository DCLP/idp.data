    <lb n="1"/>περὶ τοῦ μέλιτος καὶ οὐ-

    <lb n="2"/>κ ἔλαβον καὶ ἐ<unclear cert="high" reason="undefined">πέσ</unclear>εισα αὐτο<unclear cert="high" reason="undefined">ύ</unclear><supplied cert="high" reason="lost">ς</supplied>,

    <lb n="3"/><expan>Σωτήριχ<hi rend="superscript">ο</hi><ex cert="high">ν</ex></expan> καὶ Ἰσχυρᾶν.

    <lb n="4"/>ἐμοῦ δ᾽ ἐθέλοντος καταν-

    <lb n="5"/>τῆσαι πρός σε πρὸς τὸ

    <lb n="6"/>ἐπισκέψασθαί σε μετέ-

    <lb n="7"/>λαβον παρὰ τῶν περὶ

    <lb n="8"/><unclear cert="high" reason="undefined">Φ</unclear>ιλέρωτα <unclear cert="high" reason="undefined">ὅτι</unclear><gap extent="2" reason="illegible" unit="character"/> ὡς

    <lb n="9"/><unclear cert="high" reason="undefined">ἔ</unclear>σχηκ<unclear cert="high" reason="undefined">α</unclear>. ὑγίαινε.
<milestone rend="paragraphos" unit="undefined"/>
    <lb n="10"/><unclear cert="high" reason="undefined">Σωσ</unclear>ιβίου

    <lb n="12"/><choice><corr>γίγνωσκε τὸ πλοῖον</corr><sic>γινωσκε το πλοι<supplied cert="high" reason="omitted">ο</supplied>ν</sic></choice> τῇ <num value="25">κε</num> <choice><corr>ὤφειλε

    <lb n="13"/>παραγενέσθαι</corr><sic>οφιλε παραγενομενα</sic></choice>· καὶ ἠνδρα-

    <lb n="14"/><unclear cert="high" reason="undefined">γ</unclear>αθήσαμεν μέχρι <unclear cert="high" reason="undefined">ο</unclear>ὗ <choice><corr>ἐκβάλεται</corr><sic>εγβαλεται</sic></choice>

    <lb n="15"/>τὰ ὑπογεγραμμένα·

    <lb n="16"/>οἴνου <expan>κερ<hi rend="superscript">ά</hi><ex cert="high">μια</ex></expan> <num value="15">ιε</num>, ὄξου
<num value="2">β</num>, <expan>τρυγί<hi rend="superscript">α</hi><ex cert="high">ς</ex></expan> <num value="2">β</num>,

    <lb n="17"/><expan>ἐλαί<ex cert="high">ου</ex></expan> <expan>μετρ<hi rend="superscript">η</hi><ex cert="high">ταὶ</ex></expan>
<num value="8">η</num>, ξενικο<supplied cert="high" reason="omitted">ῦ</supplied> <expan>ἐσφρ<hi rend="superscript">α</hi><ex cert="high">γισμένη</ex></expan>
<num value="1">α</num>.

    <lb n="18"/><expan>φασήλο[<ex cert="high">υ</ex></expan>] καὶ
<expan>ἐρέγμ<hi rend="superscript">α</hi><ex cert="high">τος</ex></expan> <abbr>κοιεκ<gap extent="1" reason="illegible" unit="character"/></abbr> <num value="10">ι</num>

    <lb n="19"/>ὧν κ<supplied cert="high" reason="omitted">ε</supplied>ῖται εἰς <unclear cert="high" reason="undefined">τὸ</unclear>ν
<expan><unclear cert="high" reason="undefined">ὅ</unclear>ρ<unclear cert="high" reason="undefined">μ</unclear><ex cert="high">ον</ex></expan> <abbr>κο<hi rend="superscript">ι</hi></abbr> <num value="4">δ</num>

    <lb n="20"/><gap extent="3" reason="illegible" unit="character"/>ας <abbr>Ἀρσινοιτι<hi rend="superscript"><unclear cert="high" reason="undefined">κ</unclear></hi></abbr> <num value="4">δ</num> <add place="supralinear"><gap extent="2" reason="illegible" unit="character"/><num value="6">ϛ</num></add> <num value="3">γ</num>

    <lb n="21"/>χοῖροι <num value="3">γ</num>, κ<gap extent="2" reason="illegible" unit="character"/>αρ<gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><abbr>ιν<hi rend="superscript">α</hi></abbr> <num value="66">ξϛ</num>·

    <lb n="22"/>τό<supplied cert="high" reason="omitted">ν</supplied> δὲ τόμιον οὐκ ἔλαβον·

    <lb n="23"/>ἐστέγασται εἰς τὴν <expan>σκην<hi rend="superscript">ὴ</hi><ex cert="high">ν</ex></expan>
<expan>τ<ex cert="high">οῦ</ex></expan> <expan>π<hi rend="superscript">λ</hi><ex cert="high">οίου</ex></expan>

    <lb n="24"/>ἰτέινα <expan>ξύ<hi rend="superscript">λ</hi><ex cert="high">α</ex></expan> <num value="4">δ</num>· γράφεις δέ
<unclear cert="high" reason="undefined">μ</unclear><add place="supralinear">οι</add>

    <lb n="25"/>παραλαβεῖν <num value="5">ε</num> ὥστ̓ ἔλαβον

    <lb n="26"/>καὶ ἐλάινα πεπρασμένα <num value="4">δ</num>,

    <lb n="27"/>ἄλλα ἐλάινα, ἀντὶ ὧν μοι

    <lb n="28"/>γράφεις ἕνδεκα <num value="11"/>, δέκα <num value="10"/>·

    <lb n="29"/>παρέλαβον <expan>ὄξο<ex cert="high">υ</ex></expan>
<expan>κερ<hi rend="superscript">ά</hi><ex cert="high">μια</ex></expan> <num value="12">ιβ</num>,

    <lb n="30"/>τὰ κ<supplied cert="high" reason="omitted">ε</supplied>ίμεν<add place="supralinear">α</add> ἐν τῷ ὅρμῳ
δίχηλ<add place="supralinear">α</add>

    <lb n="31"/><num value="2">β</num> <expan>ὁμ<hi rend="superscript">οί</hi><ex cert="high">ως</ex></expan> ἐν τῷ
ὅρμῳ καὶ <choice><corr>τοῦ

    <lb n="32"/>κορμοῦ</corr><sic>της <expan>κορμο<ex cert="high">υ</ex></expan></sic></choice>· τὸ δὲ πιττάκιον

    <lb n="33"/>τοῦ μέλιτος <expan>συνεσφραγισμ<hi rend="superscript">έ</hi><ex cert="high">νον</ex></expan>·

    <lb n="34"/>πολλὰ γὰρ παρήδρευσα·

    <lb n="35"/>καὶ σεα<supplied cert="high" reason="omitted">υ</supplied>τοῦ <expan>ἐπιμέλο<ex cert="high">υ</ex></expan>
ἵν᾽ <expan>ὑγιαίν<hi rend="superscript">ῃ</hi><ex cert="high">ς</ex></expan> <supplied cert="high" reason="lost">ὃ</supplied>

