<expan>σε<add place="supralinear">α</add><supplied cert="high" reason="omitted">υ</supplied><ex cert="high">τοῦ</ex></expan> ἵν᾽
<expan>ὑγι<ex cert="high">αίνῃς</ex></expan>·
<expan>ἔρ<supplied cert="high" reason="omitted">ρ</supplied><hi rend="superscript">ω</hi><ex cert="high">σο</ex></expan>

