    <lb n="24"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> συ</supplied>γγραφ<unclear cert="high" reason="undefined">ὴ</unclear>ν παραβ<gap extent="unknown" reason="lost" unit="character"/> 

