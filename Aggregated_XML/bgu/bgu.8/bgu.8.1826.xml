    <lb n="49"/>τῶν ἀπῃτημένων ποιή<unclear reason="undefined" cert="high">σ</unclear><supplied reason="lost" cert="high">ῃ </supplied><gap reason="lost" extent="unknown" unit="character"/> </ab></div></div><div n="meta.4905" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.4905" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" notBefore="-0051-02-15">nach 15. Febr. 51 v.Chr.</date></p></div><div n="meta.4905" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">BGU</title><biblScope type="volume">8</biblScope><biblScope type="numbers">1826</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">4905</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0001</series>; <biblScope type="volume">8</biblScope>; <biblScope type="numbers">1826</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0001;8;1826" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.4905" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p>keine</p></div><div n="meta.4905" type="history" subtype="locations" org="uniform" part="N" sample="complete"><p><geogName type="nome" cert="high" full="yes">Herakleopolites</geogName>, <geogName type="ancientRegion" key="Aegyptus" cert="high" full="yes">Egypt</geogName></p></div></body></text></TEI.2>