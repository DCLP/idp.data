    <lb n="1"/><handShift new="m6"/> <unclear cert="high" reason="undefined">Ἔρβ</unclear>ει <expan>ἀντιγρ<ex cert="high">αφεῖ</ex></expan> <expan>θη<ex cert="high">σαυροῦ</ex></expan> Περὶ Πόλιν. 

    <lb n="2"/>συμπροοῦ κατὰ τὸν παρὰ 

    <lb n="3"/>Διονυσίου τοῦ συγγενο<unclear cert="high" reason="undefined">ῦ</unclear>ς καὶ 

    <lb n="4"/>στρατηγοῦ καὶ ἐπὶ τῶν προσόδων 

    <lb n="5"/>χρηματισμὸν εἰς Ἁρμᾶιν 

    <lb n="6"/>εἰς τὸν ἐν Σώβθει ἰβιῶνα 

    <lb n="7"/>πυροῦ <expan><ex cert="high">ἀρτάβας</ex></expan> δέκα τρεῖς <expan><ex cert="high">γίνονται</ex></expan> <expan><ex cert="high">πυροῦ</ex></expan> <num value="13">ιγ</num> 

    <lb n="8"/>καὶ <expan>σύ<ex cert="high">μβολα</ex></expan> καὶ <expan>ἀντισύ<ex cert="high">μβολα</ex></expan> <expan>ποίη<ex cert="high">σαι</ex></expan> πρὸς <expan>αὐ<ex cert="high">τοὺς</ex></expan> ὡς <expan>κα<ex cert="high">θήκει</ex></expan>. 
