    <lb n="10"/><handShift new="m6"/> Ἔρβει <expan>ἀντιγρα<ex cert="high">φεῖ</ex></expan> <expan>θη<ex cert="high">σαυροῦ</ex></expan> Περὶ <expan>Π<ex cert="high">όλιν</ex></expan>. 

    <lb n="11"/>συμπροοῦ κατ<supplied cert="high" reason="lost">ὰ τὸν</supplied> 

    <lb n="12"/>παρὰ Διονυσίου τ<supplied cert="high" reason="lost">οῦ <expan>συγγ<ex cert="high">ενοῦς</ex></expan></supplied> 

    <lb n="13"/>καὶ στρατηγοῦ καὶ <supplied cert="high" reason="lost">ἐπὶ τῶν</supplied> 

    <lb n="14"/>προσόδων χρηματισ<supplied cert="high" reason="lost">μὸν</supplied> 

    <lb n="15"/>εἰς τὴν καθ᾽ ἡμέραν 

    <lb n="16"/>προτιθεμένην ἀθήραν 

    <lb n="17"/>ταῖς Νεμέσεσι καὶ Ἀδραστείαις 

    <lb n="18"/>θεοῖς μεγίστοις τὰς ὑποκειμένας 

    <lb n="19"/>καὶ προειμένας ἕως τοῦ <num value="17">ιζ</num> <expan><ex cert="high">ἔτους</ex></expan> 

    <lb n="20"/>πυροῦ <expan>δοχ<ex cert="high">ικῷ</ex></expan> τεσσαράκοντα <choice><corr>ὀκτώ</corr><sic>οκτωι</sic></choice>, 

    <lb n="21"/>γίνονται πυροῦ <expan>δοχ<ex cert="high">ικῷ</ex></expan> <num value="48">μη</num> καὶ <expan>σύ<ex cert="high">μβολα</ex></expan> 

    <lb n="22"/>καὶ <expan>ἀντισύ<ex cert="high">μβολα</ex></expan> 

    <lb n="23"/><expan>ποίη<ex cert="high">σαι</ex></expan> πρὸς <expan>αὐ<ex cert="high">τοὺς</ex></expan> ὡς <expan>κα<ex cert="high">θήκει</ex></expan>. 
