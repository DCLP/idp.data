    <lb n="18"/><supplied reason="lost" cert="high">τῷ Ἡρακλεί</supplied>ῳ παρέδωκα, <unclear reason="undefined" cert="high">ὅπω</unclear><supplied reason="lost" cert="high">ς</supplied> 

    <lb n="19"/><supplied reason="lost" cert="high">πρὸς σὲ ἀποσ</supplied>ταλῇ· ἀξιῶ <supplied reason="lost" cert="high">ἐὰν</supplied> 

    <lb n="20"/><supplied reason="lost" cert="high">φαίνηται συ</supplied>ντάξαι γρ<supplied reason="lost" cert="high">άψαι</supplied> 

