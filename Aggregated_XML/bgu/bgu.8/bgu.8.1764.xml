    <lb n="5"/><gap reason="lost" extent="14" unit="character"/> μᾶλλον ἡμ<unclear reason="undefined" cert="high">ᾶς</unclear>, <space extent="unknown" unit="character" dim="horizontal"/> σὺ δ᾽ ἀν<unclear reason="undefined" cert="high">τὶ</unclear>  τῆς βασιλείας <unclear reason="undefined" cert="high">τε</unclear> <gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">φας</unclear><gap reason="lost" extent="10" unit="character"/> 

