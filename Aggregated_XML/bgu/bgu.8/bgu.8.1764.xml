    <lb n="2"/><gap reason="lost" extent="20" unit="character"/> τῶι τοῦ Ὀξυρ<unclear reason="undefined" cert="high">υ</unclear><supplied reason="lost" cert="high">γχίτου στρατη</supplied><unclear reason="undefined" cert="high">γῶι</unclear> τὴν μεγίστην πρόνοιαν 

