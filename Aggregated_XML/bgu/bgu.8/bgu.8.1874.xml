    <lb n="1"/><gap extent="7" reason="lost" unit="character"/> Ἡρακλείωι τῶι ἀδελφῶι 

    <lb n="2"/><supplied cert="high" reason="lost">χαίρειν</supplied> καὶ διὰ παντὸς ὑγιαίνειν, 

    <lb n="3"/><supplied cert="high" reason="lost">καὶ αὐτὸς</supplied> ὑγιαίνω <del rend="erasure" status="unremarkable"><gap extent="4" reason="illegible" unit="character"/></del> γράφων 

    <lb n="4"/><supplied cert="high" reason="lost">πᾶσι τοῖ</supplied>ς ἡμῶν φίλοις ἀναγκαῖον 

    <lb n="5"/><supplied cert="high" reason="lost">ἡγούμ</supplied>ην καὶ <unclear cert="high" reason="undefined">σοὶ</unclear> προσφωνῆσαι, 

    <lb n="6"/><supplied cert="high" reason="lost">ἵνα ὅ</supplied><unclear cert="high" reason="undefined">σα</unclear> καθόλου προαιρῆι διασημήνῃς. 

    <lb n="7"/><supplied cert="high" reason="lost">οὐδὲν</supplied> γὰρ παραλείψω τῶν πρὸς τὸ σοὶ 

    <lb n="8"/><unclear cert="high" reason="undefined">χρ</unclear><supplied cert="high" reason="lost">ή</supplied><unclear cert="high" reason="undefined">σιμον</unclear> ἀνηκόντων, <gap extent="1" reason="illegible" unit="character"/> πάντων δὲ 

    <lb n="9"/><supplied cert="high" reason="lost">μάλιστα</supplied> παρακαλῶ τοῦ σώματος ἐπιμέλ<unclear cert="high" reason="undefined">εσθαι</unclear>. <add place="supralinear"><gap desc="vestiges" extent="unknown" reason="illegible" unit="character"/></add> 

    <lb n="10"/><space dim="horizontal" extent="unknown" unit="character"/> ἔρρωσο <expan><ex cert="high">ἔτους</ex></expan> <num value="12">ιβ</num> <expan>Παχ<ex cert="high">ὼν</ex></expan> <num value="13">ιγ</num> 

    <lb n="11"/>τὰ καθ᾽ Ἑρμίαν τὸν <unclear cert="high" reason="undefined">Μενέωνος</unclear> πατέρα 

    <lb n="12"/><supplied cert="high" reason="lost">νῦν ἔτ</supplied><unclear cert="high" reason="undefined">υ</unclear>χεν ἧς ἐφήρμοζεν ἐγλύσεως, ὅθεν 

    <lb n="13"/><gap extent="5" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>όμενος καὶ σὲ ἡσθησόμενον εἰδὼς 

    <lb n="14"/><space dim="horizontal" extent="unknown" unit="character"/> σεσήμαγκα. ὑγίαινε. 
