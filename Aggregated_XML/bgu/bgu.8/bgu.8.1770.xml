<subst><add place="inline">συνεσταλ<add place="supralinear">μένην</add></add><del rend="corrected">συνεσταλ<del rend="erasure">κ<unclear cert="high" reason="undefined">α</unclear></del></del></subst> <del rend="erasure"><unclear cert="high" reason="undefined">οσην</unclear></del> 

