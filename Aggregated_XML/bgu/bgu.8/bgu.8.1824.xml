    <lb n="1"/>Πανίσκωι συγγενεῖ καὶ στρατη<supplied reason="lost" cert="high">γῶι καὶ ἐπὶ τῶν</supplied> 

    <lb n="2"/>προσόδων <space extent="unknown" unit="character" dim="horizontal"/> 

    <lb n="3"/>παρὰ <gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="3" unit="character"/><gap reason="lost" extent="8" unit="character"/> 

    <lb n="4"/>ἡ<unclear reason="undefined" cert="high">γε</unclear>μόνι <gap reason="illegible" extent="1" unit="character"/> π<unclear reason="undefined" cert="high">υκ</unclear>νότερ<supplied reason="lost" cert="high">ον </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>ἐπερχομένων <unclear reason="undefined" cert="high">ἄλλων</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>α<unclear reason="undefined" cert="high">πί</unclear>ωι δι<gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>μένον ὑπ᾽ ἐμοῦ <unclear reason="undefined" cert="high">χ</unclear><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>Τ<unclear reason="undefined" cert="high">α</unclear>μφνοῦθιν <unclear reason="undefined" cert="high">ἀπὸ</unclear>  <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>νοντες καὶ τὸν λοιπὸν <unclear reason="undefined" cert="high">π</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>σπόρον, <unclear reason="undefined" cert="high">ὅ</unclear>θ<unclear reason="undefined" cert="high">ε</unclear>ν το<unclear reason="undefined" cert="high">ῦ</unclear> ᾽ ἐμο<supplied reason="lost" cert="high">ῦ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>φύλακος σὺν τῶι παρ<gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><choice><corr>συστρατιώτηι</corr><sic>συνστρατιωτηι</sic></choice> <gap reason="illegible" extent="2" unit="character"/><unclear reason="undefined" cert="high">ρ</unclear><gap reason="lost" extent="3" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>μένων τούτους κατέλ<unclear reason="undefined" cert="high">αβον</unclear> <supplied reason="lost" cert="high">ν αὐ</supplied>-

    <lb n="14"/>τὸν Θεό<unclear reason="undefined" cert="high">δωρ</unclear>ο<unclear reason="undefined" cert="high">ν</unclear> <supplied reason="lost" cert="high">ὸ</supplied>ν ὄντα ἐν τῶι <unclear reason="undefined" cert="high">τόπ</unclear>ωι 

    <lb n="15"/>ἀπο<unclear reason="undefined" cert="high">σπῶ</unclear><supplied reason="lost" cert="high">ν</supplied>τα χόρτον. <choice><corr>οἱ</corr><sic>ο</sic></choice> δ᾽ ἀπολύσαντες 

    <lb n="16"/>τοῦτον καὶ ἐξ ἐπιστροφῆς παραγενό-

    <lb n="17"/><choice><corr>μενοι</corr><sic>παραγενομενος</sic></choice> σὺν Πτολεμαίωι Νίκωνος τῶν 

    <lb n="18"/>ἀπὸ τῆς <unclear reason="undefined" cert="high">μεγά</unclear>λης π<unclear reason="undefined" cert="high">λ</unclear>α<unclear reason="undefined" cert="high">τ</unclear>ικ καὶ 

    <lb n="19"/>ἄλλοις ἱππεῦσι καὶ ἐπιθέμενοι τῶι τε 

    <lb n="20"/>φύλακι καὶ τῶι <choice><corr>συστρατιώτηι</corr><sic>συνστρατιωτηι</sic></choice> Ἀπολλ<unclear reason="undefined" cert="high">ων</unclear>ίωι 

    <lb n="21"/>κατήνεγκαν αὐτῶν πάντα καὶ αὐτοὺς 

    <lb n="22"/>εἰς τὸν περὶ τοῦ ζῆν περιέστησαν κίν-

    <lb n="23"/>δυνον, ἐγβιασάμενοι καὶ ἣν περιεστάλ-

    <lb n="24"/><choice><corr>κεσαν</corr><sic>πε<unclear reason="undefined" cert="high">ριεσταλ</unclear>κεισαν</sic></choice> αὑτῶν οἱ παρ᾽ ἐμοῦ ὄνον σὺν τῶι 

    <lb n="25"/>ἐπικειμένωι χόρτωι. ἀξιῶ ἐὰν φαίνηται 

    <lb n="26"/>συντάξαι καταστῆσαι <unclear reason="undefined" cert="high">τούτου</unclear>ς ἐπὶ σὲ 

    <lb n="27"/>καὶ συνέχειν μέχρ<supplied reason="lost" cert="high">ι</supplied> τοῦ ἐκ<unclear reason="undefined" cert="high">τεῖσαι</unclear>  -

    <lb n="28"/>νυῖαν ὑπ᾽ αὐτῶν τοῦ χόρτου καὶ τοῦ σπόρου 

    <lb n="29"/>φθορὰν καὶ περὶ αὐτῶν διαλαβεῖν μισο-

    <lb n="30"/>πονήρως, ἵν᾽ ὦ τετευχὼς τῆς σῆς δικαι-

    <lb n="31"/>οσύνης. 
