    <lb n="20"/>λακίτου καὶ ὑπὸ τῶ<supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="21"/>τῆς Τέκμει ἀπαι<supplied cert="high" reason="lost">τούμενος </supplied><gap extent="8" reason="lost" unit="character"/><supplied cert="high" reason="lost"> παρὰ</supplied> 

