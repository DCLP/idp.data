    <lb n="1"/>ἀρ<unclear reason="undefined" cert="high">α</unclear><gap reason="lost" extent="22" unit="character"/><supplied reason="lost" cert="high"> λῃστρικῶι</supplied> 

    <lb n="2"/>τρόπωι ἐπιβαλόντες <gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="lost" extent="8" unit="character"/> 

    <lb n="3"/>ὡς ἐν πραγ<unclear reason="undefined" cert="high">μάτων</unclear> ἀναρχίαι <gap reason="illegible" extent="3" unit="character"/><gap reason="lost" extent="8" unit="character"/> 

    <lb n="4"/>τὰ ἔνδον ὄντα σκεύη <unclear reason="undefined" cert="high">πρὸς</unclear> <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="8" unit="character"/> 

