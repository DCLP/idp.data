    <lb n="1"/>ὑπατείας τῶν δεσπ<supplied cert="high" reason="lost">οτῶ</supplied>ν ἡμῶν Οὐάλεντος τὸ <num value="5">ε</num> καὶ 

    <lb n="2"/>Οὐαλεντινι<supplied cert="high" reason="lost">α</supplied>νοῦ ν<supplied cert="high" reason="lost">έου</supplied> τῶν αἰωνίων Αὐ<supplied cert="high" reason="lost">γο</supplied>ύ<supplied cert="high" reason="lost">σ</supplied>τ<supplied cert="high" reason="lost">ων</supplied> 

    <lb n="3"/>Ἁθὺρ <gap extent="2" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐ</supplied>ν Ἡρακλέους πόλει <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>Αὐρήλιο<supplied cert="high" reason="lost">ς </supplied><gap extent="3" reason="lost" unit="character"/>υσης Κασταλίου <expan>μη<ex cert="high">τρὸς</ex></expan> Σαρ<supplied cert="low" reason="lost">απιάδος</supplied><supplied cert="high" reason="lost"> οἰ</supplied>-

    <lb n="5"/>κῶν ἐν Ἡρακλέους πόλει λανάριος Α<supplied cert="high" reason="lost">ὐρηλίῳ </supplied><gap extent="6" reason="lost" unit="character"/> 

    <lb n="6"/>Ἀπολλωνίου ἀπὸ τῆς αὐτῆς πό<supplied cert="high" reason="lost">λεως χειροτέ</supplied>-

    <lb n="7"/>χνῳ <space dim="horizontal" extent="unknown" unit="character"/> <supplied cert="high" reason="lost">χαίρειν.</supplied> 

    <lb n="8"/>ἐπειδ<supplied cert="high" reason="lost">ὴ</supplied> πρὸ τούτου παρεθέμην σοι <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>χρυσίου νομισμάτια δ<unclear cert="high" reason="undefined">ό</unclear>κιμα ἁπλᾶ δ<supplied cert="high" reason="lost">ύο <expan>γί<ex cert="high">νεται</ex></expan></supplied> 

