    <lb n="1"/><supplied reason="lost" cert="high">ἔτο</supplied>υς πέμπτου Τιβερίου <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><supplied reason="lost" cert="high">Ἡρα</supplied>κλείδου μερίδος τοῦ Ἀρσινοεί[του νομοῦ. ὁμολογεῖ Στοτοῆτις Στοτοήτιος <gap reason="illegible" extent="unknown" unit="character"/> ὡς ἐτῶν <gap reason="illegible" extent="unknown" unit="character"/> ] 

    <lb n="3"/><gap reason="lost" extent="3" unit="character"/>στερας κα χειρὶ δεξιᾷ Χ[αιρήμονι Σωκράτους <gap reason="illegible" extent="unknown" unit="character"/> ὡς ἐτῶν <gap reason="illegible" extent="unknown" unit="character"/> οὐλὴ <gap reason="illegible" extent="unknown" unit="character"/> πεπρακέναι αὐτῷ κατὰ] 

    <lb n="4"/><supplied reason="lost" cert="high">τήν</supplied>δε τὴν ὁμολογίαν ἀπὸ τῆ<supplied reason="lost" cert="high">ς ἐνεστώσης ἡμέρας εἰς ἀεὶ τὴν ὑπάρχουσαν αὐτῷ δούλην</supplied> 

    <lb n="5"/><supplied reason="lost" cert="high">ᾗς ὄ</supplied>νομα Εὐδαιμονὶς ἣ καὶ [ <gap reason="illegible" extent="unknown" unit="character"/> ὡς ἐτῶν <gap reason="illegible" extent="unknown" unit="character"/> ταύτην τοιαύτην ἀναπόρριφον πλὴν ἐπαφῆς καὶ ἱερᾶς νόσου] 

    <lb n="6"/><supplied reason="lost" cert="high">καὶ</supplied> ἀπέχειν τὸν Στοτοῆ<supplied reason="lost" cert="high">τιν παρὰ τοῦ Χαιρήμονος τὴν συμπεφωνημένην τιμὴν πᾶσαν</supplied> 

    <lb n="7"/><supplied reason="lost" cert="high">ἐκ π</supplied>λήρους παραχρῆμα διὰ <supplied reason="lost" cert="high">χειρὸς ἐξ οἴκου ἀργυρίου δραχμὰς </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> χιλίας καὶ βεβαιώσειν αὐτὸν τῷ Χαιρήμονι</supplied> 

    <lb n="8"/><supplied reason="lost" cert="high">καὶ</supplied> τοῖς παρ᾽ αὐτοῦ τὴν πεπρ<supplied reason="lost" cert="high">αμένην αὐτῷ ὡς πρόκειται δούλην Εὐδαιμονίδα τὴν καὶ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> πάσῃ βεβαιώσει πλὴν</supplied> 

    <lb n="9"/><supplied reason="lost" cert="high">δρασ</supplied>μοῦ καὶ <choice><corr>παρεχέσθω</corr><sic>παρεχεσθωι</sic></choice> <supplied reason="lost" cert="high">ἀνέπαφον καὶ ἀνενεχύραστον καὶ ἀνεπιδάνειστον καὶ καθαρὰν</supplied> 

    <lb n="10"/><gap reason="lost" extent="3" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/> ὀφειλήματος <supplied reason="lost" cert="high">δημοσίου μέχρι τῆς ἐνεστώσης ἡμέρας ἀπὸ δὲ ἰδιωτικῶν καὶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><gap reason="lost" extent="3" unit="character"/><gap reason="illegible" extent="3" unit="character"/> <unclear reason="undefined" cert="high">καὶ</unclear> μηδένα ἐμ<supplied reason="lost" cert="high">ποιούμενον αὐτῆς μηδὲ μέρους αὐτῆς μηδὲ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> τρόπῳ μηδενὶ</supplied> 

    <lb n="12"/><supplied reason="lost" cert="high">μη</supplied>δὲ κωλύοντα Χαιρήμ<supplied reason="lost" cert="high">ονα μηδὲ τοὺς παρ᾽ αὐτοῦ κυριεύοντας αὐτῶν καὶ ὑποτιθέντας καὶ ἐξαλλοτριοῦντας καὶ</supplied> 

    <lb n="13"/><supplied reason="lost" cert="high">χρ</supplied>ω<unclear reason="undefined" cert="high">μέν</unclear>ους αὐτοῖς ὡς ἐὰν α<supplied reason="lost" cert="high">ἱρῶνται </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/><gap reason="lost" extent="3" unit="character"/><gap reason="illegible" extent="4" unit="character"/>αι, πρὸς δὲ τὰ προκεί<supplied reason="lost" cert="high">μενα μηδὲν τὸν Στοτοῆτιν ἐπικαλεῖν μήδ᾽ ἐνκαλεῖν μήδ᾽ ἐπελεύσεσθαι</supplied> 

    <lb n="15"/><supplied reason="lost" cert="high">ἐπὶ τὴ</supplied>ν πεπραμένην καθὼς π<supplied reason="lost" cert="high">ρόκειται δούλην Εὐδαιμονίδα τὴν καὶ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ὅτι δ᾽ ἂν τούτων παραβῇ ὁ</supplied> 

    <lb n="16"/><supplied reason="lost" cert="high">Στοτ</supplied>οῆτις, <choice><corr>ἀποτισάτω</corr><sic>αποτισατωι</sic></choice> τῷ Χ<supplied reason="lost" cert="high">αιρήμονι ἥν τε ἀπείληφεν τιμὴν καὶ ἀνηλωμένα διπλᾶ καὶ ἐπίτιμον</supplied> 

    <lb n="17"/>[<expan>ἀργ<ex cert="high">υρίου</ex></expan> <expan>δρα]χ<ex cert="high">μὰς</ex></expan> δισχιλίας <num value="2000"/> <choice><corr>κα<supplied reason="omitted" cert="high">ὶ</supplied></corr><sic>κα</sic></choice> μηδὲ<supplied reason="lost" cert="high">ν ἧσσον τὰ διωμολογημένα κύρια εἶναι. ὑπογραφεῖς τοῦ μὲν Στοτοήτιος</supplied> 

    <lb n="18"/><gap reason="lost" extent="3" unit="character"/><gap reason="illegible" extent="4" unit="character"/> Σαταβοὺς κ<unclear reason="undefined" cert="high">α</unclear>λ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="19"/><handShift new="m2"/> <supplied reason="lost" cert="high">Στο</supplied>τουῆτις Στοτουήτιος [ <gap reason="illegible" extent="unknown" unit="character"/> ὁμολογῶ πεπρακέναι Χαιρήμονι Σωκράτους <gap reason="illegible" extent="unknown" unit="character"/> ] 

    <lb n="20"/><gap reason="lost" extent="2" unit="character"/><supplied reason="lost" cert="high"> ἀ</supplied>πὸ τῆς ἐνεστώσης ἡμέ<supplied reason="lost" cert="high">ρας εἰς ἀεὶ τὴν ὑπάρχουσάν μοι δούλην ἧς ὄνομα Εὐδαιμονὶς</supplied> 

    <lb n="21"/><gap reason="lost" extent="3" unit="character"/><gap reason="illegible" extent="3" unit="character"/>ραεις ὡς ἐτῶν δ<gap reason="lost" extent="3" unit="character"/><supplied reason="lost" cert="high"> ταύτην τοιαύτην ἀναπόρριφον καὶ ἀπέχω τὴν τιμὴν πᾶσαν ἐκ πλή</supplied>-

    <lb n="22"/><supplied reason="lost" cert="high">ρους</supplied> παραχρῆμα διὰ <choice><corr>χειρὸ<supplied reason="lost" cert="high">ς</supplied></corr><sic>χιρο<supplied reason="lost" cert="high">ς</supplied></sic></choice> <supplied reason="lost" cert="high">ἐξ οἴκου ἀργυρίου δραχμὰς </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> χιλίας καὶ βεβαιώσω πάσῃ βεβαιώσει</supplied> 

    <lb n="23"/><gap reason="lost" extent="3" unit="character"/> <choice><corr>πλὴ<supplied reason="omitted" cert="high">ν</supplied></corr><sic>πλη</sic></choice> δρασμοῦ <choice><corr>καθὼς</corr><sic>καθος</sic></choice> π[ρόκειται <gap reason="illegible" extent="unknown" unit="character"/> ἔγραψα ὑπὲρ αὐτοῦ <gap reason="illegible" extent="unknown" unit="character"/> ] 

    <lb n="24"/><gap reason="lost" extent="3" unit="character"/><unclear reason="undefined" cert="high">ρι</unclear> 

    <lb n="24"/><gap reason="ellipsis" desc="Demotic" extent="1" unit="line"/> 

    <lb n="25"/><handShift new="m3"/> <supplied reason="lost" cert="high">Χαι</supplied>ρήμων Σωκράτους εικο<gap reason="illegible" extent="1" unit="character"/>[ <gap reason="illegible" extent="unknown" unit="character"/> <expan><ex cert="low">ἔτων</ex></expan> <gap reason="illegible" extent="unknown" unit="character"/> ] 

    <lb n="26"/><handShift new="m4"/> <supplied reason="lost" cert="high">Μεχ</supplied>εὶρ  <expan>ἀναγέγρ<ex cert="high">απται</ex></expan> δ<supplied reason="lost" cert="high">ιὰ</supplied> τοῦ κ<gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> γραφείου.</supplied> </ab></div><div n="meta.9428" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.9428" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" value="0019-01-31" notBefore="19" notAfter="19">31. Jan. 19 (Jahr und Tag unsicher)</date></p><p><date type="textDate" value="0045-01-31" notBefore="45" notAfter="45">31. Jan. 45 (Jahr und Tag unsicher)</date></p></div><div n="meta.9428" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Griechisch - demotisch. Alternativdatierung: 31. Jan. (?) 45.</p></div><div n="meta.9428" type="commentary" subtype="mentionedDates" org="uniform" part="N" sample="complete"><head>Erwähnte Daten</head></div><div n="meta.9428" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">BGU</title><biblScope type="volume">3</biblScope><biblScope type="numbers">987</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">9428</biblScope><xref type="DAHT" href="9428" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in DAHT</xref></bibl><bibl type="DDbDP" default="NO"><series>0001</series>; <biblScope type="volume">3</biblScope>; <biblScope type="numbers">987</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0001;3;987" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.9428" type="bibliography" subtype="corrections" org="uniform" part="N" sample=