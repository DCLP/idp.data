    <lb n="18"/>δ<supplied cert="high" reason="lost">ο</supplied>υ μερίδος ε<gap extent="1" reason="lost" unit="character"/><unclear cert="high" reason="undefined">η</unclear><gap extent="3" reason="lost" unit="character"/>λε<gap extent="1" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/><unclear cert="high" reason="undefined">τ</unclear>ων ἑβδομή<supplied cert="high" reason="lost">κ</supplied>οντα 

    <lb n="19"/><choice><corr><supplied cert="high" reason="lost">π</supplied>έντε</corr><sic><supplied cert="high" reason="lost">π</supplied>εντ</sic></choice> <num value="75"/> ἀρουρῶν ὧ<supplied cert="high" reason="lost">ν γείτο</supplied>νε<supplied cert="high" reason="lost">ς κα</supplied>θὼς ὑπηγόρε<supplied cert="high" reason="lost">υ</supplied>σαν 

    <lb n="20"/>ν<supplied cert="high" reason="lost">ότ</supplied>ῳ Ἀμα<supplied cert="low" reason="lost">δόκου</supplied><supplied cert="high" reason="lost"> τοῦ Πτ</supplied>ο<supplied cert="high" reason="lost">λε</supplied>μαίου καὶ <expan>ὀνόματο<ex cert="high">ς</ex></expan> <note lang="en" place="not specified">?</note>

    <lb n="21"/><gap extent="1" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/>ιτος, βορρ<supplied cert="high" reason="lost">ᾶ </supplied><gap extent="2" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ν</unclear><gap extent="1" reason="illegible" unit="character"/>ρος καὶ βασιλικὴ γῆ, ἀπηλιώ-

    <lb n="22"/>τῇ βασιλικὴ γῆ, λ<supplied cert="high" reason="lost">ι</supplied>βὶ Λ<supplied cert="high" reason="lost">ο</supplied>νγ<supplied cert="high" reason="lost">ίν</supplied>ου κλῆρος, ἐπάναγκον 

    <lb n="23"/><supplied cert="high" reason="lost">τ</supplied>ὸν <choice><corr>Κοίντον</corr><sic>κοιντος</sic></choice> Γέ<supplied cert="high" reason="lost">λλι</supplied>ον Οὐ<supplied cert="high" reason="lost">ά</supplied>λεντα <supplied cert="high" reason="lost">ἀ</supplied>ποδῶ<supplied cert="high" reason="lost">ν</supplied>αι τῷ 

    <lb n="24"/><supplied cert="high" reason="lost">Λ</supplied>ουκίῳ Οὐα<supplied cert="high" reason="lost">λερί</supplied>ῳ Ἀμμωνια<supplied cert="high" reason="lost">νῷ</supplied> <app type="BL"><lem resp="1.440"><seg cert="low" part="N"><unclear cert="high" reason="undefined">τὰς</unclear> <unclear cert="low" reason="undefined">τοῦ</unclear></seg></lem><rdg><gap extent="1" reason="lost" unit="character"/><unclear cert="high" reason="undefined">κ</unclear><gap extent="3" reason="illegible" unit="character"/>ευ</rdg></app> 

    <lb n="25"/>ἀργυρίου δραχ<supplied cert="high" reason="lost">μ</supplied>ὰς ὀκτ<supplied cert="high" reason="lost">α</supplied>κοσία<supplied cert="high" reason="lost">ς <num value="800"/> ἐν τ</supplied>ῷ προκ<supplied cert="high" reason="lost">ει</supplied>-

    <lb n="26"/>μένῳ χρόνῳ, τὸν δὲ τ<supplied cert="high" reason="lost">ό</supplied>κον κ<supplied cert="high" reason="lost">ατὰ</supplied> μῆνα ἕκ<supplied cert="high" reason="lost">α</supplied>σ-

    <lb n="27"/>τον. <subst><add place="inline">ἐὰν</add><del rend="corrected"><gap extent="3" reason="illegible" unit="character"/></del></subst> δὲ μὴ <choice><corr>ἀποδῷ</corr><sic><supplied cert="high" reason="lost">α</supplied>ποδοι</sic></choice>, <choice><corr>ἐξεῖναι</corr><sic><supplied cert="high" reason="lost">ε</supplied>ξιναι</sic></choice> τῷ Λουκίῳ Οὐαλε-

    <lb n="28"/>ρίῳ Ἀμμωνιανῷ ἐπι<supplied cert="high" reason="lost">τ</supplied>ελεῖν τὰ κατὰ τῆς ὑ-

    <lb n="29"/>ποθήκης νόμιμα πρ<supplied cert="high" reason="lost">ὸ</supplied>ς οὗ τι ἂν βαστάζ<supplied cert="high" reason="lost">ῃ</supplied> καὶ 

    <lb n="30"/>τοῦ ἐνλείψοντος γείνεσ<supplied cert="high" reason="lost">θ</supplied>αι αὐτῷ τὴν πρᾶξιν <g rend="extension" type="filler"/> 

    <lb n="31"/>ἐκ τῶν ἄλλων τοῦ ὑ<supplied cert="high" reason="lost">πο</supplied>χρέου ὑπαρχό<supplied cert="high" reason="lost">ν</supplied>των, 

    <lb n="32"/>καὶ ἐὰν, ὃ μὴ γείνοι<supplied cert="high" reason="lost">το</supplied>, συμβῇ <subst><add place="inline">κίνδυνόν</add><del rend="corrected"><gap extent="2" reason="illegible" unit="character"/>νδυνον</del></subst> τινα 

    <lb n="33"/>περὶ <supplied cert="high" reason="lost">τὴ</supplied>ν ὑποθήκην <supplied cert="high" reason="lost">ἢ μέ</supplied>ρος αὐτῆς ἐπακο<supplied cert="high" reason="lost">λ</supplied>ου-

    <lb n="34"/>θῆσ<supplied cert="high" reason="lost">αι</supplied>, καὶ οὕτως γειν<supplied cert="high" reason="lost">έ</supplied>σθαι τῷ δεδαν<supplied cert="high" reason="lost">ει</supplied>κότι 

    <lb n="35"/>τὴν π<supplied cert="high" reason="lost">ρᾶ</supplied>ξιν καθὼς κα<supplied cert="high" reason="lost">ὶ ἐ</supplied>πὶ τοῦ <choice><corr>ἐνλείμματος</corr><sic>ενλιμματ<supplied cert="high" reason="lost">ος</supplied></sic></choice> δε-

    <lb n="36"/>δήλωται, παρέχεσθαι δὲ αὐτὸν τὴν <supplied cert="high" reason="lost">ὑπ</supplied>οθή-

    <lb n="37"/>κην καθαρὰν καὶ <app type="editorial"><lem resp="bgu 3 p.4"><choice><corr>ἀνέπαφον</corr><sic>ανεπαπον</sic></choice> καὶ ἀν<supplied cert="high" reason="lost">επι</supplied>δά-

    <lb n="38"/>νειστον</lem><rdg>ἀνυπαναγκαίαν <supplied cert="high" reason="lost">ἀ</supplied>δάνειστον</rdg></app> 

    <lb n="39"/>ἄλ<supplied cert="high" reason="lost">λ</supplied>ου δαν<supplied cert="high" reason="lost">είου</supplied> καὶ πάσ<supplied cert="high" reason="lost">η</supplied>ς ὀφειλ<supplied cert="high" reason="lost">ῆς κ</supplied>αὶ 

    <lb n="40"/>μηδένα αὐτῆς <app type="editorial"><lem resp="bgu 3 p.4">ἐμπ<supplied cert="high" reason="lost">οιο</supplied>ύμενον</lem><rdg>ἐᾶσ<supplied cert="high" reason="lost">αι </supplied><gap extent="2" reason="lost" unit="character"/><unclear cert="high" reason="undefined">υ</unclear>μενον</rdg></app> τρόπ<supplied cert="high" reason="lost">ῳ μη</supplied>-

    <lb n="41"/>δεν<supplied cert="high" reason="lost">ὶ</supplied>, μὴ ἐλ<supplied cert="high" reason="lost">α</supplied>ττουμένου τοῦ Οὐαλερ<supplied cert="high" reason="lost">ίου</supplied> 

    <lb n="42"/>Ἀμμωνιαν<supplied cert="high" reason="lost">ο</supplied>ῦ <supplied cert="high" reason="lost">π</supplied>ερὶ ὧ<supplied cert="high" reason="lost">ν ἄ</supplied>λλ<supplied cert="high" reason="lost">ω</supplied>ν <choice><corr>ὀφείλει</corr><sic><supplied cert="high" reason="lost">ο</supplied>φ<supplied cert="high" reason="lost">ε</supplied>ι<supplied cert="high" reason="lost">λ</supplied>ι</sic></choice> αὐτῷ ὁ 

    <lb n="43"/>αὐτὸ<supplied cert="high" reason="lost">ς Γ</supplied>έλλι<supplied cert="high" reason="lost">ος</supplied> Οὐάλης <supplied cert="high" reason="lost">κ</supplied>αθ᾽ ἑτέρ<supplied cert="high" reason="lost">α</supplied>ν συνχώρησιν 

    <lb n="44"/>τῷ <supplied cert="high" reason="lost">διε</supplied>ληλ<supplied cert="high" reason="lost">υθ</supplied>ότι ἔτει μηνὶ Και<supplied cert="high" reason="lost">σ</supplied>α<supplied cert="high" reason="lost">ρ</supplied>είῳ ἄ<supplied cert="high" reason="lost">λλ</supplied>ων 

    <lb n="45"/>ἀργυ<supplied cert="high" reason="lost">ρ</supplied>ίου δραχμῶν <supplied cert="high" reason="lost">τετ</supplied>ρακοσ<supplied cert="high" reason="lost">ί</supplied>ων <supplied cert="high" reason="lost">ἑξήκο</supplied>ν-

    <lb n="46"/>τα <num value="460"/> καὶ τόκων καὶ οὐ <app type="BL"><lem resp="1.63"><seg cert="low" part="N"><supplied cert="low" reason="lost">λύ</supplied><unclear cert="low" reason="undefined">σ</unclear>εται</seg></lem><rdg><gap extent="2" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>εται</rdg></app> τήνδε τὴν ὑπο<supplied cert="high" reason="lost">θή</supplied>κην, 

    <lb n="47"/>εἰ μὴ πρότερον <choice><corr>ἀποδῷ</corr><sic>αποδο<supplied cert="high" reason="lost">ι</supplied></sic></choice> <supplied cert="high" reason="lost">κ</supplied>αὶ τὰς κατὰ τ<supplied cert="high" reason="lost">ὸ συνάλ</supplied>λα-

    <lb n="48"/>γμα δραχμὰς τετ<supplied cert="high" reason="lost">ρα</supplied>κοσίας ἑξήκοντα <num value="460"/> καὶ τό-

    <lb n="49"/>κο<supplied cert="high" reason="lost">υς</supplied> διὰ τὸ <app type="editorial"><lem resp="bgu 3 p.4">ἀλλη<supplied cert="high" reason="lost">λέγγ</supplied>υα</lem><rdg>ἄλλη<supplied cert="high" reason="lost">ν ἔγγ</supplied>υα</rdg></app> εἶναι. ἔτους ἑ<supplied cert="high" reason="lost">β</supplied>δόμου 

    <lb n="50"/>Αὐ<supplied cert="high" reason="lost">το</supplied>κράτορος Καί<supplied cert="high" reason="lost">σαρ</supplied>ος Τίτου Αἰλίου Ἁδ<supplied cert="high" reason="lost">ρι</supplied>ανοῦ 

    <lb n="51"/>Ἀντωνείνου Σε<supplied cert="high" reason="lost">βα</supplied>στοῦ Εὐσεβοῦς μη<supplied cert="high" reason="lost">ν</supplied>ὸς 

    <lb n="52"/>Σεβ<supplied cert="high" reason="lost">α</supplied>στοῦ Εὐσεβ<supplied cert="high" reason="lost">είο</supplied>υ <num value="16">ιϛ</num>. </ab></div><div n="meta.20057" part="N" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.20057" part="N" subtype="textDate" type="commentary"><p><date notAfter="143" notBefore="143" type="textDate" value="0143-09-14">14. Sept. 143</date></p></div><div n="meta.20057" part="N" subtype="general" type="commentary"><p>Zum Ort vgl. BL VIII, S. 33. Vgl. Stauner, Schriftwesen, S. 425f., Nr. 418.</p></div><div n="meta.20057" part="N" subtype="mentionedDates" type="commentary"><head>Erwähnte Daten</head><p>Z. 43: <date notAfter="0143-08-29" notBefore="0143-07-25" type="mentioned">25. Juli  - 29. Aug. 143</date></p></div><div n="meta.20057" part="N" subtype="principalEdition" type="bibliography"><listBibl><bibl subtype="principal" type="publication"><title level="s" type="abbreviated">BGU</title><biblScope type="volume">3</biblScope><biblScope type="numbers">741</biblScope></bibl><bibl type="Trismegistos"><biblScope type="numbers">20057</biblScope></bibl><bibl type="DDbDP"><series>0001</series>; <biblScope type="volume">3</biblScope>; <biblScope type="numbers">741</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0001;3;741" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.20057" part="N" subtype="corrections" type="bibliography"><head>BL-Einträge nach BL-Konkordanz</head><bibl type="BL"><biblScope type="volume">VIII</biblScope>, S. <biblScope type="pages">33</biblScope></bibl></div><div n="meta.20057" part="N" subtype="illustrations" type="bibliography"><p>keine</p></div><div n="meta.20057" part="N" subtype="otherPublications" type="bibliography"><head>Andere Publikationen</head><listBibl><bibl subtype="other" type="publication">M.Chr. 244</bibl><bibl subtype="other" type="publication">FIRA III 119</bibl></listBib