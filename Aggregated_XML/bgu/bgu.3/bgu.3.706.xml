    <lb n="14"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> καὶ</supplied> <unclear cert="high" reason="undefined">τ</unclear>ὰς ἐξ ἀμφοτέρων θυγατέρας Ταπεκῦσις <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="15"/><gap extent="unknown" reason="lost" unit="character"/> <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">αύ</supplied><unclear cert="high" reason="undefined">τ</unclear>ης ἄλλη <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="16"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> κ</supplied>αὶ τοῦ Ὥρου γυναῖκα Ταπιῶμιν Τεσενού<supplied cert="high" reason="lost">φεως </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="17"/><app type="editorial"><lem resp="bgu 3 p.4"><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> κ</supplied>αὶ</lem><rdg><gap extent="unknown" reason="lost" unit="character"/>ασ</rdg></app> ἐμοῦ τοῦ Ἁρπαγάθου <app type="editorial"><lem resp="bgu 3 p.4">πάντων</lem><rdg>αὐτῶν</rdg></app> προγεγραμμέ<supplied cert="high" reason="lost">να </supplied><gap extent="unknown" reason="lost" unit="character"/> 

