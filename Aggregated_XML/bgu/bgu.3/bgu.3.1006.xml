    <lb n="14"/>χρόνοις ἐσταθ <gap desc="vestiges" extent="unknown" reason="illegible" unit="character"/> 

    <lb n="15"/>κία ο<unclear cert="high" reason="undefined">ὐ</unclear>δέποτε <gap desc="vestiges" extent="unknown" reason="illegible" unit="character"/> 

