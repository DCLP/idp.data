    <lb n="1"/>ἔτους δεκάτου Αὐτοκράτορος <supplied cert="high" reason="lost">Καίσαρος Νέρουα Τραϊανοῦ</supplied> 

    <lb n="2"/>Σεβαστοῦ Γερμανικοῦ Δακικοῦ <app type="BL"><lem resp="7.16"><supplied cert="high" reason="lost">μηνὸς Ἀπελλαίου <num value="30">λ</num></supplied></lem><rdg><gap extent="unknown" reason="lost" unit="character"/></rdg></app> 

    <lb n="3"/>Φαῶφι <num value="30">λ</num> ἐν Ψενύρει <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>ὁμολογοῦσι Ἁρπαγάθης <supplied cert="high" reason="lost">Ὀννώφρεως Πέρσης</supplied> 

    <lb n="5"/>τῆς ἐπιγονῆς ὡς ἐτῶν τρ<supplied cert="high" reason="lost">ιάκοντα <num value="30"/> </supplied><gap extent="5" reason="lost" unit="character"/><supplied cert="high" reason="lost"> οὐλὴ</supplied> 

    <lb n="6"/><choice><corr>μετώπωι</corr><sic>μετοπωι</sic></choice> <supplied cert="high" reason="lost">ἐκ</supplied> δεξιῶν καὶ ἡ <supplied cert="high" reason="lost">τούτου γυνὴ </supplied><gap extent="5" reason="lost" unit="character"/> 

    <lb n="7"/>Ἁρπαγάθου <choice><corr>Περσίνη</corr><sic>περσινηι</sic></choice> ὡς <supplied cert="high" reason="lost">ἐτῶν </supplied><gap extent="7" reason="lost" unit="character"/><supplied cert="high" reason="lost"> οὐλὴ</supplied> 

    <lb n="8"/>μετώπωι ἐξ ἀριστερῶν ὑπο<gap extent="1" reason="illegible" unit="character"/><gap extent="5" reason="lost" unit="character"/><supplied cert="high" reason="lost"> μετὰ κυρίου</supplied> 

    <lb n="9"/>τοῦ ἀνδρὸς ἀλλήλων ἔγγ<supplied cert="high" reason="lost">υοι εἰς ἔκτισιν Πα</supplied>-

    <lb n="10"/>βοῦτι Ἑριέως ὡς ἐτῶν <gap extent="11" reason="lost" unit="character"/><supplied cert="high" reason="lost"> οὐλὴ</supplied> 

    <lb n="11"/>μετώπωι ἐξ ἀριστερῶν ἔχ<supplied cert="high" reason="lost">ειν παρ᾽ αὐτοῦ τοὺς ὁμο</supplied>-

    <lb n="12"/>λογοῦντας διὰ χειρὸς <supplied cert="high" reason="lost">ἐξ οἴκου ἀργυρίου δραχμὰς</supplied> 

    <lb n="13"/>ἑκατὸν εἴκ<supplied cert="high" reason="lost">οσι</supplied> <num value="120"/> παραθήκη<supplied cert="high" reason="lost">ν ἀκίνδυνον παντὸς</supplied> 

    <lb n="14"/>κινδύνο<supplied cert="high" reason="lost">υ</supplied> καὶ ἀνυπόλογον <supplied cert="high" reason="lost">παντὸς ὑπολόγου, ἣν</supplied> 

    <lb n="15"/>καὶ ἀποδότωσαν οἱ ὁμολο<supplied cert="high" reason="lost">γοῦντες, ὁπήνικα ἐὰν</supplied> 

    <lb n="16"/>αἱρῆται ὁ Παβοὺς ἄνευ <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ε πάσης ὑπερθέσεως καὶ</supplied> 

    <lb n="17"/>εὑρησι<supplied cert="high" reason="lost">λογ</supplied>ίας, ἐὰν δὲ μὴ <supplied cert="high" reason="lost">ἀποδῶσι καθὰ γέγραπται</supplied> 

    <lb n="18"/>ἀποτεισάτωσαν αὐτῷ <supplied cert="high" reason="lost">οἱ ὁμολογοῦντες τὴν παρα</supplied>-

    <lb n="19"/>θήκην διπλῆν ἀκολ<supplied cert="high" reason="lost">ο</supplied>ύθ<supplied cert="high" reason="lost">ως τῷ τῶν παραθηκῶν νόμῳ, τῆς</supplied> 

    <lb n="20"/>πράξεως οὔσης τῶι Παβοῦ<supplied cert="high" reason="lost">τι ἔκ τε τῶν ὁμολο</supplied>-

    <lb n="21"/>γούντων καὶ ἐκ τῶν ὑπαρ<supplied cert="high" reason="lost">χόντων αὐτοῖς πάντων</supplied> 

    <lb n="22"/>καθάπερ <supplied cert="high" reason="lost">ἐκ</supplied> δίκης. <choice><corr><expan><supplied cert="high" reason="omitted">ὑπο</supplied>γρ<ex cert="high">αφεὺς</ex></expan></corr><sic>γρ</sic></choice> τῶν <supplied cert="high" reason="lost">ὁμολογούντων </supplied><gap extent="6" reason="lost" unit="character"/> 

    <lb n="23"/><handShift new="m2"/> <expan><ex cert="high">ἐτῶν</ex></expan> <num value="57">ν<unclear cert="high" reason="undefined">ζ</unclear></num> <choice><corr><expan>οὐλ<ex cert="high">ὴ</ex></expan></corr><sic>ωυλ</sic></choice> <gap extent="4" reason="lost" unit="character"/><abbr><gap extent="2" reason="illegible" unit="character"/>ιστ</abbr> <gap extent="4" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="24"/>Ἁ<supplied cert="high" reason="lost">ρ</supplied><unclear cert="high" reason="undefined">π</unclear>α<unclear cert="high" reason="undefined">γ</unclear>άθ<supplied cert="high" reason="lost">ου Ὀ</supplied>ννώφρεως <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

