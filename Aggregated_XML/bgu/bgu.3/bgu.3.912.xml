    <lb n="15"/>ἡμέρας ἕως Μεσορὴ ἐπαγομένων <supplied cert="high" reason="lost"><num value="5">ε</num></supplied> 

    <lb n="16"/>τοῦ ἐνεστῶτος ἐννεακαιδε<supplied cert="high" reason="lost">κ</supplied>άτου ἔ<supplied cert="high" reason="lost">τους</supplied> 

    <lb n="17"/>Τιβερίου Καίσαρος Σεβαστοῦ εἰς <choice><corr>πᾶν</corr><sic>π<supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">ντ</unclear><supplied cert="high" reason="lost">α</supplied></sic></choice> 

    <lb n="18"/>ὃ ἐ<unclear cert="high" reason="undefined">ὰν</unclear> αἱρῆται, φ<supplied cert="high" reason="lost">όρο</supplied>υ <supplied cert="high" reason="lost">ὄ</supplied>ντος κατὰ <unclear cert="high" reason="undefined">μῆν</unclear><supplied cert="high" reason="lost">α</supplied> 

    <lb n="19"/>ἀργυρίου δραχ<supplied cert="high" reason="lost">μῶ</supplied>ν τρ<supplied cert="high" reason="lost">ι</supplied>ῶν <num value="3"/>, τῶν δὲ τροφε<supplied cert="high" reason="lost">ίων</supplied> 

    <lb n="20"/>καὶ πο<unclear cert="high" reason="undefined">τι</unclear>σ<unclear cert="high" reason="undefined">μ</unclear>οῦ καὶ παντὸς τελέσματο<supplied cert="high" reason="lost">ς</supplied> 

    <lb n="21"/>καὶ προνοίας πρὸς τὸν μεμισθωμέν<supplied cert="high" reason="lost">ον</supplied> 


    <lb n="22"/>Φάσιν, ἐφ᾽ ᾧ <choice><corr cert="low">ἐπιθήσει</corr><sic>επ<unclear cert="high" reason="undefined">ι</unclear>θηση</sic></choice> τὸ <choice><corr cert="low">κατιὸν</corr><sic>καθ<unclear cert="high" reason="undefined">ον</unclear></sic></choice> ἔνγο<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">ον,</supplied> 

    <lb n="23"/>καὶ μετὰ τὸν χρόνον παραδότω ὁ Φ<supplied cert="high" reason="lost">άσις</supplied> 

    <lb n="24"/>τὰ ὀνικὰ κτήνη <unclear cert="high" reason="undefined">ὡς</unclear> καὶ παρε<supplied cert="high" reason="lost">ί</supplied><unclear cert="high" reason="undefined">λη</unclear><supplied cert="high" reason="lost">φεν</supplied> 

