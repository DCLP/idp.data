    <lb n="7"/>[ <gap reason="illegible" extent="13" unit="character"/> χρυσοῦ <expan>ν]ομισμά<ex cert="high">των</ex></expan> τριῶν <num value="3"/> <expan>εὐστ<ex cert="high">ά</ex>θ<ex cert="high">μων</ex></expan> κατ᾽ ἔτος ὑπὲρ <expan>παραμυθ<ex cert="high">ίας</ex></expan> 

