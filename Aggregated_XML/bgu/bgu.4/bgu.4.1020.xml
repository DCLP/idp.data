    <lb n="3"/><gap reason="lost" extent="20" unit="character"/>τος αὐτο<supplied reason="lost" cert="high">ῦ </supplied><gap reason="lost" extent="6" unit="character"/> μέρους <choice><corr>ἐκ</corr><sic>εγκ</sic></choice> τοῦ <expan>ὁλοκλήρ<ex cert="high">ου</ex></expan> 

    <lb n="4"/><gap reason="lost" extent="20" unit="character"/>ς ἐμφυτευ<supplied reason="lost" cert="high">θ</supplied>έντος μοι παρ᾽ αὐτοῦ λόγῳ 

