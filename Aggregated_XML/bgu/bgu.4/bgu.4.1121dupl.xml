    <lb n="5"/><unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">αι</supplied> <unclear reason="undefined" cert="high">ὅ</unclear> <unclear reason="undefined" cert="high">τε</unclear> <supplied reason="lost" cert="high">Ἱέραξ καὶ Πά</supplied>πος παρὰ τῆς Διουν<supplied reason="lost" cert="high">σίας</supplied> <unclear reason="undefined" cert="high">εἰς</unclear> <unclear reason="undefined" cert="high">ἔ</unclear><supplied reason="lost" cert="high">τ</supplied><unclear reason="undefined" cert="high">η</unclear> [τρία <num value="3"/> ἀπὸ Θῶυθ τοῦ] ἐνεσ<supplied reason="lost" cert="high">τ</supplied>ῶτος 

