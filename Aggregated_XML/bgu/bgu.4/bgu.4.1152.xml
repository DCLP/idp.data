    <lb n="1"/>Πρωτάρχωι 

    <lb n="2"/>παρὰ Στεφάνου τοῦ Καίσαρος καὶ <unclear cert="high" reason="undefined">παρὰ</unclear> <unclear cert="high" reason="undefined">Διοδώρου</unclear> 

    <lb n="3"/>τοῦ Διοδώρου. <space dim="horizontal" extent="unknown" unit="character"/> συγχωρ<supplied cert="high" reason="lost">εῖ</supplied> ὁ Στ<supplied cert="high" reason="lost">έφα</supplied>νος <expan>ἀπ<ex cert="high">εσ</ex></expan>-

    <lb n="4"/>χηκέναι παρὰ τοῦ Διοδώρ<supplied cert="high" reason="lost">ο</supplied>υ <unclear cert="high" reason="undefined">διὰ</unclear> <unclear cert="high" reason="undefined">τῆ</unclear>ς <unclear cert="high" reason="undefined">Ἑλένου</unclear> κολ-

    <lb n="5"/>λυβιστικῆς τραπέζης <add place="supralinear">τὰ</add> <choice><corr>δάνεια</corr><sic>δανηα</sic></choice> ἃ <choice><corr>ἐδάνεισεν</corr><sic>εδανισεν</sic></choice> αὐτῷ 

    <lb n="6"/>κατὰ <choice><corr>συνχωρήσεις</corr><sic>συνχωρησις</sic></choice> τὰς διὰ τοῦ αὐτοῦ κριτηρίου 

    <lb n="7"/>γεγονυίας δύο <num value="2"/> τῶι <num value="8">η</num> <expan><ex cert="high">ἔτει</ex></expan> Καίσαρος μίαν <num value="1"/> μὲν Φα-

    <lb n="8"/>ῶφι <expan>ἀργ<ex cert="high">υρίου</ex></expan> <expan><ex cert="high">δραχμὰς</ex></expan> <num value="300">τ</num>, τὴν δὲ ἑτέραν <unclear cert="high" reason="undefined">Φαμεν</unclear><supplied cert="high" reason="lost">ὼθ</supplied> <expan>ἀργυ<ex cert="high">ρίου</ex></expan> 

    <lb n="9"/><expan><ex cert="high">δραχμὰς</ex></expan> <num value="200">σ</num>, ἀμφοτέρων δὲ τῶν <choice><corr>δανείων</corr><sic>δανηων</sic></choice> καὶ τοὺς 

    <lb n="10"/>τόκους τοῦ ὑπερπεπτωκότος χρόνου, καὶ <unclear cert="high" reason="undefined">εἶν</unclear>αι 

    <lb n="11"/>ἀκύρους τὰς δηλουμένας <choice><corr>συνχωρήσεις</corr><sic>συνχωρησις</sic></choice> σὺν ταῖ<supplied cert="high" reason="lost">ς</supplied> 

    <lb n="12"/>γενηθ<supplied cert="high" reason="lost">είσαις</supplied> <unclear cert="high" reason="undefined">πρὸς</unclear> ἑκάστην αὐτῶν διαγραφαῖς 

    <lb n="13"/><app type="BL"><lem resp="8.42">διὰ Τε<unclear cert="high" reason="undefined">τ</unclear><gap extent="3" reason="illegible" unit="character"/> <unclear cert="high" reason="undefined">τοῦ</unclear></lem><rdg>διά τε <unclear cert="high" reason="undefined">τ</unclear><gap extent="2" reason="illegible" unit="character"/></rdg></app> <unclear cert="high" reason="undefined">ἰδίου</unclear> <add place="supralinear"><gap extent="4" reason="illegible" unit="character"/><gap extent="2" reason="illegible" unit="character"/></add> <unclear cert="high" reason="undefined">καὶ</unclear> Δημητρίου κολλυβιστικῆς 

    <lb n="14"/>τραπέζης, μὴ ἐπελεύσεσθαι δὲ τὸν Στέφα-

    <lb n="15"/>νον μηδ᾽ ἄλλον ὑπὲρ αὐτοῦ ἐπὶ τὸν Διόδω-

    <lb n="16"/>ρον <unclear cert="high" reason="undefined">μηδὲ</unclear> <unclear cert="high" reason="undefined">περὶ</unclear> τῶν αὐτῶν μηδὲ περὶ ἄ<supplied cert="high" reason="lost">λλο</supplied>υ μηδε-

    <lb n="17"/>νὸς ἁ<unclear cert="high" reason="undefined">πλῶ</unclear>ς ἐνγράπτου ἢ ἀγράφου ἀπ<supplied cert="high" reason="lost">ὸ τῶ</supplied>ν ἔ<unclear cert="high" reason="undefined">ν</unclear>-

    <lb n="18"/>προ<supplied cert="high" reason="lost">σθεν</supplied> χρόνων μέχρι τῆς ἐνεστώ<supplied cert="high" reason="lost">σ</supplied>ης ἡμέ-

    <lb n="19"/>ρας ἢ χωρὶς τοῦ κύρια <choice><corr>εἶναι</corr><sic><unclear cert="high" reason="undefined">ιναι</unclear></sic></choice> τὰ σ<supplied cert="high" reason="lost">υ</supplied>νκεχ<supplied cert="high" reason="lost">ωρη</supplied><unclear cert="high" reason="undefined">μένα</unclear> 

    <lb n="20"/>ἐνέχεσθαι αὐτὸν τοῖς τε βλάβεσι <unclear cert="high" reason="undefined">κα</unclear><supplied cert="high" reason="lost">ὶ τῷ</supplied> <unclear cert="high" reason="undefined">ὡρ</unclear><supplied cert="high" reason="lost">ισ</supplied>-

    <lb n="21"/>μένῳ προστίμῳ. <choice><corr>συγχωρεῖ</corr><sic>συνχωρι</sic></choice> δὲ καὶ ὁ Δ<supplied cert="high" reason="lost">ιό</supplied>δω<supplied cert="high" reason="lost">ρος</supplied> 

    <lb n="22"/>ἀνακεκομίσθαι πα<supplied cert="high" reason="lost">ρὰ τ</supplied>οῦ Στεφάνου <unclear cert="high" reason="undefined">ἃ</unclear> <unclear cert="high" reason="undefined">ἔ</unclear><supplied cert="high" reason="lost">δω</supplied>κεν 

    <lb n="23"/>αὐτῷ <supplied cert="high" reason="lost">κ</supplied>α<unclear cert="high" reason="undefined">τὰ</unclear> <unclear cert="high" reason="undefined">τὴν</unclear> <unclear cert="high" reason="undefined">παρὰ</unclear> τοῦ Φαῶφι <unclear cert="high" reason="undefined">μην</unclear><supplied cert="high" reason="lost">ὸς</supplied> <unclear cert="high" reason="undefined">συν</unclear>-

    <lb n="24"/>χώρη<unclear cert="high" reason="undefined">σιν</unclear> <choice><corr>βιβλία</corr><sic>βυβλια</sic></choice> <gap extent="3" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ω</unclear><gap extent="1" reason="lost" unit="character"/><unclear cert="high" reason="undefined">θ</unclear><gap extent="1" reason="lost" unit="character"/>ν <unclear cert="high" reason="undefined">π</unclear>άντ<unclear cert="high" reason="undefined">α</unclear> <supplied cert="high" reason="lost">καὶ ἀντί</supplied>-

    <lb n="25"/>γραφον σ<supplied cert="high" reason="lost">υ</supplied>νχωρή<supplied cert="high" reason="lost">σ</supplied>εως <unclear cert="high" reason="undefined">κ</unclear>ατὰ δ<unclear cert="high" reason="undefined">ούλ</unclear><supplied cert="high" reason="lost">ου, οὗ τὰ</supplied> 

    <lb n="26"/><unclear cert="high" reason="undefined">πιστὰ</unclear> πα<supplied cert="high" reason="lost">ρ</supplied>έδωκ<unclear cert="high" reason="undefined">εν</unclear>. </ab></div><div n="meta.18601" part="N" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.18601" part="N" subtype="textDate" type="commentary"><p><date notAfter="-0010" notBefore="-0011" type="textDate">11 - 10 v.Chr.</date></p></div><div n="meta.18601" part="N" subtype="general" type="commentary"><p>Rückseite: BGU IV  1134, Z. 21 - 26. </p></div><div n="meta.18601" part="N" subtype="mentionedDates" type="commentary"><head>Erwähnte Daten</head><p>Z. 7-8: <date notAfter="-0023-10-27" notBefore="-0023-09-28" type="mentioned">28. Sept.  - 27. Okt. 23 v.Chr.</date></p><p>Z. 7-8: <date notAfter="-0022-03-26" notBefore="-0022-02-25" type="mentioned">25. Febr.  - 26. März 22 v.Chr.</date></p></div><div n="meta.18601" part="N" subtype="principalEdition" type="bibliography"><listBibl><bibl subtype="principal" type="publication"><title level="s" type="abbreviated">BGU</title><biblScope type="volume">4</biblScope><biblScope type="numbers">1152</biblScope></bibl><bibl type="Trismegistos"><biblScope type="numbers">18601</biblScope></bibl><bibl type="DDbDP"><series>0001</series>; <biblScope type="volume">4</biblScope>; <biblScope type="numbers">1152</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0001;4;1152" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.18601" part="N" subtype="corrections" type="bibliography"><head>BL-Einträge nach BL-Konkordanz</head><bibl type="BL"><biblScope type="volume">VIII</biblScope>, S. <biblScope type="pages">42</biblScope></bibl></div><div n="meta.18601" part="N" subtype="illustrations" type="bibliography"><p>keine</p></div><div n="meta.18601" part="N" subtype="locations" type="history"><p><placeName cert="high" type="ancientFindspot">Alexandria</placeName></p