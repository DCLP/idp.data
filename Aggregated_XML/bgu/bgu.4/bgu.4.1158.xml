    <lb n="1"/><expan>Πρωτάρχ<ex cert="high">ῳ</ex></expan> 

    <lb n="2"/>παρὰ Ὤλου <expan>Κορνηλίο<ex cert="high">υ</ex></expan> <expan>Ἰδαίο<ex cert="high">υ</ex></expan> καὶ <expan>π<ex cert="high">αρὰ</ex></expan> Κορνηλίας Τατίας τῆς Κορ-

    <lb n="3"/>νηλίου μετὰ <expan>κ<ex cert="high">υρίου</ex></expan> τοῦ <expan><supplied cert="high" reason="lost">μ</supplied><unclear cert="high" reason="undefined">ητ</unclear>ραδέλ<ex cert="high">φου</ex></expan> <expan>Λευκίο<ex cert="high">υ</ex></expan> Καλλ<unclear cert="high" reason="undefined">ί</unclear>ου <expan><unclear cert="high" reason="undefined">Παί</unclear>ωνο<ex cert="high">ς</ex></expan>. συνχωρεῖ 

    <lb n="4"/>ἡ <expan>Κορνη<ex cert="high">λία</ex></expan> <expan>κομισαμέ<ex cert="high">νη</ex></expan> παρὰ <expan>το<ex cert="high">ῦ</ex></expan> Ὤλου <expan>Κορνη<ex cert="high">λίου</ex></expan> Ἰδαίου ἐν <expan>μησ<ex cert="high">ὶν</ex></expan> ὀκτὼ <num value="8"/> <g rend="extension" type="filler"/> 

    <lb n="5"/>ἀπὸ <expan>Χοι<ex cert="high">ὰκ</ex></expan> τοῦ <expan>ἐνε<ex cert="high">στῶτος</ex></expan> <num value="22">κβ</num> <expan><ex cert="high">ἔτους</ex></expan> <expan>Καίσ<ex cert="high">αρος</ex></expan> ἃς <expan><supplied cert="high" reason="lost">ὀφ</supplied>είλ<ex cert="high">ει</ex></expan> <unclear cert="high" reason="undefined">οὗτος</unclear> <expan>ἀργυ<ex cert="high">ρίου</ex></expan> <expan>Πτολ<ex cert="high">εμαικοῦ</ex></expan> <expan><ex cert="high">δραχμὰς</ex></expan> <expan>ὀγδοήκο<ex cert="high">ντα</ex></expan> <num value="80"/> <expan>ἀτόκ<ex cert="high">ους</ex></expan> εὐθέως 

    <lb n="6"/><expan>ἀντ<supplied cert="high" reason="lost">ι</supplied>παραχωρή<ex cert="high">σειν</ex></expan> αὐτῶι <unclear cert="high" reason="undefined">Ὤλωι</unclear> ἢ οἷς ἐὰν συντάσσῃ διὰ <expan>δημο<ex cert="high">σίου</ex></expan> <choice><corr><expan>ἀρχεί<ex cert="high">ου</ex></expan></corr><sic>αρχη</sic></choice> ἣν 

    <lb n="7"/><expan>παρακεχώρη<ex cert="high">ται</ex></expan> παρ᾽ αὐτοῦ ἡ Κορν<supplied cert="high" reason="lost">η</supplied><unclear cert="high" reason="undefined">λί</unclear><supplied cert="high" reason="lost">α</supplied> <expan>κ<ex cert="high">ατὰ συγ</ex>χώρη<ex cert="high">σιν</ex></expan> διὰ τοῦ <del rend="erasure" status="unremarkable">κριτ</del> κατα-

    <lb n="8"/>λογείου τῶι <num value="21">κα</num> <expan><ex cert="high">ἔτει</ex></expan> <expan>Καί[σ<ex cert="high">αρος</ex>]</expan> <expan>Παῦν<ex cert="high">ι</ex></expan> γῆν <expan>χέρσο<ex cert="high">ν</ex></expan> ἀμπελῖτιν <expan>ἀρο<ex cert="high">υρῶν</ex></expan> πέντε <num value="5"/> 

