    <lb n="9"/>ἑκατὸν <choice><corr>ὀκτώ</corr><sic>οκκτω</sic></choice>, <expan><ex cert="high">γίνονται</ex></expan> <expan><ex cert="high">δραχμαὶ</ex></expan> <num value="108">ρη</num> <gap reason="illegible" extent="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

