    <lb n="1"/><note lang="en" place="not specified">m2?</note>  <expan>κόλ<ex cert="high">λημα</ex></expan> 

    <lb n="2"/><handShift new="m1"/> Πρωτάρχωι 

    <lb n="3"/>παρὰ Φιλάμμονος τοῦ Ἀ<unclear cert="high" reason="undefined">μμω</unclear>νίου καὶ <expan>π<ex cert="high">αρὰ</ex></expan> <expan>Δημη<ex cert="high">τρίου</ex></expan> τοῦ <expan>Σαραπ<ex cert="high">ίωνος</ex></expan> <expan>Φιλομη<ex cert="high">τορείου</ex></expan>. περὶ τῶν <expan>διεστ[αμ<ex cert="high">ένων</ex></expan> συγχω]<unclear cert="high" reason="undefined">ρεῖ</unclear> <expan><unclear cert="high" reason="undefined">Δη</unclear><ex cert="high">μήτριος</ex></expan> 

    <lb n="4"/>μήτε αὐτοὺς μητ᾽ <unclear cert="high" reason="undefined">ἄλλον</unclear> <unclear cert="high" reason="undefined">ὑπὲρ</unclear> <expan><unclear cert="high" reason="undefined">αὐ</unclear><ex cert="high">τῶν</ex></expan> <expan>μηδ<ex cert="high">ένα</ex></expan> ἐπελεύσεσθαι <unclear cert="high" reason="undefined">ἐπὶ</unclear> <unclear cert="high" reason="undefined">τὸν</unclear> <expan>Φιλάμμο<ex cert="high">να</ex></expan> <unclear cert="high" reason="undefined">ὑπὲρ</unclear> ὧν <unclear cert="high" reason="undefined">διαγε</unclear>γραφ<unclear cert="high" reason="undefined">ή</unclear><supplied cert="high" reason="lost">κ</supplied><unclear cert="high" reason="undefined">ασ</unclear><supplied cert="high" reason="lost">ιν</supplied> 

    <lb n="5"/>εἰς <expan>τιμὴ<ex cert="high">ν</ex></expan> χαρτῶν ἐν τῷ <num value="13">ι<unclear cert="high" reason="undefined">γ</unclear></num> <expan><ex cert="high">ἔτει</ex></expan> Καίσαρος <unclear cert="high" reason="undefined">Ἐπαγο</unclear>μένων Δημήτριος δραχμῶν ἑκατὸν <num value="100"/> <unclear cert="high" reason="undefined">διὰ</unclear> <expan><unclear cert="high" reason="undefined">τῆ</unclear><ex cert="high">ς</ex></expan> 

    <lb n="6"/><unclear cert="high" reason="undefined">Διονυσί</unclear>ου <expan><unclear cert="high" reason="undefined">κολ</unclear><ex cert="high">λυβιστικῆς</ex></expan> <expan>τραπ<ex cert="high">έζης</ex></expan>, Πάτρων δὲ <unclear cert="high" reason="undefined">διὰ</unclear> <expan><unclear cert="high" reason="undefined">τῆ</unclear><ex cert="high">ς</ex></expan> <expan><unclear cert="high" reason="undefined">αὐ</unclear><ex cert="high">τῆς</ex></expan> <expan><unclear cert="high" reason="undefined">τραπ</unclear><ex cert="high">έζης</ex></expan> ἐν τῶι αὐτῶι <expan><ex cert="high">ἔτει</ex></expan> 
<expan><ex cert="high"><unclear cert="high" reason="undefined">δραχμὰς</unclear></ex></expan> <num value="200"><unclear cert="high" reason="undefined">σ</unclear></num>, <unclear cert="high" reason="undefined">ὥστ</unclear>᾽ εἶναι <gap extent="4" reason="illegible" unit="character"/> 

    <lb n="7"/>τριακοσίας <num value="300"/> δραχ<supplied cert="high" reason="lost">μὰς</supplied> <gap extent="4" reason="illegible" unit="character"/> <unclear cert="high" reason="undefined">διὰ</unclear> <unclear cert="high" reason="undefined">τὸ</unclear> <unclear cert="high" reason="undefined">ὑπὲρ</unclear> <unclear cert="high" reason="undefined">τῶν</unclear> ἐπὶ τὸ αὐτὸ δραχμῶν τριακοσίων <num value="300"/> <unclear cert="high" reason="undefined">εὐπειθῆ</unclear> 

    <lb n="8"/>γεγονέναι τὸν Δημήτριον <unclear cert="high" reason="undefined">ὑπὸ</unclear> τοῦ <expan>Φιλάμμο<ex cert="high">νος</ex></expan> καὶ <expan>ἀ<ex cert="high">πεσ</ex>χηκ<ex cert="high">έναι</ex></expan> παρ᾽ αὐτοῦ διὰ χειρὸς ἐξ <expan>οἴκ<ex cert="high">ου</ex></expan> τὸ <unclear cert="high" reason="undefined">συνπε</unclear>-

    <lb n="9"/>πεισμένον· εἶναι μὲν αὐτόθεν <expan>ἀκύρο<ex cert="high">υς</ex></expan> τὰς <expan>σημαινομέ<ex cert="high">νας</ex></expan> διαγραφάς, παρέξεσθαι <unclear cert="high" reason="undefined">δὲ</unclear> <unclear cert="high" reason="undefined">καὶ</unclear> <unclear cert="high" reason="undefined">τὸν</unclear> 

    <lb n="10"/><expan>Πάτρων<ex cert="high">α</ex></expan> μὴ ἐπιπορευόμενον <unclear cert="high" reason="undefined">ἐπὶ</unclear> <expan><unclear cert="high" reason="undefined">τὸ</unclear><ex cert="high">ν</ex></expan> <expan>αὐτὸ<ex cert="high">ν</ex></expan> <expan>Φιλάμμο<ex cert="high">να</ex></expan> περὶ τῶν τοῦ <expan>ἀργυ<ex cert="high">ρίου</ex></expan> <expan><ex cert="high">δραχμῶν</ex></expan> διακοσίων <num value="200"/> <supplied cert="high" reason="lost">μη</supplied>-

    <lb n="11"/>δὲ περὶ ἄλλου <expan>μηδενὸ<ex cert="high">ς</ex></expan> <expan>ἁπ<ex cert="high">λῶς</ex></expan> <expan>ἐνγράπ<ex cert="high">του</ex></expan> ἢ ἀγράφου τῶν ἕως <expan>τῆ<ex cert="high">ς</ex></expan> <expan>ἐνεστώση<ex cert="high">ς</ex></expan> ἡμέρας, μηδεμίαν 

    <lb n="12"/>δὲ <expan>Δημη<ex cert="high">τρίῳ</ex></expan> <expan>καταλείπ<ex cert="high">εσθαι</ex></expan> ἔφοδον ἐπὶ <expan>τὸ<ex cert="high">ν</ex></expan> <expan>Φιλάμμο<ex cert="high">να</ex></expan> περὶ <expan>μηδενὸ<ex cert="high">ς</ex></expan> <expan>ὁ<unclear cert="high" reason="undefined">μοίω</unclear><ex cert="high">ς</ex></expan> <expan>ἐγγράπ<ex cert="high">του</ex></expan> ἢ ἀγράφου ἀπὸ 

    <lb n="13"/><expan>τῆ<ex cert="high">ς</ex></expan> <expan>αὐ<unclear cert="high" reason="undefined">τῆ</unclear><ex cert="high">ς</ex></expan> <expan>ἡμέρ<ex cert="high">ας</ex></expan>, τὸν δὲ καὶ <expan>ἐπ[ελ<ex cert="high">ευσόμενον</ex>]</expan> ἐπὶ τὸν <expan>Φιλάμμο<ex cert="high">να</ex></expan> περὶ <expan>τῶ<ex cert="high">ν</ex></expan> <expan>αὐ<ex cert="high">τῶν</ex></expan> αὐτ<unclear cert="high" reason="undefined">ὸν</unclear> <expan>Δημήτριο<ex cert="high">ν</ex></expan> <expan><unclear cert="high" reason="undefined">ἀποσ</unclear>[τή<ex cert="high">σειν</ex>]</expan> 

    <lb n="14"/><expan>παραχρῆ<ex cert="high">μα</ex></expan> τοῖς <expan>ἰδί<ex cert="high">οις</ex></expan> <expan>δαπ<ex cert="high">ανήμασι</ex></expan> καὶ <choice><corr>ἐκτείσειν</corr><sic>εκ<unclear cert="high" reason="undefined">τεισιν</unclear></sic></choice> <unclear cert="high" reason="undefined">αὐτ</unclear>ῷ ὃ ἐὰν πραχθῇ ἢ βλ<supplied cert="high" reason="lost">αβ</supplied>ῇ ὑπό <expan>τινο<ex cert="high">ς</ex></expan> ὁ Φιλάμμων <unclear cert="high" reason="undefined">χ</unclear><supplied cert="high" reason="lost">ά</supplied>-

    <lb n="15"/><supplied cert="high" reason="lost">ρ</supplied><unclear cert="high" reason="undefined">ιν</unclear> <unclear cert="high" reason="undefined">τού</unclear>των σὺν <expan>ἡ<ex cert="high">μιολίᾳ</ex></expan> καὶ τὰ βλάβη καὶ <expan>δα[π<ex cert="high">ανήματα</ex>]</expan> καθάπερ <unclear cert="high" reason="undefined">ἐ</unclear><supplied cert="high" reason="lost">κ<expan>δίκ<ex cert="high">ης</ex></expan>ἢ</supplied> <expan>χωρ<ex cert="high">ὶς</ex></expan> τοῦ κύρια εἶναι τὰ <expan>σ<ex cert="high">υγκε</ex>χ<ex cert="high">ωρημένα</ex></expan> 

    <lb n="16"/>[ἔτι καὶ ἐνέχεσθαι τὸν <expan>παραβ<ex cert="high">αίνοντα</ex></expan> τῷ <expan>ὡρισμ<ex cert="high">ένῳ</ex></expan> <expan>προ<ex cert="high">στίμῳ</ex></expan>] 


