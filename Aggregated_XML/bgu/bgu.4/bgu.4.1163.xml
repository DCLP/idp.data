<expan><ex cert="high"><unclear reason="undefined" cert="high">δραχμὰς</unclear></ex></expan> <num value="200"><unclear reason="undefined" cert="high">σ</unclear></num>, <unclear reason="undefined" cert="high">ὥστ</unclear>᾽ εἶναι <gap reason="illegible" extent="4" unit="character"/> 

