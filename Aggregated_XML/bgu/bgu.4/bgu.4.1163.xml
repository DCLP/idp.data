    <lb n="2"/><handShift new="m1"/> Πρωτάρχωι 

    <lb n="3"/>παρὰ Φιλάμμονος τοῦ Ἀ<unclear reason="undefined" cert="high">μμω</unclear>νίου καὶ <expan>π<ex cert="high">αρὰ</ex></expan> <expan>Δημη<ex cert="high">τρίου</ex></expan> τοῦ <expan>Σαραπ<ex cert="high">ίωνος</ex></expan> <expan>Φιλομη<ex cert="high">τορείου</ex></expan>. περὶ τῶν <expan>διεστ[αμ<ex cert="high">ένων</ex></expan> συγχω]<unclear reason="undefined" cert="high">ρεῖ</unclear> <expan><unclear reason="undefined" cert="high">Δη</unclear><ex cert="high">μήτριος</ex></expan> 

    <lb n="4"/>μήτε αὐτοὺς μητ᾽ <unclear reason="undefined" cert="high">ἄλλον</unclear>  <expan><unclear reason="undefined" cert="high">αὐ</unclear><ex cert="high">τῶν</ex></expan> <expan>μηδ<ex cert="high">ένα</ex></expan> ἐπελεύσεσθαι <unclear reason="undefined" cert="high">ἐπὶ</unclear>  <expan>Φιλάμμο<ex cert="high">να</ex></expan> <unclear reason="undefined" cert="high">ὑπὲρ</unclear> ὧν <unclear reason="undefined" cert="high">διαγε</unclear>γραφ<unclear reason="undefined" cert="high">ή</unclear><supplied reason="lost" cert="high">κ</supplied><unclear reason="undefined" cert="high">ασ</unclear><supplied reason="lost" cert="high">ιν</supplied> 

    <lb n="5"/>εἰς <expan>τιμὴ<ex cert="high">ν</ex></expan> χαρτῶν ἐν τῷ <num value="13">ι<unclear reason="undefined" cert="high">γ</unclear></num> <expan><ex cert="high">ἔτει</ex></expan> Καίσαρος <unclear reason="undefined" cert="high">Ἐπαγο</unclear>μένων Δημήτριος δραχμῶν ἑκατὸν <num value="100"/> <unclear reason="undefined" cert="high">διὰ</unclear> <expan><unclear reason="undefined" cert="high">τῆ</unclear><ex cert="high">ς</ex></expan> 

