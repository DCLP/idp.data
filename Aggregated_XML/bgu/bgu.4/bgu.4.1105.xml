    <lb n="28"/>ἔξοδον ὡς <choice><corr>καθήκει</corr><sic>καθηκι</sic></choice> καὶ <unclear reason="undefined" cert="high">τῷ</unclear> 

    <lb n="29"/><unclear reason="undefined" cert="high">δ</unclear>ὲ <choice><corr>Ἀσκληπι<supplied reason="omitted" cert="high">ά</supplied>δῃ</corr><sic>ασκληπιδη</sic></choice> <choice><corr>μεταδώσει</corr><sic>μεταδωσ<unclear reason="undefined" cert="high">ιν</unclear></sic></choice> 

    <lb n="30"/>ἀντίγραφον τοῦδε τοῦ ὑπομν<supplied reason="lost" cert="high">ή</supplied>-

