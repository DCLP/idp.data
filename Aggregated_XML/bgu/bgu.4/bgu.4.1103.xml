    <lb n="35"/>καὶ <unclear reason="undefined" cert="high">ἐν</unclear> Ἀλεξανδρέαι τοῦ <gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/> <expan>Φαρμο<ex cert="high">ῦθι</ex></expan> 

    <lb n="36"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="2" unit="character"/> πρὸς τὸ βῆμα <app type="BL"><lem resp="1.97"><unclear reason="undefined" cert="high">καὶ</unclear> <expan><unclear reason="undefined" cert="high">ἀπέχ</unclear><ex cert="high">ειν</ex></expan> <gap reason="lost" extent="unknown" unit="character"/></lem><rdg><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="2" unit="character"/></rdg></app> 

