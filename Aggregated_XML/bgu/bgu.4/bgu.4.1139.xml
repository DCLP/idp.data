    <lb n="2"/>παρὰ <expan>Σπινθῆρο<ex cert="high">ς</ex></expan> <expan>το<ex cert="high">ῦ</ex></expan> Ἁρπαήσιος <unclear cert="high" reason="undefined">καὶ</unclear> <expan><unclear cert="high" reason="undefined">τῆ</unclear><ex cert="high">ς</ex></expan> <expan><unclear cert="high" reason="undefined">το</unclear><ex cert="high">ύτου</ex></expan> <expan><unclear cert="high" reason="undefined">γυ</unclear><ex cert="high">ναικὸς</ex></expan> <app type="BL"><lem resp="8.42"><choice><corr>Ἀπολλων<supplied cert="high" reason="omitted">αρ</supplied>ίου</corr><sic>απολλωνι<unclear cert="high" reason="undefined">ου</unclear></sic></choice></lem><rdg>Ἀπολλωνί<unclear cert="high" reason="undefined">ου</unclear></rdg></app> <unclear cert="high" reason="undefined">τῆς</unclear> Λύκου <expan><unclear cert="high" reason="undefined">ἀμ</unclear>φ<unclear cert="high" reason="undefined">ο</unclear><ex cert="high">τέρων</ex></expan> 

