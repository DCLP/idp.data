    <lb n="9"/>μόνο<supplied cert="high" reason="lost">ν ἀ</supplied>λλὰ καὶ πολλάκις <unclear cert="high" reason="undefined">α</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/>σοι <choice><corr>ὅτι</corr><sic>οτει</sic></choice> τὰ <unclear cert="high" reason="undefined">ὠδητα</unclear> παραμεν<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/>μου <gap extent="4" reason="lost" unit="character"/><unclear cert="high" reason="undefined">π</unclear>ον ἐξηρτισμένον <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="12"/>μοι <gap extent="2" reason="lost" unit="character"/>οι<gap extent="2" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ις</unclear> τότε ἔπεμψα <gap extent="unknown" reason="lost" unit="character"/> 

