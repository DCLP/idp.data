    <lb n="34"/><supplied cert="high" reason="lost">μὴ ἐπελεύσεσθ</supplied><unclear cert="high" reason="undefined">αι</unclear> <unclear cert="high" reason="undefined">ἐπὶ</unclear> <unclear cert="high" reason="undefined">τὸ</unclear> <supplied cert="high" reason="omitted">τῶν τοῦ</supplied> <unclear cert="high" reason="undefined">ἀργυρίου</unclear> <expan><ex cert="high"><unclear cert="high" reason="undefined">δραχμῶν</unclear></ex></expan> <unclear cert="high" reason="undefined">ἑκα</unclear>τὸν <unclear cert="high" reason="undefined">εἴκοσι</unclear> <num value="120"/> <expan>κεφάλαι<unclear cert="high" reason="undefined">ο</unclear><ex cert="high">ν</ex></expan> 

