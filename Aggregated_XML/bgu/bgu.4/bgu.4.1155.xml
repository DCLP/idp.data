    <lb n="1"/><expan>κόλ<ex cert="high">λημα</ex></expan> 

    <lb n="2"/><expan>Πρωτάρχ<ex cert="high">ωι</ex></expan> 

    <lb n="3"/>παρὰ Ἀπολλωνίου τοῦ Θέωνος καὶ <supplied reason="lost" cert="high">παρὰ</supplied> 

    <lb n="4"/>Μάρθας τῆς Πρωτάρχου μετὰ κυρίου <supplied reason="lost" cert="high">Ἡρα</supplied>-

    <lb n="5"/>κλίδου τοῦ Ἡρακλίδου. <choice><corr>ἐπεὶ</corr><sic>επι</sic></choice> τυ<supplied reason="lost" cert="high">γ</supplied><unclear reason="undefined" cert="high">χάνει</unclear> <supplied reason="lost" cert="high">Ἀπολλώ</supplied>-

    <lb n="6"/>νιος ἐπιδεδωκὼς Ἀρτεμιδώ<unclear reason="undefined" cert="high">ρωι</unclear> τῷ <unclear reason="undefined" cert="high">ἀρ</unclear><supplied reason="lost" cert="high">χι</supplied>-

    <lb n="7"/>δικαστῇ τῷ ἐνεστῶτι <num value="20"><unclear reason="undefined" cert="high">κ</unclear></num> <expan><ex cert="high">ἔτει</ex></expan> Καίσα<supplied reason="lost" cert="high">ρ</supplied>ος <unclear reason="undefined" cert="high">Ἁθ</unclear><supplied reason="lost" cert="high">ὺρ</supplied> 

    <lb n="8"/>ὑπόμνημα ἀπαίτησιν <unclear reason="undefined" cert="high">ποιούμενος</unclear> 

    <lb n="9"/>παρά τε τῆς Μάρθας κ<supplied reason="lost" cert="high">α</supplied>ὶ ἔτι Πρω-

    <lb n="10"/>τάρχου τοῦ Πρωτάρχο<supplied reason="lost" cert="high">υ</supplied> <expan>ἀργυ<ex cert="high">ρίου</ex></expan> <expan><ex cert="high">δραχμῶν</ex></expan> <num value="200">σ</num> καὶ 

    <lb n="11"/>τόκ<unclear reason="undefined" cert="high">ων</unclear>, ἅσπερ <choice><corr>ὀφείλεσθαι</corr><sic>οφιλεσθαι</sic></choice> αὑτῷ ἔγρα-

    <lb n="12"/><unclear reason="undefined" cert="high">φεν</unclear> ὑπὸ τοῦ μετηλλαχότος τῆς μὲν 

    <lb n="13"/><unclear reason="undefined" cert="high">Μ</unclear>άρθ<unclear reason="undefined" cert="high">ας</unclear> πάτρωνος, τοῦ δὲ Πρωτάρχου 

    <lb n="14"/>πατρὸς Πρωτάρχου τοῦ Πολέμωνος 

    <lb n="15"/>ἀφ᾽ οὗ πρ<unclear reason="undefined" cert="high">οί</unclear>κατο οὗτος πιττα<unclear reason="undefined" cert="high">κί</unclear>ου μεμαρ-

    <lb n="16"/>τυρημένου δὲ δι᾽ ὧν <choice><corr>ἀνήνεγκεν</corr><sic>ανηνενκεν</sic></choice> ὁ Πρώταρ-

    <lb n="17"/>χος συνχωρήσεων, νυνὶ δὲ <unclear reason="undefined" cert="high">εὐπ</unclear><supplied reason="lost" cert="high">ε</supplied>ιθὴς γεγο-

    <lb n="18"/>νὼς <unclear reason="undefined" cert="high">ὑπὸ</unclear> <supplied reason="lost" cert="high">τῆς</supplied> <unclear reason="undefined" cert="high">Μάρθας</unclear> ἐ<supplied reason="lost" cert="high">π</supplied>ὶ <unclear reason="undefined" cert="high">το</unclear><supplied reason="lost" cert="high">ῦ</supplied> <unclear reason="undefined" cert="high">κατ</unclear>᾽ αὐτ<unclear reason="undefined" cert="high">ὴν</unclear> -

    <lb n="19"/>ρους ἡμίσους καὶ ἀπεσχ<supplied reason="lost" cert="high">ηκ</supplied>ὼς <unclear reason="undefined" cert="high">παρ</unclear>᾽ <unclear reason="undefined" cert="high">αὐτῆ</unclear><supplied reason="lost" cert="high">ς</supplied> 

    <lb n="20"/>διὰ <choice><corr>χειρὸς</corr><sic>χιρος</sic></choice> ἐξ οἴκου τὸ <supplied reason="lost" cert="high">ἐν αὐτῇ μέρος ἥμισυ</supplied> 

    <lb n="21"/><choice><corr>συνχωρεῖ</corr><sic>συνχωρι</sic></choice> Ἀπολλώνιος <supplied reason="lost" cert="high">μήτ᾽ αὐτὸς μήτ᾽ ἄλλος ὑπὲρ</supplied> 

    <lb n="22"/>αὐτοῦ <choice><corr>μηδεὶς</corr><sic>μηδις</sic></choice> <unclear reason="undefined" cert="high">ἐπελεύσε</unclear><supplied reason="lost" cert="high">σθαι ἐπ᾽ αὐτὴν μηδ᾽ ἐπὶ</supplied> 

    <lb n="23"/>τὰ ὑπὸ τοῦ τετελ<supplied reason="lost" cert="high">ε</supplied>υτ<supplied reason="lost" cert="high">ηκότος πάτρωνος Πρωτάρ</supplied>-

    <lb n="24"/><unclear reason="undefined" cert="high">χ</unclear>ου <choice><corr>ἀπολελειμμένα</corr><sic>απολελιμμ<unclear reason="undefined" cert="high">ενα</unclear></sic></choice> πε<supplied reason="lost" cert="high">ρὶ τοῦ κατὰ τὴν</supplied> 

    <lb n="25"/><unclear reason="undefined" cert="high">Μ</unclear>άρθ<unclear reason="undefined" cert="high">αν</unclear> μέρους ἡμίσους <supplied reason="lost" cert="high">τοῦ κεφαλαίου</supplied> 

    <lb n="26"/><unclear reason="undefined" cert="high">χει</unclear>  τόκων <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="27"/>μήτε περὶ τῶν <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> μήτε περὶ</supplied> 

    <lb n="28"/>ἄλλου μηδενὸ<supplied reason="lost" cert="high">ς ἁπλῶς ὀφειλήματος</supplied> 

    <lb n="29"/>ἢ ἀπαιτήματ<supplied reason="lost" cert="high">ος ἢ ἐγγράπτου ἢ ἀγρά</supplied>-

    <lb n="30"/>φου πράγματος <unclear reason="undefined" cert="high">τῶ</unclear><supplied reason="lost" cert="high">ν γενομένων ἀπὸ τῶν</supplied> 

    <lb n="31"/>ἔμπροσθεν χρόν<supplied reason="lost" cert="high">ων μέχρι τῆς ἐνεσ</supplied>-

    <lb n="32"/>τώσης ἡμ<supplied reason="lost" cert="high">έρας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="33"/>δη<unclear reason="undefined" cert="high">λω</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="34"/>την<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="35"/>τὰ συνκ<supplied reason="lost" cert="high">εχωρημένα ἐνέχεσθαι τὸν</supplied> 

    <lb n="36"/>παρ<supplied reason="lost" cert="high">αβαίνοντα τοῖς τε βλάβεσι καὶ τῷ</supplied> 

    <lb n="37"/><unclear reason="undefined" cert="high">ὡρισ</unclear><supplied reason="lost" cert="high">μένῳ προστίμῳ, μὴ ἐλαττουμένου τοῦ</supplied> 

    <lb n="38"/>Ἀπο<unclear reason="undefined" cert="high">λλ</unclear><supplied reason="lost" cert="high">ωνίου ἐν τῇ ἐπὶ τὸν Πρώταρχον</supplied> 

    <lb n="39"/><unclear reason="undefined" cert="high">ἐφόδῳ</unclear> <supplied reason="lost" cert="high">περὶ τοῦ ἑτέρου τοῦ κεφαλαίου</supplied> 

    <lb n="40"/><unclear reason="undefined" cert="high">μέρου</unclear><supplied reason="lost" cert="high">ς ἡμίσους τῶν τοῦ <expan>ἀργυ<ex cert="high">ρίου</ex></expan> <expan><ex cert="high">δραχμῶν</ex></expan> <num value="100">ρ</num> καὶ</supplied> 

    <lb n="41"/><unclear reason="undefined" cert="high">τῶν</unclear> <supplied reason="lost" cert="high">τούτων τόκων </supplied><gap reason="lost" extent="unknown" unit="character"/> 

