    <lb n="1"/><supplied cert="high" reason="lost">Πρωτάρχ</supplied>ωι 

    <lb n="2"/><supplied cert="high" reason="lost">παρὰ Σα</supplied><unclear cert="high" reason="undefined">ραπί</unclear><supplied cert="high" reason="lost">ω</supplied>νος τοῦ Διοδώρου Ἀλθαιέω<supplied cert="high" reason="lost">ς καὶ παρὰ Πυλαι</supplied>μέν<unclear cert="high" reason="undefined">ου</unclear> <unclear cert="high" reason="undefined">τοῦ</unclear> 

    <lb n="3"/><supplied cert="high" reason="lost">Ἀκρ</supplied>άτου Καίσαρ<supplied cert="high" reason="lost">ο</supplied>ς παρόντος καὶ <unclear cert="high" reason="undefined">συνευδοκ</unclear><supplied cert="high" reason="lost">οῦντος τῇδε τῇ συνχωρ</supplied>ήσει 

    <lb n="4"/><supplied cert="high" reason="lost">Γαίου Ἰουλίου Ἑρ</supplied>μίου <add place="supralinear"><gap extent="4" reason="illegible" unit="character"/> τοῦ <unclear cert="high" reason="undefined">δεσπότου</unclear> <supplied cert="high" reason="lost">Ἀ</supplied><unclear cert="high" reason="undefined">κράτου</unclear> <unclear cert="high" reason="undefined">τοῦ</unclear> <unclear cert="high" reason="undefined">Καίσαρος</unclear></add> κεχ<supplied cert="high" reason="lost">ρηματικότος δὲ Ἑρμίου τοῦ Πρίσκου. περὶ τῶν διεσ</supplied>-

    <lb n="5"/><unclear cert="high" reason="undefined">ταμένων</unclear> <supplied cert="high" reason="lost">συν</supplied>χωρεῖ ὁ Πυλαιμένης <supplied cert="high" reason="lost">εὐπειθὴς κα</supplied>τὰ πᾶν γεγονὼς 

    <lb n="6"/><unclear cert="high" reason="undefined">ὑπὸ</unclear> <unclear cert="high" reason="undefined">τοῦ</unclear> Σαρ<supplied cert="high" reason="lost">απί</supplied>ωνος καὶ ἀπεσχηκὼς <supplied cert="high" reason="lost">παρ᾽ αὐτοῦ διὰ χειρὸς ἐξ</supplied> οἴκου τὸ ἑστα-

    <lb n="7"/><supplied cert="high" reason="lost">μένον</supplied> παραχωρητικὸν παρακεχωρ<unclear cert="high" reason="undefined">η</unclear><supplied cert="high" reason="lost">κέναι αὐτῷ τοὺς ὑπάρχοντας αὐτῷ</supplied> 

    <lb n="8"/>ἐν κώμῃ Ὑ<supplied cert="high" reason="lost">ψ</supplied>ηλῇ τοῦ Λυκοπολεί<supplied cert="high" reason="lost">του τῆς Θηβαίδος ἐκ τ</supplied>οῦ πρὸς 

    <lb n="9"/>ἀπηλι<supplied cert="high" reason="lost">ώτην μέρους τόπ</supplied>ους <supplied cert="high" reason="lost">ἐν οἷς οἰκία καὶ</supplied> βαλανεῖον δημόσι<supplied cert="high" reason="lost">ο</supplied>ν 

    <lb n="10"/><supplied cert="high" reason="lost">πρότερον Διογ</supplied>ένους τοῦ <unclear cert="high" reason="undefined">Ἰσιδώρου</unclear>, ὧν <supplied cert="high" reason="lost">γείτον</supplied>ες νότωι δρόμος 

    <lb n="11"/>τοῦ ἱεροῦ Χ<supplied cert="high" reason="lost">νο</supplied>ύβεως θεοῦ μεγίστου βορρ<supplied cert="high" reason="lost">ᾶι προ</supplied>βατ<supplied cert="high" reason="lost">ι</supplied>ῶνες <unclear cert="high" reason="undefined">τοῦ</unclear> 

    <lb n="12"/>Ἀμενώθο<supplied cert="high" reason="lost">υ</supplied> ἀπηλιώτηι ὕδρευμα λιβὶ <supplied cert="high" reason="lost">κοι</supplied>νὴ εἴσοδος καὶ ἔξοδος 

    <lb n="13"/>τῶν τόπων, καὶ ἀπὸ τοῦ νῦν κρατεῖν καὶ κυρι<supplied cert="high" reason="lost">εύει</supplied>ν τὸν Σαραπίωνα 

    <lb n="14"/><unclear cert="high" reason="undefined">τῶν</unclear> <unclear cert="high" reason="undefined">παρακεχ</unclear><supplied cert="high" reason="lost">ωρη</supplied>μένων αὐτῶι καὶ <unclear cert="high" reason="undefined">ἐ</unclear>μβατεύειν <supplied cert="high" reason="lost">καὶ ἐποι</supplied>κοδομεῖν καὶ 

    <lb n="15"/>ἐγμισθοῦν <supplied cert="high" reason="lost">καὶ</supplied> ἑτέροις παραχωρεῖν καὶ χρῆσθ<supplied cert="high" reason="lost">αι καὶ</supplied> διοικεῖν καὶ <unclear cert="high" reason="undefined">ἐπιτελεῖν</unclear> 

    <lb n="16"/>περὶ αὐτῶν ὃ ἐ<supplied cert="high" reason="lost">ὰν βο</supplied>ύληται, μηδεμίαν δ᾽ εἶναι ἔφ<supplied cert="high" reason="lost">ο</supplied>δον τῷ <expan>Πυλαιμέ<ex cert="high">νῃ</ex></expan> 

    <lb n="17"/>μηδ᾽ ἄλλῳ ὑ<supplied cert="high" reason="lost">πὲρ</supplied> αὐτοῦ ἐπὶ τὸν Σαραπίωνα <unclear cert="high" reason="undefined">τὸν</unclear> <expan>παρα<unclear cert="high" reason="undefined">κεχωρ</unclear><supplied cert="high" reason="lost">η</supplied><unclear cert="high" reason="undefined">μέ</unclear><ex cert="high">νον</ex></expan> <supplied cert="high" reason="lost">μ</supplied><unclear cert="high" reason="undefined">ηδ</unclear>᾽ <supplied cert="high" reason="lost">ἐ</supplied>π<unclear cert="high" reason="undefined">ὶ</unclear> 

    <lb n="18"/>μέρος αὐτῶν <supplied cert="high" reason="lost">μ</supplied>ηδ᾽ ἐπὶ τὰ ἐξ αὐτῶν περιγινόμενα ἀπ<supplied cert="high" reason="lost">ὸ</supplied> μηδενὸς 

    <lb n="19"/>ἁπλῶς ἐνγ<supplied cert="high" reason="lost">ρά</supplied>πτου ἢ ἀγράφου κατὰ μηδένα τρόπον, ἐξακολουθεῖν 

    <lb n="20"/>δ᾽ αὐτῶι τὴν βεβαίωσιν διὰ παντὸς ἀπὸ πάντων καὶ ἀφισ<supplied cert="high" reason="lost">τάν</supplied>ειν τὸν 

    <lb n="21"/>ἐπελευ<supplied cert="high" reason="lost">σό</supplied>μενον τοῖς ἰδίοις δαπανήμασιν, <app type="BL"><lem resp="6.15"><seg cert="low" part="N"><unclear cert="low" reason="undefined">ὑπακοῦσ</unclear>αι</seg></lem><rdg><unclear cert="high" reason="undefined">υπ</unclear><gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="illegible" unit="character"/>αι</rdg></app> δ᾽ αὐτὸν καὶ αἷς 

    <lb n="22"/>ἐὰν ἄλλαις <supplied cert="high" reason="lost">προ</supplied>αιρῆται ὁ Σαραπίων περὶ τῶν αὐτῶν καὶ διὰ τῶν ἐπὶ <unclear cert="high" reason="undefined">τῶν</unclear> 

    <lb n="23"/>τόπ<supplied cert="high" reason="lost">ων</supplied> <unclear cert="high" reason="undefined">ἀ</unclear>ρχεί<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">ν</supplied> οἰκονομίαις <unclear cert="high" reason="undefined">μηδὲν</unclear> ἕτερον λαβόντα τῶν δαπα-

    <lb n="24"/>νημάτων ὄντων πρὸς τὸν Σαραπίωνα, ἢ χωρὶς τοῦ κύρια εἶναι τὰ συν-

    <lb n="25"/><supplied cert="high" reason="lost">κεχωρημέ</supplied>να <unclear cert="high" reason="undefined">ἐ</unclear><supplied cert="high" reason="lost">κτ</supplied><unclear cert="high" reason="undefined">ί</unclear>νειν αὐτὸν <unclear cert="high" reason="undefined">ὅ</unclear> <unclear cert="high" reason="undefined">τε</unclear> <add place="supralinear"><unclear cert="high" reason="undefined">ὁ</unclear> <expan><unclear cert="high" reason="undefined">Πυλαι</unclear>μέ<ex cert="high">νης</ex></expan></add> εἴληφεν παραχωρητικὸν σὺν ἡ-

    <lb n="26"/><unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ιολίᾳ κ</supplied>αὶ τὰ βλάβη καὶ δαπανήματα καὶ ἄλλας <expan>ἀρ<supplied cert="high" reason="lost">γυ<ex cert="high">ρίου</ex></supplied></expan> <expan><ex cert="high">δραχμὰς</ex></expan> <num value="1000">Α</num> καὶ τὸ ὡρι<supplied cert="high" reason="lost">σμένον</supplied> 

    <lb n="27"/><choice><corr>πρόστιμον</corr><sic>προστε<supplied cert="high" reason="lost">ι</supplied>μον</sic></choice> καθάπερ <choice><corr>ἐκ</corr><sic>εγ</sic></choice> δίκης, ἀνακεκομίσθα<supplied cert="high" reason="lost">ι</supplied> δὲ ὁ Σαραπίων 

    <lb n="28"/>παρὰ τοῦ Πυλαιμένου ἃς εἶχεν κατὰ τῶν προκειμένων ἀσφαλ<unclear cert="high" reason="undefined">είας</unclear> 

    <lb n="29"/><app type="BL"><lem resp="1.98"><unclear cert="high" reason="undefined">κατὰ</unclear> <unclear cert="high" reason="undefined">μηδὲν</unclear></lem><rdg><unclear cert="high" reason="undefined">κ</unclear><gap extent="4" reason="illegible" unit="character"/><gap extent="3" reason="illegible" unit="character"/></rdg></app> <gap extent="4" reason="illegible" unit="character"/><gap extent="6" reason="lost" unit="character"/><unclear cert="high" reason="undefined">εν</unclear>ου τοῦ Σαραπίωνος <unclear cert="high" reason="undefined">ἐν</unclear> <unclear cert="high" reason="undefined">τῇ</unclear> <unclear cert="high" reason="undefined">ἐπὶ</unclear> τὸν Γάιον 

