    <lb n="1"/>γεινώσκειν σὲ θ<unclear reason="undefined" cert="high">έ</unclear><supplied reason="lost" cert="high">λω ὅτι    </supplied><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/> 

    <lb n="2"/>μετέλαβον τὰ <unclear reason="undefined" cert="high">κ</unclear>α<unclear reason="undefined" cert="high">τ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>ηκες μὴ γράψας τ<unclear reason="undefined" cert="high">α</unclear><gap reason="lost" extent="unknown" unit="character"/> ἐρωτῶ σε 

    <lb n="4"/>οὖν, μῆ<supplied reason="lost" cert="high">τε</supplied>ρ, <choice><corr>γράψεις</corr><sic>γραψις</sic></choice> <unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">οι</supplied> τὰ <unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ε</supplied><unclear reason="undefined" cert="high">ρὶ</unclear> σεαυτῆς. 

    <lb n="5"/>τί πέπραχες εἰς Μέμφ<unclear reason="undefined" cert="high">ιν</unclear>; μὴ ἄ<unclear reason="undefined" cert="high">λ</unclear>λως 

    <lb n="6"/>ποιήσεις· γείνωσκε ὅτι Γέμελλος 

    <lb n="7"/>ἐστρά<unclear reason="undefined" cert="high">τ</unclear>ε<unclear reason="undefined" cert="high">υ</unclear>ται εἰς τὴν κλάσσαν καὶ ἔλεγε 

    <lb n="8"/>μοι ὅτι ἡ μήτηρ σοῦ δικάζεται <choice><corr>ἐν</corr><sic>εις</sic></choice> 

    <lb n="9"/>Μέμφει μετὰ τοῦ πατρὸς σοῦ 

    <lb n="10"/>ἐπὶ τῷ ἀρχιδικαστ<supplied reason="lost" cert="high">ῇ</supplied>. καλῶς ποιή<supplied reason="lost" cert="high">σεις</supplied>, 

    <lb n="11"/>μῆτερ, κομισαμέν<supplied reason="lost" cert="high">η</supplied> <unclear reason="undefined" cert="high">μ</unclear>ου <unclear reason="undefined" cert="high">τ</unclear>αύ<unclear reason="undefined" cert="high">την</unclear> 

    <lb n="12"/>τὴν ἐπιστολὴν <del rend="erasure" status="unremarkable"><gap reason="illegible" extent="2" unit="character"/>α<gap reason="illegible" extent="1" unit="character"/>λα<gap reason="illegible" extent="1" unit="character"/></del> <choice><corr>πέμψεις</corr><sic>πεμψις</sic></choice> <unclear reason="undefined" cert="high">μοι</unclear> 

    <lb n="13"/>Γερμανὸν <choice><corr>ἐπεὶ</corr><sic>επι</sic></choice> λέγε<unclear reason="undefined" cert="high">τα</unclear>ι ὅτι ἡ χώ<unclear reason="undefined" cert="high">ρτη</unclear> 

    <lb n="14"/>ἡμῶν εἰς τὴν Μαυριταν<unclear reason="undefined" cert="high">ί</unclear><supplied reason="lost" cert="high">α</supplied>ν ὑ<unclear reason="undefined" cert="high">πάγει</unclear>. 

    <lb n="15"/>μέχρι σήμερον ἡμέρας ἐξέρχεται 

    <lb n="16"/>ἡ ἄλη τῶν Μαύρων καὶ ἡ χώρτη. 

    <lb n="17"/>τῶν Ἀφρώρω<supplied reason="lost" cert="high">ν</supplied>. ἐξαναπληροῦται 

    <lb n="18"/>ἐκείνη ἡ χώρτη ἐκ τῆς ἡμῶν <expan>χώρτη[<ex cert="high">ς</ex></expan>]. 

    <lb n="19"/>ἀπὸ τριῶν <num value="3"/> ἐτῶν ἐξέρχονται. τὸ λοι-

    <lb n="20"/><supplied reason="lost" cert="high">πὸ</supplied>ν οὖν, μῆτ<supplied reason="lost" cert="high">ερ,  </supplied><gap reason="lost" extent="4" unit="character"/><gap reason="illegible" extent="3" unit="character"/>σε<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="2" unit="character"/>ρ<unclear reason="undefined" cert="high">ε</unclear><gap reason="lost" extent="4" unit="character"/> 

    <lb n="21"/>εὔχομα<supplied reason="lost" cert="high">ι  </supplied><gap reason="lost" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/>ο<unclear reason="undefined" cert="high">λ</unclear><gap reason="illegible" extent="2" unit="character"/>ς <expan>ἄσπασο<ex cert="high">ν</ex></expan> 

    <lb n="22"/>Οὐαλέριο<unclear reason="undefined" cert="high">ν</unclear> <unclear reason="undefined" cert="high">καὶ</unclear> Ἑρμανι<gap reason="illegible" extent="3" unit="character"/>ν κ<unclear reason="undefined" cert="high">αὶ</unclear> τοὺς 

    <lb n="23"/>ἐν οἴκῳ <unclear reason="undefined" cert="high">τ</unclear><gap reason="lost" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/>σ<gap reason="illegible" extent="2" unit="character"/>μαα<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="24"/>δεύτερα  <gap reason="illegible" extent="unknown" unit="character" desc="vestiges"/> 

    <lb n="25"/>τη<unclear reason="undefined" cert="high">ν</unclear>   <gap reason="illegible" extent="unknown" unit="character" desc="vestiges"/> 

    <lb n="ms,1" rend="perpendicular"/> <gap reason="illegible" extent="2" unit="character"/> ἐπεξέρχ<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="2" unit="character"/>ι ἃ δηλώσω σοὶ δι᾽ ἐπιστολῆς εισι<gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/> </ab></div><div n="meta.26497" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.26497" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" notBefore="101" notAfter="200">II</date></p></div><div n="meta.26497" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Vgl. Kongr. XXIII, S. 87-93. Zu Z. 22-23 vgl. APF 53, 2007, S. 196f.</p></div><div n="meta.26497" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">BGU</title><biblScope type="volume">15</biblScope><biblScope type="numbers">2492</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">26497</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0001</series>; <biblScope type="volume">15</biblScope>; <biblScope type="numbers">2492</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0001;15;2492" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.26497" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p><bibl type="illustration" default="NO">Microfiche im Band</bibl></p></div><div n="meta.26497" type="bibliography" subtype="otherPublications" org="uniform" part="N" sample="complete"><head>Andere Publikation</head><bibl type="publication" subtype="other" default="NO">P.Coll. Youtie I 5