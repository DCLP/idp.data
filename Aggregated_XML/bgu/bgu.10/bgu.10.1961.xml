    <lb n="1"/><supplied cert="high" reason="lost">καὶ θεῶν</supplied> 

    <lb n="2"/><supplied cert="high" reason="lost">Εὐεργ</supplied><unclear cert="high" reason="undefined">ε</unclear>τῶν καὶ θ<unclear cert="high" reason="undefined">ε</unclear>ῶν Φιλοπατόρων κ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">νηφόρου Ἀρσινόης Φιλαδέλφου Ἀγαθοκλέας</supplied> 

    <lb n="3"/><supplied cert="high" reason="lost">τῆς Διογ</supplied>νήτου μηνὸς Δίου ἐν Θώλθε<unclear cert="high" reason="undefined">ι</unclear> τοῦ <unclear cert="high" reason="undefined">Ὀ</unclear><supplied cert="high" reason="lost">ξυρυγχίτου νομοῦ. ἐδάνεισεν Θεόδωρος </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/><gap extent="2" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Κυ</supplied>ρηναῖος τῆς ἐπι<add place="supralinear">γο</add>νῆς Μενωνίδει Πέρσηι τῶν Φί<unclear cert="high" reason="undefined">λ</unclear><supplied cert="low" reason="lost">ωνος ἰδιώτηι ἀργυρίου</supplied> 

    <lb n="5"/>νομί<unclear cert="high" reason="undefined">σ</unclear>ματος ὀφθαλμ<unclear cert="high" reason="undefined">ο</unclear>φανοῦς ἐναντίον τῶν ἐγγεγραμ<unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ένων μαρτύρων δραχμὰς</supplied> 

    <lb n="6"/>ἑκατὸν <num value="100"/> τόκου ὡς <choice><corr>ἐκ</corr><sic>εγ</sic></choice> δύο <num value="2"/> δραχμῶν <choice><corr>τὴν</corr><sic>τημ</sic></choice> μνᾶν τὸν <unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ῆνα ἕκαστον. ἀποδότω</supplied> 

    <lb n="7"/>δὲ Μενωνίδης Θεοδώρωι τοὺς μὲν τόκους κατὰ μῆ<supplied cert="high" reason="lost">να, τὸ δὲ δάνειον τοῦτο</supplied> 

    <lb n="8"/><unclear cert="high" reason="undefined">ἀφ</unclear>᾽ <unclear cert="high" reason="undefined">ἧ</unclear><supplied cert="high" reason="lost">ς</supplied> ἂν ἡμέρας Θεόδωρος Μενωνίδει προείπῃ ἐν ἡμέραι<unclear cert="high" reason="undefined">ς</unclear> <supplied cert="high" reason="lost">δέκα <num value="10"/> <foreign lang="la">?.</foreign> ἐὰν δὲ μὴ</supplied> 

    <lb n="9"/>ἀποδῶι κατὰ τὰ γεγραμμένα, ἀποτεισάτω Μ<unclear cert="high" reason="undefined">ε</unclear>νων<supplied cert="high" reason="lost">ίδης Θεοδώρωι τοὺς μὲν</supplied> 

    <lb n="10"/><supplied cert="high" reason="lost">τόκο</supplied><unclear cert="high" reason="undefined">υ</unclear>ς ο<unclear cert="high" reason="undefined">ὓ</unclear>ς ἂν προσοφειλήσῃ ἁπλοῦς, τὸ δὲ δάνειον ἡ<unclear cert="high" reason="undefined">μ</unclear>ι<unclear cert="high" reason="undefined">ό</unclear><supplied cert="high" reason="lost">λιον καὶ ἡ πρᾶξις</supplied> 

    <lb n="11"/><supplied cert="high" reason="lost">ἔστω</supplied> Θεοδώρωι παρὰ Μενωνίδους καὶ ἐκ τοῦ ἐγγύου πράσσοντι κα<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ὰ τὸ διά</supplied>-

