    <lb n="27"/>γράμματα. <supplied cert="high" reason="lost">ἡ </supplied><choice><corr><supplied cert="high" reason="lost">σ</supplied>υγγραφὴ</corr><sic><supplied cert="high" reason="lost">σ</supplied><unclear cert="high" reason="undefined">υγ</unclear><supplied cert="high" reason="lost">γρα</supplied>φηι</sic></choice> κυρία <choice><corr>ἔστω</corr><sic>εστωι</sic></choice> πανταχῇ <choice><corr>ἐπιφέρουσα</corr><sic>επιφερομενηι</sic></choice> καὶ 

    <lb n="28"/>πα<gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/>σ<gap extent="6" reason="lost" unit="character"/>γει ἡι <unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">υγ</supplied>γραφῆι κυρία <choice><corr>ἔστω</corr><sic>εστωι</sic></choice> πανταχῇ <choice><corr>ἐπιφέρουσα</corr><sic>επιφερων</sic></choice> 

    <lb n="29"/><handShift new="m2"/> Ὀρσενο<supplied cert="high" reason="lost">ῦ</supplied>φι<supplied cert="high" reason="lost">ς καὶ Ἰ</supplied>σίων <supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ἱ</unclear> <num value="2"><unclear cert="high" reason="undefined">β</unclear></num> Πάπου μεμισθώμεθα τὰς προκει-

