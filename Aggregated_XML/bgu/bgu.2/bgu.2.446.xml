    <lb n="1"/><supplied cert="high" reason="lost">ἔτους </supplied><gap extent="2" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Μάρκου Αὐρηλίου Ἀντωνίν</supplied>ου Σεβαστ<supplied cert="high" reason="lost">οῦ Ἀρμενιακοῦ </supplied><app type="BL"><lem resp="8.27"><supplied cert="high" reason="lost">Πα</supplied>ρθικο<supplied cert="high" reason="lost">ῦ Μηδικοῦ μεγίστου</supplied></lem><rdg><supplied cert="high" reason="lost">Πα</supplied>ρθικο<supplied cert="high" reason="lost">ῦ </supplied><gap extent="unknown" reason="lost" unit="character"/></rdg></app> [ <gap extent="unknown" reason="illegible" unit="character"/> ἐν <app type="BL"><lem>Νείλου πόλει] 

    <lb n="2"/><supplied cert="high" reason="lost">τῆς</supplied></lem><rdg>Παρεμ<supplied cert="high" reason="lost">βολῇ</supplied></rdg></app> <supplied cert="high" reason="lost">Πολέμωνος μερίδος τοῦ Ἀρσινο</supplied>είτου νομο<supplied cert="high" reason="lost">ῦ. ὁμολογεῖ Σωτηρία Εἰρ</supplied>ηναίο<supplied cert="high" reason="lost">υ τοῦ Ἀπολλωνίου ὡς ἐτῶν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/><gap extent="10" reason="lost" unit="character"/><supplied cert="high" reason="lost"> μετὰ κυρίου τοῦ καὶ</supplied> ἑαυτῆς <hi rend="diaeresis">υ</hi>ἱοῦ Εἰρη<supplied cert="high" reason="lost">ναίου </supplied><gap extent="1" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/>στ<gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/>ω ὡς ἐτῶ<supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Στοτοήτι Ἀγ</supplied>-

    <lb n="4"/>χώ<supplied cert="high" reason="lost">φιος τοῦ </supplied><gap extent="7" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἱερεῖ Σοκνοπα</supplied>ίου θεοῦ μεγάλου <supplied cert="high" reason="lost">μεγ</supplied>άλου ὡς ἐτῶν εἴκο[σι <num value="20"/> <gap extent="unknown" reason="illegible" unit="character"/> ἀπέχειν αὐτὴν παραχρῆμα] 

    <lb n="5"/>διὰ χ<supplied cert="high" reason="lost">ειρὸς ἐξ οἴκου δραχμὰς π</supplied>εντακοσίας <num value="500"/> ἀραβῶνα <choice><corr>ἀναπόρριφον</corr><sic>αναποριφον</sic></choice> ἀπὸ τῆς <supplied cert="high" reason="lost">τιμῆς ἀργυρίου δραχμῶν ὀκτακοσί</supplied>-

    <lb n="6"/>ων <num value="800"/> τῶ<supplied cert="high" reason="lost">ν ὑπαρχόντων αὐτῇ</supplied> ἐν μὲν ἐποικίῳ Πισᾶι περὶ Ἡρακλίαν τῆς Θεμ[ίστου μερίδος τρίτον μέρος αὐλῆς καὶ <choice><corr>ἄρου]-

    <lb n="7"/>ραν</corr><sic><supplied cert="high" reason="lost">αρου</supplied>λαν</sic></choice> μ[ίαν ἥμισυ <num value="1  1/2 "/> κ]αὶ περὶ τὴν αὐτὴν <choice><corr>Ἡρακλίαν</corr><sic>ηραρακλιαν</sic></choice> κλήρου ἄρουραν μίαν ἥμ[ισυ <num value="1  1/2 "/> <gap extent="unknown" reason="illegible" unit="character"/> ] 

    <lb n="8"/>τίωνος <gap extent="8" reason="lost" unit="character"/>χου τῷ δευτέρῳ καὶ εἰκοστῷ <choice><corr>ἔτει</corr><sic>ετι</sic></choice> θεοῦ Αἰλίου Ἀντωνίνου μηνὶ <gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> πα</supplied>-

    <lb n="9"/><unclear cert="high" reason="undefined">τρικὸν</unclear> ἀ<supplied cert="high" reason="lost">ρού</supplied>ρης τέταρτον <num value=" 1/4 "/>, ὧν γείτονες τοῦ μὲν τρίτου μέρους τῆς αὐλῆς νό<supplied cert="high" reason="lost">του </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/>τῆς προγεγραμμένης καὶ ἀποδομένης Σωτηρίας, λιβὸς <choice><corr>ψιλὸς</corr><sic>ψειλος</sic></choice> τόπος καὶ τῆς Κε<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/>ἑτέρας Σωτηρίας, ἀπηλιώτου διῶρυξ, λιβὸς τῆς προγεγραμμένης καὶ τῆς ἀρο<supplied cert="high" reason="lost">ύ</supplied>ρ[ας μιᾶς <unclear cert="high" reason="undefined">ἥμισυ</unclear> <num value="1  1/2 "/> σιτικὰ] 

    <lb n="12"/>ἐδάφη, λιβὸς ὑδραγωγός, μεθ᾽ ἣν δημοσία ὁδός, ἀπηλιώτου ἑτέρων ἐδάφη, μ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/>ας τετάχθαι, κ<supplied cert="high" reason="lost">α</supplied>θὼς ἐκ συνφώνου <hi rend="diaeresis">ὑ</hi>πηγόρευσαν, καὶ <choice><corr>εἰς</corr><sic>ις</sic></choice> τὴν Σωτηρίδα παρα<supplied cert="high" reason="lost">κεχωρῆσθαι </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="14"/>εἶναι ἐν κατοικικῇ <choice><corr>τάξει</corr><sic>ταξι</sic></choice>, ἃ καὶ καταγράψει ἡ Σωτηρία τῷ Στοτοήτι, ὁπότε ἐὰ[ν <app type="BL"><lem resp="1.46">αἱρῆται <gap extent="unknown" reason="illegible" unit="character"/> καθαρὰ ἀπὸ]</lem><rdg><supplied cert="high" reason="lost">αἱρῆται </supplied><gap extent="unknown" reason="lost" unit="character"/></rdg></app> <supplied cert="high" reason="lost">δη</supplied>-

    <lb n="15"/>μοσίου ἢ ἰδιωτικοῦ καὶ μήπου πεπραμένα μηδὲ ὑποτε<supplied cert="high" reason="lost">θ</supplied>ειμένα <app type="BL"><lem resp="6.11"><seg cert="low" part="N">δι<unclear cert="high" reason="undefined">ὰ</unclear> [μηδενὸς ἀρχείου]</seg></lem><rdg>διο<gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/></rdg></app> <gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀπολαμβάνον</supplied>-

    <lb n="16"/>τος αὐτῆς τὸ λοιπὸν τῆς <choice><corr>τιμῆς</corr><sic>τειμης</sic></choice>· ἐὰν δὲ μὴ καταγράφῃ καθὰ γέγραφε, <app type="BL"><lem resp="1.46">ἐκτείσ<supplied cert="high" reason="lost">ειν αὐτὴν τὸν ἀρραβῶνα διπλοῦν</supplied></lem><rdg>ἐκτείσ<supplied cert="high" reason="lost">ειν </supplied><gap extent="unknown" reason="lost" unit="character"/></rdg></app> <supplied cert="high" reason="lost">τῷ τῶν</supplied> 

    <lb n="17"/><choice><corr>ἀρραβώνων</corr><sic>αραβωνων</sic></choice> ν<supplied cert="high" reason="lost">ό</supplied>μῳ· ἐὰν δὲ καὶ ἡ Σωτηρία ἑτοίμως ἔχουσα καταγράψαι <app type="editorial"><lem resp="bgu 3 p.2">μὴ <gap extent="unknown" reason="lost" unit="character"/></lem><rdg><unclear cert="high" reason="undefined">ω</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/></rdg></app> 

    <lb n="18"/><choice><corr>στερίσκεσθαι</corr><sic>στερικεθαι</sic></choice> αὐτὸν τοῦ <choice><corr>ἀρραβῶνος</corr><sic>αραβωνος</sic></choice>, ἔτι δὲ καὶ <choice><corr>βεβαιώσειν</corr><sic>βεβαιωσιν</sic></choice> αὐτὴν Σωτηρίαν τὰ κατὰ τ<supplied cert="high" reason="lost">αύτην τὴν ὁμολογίαν πάσῃ βεβαιώσει.</supplied> 

    <lb n="19"/>ὑπογραφεῖς τῆς μ<supplied cert="high" reason="lost">ὲ</supplied>ν ὁμολογούσης βραδέα γραφούσης ὁ ἐπιγραψάμε[νος κύριος, τοῦ δ᾽ <app type="BL"><lem resp="1.46">ἄλλου ἴδια γράμματα</lem><rdg>ἄλλους <gap extent="unknown" reason="illegible" unit="character"/> </rdg></app>]<foreign lang="la">. </foreign>

    <lb n="20"/><handShift new="m2"/> Σωτηρία Εἰρανου τοῦ Ἀ<supplied cert="high" reason="lost">πο</supplied>λωνείου μετὰ κυρίου <choice><corr>ἐμοῦ</corr><sic>αιμου</sic></choice> τοῦ <choice><corr>υἱοῦ</corr><sic>υειου</sic></choice> Εἰρηναίου ἀπ<supplied cert="high" reason="lost">έχω παρὰ Στοτοήτιος Ἀγχώφιος</supplied> 

    <lb n="21"/>τὰς προκιμένας ἀ<supplied cert="high" reason="lost">ργυρίου</supplied> δραχμὰς πεντακοσίας <num value="500"/> παραχρῆμα διὰ <choice><corr>χει<supplied cert="high" reason="lost">ρὸς</supplied></corr><sic>χι<supplied cert="high" reason="lost">ρος</supplied></sic></choice>  <supplied cert="high" reason="lost">ἐξ οἴκου ἀραβῶνα ἀναπόρρι</supplied>-

    <lb n="22"/>φον <gap extent="1" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/>τῆς τιμ<supplied cert="high" reason="lost">ῆς ἀργυ</supplied>ρίου δραχμῶν <choice><corr>ὀκτακοσίων</corr><sic>ωκτακοσιων</sic></choice> <num value="800"/> <choice><corr>τῶν</corr><sic>τω</sic></choice> ὑπαρχό<supplied cert="high" reason="lost">ντων μοι ἐν μὲν ἐποικίῳ Πισᾶι περὶ Ἡρακλίαν</supplied> 

    <lb n="23"/>τρίτον <supplied cert="high" reason="lost">μ</supplied>έρος αὐλ<supplied cert="high" reason="lost">ῆς</supplied> κα<supplied cert="high" reason="lost">ὶ ἄρουρ</supplied>αν <choice><corr>μίαν</corr><sic>μια</sic></choice> <choice><corr>ἥμισυ</corr><sic>ημισου</sic></choice> <num value="1  1/2 "/> καὶ περὶ <choice><corr>τὴν</corr><sic>τη</sic></choice> <choice><corr>αὐτὴν</corr><sic>αυτη</sic></choice> <choice><corr>Ἡρακλίαν</corr><sic>ηρακιλα</sic></choice> κλήρου ἄ[ρουραν μίαν 
ἥμισυ <num value="1  1/2 "/>, ἃ καὶ κατα]-

    <lb n="24"/>ψράψ<supplied cert="high" reason="lost">ω</supplied>, <subst><add place="inline">ὁπότε</add><del rend="corrected">οποδε</del></subst> <choice><corr>ἐὰν</corr><sic>αιαν</sic></choice> αἱρ<supplied cert="high" reason="lost">ῇ</supplied>, <choice><corr>ἀπολαμβάνοντός</corr><sic>απολαμμανοντος</sic></choice> μου τὸ <choice><corr>λοιπὸν</corr><sic>λυπον</sic></choice> τῆς τιμ<supplied cert="high" reason="lost">ῆς </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Εἰρηναῖ</supplied>-

    <lb n="25"/>ος <choice><corr>ἔγραψα</corr><sic>αγραψα</sic></choice> καὶ <choice><corr>ὑπέρ</corr><sic>υπε</sic></choice> <choice><corr>τε</corr><sic>τη</sic></choice> <choice><corr>μητρός</corr><sic>μητρως</sic></choice> μο<supplied cert="high" reason="lost">υ</supplied> <choice><corr>βραδέα</corr><sic>βρατε</sic></choice> <choice><corr>γραφούσης</corr><sic>γαρφουσης</sic></choice> <choice><corr>Σωτηρίας</corr><sic>σωητριας</sic></choice> Εἰρηναίο<supplied cert="high" reason="lost">υ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="26"/><handShift new="m3"/> Στοτοῆτις Ἀγχώφ<supplied cert="high" reason="lost">ιος</supplied> <app type="editorial"><lem resp="bgu 3 p.2"><choice><corr>γέγονε</corr><sic>γεικονε</sic></choice> <choice><corr>εἴς</corr><sic>ις</sic></choice> με</lem><rdg>εἰκονισμ<supplied cert="high" reason="lost">ὸ</supplied>σ</rdg></app> ὁ <choice><corr>ἀραβών</corr><sic>αλαβων</sic></choice> <choice><corr>καθὼς</corr><sic>καθος</sic></choice> πρόκιται. 

