    <lb n="8"/><supplied cert="low" reason="lost">τος</supplied> <unclear cert="high" reason="undefined">τ</unclear><gap extent="1" reason="lost" unit="character"/>βιριν μ<supplied cert="high" reason="lost">ό</supplied>νον ἔ<supplied cert="high" reason="lost">λ</supplied>αβα παρ᾽ αὐτοῦ 

