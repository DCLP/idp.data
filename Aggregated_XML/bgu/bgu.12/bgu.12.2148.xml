    <lb n="2"/>Αὐγούστου τὸ <num value="3">γ</num> <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">ὶ</unclear> <supplied cert="low" reason="lost">τοῦ δηλωθησομένου</supplied> 

    <lb n="3"/>Αὐρηλίῳ Κύρῳ Ἰσιδ<supplied cert="high" reason="lost">ώ</supplied><unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ου </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀπὸ Ἑρμουπόλεως</supplied> 

