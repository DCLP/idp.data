    <lb n="5"/>παρὰ Αὐρ<unclear cert="high" reason="undefined">η</unclear>λίου Πεῦτος Στεφά<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">ου ἀπὸ κώμης Ψῶβ</supplied>-

    <lb n="6"/>θον Χενα<supplied cert="high" reason="lost">ρσιή</supplied><unclear cert="high" reason="undefined">σ</unclear>εως τοῦ αὐτο<unclear cert="high" reason="undefined">ῦ</unclear> <unclear cert="high" reason="undefined">Ἑρ</unclear><supplied cert="high" reason="lost">μουπολίτου νομοῦ.</supplied> 

    <lb n="7"/>βούλομαι <supplied cert="high" reason="lost">ἑκου</supplied><unclear cert="high" reason="undefined">σ</unclear>ίως καὶ αὐθαιρέτως μ<unclear cert="high" reason="undefined">ι</unclear><supplied cert="high" reason="lost">σθώσασθαι</supplied> 

