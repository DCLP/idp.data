    <lb n="1/4"/><supplied cert="high" reason="lost">βασιλεύοντος Πτολεμαίου τοῦ Πτολεμαίου καὶ Κλεοπάτρας θεῶν Ἐπιφανῶν ἔτους ὀγδόου ἐφ᾽ ἱερ</supplied>έως <supplied cert="high" reason="lost">Ἡρακλεοδώρου τοῦ Ἀπολλοφάνου Ἀλεξάνδρου καὶ θεῶν Σωτήρων καὶ θεῶν 
Ἀδελφῶν καὶ θεῶν Εὐεργετῶν καὶ θεῶν Φιλοπατόρων καὶ θεῶν Ἐπιφανῶν καὶ θεῶν Φιλομητόρ</supplied>ων ἀθλοφόρου Βε<supplied cert="high" reason="lost">ρενίκης Εὐεργέτιδος Σαραπιάδος 
τῆς Ἀπολλων</supplied><unclear cert="high" reason="undefined">ί</unclear>ου κανήφορου Ἀρ<supplied cert="high" reason="lost">σινόης Φιλαδέλφου Ἀριστοκλείας τῆς Δημητ</supplied><unclear cert="high" reason="undefined">ρ</unclear>ίου ἱερείας Ἀρσινόης <supplied cert="high" reason="lost">Φιλοπάτορος</supplied> 

    <lb n="5"/><supplied cert="high" reason="lost">Εἰρήνης τῆς Πτολεμαί</supplied><unclear cert="high" reason="undefined">ου</unclear> μηνὸς Περιτίου π<supplied cert="high" reason="lost">έμπτηι καὶ εἰκάδι Μεσορὴ πέμπτηι κ</supplied>αὶ εἰκάδι ἐν Ἡφ<unclear cert="high" reason="undefined">αι</unclear><supplied cert="high" reason="lost">στιάδι τοῦ Ἀρσινοΐτου νομοῦ. ἐδάνεισε</supplied> Πτολεμαῖος 
Ἡφαιστ<supplied cert="high" reason="lost">ίωνος </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> αί</supplied>ωι Θραικὶ τῶν Κινέο<supplied cert="high" reason="lost">υ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> α</supplied><unclear cert="high" reason="undefined">ρο</unclear>ύρωι πυρῶν ἀρτ<supplied cert="high" reason="lost">άβας δέκα καὶ ὀκτώ <num value="18"/></supplied>, 


    <lb n="8"/><supplied cert="high" reason="lost">ἀποδότω δὲ ὁ δεῖνα ταύτας τὰς δέ</supplied>κα καὶ ὀκτὼ <num value="18"/> ἀρτάβ<supplied cert="high" reason="lost">ας τῶν πυρῶν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐὰν δέ τι πραχθῆι ὁ </supplied><gap extent="unknown" reason="lost" unit="character"/>αιος ὑπὲρ Πτολεμαίο<supplied cert="high" reason="lost">υ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> εἰς τὰ ἐν τῶι τό</supplied><unclear cert="high" reason="undefined">π</unclear>ωι βασιλικὰ τοῦ ὀγδ<supplied cert="high" reason="lost">όου ἔτους, ὑπολογείτω ὁ δεῖνα</supplied> 

    <lb n="11"/><gap extent="unknown" reason="lost" unit="character"/> <unclear cert="high" reason="undefined">ὁ</unclear> γεωργὸς γῆς ἀρουρ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="12"/><gap extent="unknown" reason="lost" unit="character"/> Πτολεμαῖος παρὰ Π<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ος Πτολεμαίωι τ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="14"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀπὸ τοῦ ὀγ</supplied>δόου ἔτους ἕως τ<unclear cert="high" reason="undefined">οῦ</unclear> <gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἔτους </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="15"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τὰς λοιπὰ</supplied>ς ὀκτὼ <num value="8"/> ἀρτάβας τῶ<unclear cert="high" reason="undefined">ν</unclear> <supplied cert="high" reason="lost">πυρῶν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="16"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> μὴ ἐξεῖναι αὐ</supplied>τῶι τῆς μισθώσεως <supplied cert="high" reason="lost">ἐκχωρεῖν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="17"/><supplied cert="high" reason="lost">ἐὰν δὲ μὴ ἀποδῶι καθὼς γέγρα</supplied>πται, ἐξέστω Πτολ<supplied cert="high" reason="lost">εμαίωι ἐάν τε βούληται πρᾶξαι</supplied> 

    <lb n="18"/><supplied cert="high" reason="lost">αὐτόν. ἐάν τε αἱρῆται ἐπιγεωρ</supplied><unclear cert="high" reason="undefined">γ</unclear>εῖν τὴν αὐτὴν γῆν <unclear cert="high" reason="undefined">τη</unclear><supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="19"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀρουρ</supplied>ῶν πέντε <num value="5"/> καὶ τὸ δωδέκ<supplied cert="high" reason="lost">ατον </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="20"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> μά</supplied>ρτυρες. Πτολεμαῖος <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="21"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Θρᾶιξ</supplied> τῶν Κινέου τῆς <unclear cert="high" reason="undefined">δευ</unclear><supplied cert="high" reason="lost">τέρας ἱππαρχίας </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="22"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ω</unclear>ς Ἰουδαῖος φυλακίτης <unclear cert="high" reason="undefined">ς</unclear><gap extent="unknown" reason="lost" unit="character"/> 

