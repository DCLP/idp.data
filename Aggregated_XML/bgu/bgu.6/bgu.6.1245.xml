    <lb n="2"/><supplied cert="high" reason="lost">ἔδαφος</supplied> <expan><ex cert="high">ἀρούρας</ex></expan> <num value="14">ιδ</num> κα<supplied cert="high" reason="lost">ὶ</supplied> τούτων πρασσομένων 

