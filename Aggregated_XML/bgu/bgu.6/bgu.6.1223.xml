    <lb n="4"/><handShift new="m1"/> <gap extent="1" reason="illegible" unit="line"/> 

    <lb n="5"/><expan>προβά<ex cert="high">των</ex></expan> καὶ τῶν συνκυρόντων <unclear cert="high" reason="undefined">ἀρ</unclear>νῶν <supplied cert="high" reason="lost">τοῦ</supplied> 

    <lb n="6"/>Μεμφίτου τοῦ αὐτοῦ ἔτους <unclear cert="high" reason="undefined">καταγαγε</unclear><supplied cert="high" reason="lost">ῖν</supplied> 

    <lb n="7"/>τὰ ὑπάρχοντα αὐτῷ ἐν Τῆνι ὑποτ<unclear cert="high" reason="undefined">ελ</unclear><supplied cert="high" reason="lost">ῆ</supplied> 

    <lb n="8"/>πρόβατα ἓξ <num value="6"/> καὶ τοὺς ἀκ<unclear cert="high" reason="undefined">ολού</unclear><supplied cert="high" reason="lost">θους</supplied> ἄρνα<supplied cert="high" reason="lost">ς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>καὶ ἐπικεχώρηται ἔτι πρότερον με<supplied cert="high" reason="lost">τάγειν</supplied> 

    <lb n="10"/>ἐπὶ νομὰς καὶ εἰς <unclear cert="high" reason="undefined">ἄλ</unclear><supplied cert="high" reason="lost">λα</supplied>ς κώμας διὰ τοῦ αὐτο<supplied cert="high" reason="lost">ῦ</supplied> 

    <lb n="11"/>νομοῦ ἀπὸ Μήας ἕως τοῦ μεγ<unclear cert="high" reason="undefined">άλου</unclear> χ<supplied cert="high" reason="lost">ώματος</supplied> 

    <lb n="12"/>καὶ <choice><corr>ποτίζειν</corr><sic>ποτιζιν</sic></choice> καὶ <choice><corr>κοιτάζειν</corr><sic>κοιταζιν</sic></choice> καὶ ἐπι<unclear cert="high" reason="undefined">μισγε</unclear><supplied cert="high" reason="lost">ιν</supplied> 

