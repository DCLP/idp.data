    <lb n="19"/>τέρα<supplied cert="high" reason="lost">ς γ</supplied>νώμης καὶ <choice><corr>θηρεύοντος</corr><sic>θηρευοντ<unclear cert="high" reason="undefined">α</unclear></sic></choice> 

    <lb n="20"/>ἐν αἷ<supplied cert="high" reason="lost">ς ὑ</supplied><unclear cert="high" reason="undefined">π</unclear>ολελείμμεθα κοίταις, 

