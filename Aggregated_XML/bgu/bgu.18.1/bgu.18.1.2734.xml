    <lb n="7"/>διασαφεῖ πρ<unclear reason="undefined" cert="high">ο</unclear>επεστάλθαι <gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="5" unit="character"/> κεφαλαίων ἄλλας φακοῦ ἀρτάβας ἑβ<sic>ι</sic>δομήκ<supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">ν</unclear><supplied reason="lost" cert="high">τα <num value="70"/> ἀντίγραφον</supplied>

