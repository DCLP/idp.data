    <lb n="1"/><supplied cert="high" reason="lost">Σαραπίων Πετειμούθῃ</supplied> χαίρειν· τοῦ π<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ὸς Λεωνίδην τὸν σιτολόγον</supplied>

    <lb n="2"/><supplied cert="high" reason="lost">χρηματισμοῦ ἀντίγραφο</supplied>ν ὑπόκειται· <unclear cert="high" reason="undefined">ἔρ</unclear><supplied cert="high" reason="lost">ρωσο· <expan><ex cert="high">ἔτους</ex></expan> <num value="31">λα</num> </supplied><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="3"/><choice><corr>Λεωνίδῃ</corr><sic><supplied cert="high" reason="lost">λ</supplied><unclear cert="high" reason="undefined">ε</unclear>ωνιδει</sic></choice>· τοῦ πα<unclear cert="high" reason="undefined">ρ</unclear>᾽ Ἡρώδου κ<unclear cert="high" reason="undefined">αὶ</unclear> Ἡρώδου τῶν π<unclear cert="high" reason="undefined">ρ</unclear>ὸ<supplied cert="high" reason="lost">ς τῇ ναυλώσει χρηματισμοῦ</supplied>

    <lb n="4"/><supplied cert="high" reason="lost">ἀ</supplied>ντί<supplied cert="high" reason="lost">γρ</supplied>αφον ὑπόκειται· <unclear cert="high" reason="undefined">κ</unclear>α<unclear cert="high" reason="undefined">τα</unclear>κολ<sic>λ</sic>ουθήσας οὖ<supplied cert="high" reason="lost">ν ἐμβαλοῦ συνεπιστέλλοντος</supplied>

    <lb n="5"/><unclear cert="high" reason="undefined">Π</unclear>ετει<unclear cert="high" reason="undefined">μ</unclear>ούθου τοῦ βασιλικοῦ <unclear cert="high" reason="undefined">γρ</unclear>αμματέως εἰς τ<unclear cert="high" reason="undefined">ὴ</unclear><supplied cert="high" reason="lost">ν διασαφουμένην δι᾽ ἡμῶν</supplied>

    <lb n="6"/>καὶ διατε<supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">α</unclear>γμένη<supplied cert="high" reason="lost">ν εἰς τὸ</supplied><unclear cert="high" reason="undefined">ν</unclear> ν<supplied cert="high" reason="lost">ο</supplied>μὸν <unclear cert="high" reason="undefined">ἐ</unclear>π᾽ <supplied cert="high" reason="lost">ἀνάληψιν πυροῦ ἀπὸ τῶν γενημάτων</supplied>

    <lb n="7"/>τοῦ <num value="31">λα</num> <expan><ex cert="high">ἔτους</ex></expan> <sic>εἰς</sic> προσαγωγ<supplied cert="high" reason="lost">ίδα</supplied> <unclear cert="high" reason="undefined">β</unclear><supplied cert="high" reason="lost">α</supplied>σιλέως, ἧς μ<unclear cert="high" reason="undefined">ι</unclear><supplied cert="high" reason="lost">σθωτὴς ὁ ναύκληρος</supplied>

    <lb n="8"/>Δωρίων <add place="supralinear"><del rend="erasure"><supplied cert="high" reason="lost">ἀ</supplied><unclear cert="high" reason="undefined">γωγῆ</unclear><supplied cert="high" reason="lost">ς</supplied> <num value="5000"><unclear cert="high" reason="undefined">Ε</unclear></num></del></add> Ἄνδρωνος, ἀγ<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">γῆς</supplied> <num value="5000"><unclear cert="high" reason="undefined">Ε</unclear></num>, πυροῦ καθ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ροῦ ἀδόλου κεκοσκινευμένου</supplied>

    <lb n="9"/><unclear cert="high" reason="undefined">μέτ</unclear>ρῳ δοχικῷ τῶι πρὸς <supplied cert="high" reason="lost">τὸ χαλ</supplied>κοῦν καὶ σκυτάλ<unclear cert="high" reason="undefined">ῃ</unclear> <supplied cert="high" reason="lost">δικαίᾳ, </supplied><gap extent="2" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐπιβιβασθέντων</supplied>

    <lb n="10"/>φυλακιτῶν κεκληρου<unclear cert="high" reason="undefined">χημέ</unclear><supplied cert="high" reason="lost">νων</supplied> τῶ<unclear cert="high" reason="undefined">ν</unclear> <unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ά</supplied>λι<unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">τα πίστιν ἐχόντων, οἷς καὶ τὸ δεῖγμα</supplied>

    <lb n="11"/>κατεσφραγισμέν<unclear cert="high" reason="undefined">ον</unclear> <unclear cert="high" reason="undefined">ἐπ</unclear>ιτεθ<unclear cert="high" reason="undefined">ή</unclear><supplied cert="high" reason="lost">σεται</supplied> ἐ<unclear cert="high" reason="undefined">ν</unclear> γε<unclear cert="high" reason="undefined">ΐνοι</unclear><supplied cert="high" reason="lost">ς ὠμοῖς ἀγγείοις καὶ παρακομιοῦσι</supplied>

    <lb n="12"/>παρὰ τοῦ πρὸς τῇ σιτηρᾷ τῆς <unclear cert="high" reason="undefined">ἐ</unclear><supplied cert="high" reason="lost">ξαιρέσεως τοὺς</supplied> <unclear cert="high" reason="undefined">κ</unclear>αθ<supplied cert="high" reason="lost">ήκοντας χρηματισμοὺς</supplied>

    <lb n="13"/>μετὰ τῆς Φίλωνος καὶ Πατρό<unclear cert="high" reason="undefined">κ</unclear>λ<supplied cert="high" reason="lost">ου τῶν <expan><ex cert="high">πρώτων</ex></expan> φίλων καὶ τῶν</supplied>

    <lb n="14"/>περὶ τὴν διοίκησιν <unclear cert="high" reason="undefined">μα</unclear>χ<supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">ιρ</unclear><supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">φ</unclear><supplied cert="high" reason="lost">όρων καὶ ἐπίπλων γνώμης, ἀρτάβας δισ-</supplied>

