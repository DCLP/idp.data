    <lb n="1"/>Εὐτυχίδηι Σαρ<unclear cert="high" reason="undefined">α</unclear>πίωνος 

    <lb n="2"/>παρὰ Κάστορος Πανεχώτου τῶν ἀ<unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ὸ</supplied> 

    <lb n="3"/>κώμης Ἀκώρεως καταγινομέ-

    <lb n="4"/><choice><corr>νου</corr><sic>καταγε<unclear cert="high" reason="undefined">ιν</unclear>ομενωι</sic></choice> ἐν κώμηι Μνάχει. βούλομαι 

    <lb n="5"/>μισθώσασθαι παρὰ σοῦ ἐπ᾽ ἔτη δύο <num value="2"/> ἀπὸ 

    <lb n="6"/>τοῦ ἐνεστῶτος <num value="13">ι<unclear cert="high" reason="undefined">γ</unclear></num> <expan><ex cert="high">ἔτους</ex></expan> Ἁδριανοῦ Καίσαρος 

    <lb n="7"/>τοῦ κυρίου ἀφ᾽ ὧν ἔχεις ἐν μισθώσει 

    <lb n="8"/>παρὰ Οὐλπίας Εὐδαιμονίδος ἐν τῶι 

    <lb n="9"/>Πάσκωι περὶ Μνᾶχιν ἐν δυσὶ <num value="2"/> κοί-

    <lb n="10"/>ταις ἀρούρας ἑπτὰ <num value="7"/> ὧν <choice><corr>μία</corr><sic>μιας</sic></choice> <num value="1"/> μὲν ἀρου-

    <lb n="11"/>ρῶν τεσσάρων <num value="4"/> <choice><corr>ἑτέρα</corr><sic>ετερας</sic></choice> δὲ ἀρουρῶν 

    <lb n="12"/>τριῶν <num value="3"/> πάσας ἀποτάκτου ἐκφορίου 

    <lb n="13"/>τῶν μὲν ἀρουρῶν τεσσάρων <num value="4"/> τοῦ 

    <lb n="14"/><num value="13">ιγ</num> <expan><ex cert="high">ἔτους</ex></expan> Ἁδριανοῦ Καίσαρος τοῦ κυρίου 

    <lb n="15"/>ἑκάστης ἀρούρης πυροῦ ἀρταβῶν 

    <lb n="16"/>ἐννέα <num value="9"/> τοῦ δὲ ἑξῆς <num value="14">ιδ</num> <expan><ex cert="high">ἔτους</ex></expan> ἑκάστης 

    <lb n="17"/>ἀρ<supplied cert="high" reason="lost">ο</supplied>ύρης κριθῆς <choice><corr><supplied cert="high" reason="omitted">ἀ</supplied>ρταβῶν</corr><sic>ρταβων</sic></choice> ἑπτὰ <num value="7"/> <del rend="erasure" status="unremarkable"><unclear cert="high" reason="undefined">α</unclear></del> τῶν 

    <lb n="18"/>δὲ λοιπῶν ἀρουρῶν τριῶν <num value="3"/> ἐκφορίου 

    <lb n="19"/>τοῦ μὲν ἐνεστῶτος <num value="13">ιγ</num> <expan><ex cert="high">ἔτους</ex></expan> ἀνὰ <expan>κριθ<ex cert="high">ῆς</ex></expan> 

    <lb n="20"/>ἀρτάβας ὀκτώ <num value="8"/> τοῦ δὲ εἰσ<hi rend="diaeresis">ι</hi>όντος 

    <lb n="21"/><num value="14">ι<unclear cert="high" reason="undefined">δ</unclear></num> <supplied cert="high" reason="lost"><expan><ex cert="high">ἔτους</ex></expan></supplied> <unclear cert="high" reason="undefined">ἀν</unclear>ὰ πυροῦ ἀρτάβας ὀκτώ <num value="8"/>. πάντα 

    <lb n="22"/>δ<supplied cert="high" reason="lost">ὲ</supplied> μετρήσω κατ᾽ ἔτος ὑπὲρ σοῦ εἰς 

    <lb n="23"/>τὸ δ<unclear cert="high" reason="undefined">η</unclear><supplied cert="high" reason="lost">μόσ</supplied>ιον μέτρῳ δημοσίωι 

    <lb n="24"/><supplied cert="high" reason="lost">δοχικ</supplied><unclear cert="high" reason="undefined">ῶ</unclear>ι ὧν καὶ μέτρησίν σοι ἀνα-

    <lb n="25"/><unclear cert="high" reason="undefined">δώ</unclear><supplied cert="high" reason="lost">σω</supplied> <subst><add place="inline">πρώτ<add place="supralinear">ῃ</add></add><del rend="corrected" status="unremarkable">πρωτω</del></subst> μετρήσει καὶ δώσω 

    <lb n="26"/>κατ᾽ ἔτος μισθωτικοῦ <expan>δραχ<ex cert="high">μὰς</ex></expan> τέσσαρας <num value="4"/> 

    <lb n="27"/>καὶ παιδα<supplied cert="high" reason="lost">ρ</supplied>ί<gap extent="1" reason="illegible" unit="character"/>ς τῶι μὲν ἐνεστῶτι <expan><ex cert="high">ἔτει</ex></expan> 

    <lb n="28"/>πυροῦ <expan><ex cert="high">ἀρτάβης</ex></expan> ἥ<unclear cert="high" reason="undefined">μ</unclear>ισυ <num value=" 1/2 "/> τῷ δὲ εἰσιόντι ἔτει 

    <lb n="29"/><choice><corr>κριθῆς</corr><sic>κριθη</sic></choice> ἥμ<unclear cert="high" reason="undefined">ι</unclear>συ <num value=" 1/2 "/> τῶν δημοσίων κα-

    <lb n="30"/>τ᾽ <supplied cert="high" reason="lost">ἔ</supplied>τος ὄν<supplied cert="high" reason="lost">τω</supplied><unclear cert="high" reason="undefined">ν</unclear> <supplied cert="high" reason="lost">π</supplied><unclear cert="high" reason="undefined">ρὸ</unclear>ς τὸν γεοῦχον. 

    <lb n="31"/><expan><ex cert="high">ἔτους</ex></expan> <choice><corr>τρεισκαιδ<supplied cert="high" reason="lost">ε</supplied>κάτου</corr><sic>τρισ<unclear cert="high" reason="undefined">καιδ</unclear><supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">κα</unclear>του</sic></choice> Αὐτοκράτορος 

    <lb n="32"/>Κ<unclear cert="high" reason="undefined">αί</unclear>σαρος Τραι<supplied cert="high" reason="lost">α</supplied>νοῦ Ἁδριανοῦ Σεβαστοῦ 

    <lb n="33"/>Θὼτ <num value="11"><unclear cert="high" reason="undefined">ια</unclear></num>. <handShift new="m2"/> Εὐτ<unclear cert="high" reason="undefined">υ</unclear><supplied cert="high" reason="lost">χί</supplied>δ<unclear cert="high" reason="undefined">ης</unclear> <expan>ἐμί<hi rend="superscript">σθ</hi><ex cert="high">ωσα</ex></expan> καθὼς <expan>πρόκ<ex cert="high">ειται</ex></expan>. 
