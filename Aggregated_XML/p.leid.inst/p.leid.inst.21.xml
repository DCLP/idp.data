    <lb n="1"/>ἔτους <num value="3">γ</num> Παῦνι <num value="30">λ</num> <expan>πέ<ex cert="high">πτωκεν</ex></expan> ἐπὶ τὴν 

    <lb n="2"/>ἐν <expan><unclear reason="undefined" cert="high">Π</unclear>το<ex cert="high">λεμαίδι</ex></expan> <expan><unclear reason="undefined" cert="high">Εὐε</unclear>ργέ<ex cert="high">τιδι</ex></expan> <expan>τρά<ex cert="high">πεζαν</ex></expan> Φιλοξένῳ <expan>τρα<ex cert="high">πεζίτῃ</ex></expan> 

    <lb n="3"/>ὡς <expan>βα<ex cert="high">σιλεῖ</ex></expan> παρὰ Ἑλένης τῆς 

    <lb n="4"/>Ἀφροδισίου τέλος <expan>ἀμ<ex cert="high">πελῶνος</ex></expan> <expan>συνφύ<ex cert="high">του</ex></expan> <expan>ἀναδενδρα<ex cert="high">δικοῦ</ex></expan> 

    <lb n="5"/><expan>ἡμιαρου<ex cert="high">ρίου</ex></expan> ἢ ὅσων ἐὰν ᾖ καὶ τοῦ 

    <lb n="6"/>ἐπιβάλλοντος <expan><ex cert="high">πέμπτου</ex></expan> <expan>μέ<ex cert="high">ρους</ex></expan> <expan>ὑποδοχ<ex cert="high">είου</ex></expan> 

    <lb n="7"/>καὶ <expan>κρή<ex cert="high">νης</ex></expan> λιθίνης καὶ τῶν <expan>συ<ex cert="high">γκυρόντων</ex></expan> <expan>πά<ex cert="high">ντων</ex></expan> 

    <lb n="8"/>τῶν ὄντων ἐν <abbr>β</abbr> <gap reason="illegible" extent="1" unit="character"/><abbr>η<gap reason="illegible" extent="1" unit="character"/>ς</abbr> <abbr>κ<unclear reason="undefined" cert="high">αι</unclear><gap reason="illegible" extent="2" unit="character"/>υφο</abbr> 

    <lb n="9"/>περὶ Ἰβιῶνα <expan><ex cert="high">Εἰκοσιπενταρούρων</ex></expan> τῆς <expan>Πο<ex cert="high">λέμωνος</ex></expan> <expan>με<ex cert="high">ρίδος</ex></expan>, <choice><corr>γεί</corr><sic>γι</sic></choice><expan><ex cert="high">τονες</ex></expan> <expan>νό<ex cert="high">του</ex></expan> 

    <lb n="10"/>Σενήσιος <subst><add place="inline">τῆς</add><del rend="corrected" status="unremarkable">του</del></subst> Ζωισίμου <expan>ἀμ<ex cert="high">πελὼν</ex></expan> 


    <lb n="11"/>δι᾽ οὗ <expan>ποτίζε<ex cert="high">ται</ex></expan> ὁ <expan>προγεγρ<ex cert="high">αμμένος</ex></expan> <expan>ἀμ<ex cert="high">πελών</ex></expan>, <expan>βο<ex cert="high">ρρᾶ</ex></expan> <expan>εἴσο<ex cert="high">δος</ex></expan> <expan>κοι<ex cert="high">νὴ</ex></expan> 

    <lb n="12"/>καὶ <expan>ὑδραγω<ex cert="high">γός</ex></expan>, <expan>λι<ex cert="high">βὸς</ex></expan> Ἰσιδώρας τῆς Ἰσιδ<unclear reason="undefined" cert="high">ώρου</unclear> <expan>ἀμ<ex cert="high">πελών</ex></expan>, 

    <lb n="13"/><expan>ἀπη<ex cert="high">λιώτου</ex></expan> Ἡρωίδου τοῦ Ἀπολλωνίου ἐλαιωνο-

    <lb n="14"/><choice><corr>παράδει</corr><sic>ελαιονοπαραδει</sic></choice><expan><ex cert="high">σος</ex></expan> ἀνὰ <expan>μέ<ex cert="high">σον</ex></expan> οὔσης τῆς <gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">η</unclear>ς 

