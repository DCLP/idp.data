    <lb n="12"/>λόγον ἐνι<unclear cert="high" reason="undefined">αυ</unclear><supplied cert="high" reason="lost">τοῦ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/>καὶ τ<unclear cert="high" reason="undefined">ῶ</unclear>ν <gap extent="3" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

