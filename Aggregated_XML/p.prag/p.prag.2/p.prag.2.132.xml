    <lb n="9"/>τὸν ἕνα <num value="1"/> <unclear cert="high" reason="undefined">ἀ</unclear><supplied cert="high" reason="lost">πὸ μηνὸς </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τοῦ ἐνεστῶτος</supplied> 

    <lb n="10"/>ἑβδόμου <unclear cert="high" reason="undefined">ἔ</unclear><supplied cert="high" reason="lost">τους </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Καίσαρος τοῦ κυρίου</supplied> 

