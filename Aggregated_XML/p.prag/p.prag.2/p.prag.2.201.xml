    <lb n="22"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" extent="1" unit="character"/> τ<unclear reason="undefined" cert="high">α</unclear><gap reason="lost" extent="unknown" unit="character"/> 

