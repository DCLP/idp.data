    <lb n="18"/><gap reason="lost" extent="1" unit="character"/> <unclear reason="undefined" cert="high">π</unclear>ρὸς Θε<unclear reason="undefined" cert="high">ο</unclear><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="19"/><gap reason="lost" extent="1" unit="character"/>λω<unclear reason="undefined" cert="high">ν</unclear> ε<unclear reason="undefined" cert="high">χ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

