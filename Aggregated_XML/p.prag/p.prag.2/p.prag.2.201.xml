    <lb n="24"/>καὶ <add place="supralinear">ἄ</add>νε<unclear reason="undefined" cert="high">υ</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="25"/>κατὰ σ<unclear reason="undefined" cert="high">ι</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="26"/>πω οι<unclear reason="undefined" cert="high">ζ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="27"/>ὅτι καὶ νε<gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="28"/>μα μὲν <unclear reason="undefined" cert="high">κ</unclear>ατα<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="29"/>σαι οὖν αὐτοὶ <gap reason="lost" extent="2" unit="character"/> αἱ τα<gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

