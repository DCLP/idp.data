    <lb n="1"/>Ἡερκλείδης Διοσκουρίδει χαίρειν. εἰ ἔρρωσαι <unclear reason="undefined" cert="high">σ</unclear>ύ τε καὶ τὰ τέκνα σου καὶ οἱ παρὰ σοῦ <choice><corr>πάντες</corr><sic>παντας</sic></choice> τἆλλ᾽ ἐστί σοι <choice><corr>ἅπαντα</corr><sic>απανται</sic></choice>

    <lb n="2"/>κατὰ τρ<unclear reason="undefined" cert="high">όπ</unclear>ον, εἴη ἄν ὡς βούλομαι, <unclear reason="undefined" cert="high">κ</unclear>ἀ<unclear reason="undefined" cert="high">γὼ</unclear> δὲ ὑγίαινον. <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ῇ</supplied> <num value="16">ι<unclear reason="undefined" cert="high">ϛ</unclear></num> <unclear reason="undefined" cert="high">το</unclear>ῦ <unclear reason="undefined" cert="high">ὑπ</unclear><supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">κ</unclear>ει<unclear reason="undefined" cert="high">μέν</unclear><supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">υ</unclear> <supplied reason="lost" cert="high">μ</supplied>ηνὸς συνπεσόντος μου Φιλώται

    <lb n="3"/>τῶι ἀποδιδόντι σο<unclear reason="undefined" cert="high">ι</unclear> τ<supplied reason="lost" cert="high">ὴ</supplied>ν ἐπιστολὴ<unclear reason="undefined" cert="high">ν</unclear> ἐν τῶι <choice><corr>Δημητρείωι</corr><sic>δημη<unclear reason="undefined" cert="high">τρ</unclear><supplied reason="lost" cert="high">ι</supplied><unclear reason="undefined" cert="high">ω</unclear>ι</sic></choice> β<unclear reason="undefined" cert="high">ο</unclear>υλομέν<unclear reason="undefined" cert="high">ω</unclear>ι ἐπιδ<unclear reason="undefined" cert="high">οῦν</unclear>αι κατὰ σοῦ ὅσα σοι συνιστόρηκεν ἐν τῶι

    <lb n="4"/>πα<unclear reason="undefined" cert="high">ν</unclear>τὶ βίωι τῶι βασιλε<supplied reason="lost" cert="high">ῖ</supplied> καὶ ἐμπλεκ<unclear reason="undefined" cert="high">ε</unclear>ὶς αὐτῶι μεθ᾽ ἑτ<unclear reason="undefined" cert="high">έ</unclear>ρω<unclear reason="undefined" cert="high">ν</unclear> κατ<unclear reason="undefined" cert="high">ηξί</unclear>ωσ<unclear reason="undefined" cert="high">α</unclear> μὲν <unclear reason="undefined" cert="high">κα</unclear><supplied reason="lost" cert="high">ὶ</supplied> <unclear reason="undefined" cert="high">ἀ</unclear>νεδεξάμην αὐτὸν ἕκαστα τοῦ μὴ

    <lb n="5"/>ἀδικηθῆναι ἀυτὸν, ἀλ<unclear reason="undefined" cert="high">λ</unclear>ὰ πᾶν τὸ δέον αὐτῶι ἔσται. <choice><corr>οὕτω</corr><sic>ουτωι</sic></choice> <unclear reason="undefined" cert="high">συν</unclear>ε<unclear reason="undefined" cert="high">πεί</unclear>σ<supplied reason="lost" cert="high">θ</supplied><unclear reason="undefined" cert="high">η</unclear> καὶ <unclear reason="undefined" cert="high">μετὰ</unclear> <supplied reason="lost" cert="high">λ</supplied>οῦ ἱδρῶτος ἀπεσπάσαμεν· ὅθεν καὶ

    <lb n="6"/><unclear reason="undefined" cert="high">π</unclear>ρο<unclear reason="undefined" cert="high">η</unclear>νέγκα<unclear reason="undefined" cert="high">τ</unclear>ο <unclear reason="undefined" cert="high">ἠδ</unclear><supplied reason="lost" cert="high">ι</supplied>κ<unclear reason="undefined" cert="high">ῆσθα</unclear>ι ὑπὸ σοῦ πολλά τι<unclear reason="undefined" cert="high">ν</unclear>α <unclear reason="undefined" cert="high">κ</unclear>αὶ <unclear reason="undefined" cert="high">ἀποστ</unclear><supplied reason="lost" cert="high">ε</supplied><unclear reason="undefined" cert="high">ρε</unclear><supplied reason="lost" cert="high">ῖ</supplied><unclear reason="undefined" cert="high">σ</unclear>θαί τινων <unclear reason="undefined" cert="high">φ</unclear>ι<unclear reason="undefined" cert="high">λ</unclear>αν<unclear reason="undefined" cert="high">θρώ</unclear>πων ὧν ἑστάκεις αὐτῶι καὶ <choice><corr>πλεονάκις</corr><sic>πλειονα<unclear reason="undefined" cert="high">κι</unclear><supplied reason="lost" cert="high">ς</supplied></sic></choice>

    <lb n="7"/><unclear reason="undefined" cert="high">π</unclear>ρ<unclear reason="undefined" cert="high">οσ</unclear>απεσταλ<unclear reason="undefined" cert="high">κέναι</unclear> σ<unclear reason="undefined" cert="high">οι</unclear> μηδὲ κατ᾽ ἀν<unclear reason="undefined" cert="high">θ</unclear>ρώπο<unclear reason="undefined" cert="high">υ</unclear> αὐτοῦ σχεῖ<supplied reason="lost" cert="high">ν</supplied>, <unclear reason="undefined" cert="high">ἀλλ</unclear>ὰ ἀπει<unclear reason="undefined" cert="high">λὰς</unclear> σα<unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">έστ</supplied>ελλες· διὸ καὶ εἰς τοῦτο <choice><corr>προήχθη</corr><sic>προηχθηι</sic></choice> τ<unclear reason="undefined" cert="high">οῦ</unclear>

    <lb n="8"/>κα<unclear reason="undefined" cert="high">τ</unclear>ὰ <unclear reason="undefined" cert="high">σ</unclear>ο<unclear reason="undefined" cert="high">ῦ</unclear> ενέσθαι. ἐγ<unclear reason="undefined" cert="high">ὼ</unclear> οὖν <unclear reason="undefined" cert="high">τ</unclear>εθα<unclear reason="undefined" cert="high">ύ</unclear>μακα, ε<unclear reason="undefined" cert="high">ι</unclear>̓ σὺ συν<unclear reason="undefined" cert="high">αυ</unclear>ψ<unclear reason="undefined" cert="high">ω</unclear><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="1" unit="character"/>αντων κα<gap reason="illegible" extent="5" unit="character"/>ς <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ὸν</supplied> ἐνεστῶτα καιρὸν <choice><corr>εἴασας</corr><sic>ηασας</sic></choice> αὐτὸν κατα-

    <lb n="9"/>π<unclear reason="undefined" cert="high">λ</unclear>εῦσαι ου<unclear reason="undefined" cert="high">λ</unclear>υ<unclear reason="undefined" cert="high">σασ</unclear>τα<unclear reason="undefined" cert="high">π</unclear><gap reason="illegible" extent="3" unit="character"/><unclear reason="undefined" cert="high">σεκ</unclear><gap reason="illegible" extent="1" unit="character"/>αυ<unclear reason="undefined" cert="high">η</unclear><gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">ρ</unclear><gap reason="lost" extent="1" unit="character"/>ον<unclear reason="undefined" cert="high">τ</unclear><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="1" unit="character"/><unclear reason="undefined" cert="high">οι</unclear><gap reason="illegible" extent="1" unit="character"/>ιλοι και <unclear reason="undefined" cert="high">μ</unclear><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="3" unit="character"/><gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">α</unclear><gap reason="illegible" extent="3" unit="character"/><unclear reason="undefined" cert="high">θ</unclear>ρ<gap reason="illegible" extent="5" unit="character"/><gap reason="lost" extent="3" unit="character"/><gap reason="illegible" extent="1" unit="character"/>εραν ὅμως ψοῦν καὶ ἀδελφοὶ μάχοντ<supplied reason="lost" cert="high">αι·</supplied>

    <lb n="10"/>ἔτι καὶ νῦν καλῶς ποιήσεις καθότ<unclear reason="undefined" cert="high">ι</unclear> αὶ <unclear reason="undefined" cert="high">αναδ</unclear><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="10" unit="character"/><gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">ο</unclear><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="3" unit="character"/><unclear reason="undefined" cert="high">π</unclear><gap reason="illegible" extent="3" unit="character"/><unclear reason="undefined" cert="high">λ</unclear><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">ο</unclear><gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">ει</unclear><gap reason="illegible" extent="8" unit="character"/><unclear reason="undefined" cert="high">τ</unclear>ωι εἰς λόγον μάλιστα μὲν κατὰ μόνας

    <lb n="11"/>καὶ τὰ παρ᾽ αὐτοῦ μεταλαβὼν <unclear reason="undefined" cert="high">ἀ</unclear>ὐτοῦ μεταλαβὼν <unclear reason="undefined" cert="high">ἀ</unclear>ποπε<unclear reason="undefined" cert="high">ισ</unclear>θήτω<gap reason="lost" extent="10" unit="character"/><gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">αν</unclear><gap reason="illegible" extent="12" unit="character"/><gap reason="lost" extent="5" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="1" unit="character"/> <add place="supralinear">περὶ τῶν αὐτῶν καλὰ ἀλλή<unclear reason="undefined" cert="high">λο</unclear>υς ἀσπά<unclear reason="undefined" cert="high">σα</unclear>σθε</add> νομίσας ὅτι ἐγώ σοι προσελήλυθα

    <lb n="12"/>κα<unclear reason="undefined" cert="high">ὶ</unclear> συ<unclear reason="undefined" cert="high">μβ</unclear>έ<unclear reason="undefined" cert="high">β</unclear>λησαί τί μοι. καὶ γὰρ περὶ τῶν αὐτῶν <unclear reason="undefined" cert="high">ι</unclear><gap reason="lost" extent="8" unit="character"/><gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">η</unclear>φενη<unclear reason="undefined" cert="high">φ</unclear><gap reason="illegible" extent="8" unit="character"/><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">σ</unclear>οι. ἐν δὲ τοῖς μάλιστα χαριεῖ τοῦ σώματο<supplied reason="lost" cert="high">ς</supplied>

