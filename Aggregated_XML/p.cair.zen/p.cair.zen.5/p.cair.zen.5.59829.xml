    <lb n="8"/><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> ἵνα πεμ<unclear cert="high" reason="undefined">ψ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>σομ<unclear cert="high" reason="undefined">έν</unclear>ου 
