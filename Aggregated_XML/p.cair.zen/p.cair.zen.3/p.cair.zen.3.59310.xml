    <lb n="1"/>Ἀρτεμίδωρος Ζήνω<supplied reason="lost" cert="high">ν</supplied>ι χαίρειν. εἰ ἔρ<supplied reason="lost" cert="high">ρω</supplied>σαι, εὖ ἂ<supplied reason="lost" cert="high">ν</supplied> ἔχ<unclear reason="undefined" cert="high">οι</unclear>· ἔρρωμαι δὲ κα<supplied reason="lost" cert="high">ὶ ἐγ</supplied>ώ. ἔ<supplied reason="lost" cert="high">γραψεν</supplied> 

    <lb n="2"/>Κλειτόριος ὅτι Πετῶς <add place="supralinear">ὁ</add> ὑοφορβὸς ἀνακεχ<supplied reason="lost" cert="high">ώρ</supplied>ηκεν κ<supplied reason="lost" cert="high">α</supplied>τα<unclear reason="undefined" cert="high">λ</unclear><supplied reason="lost" cert="high">ι</supplied>πὼν ἱερεῖα <num value="7">ζ</num> <unclear reason="undefined" cert="high">καὶ</unclear> δελφάκ<supplied reason="lost" cert="high">ια </supplied><gap reason="lost" extent="1" unit="character"/> 

    <lb n="3"/><unclear reason="undefined" cert="high">εἶχ</unclear><supplied reason="lost" cert="high">ε</supplied><unclear reason="undefined" cert="high">ν</unclear> ᾽ ἡ<unclear reason="undefined" cert="high">μῖ</unclear>ν <num value="400"><unclear reason="undefined" cert="high">υ</unclear></num> ἱερεῖα ἔ<unclear reason="undefined" cert="high">μφ</unclear><supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">ρα</unclear> <supplied reason="lost" cert="high">τέλεια, καὶ δελ</supplied>φ<unclear reason="undefined" cert="high">ά</unclear>κια <supplied reason="lost" cert="high">π</supplied><unclear reason="undefined" cert="high">ρ</unclear><supplied reason="lost" cert="high">οσ</supplied><unclear reason="undefined" cert="high">ώ</unclear>φειλε<supplied reason="lost" cert="high">ν</supplied> εἰς <choice><corr>τὸν</corr><sic>τ<unclear reason="undefined" cert="high">ομ</unclear></sic></choice> <unclear reason="undefined" cert="high">φ</unclear><supplied reason="lost" cert="high">ό</supplied><unclear reason="undefined" cert="high">ρον</unclear> <num value="211"><unclear reason="undefined" cert="high">σι</unclear>α</num>. 

    <lb n="4"/>καλῶς <unclear reason="undefined" cert="high">ἂν</unclear> ὖν ποιήσαις σπουδάσας ἵνα <choice><corr>ἀναζητ<supplied reason="omitted" cert="high">ηθ</supplied>ῆι</corr><sic>αναζητ<unclear reason="undefined" cert="high">ηι</unclear></sic></choice> ὁ ἄνθρωπος ποῦ ἀν<unclear reason="undefined" cert="high">α</unclear>κεχώρηκεν, 

    <lb n="5"/>ἵνα μὴ διαπέσηι ἡμῖν τ<supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">σα</unclear>ῦτα ἱερεῖα, καὶ τοὺς ἐγ<unclear reason="undefined" cert="high">γ</unclear>ύους δὲ οἵτινες ἠγγύηνται αὐτόν, 

    <lb n="6"/>ἵνα ἢ <subst><add place="inline">συλληφθέντες</add><del rend="corrected" status="unremarkable">συλληφθωσινες</del></subst> ἀπαχ<unclear reason="undefined" cert="high">θ</unclear>ῶσ<unclear reason="undefined" cert="high">ι</unclear>ν ἢ διεγγυηθ<unclear reason="undefined" cert="high">ῶ</unclear>σιν ἕως ἂν 

    <lb n="7"/>γένηται ἡμῖν τὰ δίκαια. 

    <lb n="8"/>κε<unclear reason="undefined" cert="high">κο</unclear><supplied reason="lost" cert="high">μί</supplied><unclear reason="undefined" cert="high">σ</unclear>μεθα δὲ κα<supplied reason="lost" cert="high">ὶ</supplied> τὸ ὑ<supplied reason="lost" cert="high">ι</supplied><unclear reason="undefined" cert="high">κὸ</unclear>ν ἱερε<supplied reason="lost" cert="high">ῖο</supplied>ν ὃ ἀπ<unclear reason="undefined" cert="high">έ</unclear><supplied reason="lost" cert="high">στ</supplied>ειλ<unclear reason="undefined" cert="high">ας</unclear> <supplied reason="lost" cert="high">ῖ</supplied>ν. 

    <lb n="9"/>ἔρρωσο. <expan><ex cert="high">ἔτους</ex></expan> <num value="36">λϛ</num>, Λωίου <gap reason="lost" extent="1" unit="character"/><num value="5">ε</num>. 
