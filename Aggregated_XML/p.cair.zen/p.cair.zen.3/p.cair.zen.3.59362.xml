    <lb n="6"/><unclear reason="undefined" cert="high">Ζή</unclear><supplied reason="lost" cert="high">νων</supplied> <unclear reason="undefined" cert="high">Πύρρ</unclear>ωι <supplied reason="lost" cert="high">ἀ</supplied>πέ<unclear reason="undefined" cert="high">χ</unclear><supplied reason="lost" cert="high">ομ</supplied>εν παρὰ σοῦ, κατὰ 

