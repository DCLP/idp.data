    <lb n="1"/><supplied reason="lost" cert="high">Εὔ</supplied><unclear reason="undefined" cert="high">δ</unclear>η<supplied reason="lost" cert="high">μο</supplied>ς Ζή<supplied reason="lost" cert="high">ν</supplied>ωνι χαίρειν. 

    <lb n="2"/>αὐτὸ<unclear reason="undefined" cert="high">ς</unclear> μὲν οὐ δύναμαι 

    <lb n="3"/><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="1" unit="character"/>μο<unclear reason="undefined" cert="high">υ</unclear> αι διὰ τὸ 

    <lb n="4"/><unclear reason="undefined" cert="high">π</unclear>ρός τε τῶι <unclear reason="undefined" cert="high">θε</unclear>ρισμῶι εἶναι 

    <lb n="5"/>καὶ τῆι <unclear reason="undefined" cert="high">συ</unclear>ναγωγῆι τοῦ σίτου· 

    <lb n="6"/><supplied reason="lost" cert="high">ἅμ</supplied><unclear reason="undefined" cert="high">α</unclear> ὲ καὶ οἱ ἄγριοι βόες ἡμᾶς 

    <lb n="7"/><supplied reason="lost" cert="high">κεκωλύκ</supplied><unclear reason="undefined" cert="high">ασι</unclear>ν· Θεοδώραν δὲ 

    <lb n="8"/><supplied reason="lost" cert="high">ἀπέστα</supplied>λκά σοι κομίζου-

    <lb n="9"/>σαν τὸν χιτῶνα τὸν λευ-

    <lb n="10"/>κόφαιον. σὺ <unclear reason="undefined" cert="high">δὲ</unclear> λῶς ποι-

    <lb n="11"/>ήσεις γράψ<unclear reason="undefined" cert="high">ας</unclear> Φίλωνι ὅπως 

    <lb n="12"/>μετριάσηι <unclear reason="undefined" cert="high">τι</unclear> μῖν ἀπὸ 

    <lb n="13"/>τοῦ ἐκφορίου· <choice><corr>εἰσὶν</corr><sic><unclear reason="undefined" cert="high">ε</unclear>ισιγ</sic></choice> γὰρ ἡμῖν 


    <lb n="14"/><supplied reason="lost" cert="high">σ</supplied><unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">ω</supplied><unclear reason="undefined" cert="high">λ</unclear>ηκοβρώτου καὶ κα-

    <lb n="15"/><supplied reason="lost" cert="high">κοῦ σίτου</supplied> <expan><ex cert="high">ἄρουραι</ex></expan> <num value="15">ιε</num>, ἔστιν δὲ 

    <lb n="16"/>ἡ κατεσπαρμένη ὑφ᾽ ἡ-

    <lb n="17"/>μῶν <expan><ex cert="high">ἄρουραι</ex></expan> <gap reason="illegible" extent="2" unit="character"/> <supplied reason="lost" cert="high">ἐ</supplied>ὰν οὖν 

    <lb n="18"/>ἡμῖν <unclear reason="undefined" cert="high">ἀφῆι</unclear> πὶ τῆι <expan><ex cert="high">ἀρούραι</ex></expan> <expan>ἀρ<ex cert="high">τάβης</ex></expan> <num value="1/2">𐅵</num>, 

    <lb n="19"/>ἐκείνωι τε <unclear reason="undefined" cert="high">τ</unclear><gap reason="lost" extent="2" unit="character"/><unclear reason="undefined" cert="high">ι</unclear>α ποιήσο-

    <lb n="20"/>μεν κα<unclear reason="undefined" cert="high">ὶ</unclear> <supplied reason="lost" cert="high">σοί</supplied>· <unclear reason="undefined" cert="high">οὐ</unclear> ὰρ μικρὸν 

    <lb n="21"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" extent="1" unit="character"/> <gap reason="illegible" extent="1" unit="character"/> <expan>ἀρ<ex cert="high">τάβαι</ex></expan> <num value="15">ιε</num> <expan><ex cert="high">ἀρουρῶν</ex></expan>. 

    <lb n="22"/>καλῶς δ<supplied reason="lost" cert="high">ὲ ποιήσ</supplied>εις καὶ τοὺς 

    <lb n="23"/>Ἄραβας <unclear reason="undefined" cert="high">ε</unclear><supplied reason="lost" cert="high">ἰσ</supplied><unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ρά</supplied>ξας <choice><corr>τὸν</corr><sic>τομ</sic></choice> φό-

    <lb n="24"/>ρον τῶν <unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ρο</supplied>βάτων. ἔτι γὰρ 

    <lb n="25"/>καὶ νῦν <unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">ὐ</supplied><unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ὰ</supplied> ἔχουσιν τά 

    <lb n="26"/>τε πρ<unclear reason="undefined" cert="high">όβατ</unclear>α καὶ τὰς κου-

    <lb n="27"/>ρὰς ὅ τε <unclear reason="undefined" cert="high">Ἑριεὺς</unclear> καὶ Πόρτις. 

    <lb n="28"/>παρὰ <supplied reason="lost" cert="high">δὲ</supplied> Πτολεμαίου κεκό-

    <lb n="29"/>μισμαι ἀπὸ προβάτων <num value="31"><unclear reason="undefined" cert="high">λα</unclear></num> 

    <lb n="30"/><gap reason="lost" extent="5" unit="character"/><gap reason="illegible" extent="1" unit="character"/> δε πτ<unclear reason="undefined" cert="high">α</unclear> ἐπὶ σοῦ 

    <lb n="31"/><choice><corr>δείξει</corr><sic>δεξει</sic></choice> τί γεγόνασιν· ἐλέγ-

    <lb n="32"/>χεται γάρ <num value="1">α</num> αὐτὸς 

    <lb n="33"/>ὑφείρημαι. ἔρρωσο. 
