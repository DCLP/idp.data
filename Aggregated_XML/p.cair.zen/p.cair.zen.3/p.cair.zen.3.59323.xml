    <lb n="1"/>ἀντίγραφον ἐπιστο-

    <lb n="2"/>λῆς τῆς Λυσαγόρου. 

    <lb n="3"/>Λυσαγόρας Διομή-

    <lb n="4"/>δηι χα<unclear cert="high" reason="undefined">ίρει</unclear>ν. ἐγδ<unclear cert="high" reason="undefined">έχ</unclear><supplied cert="high" reason="lost">ο</supplied>-

    <lb n="5"/><unclear cert="high" reason="undefined">μα</unclear>ί σοι Β<supplied cert="high" reason="lost">ί</supplied>ω<supplied cert="high" reason="lost">να</supplied> <unclear cert="high" reason="undefined">τ</unclear>ὸν <gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/> 

    <lb n="6"/><gap extent="5" reason="lost" unit="character"/><unclear cert="high" reason="undefined">θέ</unclear>ν<unclear cert="high" reason="undefined">τ</unclear>α <gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ν</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/> 

    <lb n="7"/>ι<unclear cert="high" reason="undefined">με</unclear>δον<supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ος</unclear> <unclear cert="high" reason="undefined">δ</unclear><gap extent="5" reason="lost" unit="character"/> 

    <lb n="ms,8"/><num value="300">τ</num> 

    <lb n="8"/>ρίωι πρὸς <expan><ex cert="high">δραχμὰς</ex></expan> <num value="46">μϛ</num><supplied cert="high" reason="lost"><num value="8">η</num></supplied> 

    <lb n="9"/>κα<unclear cert="high" reason="undefined">ταστή</unclear>σειν <add place="supralinear">ἐ<supplied cert="high" reason="lost">ν</supplied></add> <del rend="erasure"><gap extent="1" reason="illegible" unit="character"/></del> <supplied cert="high" reason="lost">Ἀλε</supplied>-

    <lb n="10"/>ξανδρε<supplied cert="high" reason="lost">ί</supplied>αι εἰς τὰ Ἀ-

    <lb n="11"/>πολλω<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">ίο</supplied><unclear cert="high" reason="undefined">υ</unclear>. ἂν δ<unclear cert="high" reason="undefined">ὲ</unclear> μὴ 

    <lb n="12"/>κατα<unclear cert="high" reason="undefined">σ</unclear>τ<unclear cert="high" reason="undefined">ή</unclear>σω, ἀποτεί-

    <lb n="13"/>σω σοι τὸ ἐπικαλού-

    <lb n="14"/><choice><corr>μενον</corr><sic>ε<unclear cert="high" reason="undefined">π</unclear>ικαλαλο<unclear cert="high" reason="undefined">υ</unclear>μενον</sic></choice> αὐτῶι <expan><ex cert="high">δραχμὰς</ex></expan> <num value="468">υξη</num> 

    <lb n="15"/>ἔτους <num value="36">λϛ</num> ἐν μηνὶ 

    <lb n="16"/>Αὐδναίωι, ἂν μὴ βασιλι-

    <lb n="17"/>κὴ λειτουργία παραν-

    <lb n="18"/>γελῆι. ἔρρωσο. 
</ab></div><div n="v" part="N" type="textpart"><ab part="N">

