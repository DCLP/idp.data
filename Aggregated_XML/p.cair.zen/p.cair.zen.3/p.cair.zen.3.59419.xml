    <lb n="1"/>Δημήτριος Ζή<supplied reason="lost" cert="high">νωνι χαίρειν</supplied>. 

    <lb n="2"/><choice><corr>ἔστιν</corr><sic>ειστιν</sic></choice> Δημητρ<supplied reason="lost" cert="high">ίου τοῦ </supplied><app type="editorial"><lem><supplied reason="lost" cert="high">ἐμοῦ</supplied></lem><rdg>Δημητρ<supplied reason="lost" cert="high">ίου </supplied><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="3"/>μικροῦ τὰ γενέ[θλια τῆι <app type="editorial"><lem><num value="17">ιζ</num>]</lem><rdg>γενέ<supplied reason="lost" cert="high">θλια τῆι </supplied><gap reason="lost" extent="1" unit="character"/></rdg></app> 

    <lb n="4"/>τοῦ Φαμενώθ. καλῶς <app type="editorial"><lem>οὖν</lem><rdg>κ<supplied reason="lost" cert="high">αλῶς οὖν</supplied></rdg></app> 

    <lb n="5"/>ποιήσεις δοὺς αὐτῶι <app type="editorial"><lem>οἰνάριον</lem><rdg>δοὺς α<gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="6"/>καὶ χοιρίδιον ἀφ᾽ ὧν καὶ <app type="editorial"><lem>αὐτὸς</lem><rdg>α<unclear reason="undefined" cert="high">φ</unclear><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

