    <lb n="1"/>Ἀπολλώνιος Ζήνωνι χαίρειν. <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>ἐμβαλ<unclear cert="high" reason="undefined">όμε</unclear>νος εἰς πλοῖον ἀπόστειλ<supplied cert="high" reason="lost">ον </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/><supplied cert="high" reason="lost">τῶι ἐ</supplied>πιπλ<supplied cert="high" reason="lost">έο</supplied>ντι προσαγαγεῖν επι<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>εἰς τὸν Σαίτην νομόν. τὰ δ<supplied cert="high" reason="lost">ὲ δ</supplied>ελφ<unclear cert="high" reason="undefined">ά</unclear><supplied cert="high" reason="lost">κια </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>ἐπιμελῶς καὶ τοκάδας ὅτι πλ<supplied cert="high" reason="lost">είστας </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/>ἔρρωσο. <expan><ex cert="high">ἔτους</ex></expan> <gap extent="unknown" reason="lost" unit="character"/> 
</ab></div><div n="v" org="uniform" part="N" sample="complete" type="textpart"><ab part="N">

    <lb n="7"/><supplied cert="high" reason="lost"><expan><ex cert="high">ἔτους</ex></expan> <num value="30">λ</num>, Δαισίου</supplied> <num value="11"><unclear cert="high" reason="undefined">ια</unclear></num> Παῦνι <num value="9"><unclear cert="high" reason="undefined">θ</unclear></num>. 

    <lb n="8"/><supplied cert="high" reason="lost">Ἀπολλώνιος ὑπ</supplied>ὲρ τῶν 

    <lb n="9"/><gap extent="9" reason="lost" unit="character"/><supplied cert="high" reason="lost"> δελ</supplied>φάκων τροφῆς. 

