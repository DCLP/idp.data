    <lb n="4"/>καὶ μ<unclear reason="undefined" cert="high">ν</unclear>η<supplied reason="lost" cert="high">σθε</supplied>ὶς Μητρο<unclear reason="undefined" cert="high">δ</unclear><supplied reason="lost" cert="high">ώρωι</supplied> 

    <lb n="5"/>ἵνα ἡμῖν ὀψώνιον τ<unclear reason="undefined" cert="high">ά</unclear><supplied reason="lost" cert="high">ξ</supplied><unclear reason="undefined" cert="high">η</unclear><supplied reason="lost" cert="high">ται</supplied> 

