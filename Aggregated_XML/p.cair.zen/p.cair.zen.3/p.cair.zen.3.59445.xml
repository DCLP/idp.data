    <lb n="1"/>ὑπόμνη<supplied reason="lost" cert="high">μα Ζήνω</supplied>νι παρὰ Θευφί-

    <lb n="2"/><unclear reason="undefined" cert="high">λ</unclear><supplied reason="lost" cert="high">ου τ</supplied>ο<unclear reason="undefined" cert="high">ῦ</unclear> <unclear reason="undefined" cert="high">ερ</unclear><gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="3" unit="character"/><unclear reason="undefined" cert="high">ου</unclear> περὶ τῶν ἔργων 

