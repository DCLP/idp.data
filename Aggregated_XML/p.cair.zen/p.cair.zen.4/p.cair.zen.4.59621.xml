    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ᾠκοδο</supplied>μηκέναι λαβοῦσα τόπον <unclear reason="undefined" cert="high">ψιλ</unclear><supplied reason="lost" cert="high">ὸν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><del rend="erasure" status="unremarkable"><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἀρχιτέ</supplied><unclear reason="undefined" cert="high">κτ</unclear><supplied reason="lost" cert="high">τονο</supplied><unclear reason="undefined" cert="high">ς</unclear> <unclear reason="undefined" cert="high">Θε</unclear>οδώρου</del> καὶ εἰσανηλωκέναι εἰς τὴν οἰκοδομ<unclear reason="undefined" cert="high">ὴ</unclear><supplied reason="lost" cert="high">ν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> Φιλ</supplied><unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">δ</supplied><unclear reason="undefined" cert="high">ε</unclear>λφείαι ἐπ᾽ Ἀγήνορος τοῦ σ<supplied reason="lost" cert="high">τ</supplied>ρατηγοῦ καὶ Ἀρσινόη <unclear reason="undefined" cert="high">οὔ</unclear><supplied reason="lost" cert="high">τε</supplied> τοὺ<unclear reason="undefined" cert="high">ς</unclear> <unclear reason="undefined" cert="high">ε</unclear><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" extent="2" unit="character"/> <unclear reason="undefined" cert="high">ο</unclear>ὔτε ὡς <choice><corr>ᾠκοδομήκει</corr><sic>ωικοδομηκει</sic></choice> τὴν οἰκίαν, τοῦ τε Εὐτύχου φαμένο<supplied reason="lost" cert="high">υ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> οἰκο</supplied>δ<supplied reason="lost" cert="high">ο</supplied>μὴν καὶ περὶ τούτ<unclear reason="undefined" cert="high">ων</unclear> <unclear reason="undefined" cert="high">β</unclear><supplied reason="lost" cert="high">αλομ</supplied><unclear reason="undefined" cert="high">έν</unclear><supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">υ</unclear> εἴ<supplied reason="lost" cert="high">ς τ</supplied><unclear reason="undefined" cert="high">ε</unclear> <unclear reason="undefined" cert="high">τ</unclear>οὺς οἰκοδόμους καὶ <add place="supralinear"><unclear reason="undefined" cert="high">τοὺ</unclear><supplied reason="lost" cert="high">ς </supplied><gap reason="lost" extent="unknown" unit="character"/></add> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> οἰκοδ</supplied>όμος καὶ ὁ τὰ ξύλα ἀποδόμενος κα<gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> τὴ</supplied>ν τιμὴν καὶ τοὺς μισθοὺς εἰληφέναι παρ᾽ Εὐτύχου τοῦ παιδός μου κο<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἀνα</supplied>χωρήσαντ<supplied reason="lost" cert="high">ο</supplied>ς περὶ τῆς βολῆς ταύτης, διὸ παραλογισθεὶς Ἀγήνω<supplied reason="lost" cert="high">ρ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἐμ</supplied><unclear reason="undefined" cert="high">ὲ</unclear> ὡρίσθαι ταῦτα, γράφει Κλειτορίωι <choice><corr>ἀνακαλεσάμενόν</corr><sic>ανακαλεσαμενομ</sic></choice> με συν<supplied reason="lost" cert="high">τάξαι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> βασι</supplied><unclear reason="undefined" cert="high">λεῦ</unclear>, προστάξα<supplied reason="lost" cert="high">ι Ἀ</supplied>γή<supplied reason="lost" cert="high">νορι τ</supplied>ῶι στρατηγῶι ἐπισκεψάμεν<unclear reason="undefined" cert="high">ον</unclear> <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><gap reason="lost" extent="unknown" unit="character"/>αι ὡς εἴ<supplied reason="lost" cert="high">λη</supplied>φεν τὸν τόπον ἐκ τοῦ <supplied reason="lost" cert="high">βασιλικοῦ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><gap reason="lost" extent="unknown" unit="character"/> τὴν πλίνθον καὶ τὰ ξύλα καὶ τοὺς οἰκοδόμους <add place="supralinear">παρασχέσθαι</add> μαρτυροῦντας αὐτῆι <add place="supralinear">εν<unclear reason="undefined" cert="high">α</unclear>μ<gap reason="lost" extent="unknown" unit="character"/></add> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12a"/>ἐ<supplied reason="lost" cert="high">ρη</supplied>μοῦμαι δὲ καὶ ἐγὼ μαρτυρίας περὶ τούτων, ὅπως πάλιν δι<supplied reason="lost" cert="high">ακούσας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/><gap reason="lost" extent="unknown" unit="character"/>ους, ἐὰν δὲ μὴ ἐνδείξηται, εἶναι τὴν οἰκίαν <subst><add place="inline">ἐμήν</add><del rend="corrected" status="unremarkable">ημετεραν</del></subst>, ἵνα κ<supplied reason="lost" cert="high">αὶ ἐγὼ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13a"/><gap reason="lost" extent="unknown" unit="character"/>θέντα κρίνηι ὡς ἂν αὐτῶι φαίνηται, ἵνα καὶ ἐγὼ διὰ σέ, βασιλ<supplied reason="lost" cert="high">εῦ, τοῦ δικαίου τύχω</supplied>. </ab></div><div n="meta.1253" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.1253" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" notBefore="-0248" notAfter="-0239">248 - 239 v.Chr.</date></p></div><div n="meta.1253" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Gehört zu P.Cair. Zen. IV 59620. Zur Datierung vgl. P.Cair. Zen. IV 59620 und P.Lugd. Bat. XXI A, S. 181.</p></div><div n="meta.1253" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">P.Cair. Zen.</title><biblScope type="volume">4</biblScope><biblScope type="numbers">59621</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">1253</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0092</series>; <biblScope type="volume">4</biblScope>; <biblScope type="numbers">59621</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0092;4;59621" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.1253" type="bibliography" subtype="corrections" org="uniform" part="N" sample="complete"><head>BL-Einträge nach BL-Konkordanz</head><listBibl default="NO"><bibl type="BL" default="NO"><biblScope type="volume">VIII</biblScope>, S. <biblScope type="pages">80</biblScope></bibl><bibl type="BL" default="NO"><biblScope type="volume">IX</biblScope>, S. <biblScope type="pages">55</biblScope></bibl></listBibl></div><div n="meta.1253" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p>