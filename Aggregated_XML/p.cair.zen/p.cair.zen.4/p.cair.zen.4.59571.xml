    <lb n="1"/><supplied cert="high" reason="lost">ὁ δεῖνα Ζήνωνι</supplied> <unclear cert="high" reason="undefined">χαίρε</unclear>ιν. 

    <lb n="2"/>εἰ ἔρρω<supplied cert="high" reason="lost">σαι, κα</supplied>λῶς ἂν ἔχοι· 

    <lb n="3"/>ὑ<unclear cert="high" reason="undefined">γί</unclear>αινον δ<unclear cert="high" reason="undefined">ὲ</unclear> κα<unclear cert="high" reason="undefined">ὶ</unclear> ἐγώ. περὶ 

    <lb n="4"/>μὲν <unclear cert="high" reason="undefined">τ</unclear>ῶν ἄλλων οὐκ ἐ-

    <lb n="5"/>δοκίμα<unclear cert="high" reason="undefined">ζ</unclear>ον ἐπιτήδειον 

    <lb n="6"/><unclear cert="high" reason="undefined">ε</unclear>ἶναι γράφειν σοι· οὐ γὰρ εἶχον σαφὲς οὐθέν· 

    <lb n="7"/><add place="supralinear"><supplied cert="high" reason="lost">τὰ δ</supplied>ὲ</add> <unclear cert="high" reason="undefined">στ</unclear>ρωμάτια καὶ τὰς ψιλὰς 

    <lb n="8"/><supplied cert="high" reason="lost">καλ</supplied>ῶς ποιήσ<supplied cert="high" reason="lost">εις ἀ</supplied><unclear cert="high" reason="undefined">γ</unclear>ο<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">άσας</supplied> 

    <lb n="9"/><supplied cert="high" reason="lost">Νέ</supplied>ωνι τῶι ἰα<supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ρ</unclear>ῶ<supplied cert="high" reason="lost">ι</supplied>, <unclear cert="high" reason="undefined">ὅπ</unclear><supplied cert="high" reason="lost">ως</supplied> 

    <lb n="10"/><supplied cert="high" reason="lost">π</supplied>αραγ<supplied cert="high" reason="lost">ενό</supplied>μενος κομίσηται. 

    <lb n="11"/>καὶ γὰρ ν<supplied cert="high" reason="lost">ῦ</supplied>ν ἐνεκάλει περὶ 

    <lb n="12"/>τούτων. γίνωσκε δὲ καὶ 

    <lb n="13"/>Νέωνα εὐημεροῦντα 

    <lb n="14"/>παρὰ τῶι βασιλεῖ. 

    <lb n="15"/>ἔρρωσο. <expan><ex cert="high">ἔτους</ex></expan> <num value="4">δ</num>, 

    <lb n="16"/><supplied cert="high" reason="lost">Ὑπερβε</supplied><unclear cert="high" reason="undefined">ρε</unclear>ταίου <unclear cert="high" reason="undefined">ἐ</unclear>μβολίμο<supplied cert="high" reason="lost">υ </supplied><gap extent="1" reason="lost" unit="character"/><num value="3"><unclear cert="high" reason="undefined">γ</unclear></num>. 

    <lb n="17"/><gap extent="unknown" reason="lost" unit="character"/>σι<gap extent="unknown" reason="lost" unit="character"/><num value="3">γ</num>. 
