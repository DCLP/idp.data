    <lb n="1"/>Ἀμμώνιος ὁ παρὰ Ἡρακλείδου σι<supplied cert="high" reason="lost">τολόγου</supplied> 

    <lb n="2"/>Ζήνωνι χαίρειν. μεμέτρηκεν Ε<gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ὑ</supplied>-

    <lb n="3"/>πὲρ Εὐτυχίδου εἰς τὸν ἐν Σύρ<supplied cert="high" reason="lost">ων κώμηι θησαυ</supplied>-

    <lb n="4"/>ρὸν τοῦ Φαρμοῦθι <gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/> <expan>πυ<ex cert="high">ρῶν</ex></expan> <num value="108">ρη</num>, <unclear cert="high" reason="undefined">ὀ</unclear><supplied cert="high" reason="lost">λυρῶν <num value="70">ο</num></supplied>, 

    <lb n="5"/>καὶ τῆι <num value="22">κβ</num> ὀλυρῶν <num value="261">σξα</num> <num value="1/2">𐅵</num> <expan><ex cert="high">γίνονται</ex></expan> τοῦ Φαρ<supplied cert="high" reason="lost">μοῦθι <expan>πυ<ex cert="high">ρῶν</ex></expan> <num value="108">ρη</num></supplied>, 

    <lb n="6"/>ὀλυρῶν <num value="331">τλα</num> <num value="1/2">𐅵</num>, καὶ τοῦ Παχὼνς <gap extent="1" extentmax="2" reason="lost" unit="character"/> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/>καὶ τῆι <num value="2">β</num> ὀλυρῶν <num value="105">ρε</num> καὶ τῆι <gap extent="1" extentmax="2" reason="lost" unit="character"/> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>καὶ <num value="16">ιϛ</num> <expan>πυ<ex cert="high">ρῶν</ex></expan> <num value="6">ϛ</num> <expan><ex cert="high">γίνονται</ex></expan> τοῦ Παχὼν<supplied cert="high" reason="lost">ς πυρῶν </supplied><gap extent="2" reason="lost" unit="character"/>, 

    <lb n="9"/>ὀλυρῶν <num value="105">ρε</num>, εἰς τὸ <choice><corr>αὐτὸ</corr><sic>ταυτο</sic></choice> πυρ<supplied cert="high" reason="lost">ῶν </supplied><gap extent="3" reason="lost" unit="character"/> 

    <lb n="10"/>καὶ ὀλυρῶν <num value="436">υλϛ</num> <num value="1/2">𐅵</num>. 

    <lb n="11"/>ἔρρωσο. <expan><ex cert="high">ἔτους</ex></expan> <num value="3">γ</num>, Θῶυθ <num value="7">ζ</num>. 

