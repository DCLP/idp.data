    <lb n="5a"/>και<gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="2" unit="character"/> 

    <lb n="6"/>δὲ παρ᾽ ἡμῶν εκ<gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="3" unit="character"/> 

