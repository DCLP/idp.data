    <lb n="5a"/>και<gap extent="1" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/> 

    <lb n="6"/>δὲ παρ᾽ ἡμῶν εκ<gap extent="2" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/> 

    <lb n="7"/><gap extent="unknown" reason="lost" unit="line"/> 

    <lb n="7"/><gap extent="6" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀπ</supplied>οστέλλειν. 

    <lb n="8"/>εὖ <choice><corr>οὖν</corr><sic>ουμ</sic></choice> ποιήσεις γράψας 

    <lb n="9"/>Θεμιστοκλεῖ καὶ Πετοσίρι 

    <lb n="10"/>τῶι βασιλικῶι γραμματεῖ 

    <lb n="11"/>γράψα<unclear cert="high" reason="undefined">ι</unclear> Ἀξάπηι ἕως 

    <lb n="12"/>ἐπιδημεῖ <del rend="erasure" status="unremarkable"><gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/></del> 

