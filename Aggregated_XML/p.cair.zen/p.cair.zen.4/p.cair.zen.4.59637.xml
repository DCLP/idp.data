    <lb n="4"/>ἀπὸ <expan><ex cert="high">δραχμῶν</ex></expan> <num value="300">τ</num> <expan><ex cert="high">δραχμὰς</ex></expan> <num value="250">σν</num>· <choice><corr>τελείως</corr><sic>τελεως</sic></choice> <unclear reason="undefined" cert="high">γ</unclear><supplied reason="lost" cert="high">ὰρ</supplied> 

