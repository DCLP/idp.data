    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> χαίρει</supplied>ν. καλῶς ποιεῖς εἰ ἔρρωσαι αὐτὸς τε <supplied reason="lost" cert="high">καὶ οὓς βούλει, </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/> παραγενόμενος εἰς Σύρων κώμην εὗ<unclear reason="undefined" cert="high">ρ</unclear><supplied reason="lost" cert="high">ον </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> Ὄλ</supplied><unclear reason="undefined" cert="high">υ</unclear>μπος μὲν οὖν ἀνέπλευσεν ἄνω π<unclear reason="undefined" cert="high">α</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" extent="1" unit="character"/>ου συνέμειξα Σωστράτωι βουλόμε<supplied reason="lost" cert="high">νος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/><unclear reason="undefined" cert="high">α</unclear>ν Ὀλύμπωι· ὁ δὲ ἕως μὲν οὖν περιέφ<supplied reason="lost" cert="low">υγέν με </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><gap reason="lost" extent="unknown" unit="character"/><unclear reason="undefined" cert="high">ν</unclear>ωνος οὐδὲ συνέστησέ<unclear reason="undefined" cert="high">ν</unclear> <supplied reason="lost" cert="high">με</supplied> <unclear reason="undefined" cert="high">ο</unclear>ὐθε<supplied reason="lost" cert="high">νὶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/><unclear reason="undefined" cert="high">ρ</unclear>ας σοι καταστήσω τὰ ἔρια ον<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><gap reason="lost" extent="unknown" unit="character"/>ου παραγενόμενον ἔ<unclear reason="undefined" cert="high">ρ</unclear>ευν<unclear reason="undefined" cert="high">α</unclear>ν <unclear reason="undefined" cert="high">α</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><gap reason="lost" extent="unknown" unit="character"/>ς δὲ ἑώρ<add place="supralinear">ω</add><subst><add place="inline">ν</add><del rend="corrected" status="unremarkable">εωρ<del rend="erasure" status="unremarkable">ο</del>ν</del></subst> Λέοντα τὸν παρ᾽ Ἑρμολάου <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/><gap reason="lost" extent="unknown" unit="character"/>ην πάντοθεν· ἀπεσταλμένοι δέ εἰ<supplied reason="lost" cert="high">σιν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><gap reason="lost" extent="unknown" unit="character"/> <unclear reason="undefined" cert="high">Μ</unclear>αγδω<unclear reason="undefined" cert="high">λ</unclear>ίου εὗρον διὰ Σωστράτου α<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἐ</supplied>πυθόμην δὲ καὶ περὶ τῆς <num value="6">ϛ</num>, καὶ ἔφατο <unclear reason="undefined" cert="high">γ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἐ</supplied>πὶ τὰς Κοίτας καὶ ἐπῆλθον τὸ σήσαμον <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> σ</supplied>υνφύτου, οἱ δ᾽ ἔφασαν ὑπ<add place="supralinear">ὲρ</add> τὰς <num value="7000">Ζ</num> ἔσεσ<supplied reason="lost" cert="high">θαι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><gap reason="lost" extent="unknown" unit="character"/><unclear reason="undefined" cert="high">ρ</unclear>ιον παρεγένετο Εὔδημος λέγων δι<unclear reason="undefined" cert="high">ό</unclear><supplied reason="lost" cert="high">τι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἄ</supplied>γριοι βόες. ἐγὼ οὖν καλέσας Μενίτα<unclear reason="undefined" cert="high">ν</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/><gap reason="lost" extent="unknown" unit="character"/>ν εἶναι. ἐπελθὼν οὖν ἐκεῖ ἀπένειμα ε<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἐάν</supplied> τις αὐτῶν ἀτακτήσῃ, δήσας πρὸς σ<supplied reason="lost" cert="high">ὲ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="19"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> Κάλλ</supplied>ιππον· ἀναγκαῖον γὰρ ἔφασαν εἶναι ἵν<unclear reason="undefined" cert="high">α</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/><gap reason="lost" extent="unknown" unit="character"/><unclear reason="undefined" cert="high">τ</unclear> ἀπελαύνῃ. ἀπέστελλεν δὲ καὶ Ἰάσ<supplied reason="lost" cert="high">ων </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="21"/><gap reason="lost" extent="unknown" unit="character"/><unclear reason="undefined" cert="high">τ</unclear>ον, ἐγὼ δὲ <choice><corr>κατέσχηκα</corr><sic>κατεισχηκα</sic></choice> αὐτὸν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="22"/><gap reason="lost" extent="unknown" unit="character"/> δέ σοι καὶ πῶλος ἄρσην ἐκ τῆς Καλλικ<supplied reason="lost" cert="high">ράτους κώμης </supplied><gap reason="lost" extent="unknown" unit="character"/> 
