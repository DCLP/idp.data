    <lb n="29"/><supplied reason="lost" cert="high">καὶ τὰ δὲ λ</supplied><unclear reason="undefined" cert="high">οιπ</unclear>ὰ <unclear reason="undefined" cert="high">κ</unclear>αλῶς <unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">οι</supplied>-

