    <lb n="26"/>δ<supplied reason="lost" cert="high">ιαπυνθά</supplied>νεσθαι π<unclear reason="undefined" cert="high">ερὶ</unclear> <unclear reason="undefined" cert="high">ὧν</unclear> 

    <lb n="27"/>ἂ<supplied reason="lost" cert="high">ν βουλώμ</supplied>εθα καὶ μ<supplied reason="lost" cert="high">ὴ τῶν βα</supplied>-

