    <lb n="2"/>τὸ μολύβδου <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>Σώπα<supplied reason="lost" cert="high">τρος. καλῶς ἂν οὖν ποιή</supplied>-

    <lb n="4"/>σαις δ<unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">ὺς αὐτῶι ὅπως ἂν ἀπο</supplied>-

    <lb n="5"/>στείληι καὶ μὴ <unclear reason="undefined" cert="high">α</unclear><gap reason="lost" extent="9" unit="character"/> 

    <lb n="6"/>χρεία γὰρ αὐτοῦ ἐστ<supplied reason="lost" cert="high">ιν. τὰς δὲ</supplied> 

