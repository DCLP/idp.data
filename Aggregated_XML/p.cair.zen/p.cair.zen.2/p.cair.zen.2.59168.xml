    <lb n="1"/>Ἀπολλώνιος Ζήνωνι χαίρ<unclear cert="high" reason="undefined">ει</unclear>ν. <supplied cert="high" reason="lost">σύνταξον πρὸς τῶι</supplied> 

    <lb n="2"/>Ἰσιείωι οἰκ<supplied cert="high" reason="lost">ο</supplied>δ<unclear cert="high" reason="undefined">ο</unclear>μῆσαι <unclear cert="high" reason="undefined">Σα</unclear><supplied cert="high" reason="lost">ραπιεῖον παρὰ τὸ τῶν</supplied> 

    <lb n="3"/>Διοσκούρων ἱε<unclear cert="high" reason="undefined">ρὸ</unclear>ν καὶ τ<unclear cert="high" reason="undefined">ὸ</unclear>ν ὥ<supplied cert="high" reason="lost">στε </supplied><gap extent="12" reason="lost" unit="character"/> 

    <lb n="4"/><unclear cert="high" reason="undefined">ἀ</unclear>πολ<supplied cert="high" reason="lost">ελειμμένον τόπον. ἐπιμελὲς δέ σοι ἔστω</supplied> 

    <lb n="5"/>ὅπως παρὰ τὴν διώρυγα εἷς δρ<supplied cert="high" reason="lost">όμος ἀμφοτέρων</supplied> 

    <lb n="6"/>ἱερῶν γένηται. 

    <lb n="7"/>ἔ<unclear cert="high" reason="undefined">ρ</unclear>ρω<unclear cert="high" reason="undefined">σο</unclear>. <expan><ex cert="high">ἔτους</ex></expan> <gap extent="unknown" reason="lost" unit="character"/> 
</ab></div><div n="v" part="N" type="textpart"><ab part="N">

    <lb n="8"/><supplied cert="high" reason="lost"><expan><ex cert="high">ἔτους</ex></expan> </supplied><gap extent="1" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Δύστρ</supplied>ου <num value="4">δ</num>, Φαμενὼθ <num value="4"><unclear cert="high" reason="undefined">δ</unclear></num>. 

    <lb n="9"/><supplied cert="high" reason="lost">Ἀπολλώνι</supplied>ος Σαραπιείου. 

