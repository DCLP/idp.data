    <lb n="5"/><app type="editorial"><lem/><rdg>τὸν περικ<gap extent="unknown" reason="lost" unit="character"/></rdg></app> 

    <lb n="6"/>τὸν Ἑρμά<unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">ι</supplied> τῶι ἐκεῖ ἀρχ<supplied cert="high" reason="lost">ιφυ</supplied>λακίτ<supplied cert="high" reason="lost">ηι. </supplied><app type="editorial"><lem><supplied cert="high" reason="lost">καλῶς</supplied></lem><rdg>τὸν Ἑρμα<unclear cert="high" reason="undefined">φ</unclear><gap extent="unknown" reason="lost" unit="character"/></rdg></app> 

