    <lb n="1"/><supplied cert="high" reason="lost">Ἀ</supplied>πολλώνιος Ζήνω<supplied cert="high" reason="lost">νι χαίρειν</supplied>. ἀπεστάλκ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">μέν σοι ἐκ τοῦ</supplied> 

    <lb n="2"/><supplied cert="high" reason="lost">Ἡ</supplied>λιοπολίτου νομοῦ εἰς πρᾶσιν οἴνου <expan>κερ<ex cert="high">άμια</ex></expan> <num value="100"><unclear cert="high" reason="undefined">ρ</unclear></num>. <unclear cert="high" reason="undefined">φρ</unclear><supplied cert="high" reason="lost">όντισον οὖν</supplied> 

    <lb n="3"/><supplied cert="high" reason="lost">ὅπω</supplied><unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">π</unclear>ραθῆι τοῦ καλῶς ἐχοντος, τὴν <unclear cert="high" reason="undefined">δ</unclear><supplied cert="high" reason="lost">ὲ τιμὴν αὐτοῦ δὸς</supplied> 

    <lb n="4"/><supplied cert="high" reason="lost">εἰς</supplied> τοῦς ἀμφιτάπους. <supplied cert="high" reason="lost">γρά</supplied>ψον δ᾽ ἡμῖν πῶ<supplied cert="high" reason="lost">ς</supplied> τ<gap extent="1" reason="illegible" unit="character"/><gap extent="6" reason="lost" unit="character"/> 

    <lb n="5"/><supplied cert="high" reason="lost">α</supplied>ὐτὸν ἀποδόσθαι καὶ <gap extent="7" reason="lost" unit="character"/> σοι προσαποστ<unclear cert="high" reason="undefined">ε</unclear><gap extent="7" reason="lost" unit="character"/> 

    <lb n="6"/><supplied cert="high" reason="lost">ὑπ</supplied><unclear cert="high" reason="undefined">ά</unclear>ρχει γὰρ ἡμῖν καὶ <choice><corr>πλείω<supplied cert="high" reason="omitted">ν</supplied></corr><sic>πλειω</sic></choice> οἶνος ἐν τῶι Ἡλιοπολ<supplied cert="high" reason="lost">ίτηι</supplied>. 

    <lb n="7"/>ἔρρωσο. <expan><ex cert="high">ἔτους</ex></expan> <num value="30">λ</num>, Δύστρου <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 
