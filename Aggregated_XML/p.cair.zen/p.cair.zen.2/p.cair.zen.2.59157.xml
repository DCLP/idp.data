    <lb n="8"/><supplied reason="lost" cert="high">σ</supplied>τροβίλ<unclear reason="undefined" cert="high">ων</unclear> 

    <lb n="7"/>Ζή<supplied reason="lost" cert="high">νωνι</supplied>. 

