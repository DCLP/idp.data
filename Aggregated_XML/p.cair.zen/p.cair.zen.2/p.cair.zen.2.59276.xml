    <lb n="2"/><supplied cert="high" reason="lost">κομισώμεθα παρὰ</supplied> Ψεμθέως. κομισάμε<unclear cert="high" reason="undefined">ν</unclear>οι οὖν παρ᾽ αὐτοῦ κύβ<supplied cert="high" reason="lost">ους </supplied><gap extent="1" reason="lost" unit="character"/> 

    <lb n="3"/><supplied cert="low" reason="lost">οὓς κατήγαγεν ἐ</supplied><unclear cert="high" reason="undefined">μ</unclear>βαλόμενος <unclear cert="high" reason="undefined">ἐν</unclear> πλοίοις παρεδώκαμεν Διο<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">υσίωι</supplied> 

    <lb n="4"/><gap extent="7" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀποδόν</supplied>τες ναῦλον <expan><ex cert="high">δραχμὰς</ex></expan> <num value="5">ε</num> <expan><ex cert="high">ὀβολοῦς 4</ex></expan><num value="4"/> ἐργάταις τοῖς ἄρασιν <unclear cert="high" reason="undefined">το</unclear><gap extent="4" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> 

    <lb n="5"/><gap extent="5" reason="lost" unit="character"/><supplied cert="high" reason="lost"> δὲ τῶι παρ᾽</supplied> <unclear cert="high" reason="undefined">ἡ</unclear>μῶν ἐν Πτολεμαίδι ἔγραψα δοῦναι μι<unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">θὸν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ι τὸν μέγαν ποταμ<supplied cert="high" reason="lost">ό</supplied>ν. περὶ δὲ τῶν μὴ κατ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">χθέ</supplied>-

    <lb n="7"/><supplied cert="high" reason="lost">ντων κύβων συ</supplied>νετάξαμεν Ψεμθεῖ <unclear cert="high" reason="undefined">τ</unclear>ὴν ταχίστην ἐπισκ<unclear cert="high" reason="undefined">ε</unclear><gap extent="unknown" reason="lost" unit="character"/> 

