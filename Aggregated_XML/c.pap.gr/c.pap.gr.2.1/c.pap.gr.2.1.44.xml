    <lb n="1"/>Θεογένει τῷ καὶ <unclear reason="undefined" cert="high">Λ</unclear>αδι<unclear reason="undefined" cert="high">κ</unclear>ην<unclear reason="undefined" cert="high">ῶι</unclear> 

    <lb n="2"/><expan>γραμμα<ex cert="high">τεῖ</ex></expan> πόλ<unclear reason="undefined" cert="high">εω</unclear>ς 

    <lb n="3"/>παρὰ Ἀγ<unclear reason="undefined" cert="high">α</unclear>θοκλείας τῆς καὶ 

    <lb n="4"/>Ἀπολλωνίας Ἰσχ<unclear reason="undefined" cert="high">υρίω</unclear>νος ἀπ᾽ <supplied reason="lost" cert="high">Ὀ</supplied>-

    <lb n="5"/>ξυρύγχων πόλεως μετὰ κυρίου 

    <lb n="6"/>τοῦ υἱοῦ Ἰσχυρίωνος Θέωνος. 

    <lb n="7"/>Σαραπᾶς δοῦλος μου ἄτεχνος 

    <lb n="8"/>ἀναγραφό<unclear reason="undefined" cert="high">μενος</unclear> ᾽ ἀμφόδου 

    <lb n="9"/>Ἑρμαί<supplied reason="lost" cert="high">ο</supplied>υ ἐτελεύτησεν τῷ 

    <lb n="10"/>ἐνεστῶτι μηνὶ Ἁθύρ. διὸ ἀξιῶ 

    <lb n="11"/>ἀναγραφῆναι τ<unclear reason="undefined" cert="high">οῦτ</unclear><supplied reason="lost" cert="high">ο</supplied>ν ἐν τῇ 

    <lb n="12"/>τῶν ὁμοί<unclear reason="undefined" cert="high">ων</unclear> τάξει ὡς προσήκει, 

    <lb n="13"/>καὶ ὀμνύω Αὐτοκράτορα Καίσαρα 

    <lb n="14"/>Τίτον Αἴλιον Ἁδρια<unclear reason="undefined" cert="high">νὸν</unclear> 

    <lb n="15"/>Ἀντωνεῖνον Σε<unclear reason="undefined" cert="high">β</unclear>αστὸν Ε<unclear reason="undefined" cert="high">ὐ</unclear><supplied reason="lost" cert="high">σεβ</supplied>ῆ 

    <lb n="16"/>ἀλ<unclear reason="undefined" cert="high">η</unclear><supplied reason="lost" cert="high">θ</supplied>ῆ εἶναι τὰ <unclear reason="undefined" cert="high">π</unclear>ρογεγρ<supplied reason="lost" cert="high">άμμεν</supplied>α. 

    <lb n="17"/><expan><ex cert="high">ἔτους</ex></expan> <num value="17"><unclear reason="undefined" cert="high">ιζ</unclear></num> Αὐτοκράτορος Καίσαρος Τίτου 

    <lb n="18"/>Αἰλί<unclear reason="undefined" cert="high">ου</unclear> Ἁδριανοῦ <unclear reason="undefined" cert="high">Ἀ</unclear>ντωνείνου 

    <lb n="19"/><unclear reason="undefined" cert="high">Σεβασ</unclear>τοῦ <unclear reason="undefined" cert="high">Εὐ</unclear>σεβ<supplied reason="lost" cert="high">οῦς</supplied> Ἁθὺρ <subst><add place="inline"><num value="29">κθ</num></add><del rend="corrected" status="unremarkable"><num value="27">κζ</num></del></subst>. </ab></div><div n="meta.16887" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.16887" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" value="0153-11-25" notBefore="153" notAfter="153">25. Nov. 153</date></p></div><div n="meta.16887" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Zur Datierung vgl. BL VIII, S. 259.</p></div><div n="meta.16887" type="commentary" subtype="mentionedDates" org="uniform" part="N" sample="complete"><head>Erwähnte Daten</head></div><div n="meta.16887" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">P.Oxy.</title><biblScope type="volume">31</biblScope><biblScope type="numbers">2564</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">16887</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0006</series>; <biblScope type="volume">2.1</biblScope>; <biblScope type="numbers">44</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0006;2.1;44" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.16887" type="bibliography" subtype="corrections" org="uniform" part="N" sample="complete"><head>BL-Einträge nach BL-Konkordanz</head><bibl type="BL" default="NO"><biblScope type="volume">VIII</biblScope>, S. <biblScope type="pages">259</biblScope></bibl></div><div n="meta.16887" type="bibliography" subtype="illust