    <lb n="20"/><supplied reason="lost" cert="high">Σεβαστοῦ </supplied><gap reason="lost" extent="unknown" unit="character"/> <num value="4"><unclear reason="undefined" cert="high">δ</unclear></num>. </ab></div><div n="meta.15658" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.15658" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" notBefore="0078" notAfter="0079">78 - 79</date></p></div><div n="meta.15658" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Zur Datierung vgl. BL VIII, S. 271.</p></div><div n="meta.15658" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">P.Oxy.</title><biblScope type="volume">49</biblScope><biblScope type="numbers">3510</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">15658</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0006</series>; <biblScope type="volume">2.1</biblScope>; <biblScope type="numbers">15</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0006;2.1;15" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.15658" type="bibliography" subtype="corrections" org="uniform" part="N" sample="complete"><head>BL-Einträge nach BL-Konkordanz</head><listBibl default="NO"><bibl type="BL" default="NO"><biblScope type="volume">VIII</biblScope>, S. <biblScope type="pages">271</biblScope></bibl><bibl type="BL" default="NO"><biblScope type="volume">XI</biblScope>, S. <biblScope type="pages">169</biblScope></bibl></listBibl></div><div n="meta.15658" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p><bibl type="illustration" default="NO">ZPE 30, 1978, Tafel VII a</bibl><bibl ty