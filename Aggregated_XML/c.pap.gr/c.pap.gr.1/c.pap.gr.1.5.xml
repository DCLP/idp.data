    <lb n="1"/><handShift new="m1"/> <supplied cert="high" reason="lost">Πρωτάρχω</supplied>ι τῶι ἐπὶ τοῦ κριτηρίου 

    <lb n="2"/><handShift new="m2"/> <supplied cert="high" reason="lost">παρὰ</supplied> <unclear cert="high" reason="undefined">Μ</unclear>άρκου Αἰμιλίου Μάρκου Καλλιδία κ<supplied cert="high" reason="lost">αὶ παρὰ</supplied> 

    <lb n="3"/>Θ<supplied cert="high" reason="lost">εοδ</supplied><unclear cert="high" reason="undefined">ό</unclear>της τῆς Δωσιθέου <choice><corr>Περσίνης</corr><sic>περσει<supplied cert="high" reason="lost">ν</supplied>ης</sic></choice> με<supplied cert="high" reason="lost">τὰ κυρίου</supplied> 

    <lb n="4"/>κ<supplied cert="high" reason="lost">αὶ ἐ</supplied>γγύου τῶν κατὰ τήνδε τὴν συνχώ<supplied cert="high" reason="lost">ρησιν</supplied> 

    <lb n="5"/>σημαινομένων τοῦ ἀνδρὸς Σώφρ<unclear cert="high" reason="undefined">ονος</unclear> <supplied cert="high" reason="lost">τοῦ</supplied> 

    <lb n="6"/><gap extent="4" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/>άρχου <del rend="erasure" status="unremarkable">Σώφρονος</del> Πέρ<supplied cert="high" reason="lost">σ</supplied><unclear cert="high" reason="undefined">ου</unclear> τῆ<supplied cert="high" reason="lost">ς ἐπι</supplied>-

    <lb n="7"/><supplied cert="high" reason="lost">γονῆς.</supplied> <unclear cert="high" reason="undefined">περ</unclear>ὶ τῶν διεσταμένων <supplied cert="high" reason="lost">συγχωρεῖ ἡ Θεο</supplied>-

    <lb n="8"/>δ<unclear cert="high" reason="undefined">ότ</unclear>η ἐπὶ χρόνον μῆνας δέκα <supplied cert="high" reason="lost">ὀκτὼ <num value="18"/> ἀπὸ Φα</supplied>-

    <lb n="9"/>μ<unclear cert="high" reason="undefined">ενὼθ</unclear> τοῦ ἐνεστῶτος <num value="17"><unclear cert="high" reason="undefined">ιζ</unclear></num> <supplied cert="high" reason="omitted"><expan><ex cert="high">ἔτους</ex></expan></supplied> Καί<supplied cert="high" reason="lost">σαρος τρο</supplied>-

    <lb n="10"/><supplied cert="high" reason="lost">φεύσειν καὶ θηλάσειν ἔξω</supplied> παρ᾽ <choice><corr>ἑαυτῇ</corr><sic>εατη</sic></choice> <supplied cert="high" reason="lost">κατὰ πόλιν</supplied> 

    <lb n="11"/><supplied cert="high" reason="lost">τῷ ἰδίῳ αὐτῆς γάλακτι καθαρῷ καὶ ἀφθόρῳ</supplied> 

    <lb n="12"/><supplied cert="high" reason="lost">ὃ ἐνκεχείρικεν αὐτῇ ὁ Μᾶρκος ἀναίρετον</supplied> 

    <lb n="13"/><supplied cert="high" reason="lost">δουλικὸν παιδίον ἑαυτοῦ ὑποτίτθιον Τύχην</supplied> 

    <lb n="14"/><supplied cert="high" reason="lost">λαμβάνουσα παρ᾽ αὐ</supplied>τοῦ <unclear cert="high" reason="undefined">κατὰ</unclear> μ<supplied cert="high" reason="lost">ῆνα ἕκαστον</supplied> 

    <lb n="15"/><supplied cert="high" reason="lost">μισθὸν τοῦ τε γάλακτος καὶ τῆς τροφείας σὺν</supplied> 

    <lb n="16"/><supplied cert="high" reason="lost">ἐλαίῳ ἀργυρίου <expan><ex cert="high">δραχμὰς</ex></expan> <num value="8">η</num>. ἀπέσχηκεν δὲ ἡ Θεο</supplied>-

    <lb n="17"/><supplied cert="high" reason="lost">δότη</supplied> <unclear cert="high" reason="undefined">δι</unclear>᾽ ἐγγύου <supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">οῦ</unclear> <unclear cert="high" reason="undefined">Σ</unclear><supplied cert="high" reason="lost">ώ</supplied><unclear cert="high" reason="undefined">φρονος</unclear> παρὰ <unclear cert="high" reason="undefined">τοῦ</unclear> Μ<supplied cert="high" reason="lost">άρκου</supplied> 

    <lb n="18"/><supplied cert="high" reason="lost">διὰ χειρὸς ἐ</supplied>ξ οἴκου εἰς τοὺς προκιμένους μῆν<supplied cert="high" reason="lost">ας δέκα</supplied> 

    <lb n="19"/><supplied cert="high" reason="lost">ὀκτὼ <num value="18"/> </supplied><choice><corr><supplied cert="high" reason="lost">τ</supplied>ροφεῖα</corr><sic><supplied cert="high" reason="lost">τ</supplied>ροφηα</sic></choice> μηνῶν ἐννέα <num value="9"/> τὰς συναγο<supplied cert="high" reason="lost">μένας</supplied> 

    <lb n="20"/><supplied cert="high" reason="lost">δραχμὰ</supplied>ς ἑβδομήκοντα δύο <num value="72"/>, καὶ ἐὰν συνβῇ ἐντὸς <supplied cert="high" reason="lost">τού</supplied>-

    <lb n="21"/><supplied cert="high" reason="lost">των</supplied> <choice><corr>παθεῖν</corr><sic>παθιν</sic></choice> τι ἀνθρώπινον τὸ παιδίον, <unclear cert="high" reason="undefined">τ</unclear>ὴ<supplied cert="high" reason="lost">ν Θεο</supplied>-

    <lb n="22"/><supplied cert="high" reason="lost">δότην ἀ</supplied><unclear cert="high" reason="undefined">ν</unclear>ελομένην ἕτερον παιδίον τρο<supplied cert="high" reason="lost">φεύσειν</supplied> 

    <lb n="23"/><supplied cert="high" reason="lost">καὶ θηλ</supplied>άσειν καὶ παραστήσειν τῷ Μάρκ<supplied cert="high" reason="lost">ῳ ἐπὶ</supplied> 

    <lb n="24"/><supplied cert="high" reason="lost">τῶν</supplied> <unclear cert="high" reason="undefined">ἴσ</unclear>ων μηνῶν ἐννέα <num value="9"/> μηδὲν τὸ καθόλου <unclear cert="high" reason="undefined">λα</unclear>-

    <lb n="25"/><supplied cert="high" reason="lost">βοῦσ</supplied>α διὰ ἀθάνατον αὐτὴν ἐπιδεδέχθαι τρ<supplied cert="high" reason="lost">ο</supplied>-

    <lb n="26"/><supplied cert="high" reason="lost">φεύει</supplied>ν. εὐτακτουμένην αὐτὴν τοῖς <unclear cert="high" reason="undefined">λο</unclear><supplied cert="high" reason="lost">ιποῖς</supplied> 

    <lb n="27"/><supplied cert="high" reason="lost">κατ</supplied><unclear cert="high" reason="undefined">ὰ</unclear> <unclear cert="high" reason="undefined">μ</unclear>ῆνα <choice><corr>τροφείοις</corr><sic>τροφηοις</sic></choice> ποιεῖσθαι τήν τε <choice><corr>ἑαυτῆς</corr><sic>εατης</sic></choice> 

    <lb n="28"/><supplied cert="high" reason="lost">καὶ τοῦ</supplied> παιδίου προσήκουσαν <choice><corr>ἐπιμέλειαν</corr><sic>επιμεληαν</sic></choice> μὴ 


    <lb n="29"/><supplied cert="high" reason="lost">φθείρουσ</supplied><unclear cert="high" reason="undefined">αν</unclear> <unclear cert="high" reason="undefined">τὸ</unclear> γάλα μηδ᾽ ἀνδροκοιτοῦσαν μηδ᾽ ἐ<supplied cert="high" reason="lost">πι</supplied>-

    <lb n="30"/><supplied cert="high" reason="lost">κ</supplied><unclear cert="high" reason="undefined">υοῦσ</unclear>αν μηδ᾽ ἕτερον παραθηλάζουσαν παιδί-

    <lb n="31"/>ον, ἅ τε ἐὰν λάβῃ ἢ πιστευθῇ σῶα <choice><corr>συντηρήσειν</corr><sic>συντηρησιν</sic></choice> 

    <lb n="32"/>καὶ ἀποδώσειν ὅ<unclear cert="high" reason="undefined">ταν</unclear> ἀπαιτῆται ἢ ἐκτίσειν τὴν 

    <lb n="33"/><unclear cert="high" reason="undefined">ἑκάστου</unclear> ἀξίαν πλὴν συνφανοῦς <choice><corr>ἀπωλείας</corr><sic><unclear cert="high" reason="undefined">απ</unclear>ωληας</sic></choice> ἧς καὶ 

    <lb n="34"/>φανερᾶς γενηθείσ<supplied cert="high" reason="lost">ης ἀ</supplied>πολελύσθω, καὶ μὴ <choice><corr>ἐκλιπεῖν</corr><sic>εγ<unclear cert="high" reason="undefined">λειπ</unclear><supplied cert="high" reason="lost">ειν</supplied></sic></choice> 

    <lb n="35"/>τὴν <choice><corr>τροφείαν</corr><sic>τρ<supplied cert="high" reason="lost">ο</supplied>φηαν</sic></choice> ἐντὸς τοῦ <supplied cert="high" reason="lost">χ</supplied>ρόν<unclear cert="high" reason="undefined">ου</unclear>· ἐ<supplied cert="high" reason="lost">ὰν δ</supplied>έ τι παραβαί-

    <lb n="36"/>νῃ, εἶναι αὐτήν τε καὶ τὸν Σώφρον<supplied cert="high" reason="lost">α ἀ</supplied><unclear cert="high" reason="undefined">γ</unclear>ωγίμους 

    <lb n="37"/>καὶ συνέχεσθαι <choice><corr>μέχρι</corr><sic>μεχρει</sic></choice> τοῦ <choice><corr>ἐκτῖσαι</corr><sic>εκτε<supplied cert="high" reason="lost">ισ</supplied>αι</sic></choice> ἅ τε εἴλη-

    <lb n="38"/><unclear cert="high" reason="undefined">φεν</unclear> <choice><corr>τροφεῖα</corr><sic>τ<supplied cert="high" reason="lost">ρο</supplied>φηα</sic></choice> καὶ ἃ ἐὰν λάβ<supplied cert="high" reason="lost">ῃ</supplied> σὺν ἡμ<supplied cert="high" reason="lost">ι</supplied>ολίᾳ καὶ τὰ 

    <lb n="39"/>βλάβη καὶ δαπανήματα καὶ ἄλλας <expan>ἀργυ<ex cert="high">ρίου</ex></expan> <expan><ex cert="high">δραχμὰς</ex></expan> <num value="300"><unclear cert="high" reason="undefined">τ</unclear></num>, <unclear cert="high" reason="undefined">τῆς</unclear> 

    <lb n="40"/><unclear cert="high" reason="undefined">πράξ</unclear><supplied cert="high" reason="lost">ε</supplied>ως γινομένης ἔκ τε ἀμφοτέρων ὄντω<supplied cert="high" reason="lost">ν</supplied> 

    <lb n="41"/><choice><corr>ἀλληλεγγύων</corr><sic>αλληλενγυων</sic></choice> εἰς ἔκτισιν καὶ ἐξ ἑνὸς καὶ ἐξ οὗ ἐ<supplied cert="high" reason="lost">ὰν</supplied> 

    <lb n="42"/>αὐτῶν αἱρῆται καὶ ἐκ τῶν ὑπαρχόντων αὐτοῖς 

    <lb n="43"/>πάντων καθάπερ <choice><corr>ἐκ</corr><sic>εγ</sic></choice> δίκης, ἀκύρ<supplied cert="high" reason="lost">ω</supplied>ν οὐσῶν καὶ 

    <lb n="44"/>ὧν ἐ<supplied cert="high" reason="lost">ὰν ἐ</supplied>πενέγκωσιν πίστεων πασ<supplied cert="high" reason="lost">ῶ</supplied>ν σκέπ<supplied cert="high" reason="lost">ης</supplied> 

    <lb n="45"/>πάσης, ἕκαστα δὲ αὐτῆς ἐπιτελ<unclear cert="high" reason="undefined">ούσης</unclear> <unclear cert="high" reason="undefined">καὶ</unclear> <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ὸν</supplied> 

    <lb n="46"/><supplied cert="high" reason="lost">Μᾶρκο</supplied>ν Αἰμίλιον χορηγεῖν αὐτῇ τὰ κατὰ μῆ<supplied cert="high" reason="lost">να</supplied> 

    <lb n="47"/><choice><corr>τροφεῖα</corr><sic>τροφηα</sic></choice> ἐπὶ τοὺς λοιποὺς μῆνας ἐννέα <num value="9"/> καὶ <unclear cert="high" reason="undefined">μὴ</unclear> <choice><corr><supplied cert="high" reason="lost">ἀ</supplied>-

    <lb n="48"/>ποσπάσειν</corr><sic><supplied cert="high" reason="lost">α</supplied>πο<unclear cert="high" reason="undefined">σπ</unclear>ασιν</sic></choice> τὸ παιδίον ἐντὸς τοῦ χρόνου ἢ <unclear cert="high" reason="undefined">κα</unclear><supplied cert="high" reason="lost">ὶ αὐ</supplied>-

    <lb n="49"/>τὸν <choice><corr>ἐκτίνειν</corr><sic>εκτινιν</sic></choice> τὸ ἴσον ἐπίτιμον. παραβαλε<supplied cert="high" reason="lost">ῖ δὲ</supplied> 

    <lb n="50"/>ἡ Θεοδότη πρὸς τὸν Μᾶρκον τοῦ μηνὸς <gap extent="4" reason="illegible" unit="character"/> 

    <lb n="51"/>ἑκοῦσα καὶ τὸ παιδίον πρὸς τὸ ἐπιθεωρεῖσθαι 

    <lb n="52"/>ὑπ᾽ αὐτοῦ. 

