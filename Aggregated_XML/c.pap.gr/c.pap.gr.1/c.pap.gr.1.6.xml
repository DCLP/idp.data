    <lb n="3"/>παρὰ Ἰσιδώρας τῆς <unclear reason="undefined" cert="high">Κομ</unclear><gap reason="lost" extent="11" unit="character"/> <unclear reason="undefined" cert="high">μετὰ</unclear> <unclear reason="undefined" cert="high">κυρίου</unclear> <unclear reason="undefined" cert="high">τοῦ</unclear> <unclear reason="undefined" cert="high">ἀ</unclear><supplied reason="lost" cert="high">δελφοῦ Εὐτυχίδου</supplied> 

