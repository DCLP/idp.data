    <lb n="4"/><supplied cert="high" reason="lost">ὁ</supplied>μολογεῖ Θεμιστοκλῆς Πτολεμαίου τοῦ Θε<supplied cert="high" reason="lost">μιστοκλέους </supplied><gap extent="8" reason="lost" unit="character"/> 

