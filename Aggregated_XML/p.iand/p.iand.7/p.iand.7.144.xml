    <lb n="1"/>α<supplied reason="lost" cert="high">ὐρήλ</supplied><unclear reason="undefined" cert="high">ιος</unclear> <gap reason="lost" extent="6" unit="character"/>δης ὁ κα<supplied reason="lost" cert="high">ὶ </supplied><gap reason="lost" extent="5" unit="character"/><supplied reason="lost" cert="high"> βα</supplied>σιλικὸς γ<supplied reason="lost" cert="high">ραμμ</supplied><unclear reason="undefined" cert="high">ατ</unclear><supplied reason="lost" cert="high">εὺς </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> Αὐρηλίῳ τῷ δεῖνι</supplied> 

