    <lb n="1"/><g type="chirho"/> <choice><corr>ὑπατείας</corr><sic><hi rend="diaeresis">υ</hi>πατια<supplied reason="lost" cert="high">ς</supplied></sic></choice> Φλαου<hi rend="diaeresis">ί</hi>ου Φιλοξένου τοῦ 

    <lb n="2"/>λαμπροτάτου Φαῶφι <num value="17">ιζ</num> <expan>ἰν<unclear reason="undefined" cert="high">δι</unclear>κ<ex cert="high">τίονος</ex></expan> <num value="4">δ</num> 

    <lb n="3"/>Φλαου<hi rend="diaeresis">ί</hi>ῳ Σερήνῳ τῷ <expan>λαμπροτ<ex cert="high">άτῳ</ex></expan> ἀ<unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ὸ</supplied> 

