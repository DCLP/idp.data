    <lb n="8"/>αὐτ<supplied cert="high" reason="lost">ὸ</supplied><unclear cert="high" reason="undefined">ς</unclear> καθο<unclear cert="high" reason="undefined">ν</unclear><gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="6" reason="illegible" unit="character"/>

    <lb n="9"/>μηδέπ<unclear cert="high" reason="undefined">ο</unclear>τε <add place="supralinear">με</add>μισθωκέν<unclear cert="high" reason="undefined">αι</unclear>

    <lb n="10"/>αὐτῷ τὰς ἀρούρας. ἐξέ-

    <lb n="11"/>τασον δὲ τοὺς πέρυσι καὶ

    <lb n="12"/>προπ<unclear cert="high" reason="undefined">έ</unclear>ρυσ<supplied cert="high" reason="lost">ι</supplied> πράκτ<supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ρ</unclear>ας,

    <lb n="13"/>καὶ, ἐὰν <choice><corr>πεισθῇς</corr><sic>πισθης</sic></choice> π<supplied cert="high" reason="lost">ε</supplied>ρὶ τού-

    <lb n="14"/>του, <choice><corr>ἀπαρενόχλητον</corr><sic>απαρανοχλη<unclear cert="high" reason="undefined">τ</unclear>ον</sic></choice>

