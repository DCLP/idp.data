    <lb n="1"/><supplied cert="high" reason="lost">ὑπ</supplied><unclear cert="high" reason="undefined">ατεί</unclear><supplied cert="high" reason="lost">ας</supplied> <expan>Φλ<ex cert="high">αουίων</ex></expan> <supplied cert="high" reason="lost">Μα</supplied>ξ<supplied cert="high" reason="lost">ίμου τὸ <num value="2">β</num> καὶ Πατερίου τῶν</supplied> 

