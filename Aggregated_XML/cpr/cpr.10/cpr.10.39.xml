    <lb n="3"/><expan>ἰνδικ<ex cert="high">τίωνος</ex></expan> ἐν Ἡρακλέους πόλει Φλαυ<supplied cert="high" reason="lost">ίῳ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> καθοσιωμέ</supplied>-

    <lb n="4"/>νῳ μαγιστριανῷ τῶν θείω<unclear cert="high" reason="undefined">ν</unclear> <supplied cert="high" reason="lost">ὀφφικίων ἐν τῷ</supplied> 

    <lb n="5"/>Ἡρακλεοπολίτῃ Αὐρήλιο<unclear cert="high" reason="undefined">ς</unclear> <supplied cert="high" reason="lost">Ἀνδρόνικος Νείλου</supplied> 

    <lb n="6"/><unclear cert="high" reason="undefined">ἀπ</unclear><supplied cert="high" reason="lost">ὸ τ</supplied>ῆς μεγ<unclear cert="high" reason="undefined">άλη</unclear>ς πόλεως Ἀλεξ<supplied cert="high" reason="lost">ανδρείας</supplied> 

    <lb n="7"/><supplied cert="high" reason="lost">ἐν Ἡ</supplied>ρακλέους πό<supplied cert="high" reason="lost">λει</supplied> κ<unclear cert="high" reason="undefined">ον</unclear>διτάριος ἑξ<supplied cert="high" reason="lost">ῆς ὑπογράφων ἰδίοις</supplied> 

    <lb n="8"/><supplied cert="high" reason="lost">γράμ</supplied>μασιν <space dim="horizontal" extent="unknown" unit="character"/> χαίρειν. ὁ<supplied cert="high" reason="lost">μο</supplied>λ<supplied cert="high" reason="lost">ο</supplied>γ<supplied cert="high" reason="lost">ῶ</supplied> με <choice><corr>ἔχειν</corr><sic><unclear cert="high" reason="undefined">ε</unclear>χιν</sic></choice> 

    <lb n="9"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> παρὰ σοῦ τὴν μεταξὺ συμφωνηθεῖ-

    <lb n="10"/><supplied cert="high" reason="lost">σαν </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τιμὴ</supplied>ν κούφων εὐαρέστων δισχιλί-

