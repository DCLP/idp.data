    <lb n="16"/><supplied cert="high" reason="lost">ἀπηλιώ</supplied>του κληρονόμοι Φαυστίνου <gap extent="5" reason="lost" unit="character"/>νις <unclear cert="high" reason="undefined">ανγιος</unclear>

