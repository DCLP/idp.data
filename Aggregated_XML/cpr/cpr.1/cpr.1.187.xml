    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/>νου ως<gap reason="lost" extent="unknown" unit="character"/>τατ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><supplied reason="lost" cert="high">ἐν τῇ π</supplied>όλει ἀγορ<supplied reason="lost" cert="high">αν</supplied><gap reason="lost" extent="3" unit="character"/> ἀπὸ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="7" unit="character"/>ης Ἀμμων<gap reason="lost" extent="3" unit="character"/><supplied reason="lost" cert="high"> τέτ</supplied>αρτον μ<supplied reason="lost" cert="high">έρος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>τρι<gap reason="lost" extent="5" unit="character"/> πάντων ἐπ᾽ ἀμ<supplied reason="lost" cert="high">φ</supplied>όδου Φανησίο<supplied reason="lost" cert="high">υ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>σὺν κ<gap reason="lost" extent="3" unit="character"/><supplied reason="lost" cert="high"> νό</supplied>του δημοσία πλατεῖα, βορρᾶ ἑτέρων ομο<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>τετ<supplied reason="lost" cert="high">άχ</supplied>θαι καθὼς αἱ περὶ αὐτῶν προκτήσεις περιέχουσι <supplied reason="lost" cert="high">καὶ ἔχειν τὴν τιμὴν</supplied> 

    <lb n="7"/><supplied reason="lost" cert="high">τὴν πρὸς ἀλλήλους συμ</supplied>πεφωνημένην τοῦ πεπραμένου καθὼς πρόκειτα<supplied reason="lost" cert="high">ι ἀργυρίου δραχμὰς</supplied> 

    <lb n="8"/><gap reason="lost" extent="3" unit="character"/> ἑκατὸν <num value="100"/> διὰ τῆς Διδύμου τραπέζης Φανησίου καὶ βεβ<supplied reason="lost" cert="high">αιώσει τῇ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>καὶ τοῖς παρ᾽ α<supplied reason="lost" cert="high">ὐτῆς</supplied> τὸ πεπραμένον τέταρτον μέρος τ<supplied reason="lost" cert="high">ῆς οἰκίας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/><supplied reason="lost" cert="high">ἀνέπα</supplied>φον καὶ ἀνενεχύραστον καὶ <choice><corr>ἀνεπιδάνειστον</corr><sic>ανεπιδανιστον</sic></choice> καὶ καθα<supplied reason="lost" cert="high">ρὸν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>ἀπό <choice><corr>τε</corr><sic>δε</sic></choice> λαογραφιῶν μεχρὶ ἑτέρας κατ᾽ οἰκίαν ἀπογρ<supplied reason="lost" cert="high">αφῆς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>μηδέ<supplied reason="lost" cert="high">να ἐ</supplied>μποιούμενον αὐτοῦ μηδὲ μέρους μηδε<supplied reason="lost" cert="high">νὸς </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> εἰσοδεύ</supplied>-

    <lb n="13"/>οντας καὶ ἐξοδεύοντας καὶ καθαιροῦντας καὶ <app type="BL"><lem resp="8.99">ἀνοικοδ<supplied reason="lost" cert="high">ομοῦντας</supplied></lem><rdg><choice><corr>ἀντικατ<supplied reason="lost" cert="high">αλάσσοντας</supplied></corr><sic>αντικαδ<supplied reason="lost" cert="high">αλασσοντας</supplied></sic></choice></rdg></app><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/>ἃς καὶ πρὸς ταῦτα τὸν Ἀμμώνιον μὴ ἐνκαλεῖν μηδὲ ἀ<supplied reason="lost" cert="high">μφισβητεῖν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><choice><corr>τιμὴν</corr><sic>τειμην</sic></choice> μεθ᾽ ἡμιολίας καὶ τὰ βλάβη καὶ ἀνηλώματα διπλᾶ καὶ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>τῶν <expan>προγεγρ<ex cert="high">αμμένων</ex></expan> <expan>ὑπογρ<ex cert="high">αφεῖς</ex></expan> τοῦ μὲν Ἀμμωνίου Χαιρήμων Χαιρήμονος <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/><handShift new="m2"/> Ἀλ<supplied reason="lost" cert="high">έξ</supplied>α<supplied reason="lost" cert="high">νδρ</supplied>ος Διοσκόρου ὁμολογῶ πεπρακέναι τὸ προκ<supplied reason="lost" cert="high">είμενον </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/>Ἀμμωνίου τέταρτον μέρος οἰκίας κοινὸν καὶ ἀδιαίρετον πρὸς <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> μετὰ παντὸς αὐτῶν</supplied> 

    <lb n="19"/>τοῦ δικαίου ὧν καὶ αἱ γειτνίαι πρόκινται καὶ ἀπέχω τὴν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/>ἀμφόδου Φανησίου καὶ βεβαιώσω πάσῃ <choice><corr>βεβαιώσει</corr><sic>βεβαιωσι</sic></choice> καὶ καθαρὰ <supplied reason="lost" cert="high">παρέξω ὡς</supplied> πρόκι<supplied reason="lost" cert="high">ται </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="21"/>σα Νικάριον Ἀμμωνίου μετὰ κυρίου τοῦ ἀδελφοῦ Σαρ<gap reason="lost" extent="4" unit="character"/> <choice><corr>ἐώνημαι</corr><sic>εωνημε</sic></choice> <gap reason="illegible" extent="1" unit="character"/>ν<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="22"/>ἔγραψα ὑπὲρ αὐτῶν Τιμόθεως Μάρωνος μὴ <choice><corr>εἰδότων</corr><sic>ιδοτων</sic></choice> γράμματα. </ab></div><div n="meta.9835" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.9835" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" value="0145-01-04" notBefore="145" notAfter="145">4. Jan. 145</date></p></div><div n="meta.9835" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Gehört zu CPR I 173 und CPR I 195. Zum Ort und zur Datierung  vgl. CPR I 195.</p></div><div n="meta.9835" type="commentary" subtype="mentionedDates" org="uniform" part="N" sample="complete"><head>Erwähnte Daten</head></div><div n="meta.9835" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">CPR</title><biblScope type="volume">1</biblScope><biblScope type="numbers">187</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">9835</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0009</series>; <biblScope type="volume">1</biblScope>; <biblScope type="numbers">187</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0009;1;187" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.9835" type="bibliography" subtype="corrections" org="uniform" part="N" sample="complete"><head>BL-Einträge nach BL-Konkordanz</head><bibl type="BL" default="NO"><biblScope type="volume">VIII</biblScope>, S. <biblScope type="pages">99</biblScope></bibl></div><div n="meta.9835" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p>keine</p></div><div n="meta.9835" type="history" subtype="locations" org="uniform" part="N" sample="complete"><p><placeName type="