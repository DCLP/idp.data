    <lb n="19"/>ἥ ἐστιν ἀργ<supplied reason="lost" cert="high">υρίου σεβαστοῦ νομίσματ</supplied>ος δραχμὰς ἑξ<supplied reason="lost" cert="high">ακοσίας</supplied> <num value="600"/>· 

    <lb n="20"/>προσομολογεῖ ὁ πωλ<supplied reason="lost" cert="high">ῶν ἀπεσχηκέ</supplied>ναι παρὰ τῆς ὠ<supplied reason="lost" cert="high">νουμένης</supplied> 

    <lb n="21"/>ταύτας ἐκ πλήρους δι<supplied reason="lost" cert="high">ὰ χειρὸς ἐξ οἴκ</supplied>ου καὶ παρέξεται <supplied reason="lost" cert="high">αὐ</supplied>-

    <lb n="22"/>τὰς καθαρὰς ἀπό τε <supplied reason="lost" cert="high">ἄλλων πράσε</supplied>ων ἢ ἑτέρων οἰκ<supplied reason="lost" cert="high">ονομιῶν</supplied> 

    <lb n="23"/>καὶ ἀπὸ παντὸς τοῦ ἐπελευ<supplied reason="lost" cert="high">σομέν</supplied>ου ἢ ἐμποιησομ<supplied reason="lost" cert="high">ένου ἀπὸ τοῦ</supplied> 

    <lb n="24"/>νῦν ἐπὶ τὸν ἅ<supplied reason="lost" cert="high">παντα</supplied> χρόνον, <supplied reason="lost" cert="high">βεβαιώσει δὲ</supplied> πάσῃ βεβαιώ<supplied reason="lost" cert="high">σει ἐνέ</supplied>-

    <lb n="25"/>χεσθαι αὐτὸν παραβάντα τῷ ὡ<supplied reason="lost" cert="high">ρισμένῳ κατὰ τῶν</supplied> παρ<supplied reason="lost" cert="high">ασυγγρα</supplied>-

    <lb n="26"/>φούντων ἐπ<supplied reason="lost" cert="high">ιτίμω</supplied>ι καὶ μηδὲ<supplied reason="lost" cert="high">ν ἦσσον τὰ διωμολογ</supplied>ημέ<supplied reason="lost" cert="high">να κύρια</supplied> 

    <lb n="27"/>εἶναι· ὁ δὲ χ<supplied reason="lost" cert="high">ρηματισ</supplied>μὸς <choice><corr>ἐτελειώ<supplied reason="lost" cert="high">θη</supplied></corr><sic>ετελιω<supplied reason="lost" cert="high">θη</supplied></sic></choice> <supplied reason="lost" cert="high">κινδύνῳ αὐτῶν πρωτοπράξιας </supplied><gap reason="lost" extent="3" unit="character"/><supplied reason="lost" cert="high"> περὶ δὲ τοῦ</supplied> 

    <lb n="28"/>ταῦτα ὀρθῶς καὶ καλῶς γεγε<supplied reason="lost" cert="high">νῆσθαι ἐπερωτηθέντες ὡμολόγησαν.  κυρία</supplied> 

