    <lb n="1"/><supplied cert="high" reason="lost">ὧν ἐπέδωκα λιβέλλων περί τινων κακούργων ἐπιχειρη</supplied>-

    <lb n="1"/>σ<unclear cert="high" reason="undefined">άντων</unclear> <unclear cert="high" reason="undefined">ἐπιβῆν</unclear>αι γῇ ἡμετέρᾳ κ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ὶ</supplied> ἧς ἔτυχον ὑπογρα<unclear cert="high" reason="undefined">φ</unclear><supplied cert="high" reason="lost">ῆ</supplied><unclear cert="high" reason="undefined">ς</unclear> 

    <lb n="2"/>τῆς προσταττ<supplied cert="high" reason="lost">ού</supplied><unclear cert="high" reason="undefined">σ</unclear>ης σοι τῷ πάντα ἀρί<unclear cert="high" reason="undefined">στ</unclear>ῳ ἐξάκτορ<unclear cert="high" reason="undefined">ι</unclear> 

    <lb n="3"/>μεταξὺ <add place="supralinear">ἡμῶν</add> διαλή<unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ψ</supplied>εσθαι καὶ τὸν προ<unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">ή</supplied><unclear cert="high" reason="undefined">κ</unclear>οντα ὅρον δώ<unclear cert="high" reason="undefined">σε</unclear>ιν ἴσο<unclear cert="high" reason="undefined">ν</unclear> 

    <lb n="4"/>ὡς περιέχει ἐντ<unclear cert="high" reason="undefined">ά</unclear><supplied cert="high" reason="lost">ξα</supplied><unclear cert="high" reason="undefined">ς</unclear>, ἐπιδίδωμί σοι τά<supplied cert="high" reason="lost">δ</supplied><unclear cert="high" reason="undefined">ε</unclear> τὰ βιβλία ἀξιῶν ἐπισταλῆναι 

    <lb n="5"/>τοὺς <expan>προκ<ex cert="high">ειμένους</ex></expan> αὐτὰ ταῦ<supplied cert="high" reason="lost">τα</supplied>, <hi rend="diaeresis">ἵ</hi>ν᾽ εἰδῶσι καὶ ἀπαν<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ή</supplied><unclear cert="high" reason="undefined">σ</unclear>ωσι ἐπὶ τήν δ<unclear cert="high" reason="undefined">ίαιταν</unclear> ἢ <unclear cert="high" reason="undefined">δὲ</unclear> <unclear cert="high" reason="undefined">δ</unclear><supplied cert="high" reason="lost">ι</supplied> 

    <lb n="6"/><supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">ὐ</unclear>τῆς ὑπεναν<supplied cert="high" reason="lost">τι</supplied>ώ<unclear cert="high" reason="undefined">σα</unclear>σθαι ἄχρι κρίσεως.  ἔστι δέ· Ο<unclear cert="high" reason="undefined">ὐ</unclear>αλ<unclear cert="high" reason="undefined">ερί</unclear><supplied cert="high" reason="lost">ῳ Οὐικτωρι</supplied>-

    <lb n="7"/><supplied cert="high" reason="lost">νια</supplied><unclear cert="high" reason="undefined">ν</unclear>ῷ τῷ διασημο<unclear cert="high" reason="undefined">τά</unclear><supplied cert="high" reason="lost">τῳ ἡγ</supplied><unclear cert="high" reason="undefined">εμ</unclear>όνι πα<unclear cert="high" reason="undefined">ρὰ</unclear> <sic><unclear cert="high" reason="undefined">παρ</unclear></sic> Ἀ<unclear cert="high" reason="undefined">δ</unclear><supplied cert="high" reason="lost">ελ</supplied>φί<unclear cert="high" reason="undefined">ο</unclear>υ <unclear cert="high" reason="undefined">Ἀδελφ</unclear><supplied cert="high" reason="lost">ίου·  ἔστι μοι</supplied> 


    <lb n="8"/><supplied cert="high" reason="lost">γ</supplied>ῆ, ἡγεμ<unclear cert="high" reason="undefined">ὸ</unclear>ν δέσπ<supplied cert="high" reason="lost">οτα, ἐν</supplied> <unclear cert="high" reason="undefined">π</unclear>εδ<unclear cert="high" reason="undefined">ίο</unclear>ις κώμ<unclear cert="high" reason="undefined">ης</unclear> Θύνεως καὶ Σ<unclear cert="high" reason="undefined">αραπιήου</unclear>. 

    <lb n="9"/><supplied cert="high" reason="lost">ἀλ</supplied>λὰ ταύτης ἐπι<unclear cert="high" reason="undefined">μελη</unclear>θεὶς καὶ σπορᾷ παραδέδωκα. <choice><corr>ἐπειδὴ</corr><sic>επ<unclear cert="high" reason="undefined">ιδη</unclear></sic></choice> <unclear cert="high" reason="undefined">ο</unclear>ὖν <unclear cert="high" reason="undefined">Ἀνυ</unclear>-

    <lb n="10"/>σιος καὶ Φιβίων καὶ κοινωνοὶ <num value="6">ϛ</num> ἀ<unclear cert="high" reason="undefined">π</unclear><hi rend="subscript"><unclear cert="high" reason="undefined">ὸ</unclear></hi> κώμης Πρερὴ <hi rend="subscript"><unclear cert="high" reason="undefined">ἐ</unclear></hi><unclear cert="high" reason="undefined">πε</unclear><supplied cert="high" reason="lost">χείρησαν</supplied> 

    <lb n="11"/><subst><add place="inline">ἐπιβαίνειν</add><del rend="corrected" status="unremarkable">επιβαινιν</del></subst> τῇ γῇ, ἧς κατὰ πάντας τοὺς <unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">ό</supplied><unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ου</supplied><unclear cert="high" reason="undefined">ς</unclear> ἐ<unclear cert="high" reason="undefined">π</unclear>ιδ<gap extent="4" reason="lost" unit="character"/> 

    <lb n="12"/>καὶ τήν νομὴν ἔχω, δέομαί σου τῆς ἀ<unclear cert="high" reason="undefined">ρε</unclear>τῆς πρ<unclear cert="high" reason="undefined">οσ</unclear>τάξαι ᾧ <unclear cert="high" reason="undefined">ἂν</unclear> 

    <lb n="13"/>δοκιμάσῃς τὴν κατ᾽ ἐμοῦ <choice><corr>γιγνομένην</corr><sic>γειγνομενην</sic></choice> <choice><corr>ἐπήρειαν</corr><sic>επηριαν</sic></choice> <choice><corr>ἐπισχ<supplied cert="high" reason="lost">εῖ</supplied>ν</corr><sic>επεισχ<supplied cert="high" reason="lost">ει</supplied>ν</sic></choice>. 

    <lb n="14"/>διευτύχει.  ὑπατείας τῶν δεσποτῶν ἡμῶν Λικι<unclear cert="high" reason="undefined">νν</unclear>ί<supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">υ</unclear> 

    <lb n="15"/>Σεβαστοῦ τὸ <num value="6">ϛ</num> καὶ Λικιννίου τοῦ ἐπιφανεστάτου <unclear cert="high" reason="undefined">Καί</unclear>σα<unclear cert="high" reason="undefined">ρος</unclear> <supplied cert="high" reason="lost">τὸ</supplied> <num value="2"><unclear cert="high" reason="undefined">β</unclear></num>, 

    <lb n="16"/>πρὸ <num value="8">η</num> Καλανδῶν Μαρτίων.  ὁ ἐξάκ<unclear cert="high" reason="undefined">τ</unclear>ωρ μεταξὺ ὑμῶν <unclear cert="high" reason="undefined">δ</unclear><supplied cert="high" reason="lost">ιαλ</supplied><unclear cert="high" reason="undefined">ήψεται</unclear> 

    <lb n="17"/>τὸν προσήκοντα ὅρον <choice><corr>ἐξοίσειν</corr><sic>εξοισιν</sic></choice>. <expan>κολ<ex cert="high">λήματος</ex></expan> <num value="61">ξα</num>, <expan>τόμ<ex cert="high">ου</ex></expan> <num value="1">α</num>.  ἕως το<unclear cert="high" reason="undefined">ύτ</unclear>ω<unclear cert="high" reason="undefined">ν</unclear>. 

    <lb n="18"/>δ<supplied cert="high" reason="lost">ιε</supplied><unclear cert="high" reason="undefined">υτ</unclear>ύχει. 

