    <lb n="14"/>κωμητικῇ αὐθαδίᾳ χρησάμεν<supplied cert="high" reason="lost">οι</supplied> ἐπῆλθον κω-

    <lb n="15"/>λύοντες τοὺς καρπο<supplied cert="high" reason="lost">ύ</supplied>ς, καὶ <hi rend="diaeresis">ἵ</hi>να <unclear cert="high" reason="undefined">μ</unclear>ηδεμί<unclear cert="high" reason="undefined">α</unclear> σ<supplied cert="high" reason="lost">ύ</supplied>ν<unclear cert="high" reason="undefined">χ</unclear>υσις 

    <lb n="16"/>κατὰ τοὺς αὐτοὺς γένηται, ἐπιδίδωμι τῇ σῇ <choice><corr>ἐπιμελείᾳ</corr><sic>επιμελειας</sic></choice> 

    <lb n="17"/><supplied cert="high" reason="lost">τάδε</supplied> τὰ βι<unclear cert="high" reason="undefined">β</unclear><supplied cert="high" reason="lost">λ</supplied>ί<unclear cert="high" reason="undefined">α</unclear> ἀξιῶν τὴ<unclear cert="high" reason="undefined">ν</unclear> β<unclear cert="high" reason="undefined">ί</unclear><supplied cert="high" reason="lost">αν</supplied> <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">ατα</supplied><unclear cert="high" reason="undefined">μῦ</unclear>ναι καὶ ἐ<unclear cert="high" reason="undefined">πι</unclear>-

    <lb n="18"/><supplied cert="high" reason="lost">τρα</supplied>πῆν<supplied cert="high" reason="lost">αι το</supplied>ὺς καρποὺς σ<unclear cert="high" reason="undefined">υ</unclear><supplied cert="high" reason="lost">νκομί</supplied><unclear cert="high" reason="undefined">σα</unclear>σθαι <subst><add place="inline">οὓς</add><del rend="corrected">ου<del rend="erasure">ω</del>ς</del></subst> αὐτὸς 

    <lb n="19"/><supplied cert="high" reason="lost">κατ</supplied>εθέμη<unclear cert="high" reason="undefined">ν</unclear> εἰς τὴν ἰδίαν γῆν.  <unclear cert="high" reason="undefined">εἰ</unclear> <supplied cert="high" reason="lost">δὲ ν</supplied>ομίζουσι δίκαιόν 

    <lb n="20"/><supplied cert="high" reason="lost">τι</supplied> ἔχειν ἐπὶ τῆς γῆς ταύτης, ἀ<unclear cert="high" reason="undefined">π</unclear>αντησάτωσαν 

    <lb n="21"/><supplied cert="high" reason="lost">ἐ</supplied><unclear cert="high" reason="undefined">π</unclear>ὶ τὸ ἡγ<supplied cert="high" reason="lost">ε</supplied>μονικὸν δικαστήριον. 

