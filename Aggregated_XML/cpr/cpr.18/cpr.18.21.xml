    <lb n="7"/>ας καὶ αὐλῆ<supplied reason="lost" cert="high">ς καὶ</supplied> τ<unclear reason="undefined" cert="high">ῶ</unclear>ν <unclear reason="undefined" cert="high">σ</unclear>υνκυρόντων ἅ εἰ<unclear reason="undefined" cert="high">σ</unclear>ιν ἐν Λυσιμα-

    <lb n="8"/><supplied reason="lost" cert="high">χίδι τῆς Πολέμ</supplied>ωνος μερίδος, ἃ κατ<unclear reason="undefined" cert="high">έ</unclear><supplied reason="lost" cert="high">λιπε</supplied><unclear reason="undefined" cert="high">ν</unclear> τελευτ<unclear reason="undefined" cert="high">ῶν</unclear> 

    <lb n="9"/>Ἀσκληπιάδηι ὁ <unclear reason="undefined" cert="high">Δ</unclear><supplied reason="lost" cert="high">ιο</supplied>δώρας ἀδ<unclear reason="undefined" cert="high">ε</unclear>λφὸς <unclear reason="undefined" cert="high">φιλοφ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

