    <lb n="6"/><supplied cert="high" reason="lost">τα ἀποκριθ</supplied><unclear cert="high" reason="undefined">ή</unclear>σετ<supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">ι</unclear>, <unclear cert="high" reason="undefined">φ</unclear>ρόντ<unclear cert="high" reason="undefined">ισ</unclear>ον <unclear cert="high" reason="undefined">δὲ</unclear> <supplied cert="high" reason="lost">κἂ</supplied><unclear cert="high" reason="undefined">ν</unclear> νῦν, <unclear cert="high" reason="undefined">ὡ</unclear><supplied cert="high" reason="lost">ς</supplied> 

