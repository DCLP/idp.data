    <lb n="3"/>μένας δοθῆναι παρὰ σοῦ τῷ α<gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐπὶ</supplied> 

    <lb n="4"/>τῷ <choice><corr>πάσης</corr><sic>πασας</sic></choice> <hi rend="diaeresis">ἰ</hi>χθύος <hi rend="diaeresis">ὑ</hi>πηρεσίας <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>λα καὶ π<unclear cert="high" reason="undefined">οῖο</unclear>ν ἐκ προσώπου σ<supplied cert="high" reason="lost">ου </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/><gap extent="9" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">αι</unclear> τῆς <choice><corr>συνηθείας</corr><sic>συνηθιας</sic></choice> τ<unclear cert="high" reason="undefined">ο</unclear><gap extent="2" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/><gap extent="9" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ς <subst><add place="inline">συνηθείας</add><del rend="corrected">συν<del rend="erasure">ει</del>ηθειας</del></subst> ὀφείλομέν σοι ᾡδήποτ<unclear cert="high" reason="undefined">ε</unclear> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/><gap extent="3" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><supplied cert="high" reason="lost"> δοθ</supplied><unclear cert="high" reason="undefined">ῆ</unclear>ναι παρὰ σοῦ. προσεπὶ τούτοις δὲ παρασχ<unclear cert="high" reason="undefined">εῖ</unclear><supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>ἡμᾶς καὶ τῇ σῇ εὐλαβείᾳ μηνιαίως ἐπὶ πάντα τὸν εἰρημένον χ<supplied cert="high" reason="lost">ρό</supplied>-

    <lb n="10"/>νον χρυσίου νομισματίου ἥμισυ πα<unclear cert="high" reason="undefined">ρ</unclear>ὰ κεράτια τρία ἥμισυ <expan>τ<unclear cert="high" reason="undefined">έ</unclear><supplied cert="high" reason="lost">τ<ex cert="high">αρτον</ex></supplied></expan> 

    <lb n="11"/><choice><corr>ὄγδοον</corr><sic>ογδον</sic></choice> <expan>χρ<ex cert="high">υσίου</ex></expan> <expan>νο<ex cert="high">μισματίου</ex></expan> <num value="1/2">𐅵</num> <expan>π<unclear cert="high" reason="undefined">αρ</unclear><ex cert="high">ὰ</ex></expan> <expan><unclear cert="high" reason="undefined">κε</unclear><supplied cert="high" reason="lost">ρ<ex cert="high">άτια</ex></supplied></expan> <num value="3"><unclear cert="high" reason="undefined">γ</unclear></num> <num value="1/2"><unclear cert="high" reason="undefined">𐅵</unclear></num> <num value="1/4"><unclear cert="high" reason="undefined">δ</unclear></num> <num value="1/8"><unclear cert="high" reason="undefined">η</unclear></num> <unclear cert="high" reason="undefined">εἰ</unclear><supplied cert="high" reason="lost">ς</supplied> <unclear cert="high" reason="undefined">χρεία</unclear>ν σου τοῦ ἰχθ<supplied cert="high" reason="lost">υο </supplied><gap extent="5" reason="lost" unit="character"/> 

    <lb n="12"/>ὁπόταν ἔλθῃς πρὸς ἡμᾶς ἐν τῇ <supplied cert="high" reason="lost">ἡ</supplied><unclear cert="high" reason="undefined">μ</unclear>ῶν ἀποθήκῃ τῆ<supplied cert="high" reason="lost">ς αὐτῆς πό</supplied>-

    <lb n="13"/>λεως, καὶ μὴ ἐξεῖναι <del rend="erasure">μοι</del> ἡμῖν <choice><corr>ὑπαναχωρεῖν</corr><sic>υπαναχωρεν</sic></choice> ταύτης <supplied cert="high" reason="omitted">τῆς</supplied> ὁμολ<supplied cert="high" reason="lost">ο</supplied>γί<supplied cert="high" reason="lost">ας</supplied> 

    <lb n="14"/>πρὸ συμπληρώσ<unclear cert="high" reason="undefined">εως</unclear> <unclear cert="high" reason="undefined">τ</unclear>ο<unclear cert="high" reason="undefined">ῦ</unclear> <unclear cert="high" reason="undefined">χ</unclear>ρόνου μήτε σοι ἀποβαλέσθαι ἡμ<unclear cert="high" reason="undefined">ᾶς</unclear> <unclear cert="high" reason="undefined">ἐ</unclear><supplied cert="high" reason="lost">ν</supplied>-

    <lb n="15"/>τὸς τούτου. εἰ δὲ ὑ<unclear cert="high" reason="undefined">πα</unclear>ναχωρήσωμεν εἴσω τοῦ χρόνου ἐνέχεσθ<unclear cert="high" reason="undefined">αι</unclear> 

    <lb n="16"/>ἡμᾶς παραχρῆμα <subst><add place="inline">δοῦναι</add><del rend="corrected">δουν<del rend="erasure">ν</del>αι</del></subst> λόγῳ προστίμο<supplied cert="high" reason="lost">υ</supplied> χρυσίου νομισμάτια ἕξ, 

    <lb n="17"/><expan>νο<ex cert="high">μισμάτια</ex></expan> <num value="6">ϛ</num> ἐξ <hi rend="diaeresis">ὑ</hi>παρχόντων μο<unclear cert="high" reason="undefined">υ</unclear> πάντων. εἰ δὲ καὶ σὺ <choice><corr>ἀποβάλῃς</corr><sic>αποβαλη</sic></choice> ἡμᾶς πρὸ τῆς <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ού</supplied>-

    <lb n="18"/>του συμπληρώσεως ἐ<unclear cert="high" reason="undefined">ν</unclear>έχεσθαι καί σε δοῦναι παρ᾽ εὐθὺ<unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">ἡμῖ</unclear><supplied cert="high" reason="lost">ν</supplied> 

    <lb n="19"/>λόγῳ προστίμου τὰ ἴσα <unclear cert="high" reason="undefined">τ</unclear>οῦ χρυσίου νομισμάτια ἕξ, <expan>χρ<ex cert="high">υσίου</ex></expan> <expan>νο<ex cert="high">μισμάτια</ex></expan> <num value="6">ϛ</num>, καὶ <expan>ἐπ<unclear cert="high" reason="undefined">ερ</unclear><ex cert="high">ωτηθέντες</ex></expan> <expan><supplied cert="high" reason="lost">ὡμ</supplied><ex cert="high">ολογήσαμεν</ex></expan>. 

    <lb n="20"/><handShift new="m2"/> Αὐρήλιοι Σερῆνος <expan><ex cert="high">καὶ</ex></expan> Ἀμαῖος <choice><corr>υἱοὶ</corr><sic><hi rend="diaeresis">υ</hi>ιος</sic></choice> Ἀπολλῶ ἰχθυοπ<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ᾶται</supplied> 

    <lb n="21"/>οἱ προκείμενοι <unclear cert="high" reason="undefined">στοιχεῖ</unclear> ἡμῖν τὸ γραμματεῖον ὡς 

    <lb n="22"/>πρόκειται. <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> <expan><unclear cert="high" reason="undefined">Ἡράκλ</unclear><ex cert="high">ειος</ex></expan> Ἰωάννου <expan>ἔγρ<ex cert="high">αψα</ex></expan> <expan><ex cert="high">ὑπὲρ</ex></expan> <subst><add place="inline">αὐτῶν</add><del rend="corrected">αυτου</del></subst> <expan>παρ<ex cert="high">όντων</ex></expan> 

    <lb n="23"/>ἀγραμμάτων ὄντων. <g type="stauros"/> 

    <lb n="24"/><handShift new="m3"/><g type="stauros"/> δι εμυ <add place="supralinear"><g type="stauros"/></add> <unclear cert="high" reason="unspecified">η</unclear>λια <g type="monogram"/> 

    <lb n="24"/>δι᾽ ἐμοῦ <add place="supralinear"><g type="stauros"/></add> Ἠλία 
