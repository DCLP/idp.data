    <lb n="1"/>χμγ 

    <lb n="2"/>εἰδοῦ <choice><corr>ἀπέστειλά</corr><sic>απεστιλα</sic></choice> σοι διὰ ἄπα Σιρίου Πὲς <unclear reason="undefined" cert="high">καὶ</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>λάη μ<unclear reason="undefined" cert="high">εγ</unclear><supplied reason="lost" cert="high">ά</supplied>λα ἓξ <num value="6"/> καὶ διπλᾶ μικρὰ τριά<unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">οντα <num value="3?1"/> </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>πέντε <num value="5"/> καὶ σέλλιον ἓν <num value="1"/> καὶ <choice><corr>καρακάλλιον</corr><sic>καλακαριον</sic></choice> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>κνίδι<unclear reason="undefined" cert="high">α</unclear> καλ<unclear reason="undefined" cert="high">ά</unclear>. σὺ μὴ οὖν <choice><corr>ἀμελήσῃς</corr><sic>αμελησις</sic></choice> ὅπ<unclear reason="undefined" cert="high">ω</unclear><supplied reason="lost" cert="high">ς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>πέμψαι <choice><corr>μοι</corr><sic>με</sic></choice> τὴν φάσιν μετὰ τοῦ γραμματ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>οὖν <choice><corr>ἀμελήσῃς</corr><sic>αμελησις</sic></choice> πέμψαι τὸν ἄπα Σίρ<supplied reason="lost" cert="high">ιον </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><hi rend="diaeresis">ἵ</hi>να <choice><corr>ἐργάσησθε</corr><sic>εργασασθαι</sic></choice>. πολλὰ δὲ προσαγορ<supplied reason="lost" cert="high">εύω </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>καὶ <choice><corr>μείζονας</corr><sic>μιζονας</sic></choice> τῆς κώμης, Ἀπολλω<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>καὶ πασῶν τῶν δε<gap reason="illegible" extent="2" unit="character"/>ρα<gap reason="illegible" extent="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>ἡμῶν δέσποτα <g type="stauros"/>. πολλὰ ὑμῖν προ<supplied reason="lost" cert="high">σκυνῶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 
