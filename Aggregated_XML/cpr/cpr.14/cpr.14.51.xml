    <lb n="1"/>χμγ 

    <lb n="2"/>εἰδοῦ <choice><corr>ἀπέστειλά</corr><sic>απεστιλα</sic></choice> σοι διὰ ἄπα Σιρίου Πὲς <unclear cert="high" reason="undefined">καὶ</unclear> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>λάη μ<unclear cert="high" reason="undefined">εγ</unclear><supplied cert="high" reason="lost">ά</supplied>λα ἓξ <num value="6"/> καὶ διπλᾶ μικρὰ τριά<unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">οντα <num value="3?1"/> </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>πέντε <num value="5"/> καὶ σέλλιον ἓν <num value="1"/> καὶ <choice><corr>καρακάλλιον</corr><sic>καλακαριον</sic></choice> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>κνίδι<unclear cert="high" reason="undefined">α</unclear> καλ<unclear cert="high" reason="undefined">ά</unclear>. σὺ μὴ οὖν <choice><corr>ἀμελήσῃς</corr><sic>αμελησις</sic></choice> ὅπ<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">ς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/>πέμψαι <choice><corr>μοι</corr><sic>με</sic></choice> τὴν φάσιν μετὰ τοῦ γραμματ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/>οὖν <choice><corr>ἀμελήσῃς</corr><sic>αμελησις</sic></choice> πέμψαι τὸν ἄπα Σίρ<supplied cert="high" reason="lost">ιον </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/><hi rend="diaeresis">ἵ</hi>να <choice><corr>ἐργάσησθε</corr><sic>εργασασθαι</sic></choice>. πολλὰ δὲ προσαγορ<supplied cert="high" reason="lost">εύω </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>καὶ <choice><corr>μείζονας</corr><sic>μιζονας</sic></choice> τῆς κώμης, Ἀπολλω<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/>καὶ πασῶν τῶν δε<gap extent="2" reason="illegible" unit="character"/>ρα<gap extent="3" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/>ἡμῶν δέσποτα <g type="stauros"/>. πολλὰ ὑμῖν προ<supplied cert="high" reason="lost">σκυνῶ </supplied><gap extent="unknown" reason="lost" unit="character"/> 
</ab></div><div n="v" org="uniform" part="N" sample="complete" type="textpart"><ab part="N">

    <lb n="12"/>τῷ τὰ πάντα ἐναρέτῳ Πέτρῳ <choice><corr>ἐξκέ<supplied cert="high" reason="omitted">π</supplied>το<supplied cert="high" reason="lost">ρι</supplied></corr><sic>εκσκετο<supplied cert="high" reason="lost">ρι</supplied></sic></choice> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/><expan>π<ex cert="high">αρὰ</ex></expan> <g type="stauros"/> Πέτρου φροντι<supplied cert="high" reason="lost">σ</supplied>τοῦ </ab></div></div><div n="meta.37206" org="uniform" part="N" sample="complete" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.37206" org="uniform" part="N" sample="complete" subtype="textDate" type="commentary"><p><date notAfter="600" notBefore="501" type="textDate">VI</date></p></div><div n="meta.37206" org="uniform" part="N" sample="complete" subtype="principalEdition" type="bibliography"><listBibl default="NO"><bibl default="NO" subtype="principal" type="publication"><title level="s" type="abbreviated">CPR</title><biblScope type="volume">14</biblScope><biblScope type="numbers">51</biblScope></bibl><bibl default="NO" type="Trismegistos"><biblScope type="numbers">37206</biblScope></bibl><bibl default="NO" type="DDbDP"><series>0009</series>; <biblScope type="volume">14</biblScope>; <biblScope type="numbers">51</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0009;14;51" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.37206" org="uniform" part="N" sample="complete" subtype="corrections" type="bibliography"><head>BL-Einträge nach BL-Konkordanz</head><bibl default="NO" type="BL"><biblScope type="volume">X</biblScope>, S. <biblScope type="pages">57</biblScope></bibl></div><div n="meta.37206" org="uniform" part="N" sample="complete" subtype="illustrations" type="bibliography"><p><bibl default="NO" type="illustration">Plates 43 + 44</bibl></p></div><div n="meta.37206" org="uniform" part="N" sample="complete" subtype="locations" type="history"><p>unbekannt</p></div></body></text></TEI.2>