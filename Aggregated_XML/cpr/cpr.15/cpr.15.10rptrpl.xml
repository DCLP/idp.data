    <lb n="2"/><supplied cert="high" reason="lost">παίου</supplied> <unclear cert="high" reason="undefined">θε</unclear>οῦ με<supplied cert="high" reason="lost">γάλου μεγάλου ἱεροῦ</supplied> 

    <lb n="3"/>μὴ ἐφάψεσ<supplied cert="high" reason="lost">θαι αὐτὸν τούτων ἀλλ᾽ εἶναι</supplied> 

