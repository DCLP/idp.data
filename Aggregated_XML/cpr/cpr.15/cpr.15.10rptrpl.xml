    <lb n="5"/>τὸ προκίμενον κεφάλ<supplied cert="high" reason="lost">αιον καὶ τοὺς</supplied> 

    <lb n="6"/>τόκους. τοῦ <subst><add place="inline">οὖν</add><del rend="corrected" status="unremarkable">ουυ</del></subst> Ἁρπ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">γάθου μὴ βου</supplied>-

    <lb n="7"/>λομένου ἑκ<supplied cert="high" reason="lost">ο</supplied>υσίως ἀ<unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">οδοῦναι</supplied> 

    <lb n="8"/>τὸ πρ<unclear cert="high" reason="undefined">ο</unclear>κίμεν<supplied cert="high" reason="lost">ον</supplied> κεφά<unclear cert="high" reason="undefined">λ</unclear><supplied cert="high" reason="lost">αιον καὶ τοὺς</supplied> 

    <lb n="9"/>τόκους ἀλλ<supplied cert="high" reason="lost">ὰ διαπλα</supplied><unclear cert="high" reason="undefined">νῶν</unclear><supplied cert="high" reason="lost">τος προ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/>διὰ τῆς ση<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/>καὶ <choice><corr>ἀξιῶ</corr><sic>αξιωι</sic></choice> <expan>κατα[χωρ<ex cert="high">ισθέντος</ex></expan> μου τοῦ <expan>ὑπομν<ex cert="high">ήματος</ex></expan>] 

    <lb n="12"/>μεταδοθ[ῆναι τὸ <expan>ἀντίγρ<ex cert="high">αφον</ex></expan> τῶι τῶν ἱερέων] 

    <lb n="13"/>τοῦ θεοῦ Σοκ<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">οπαίου ἡγουμένωι Σαταβοῦτι</supplied> 

    <lb n="14"/><unclear cert="high" reason="undefined">Ὥ</unclear>ρ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">υ</supplied> ὅπ<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">ς εἰδῆι ἀκολούθως τῆι δηλου</supplied>-

    <lb n="15"/>μέν<supplied cert="high" reason="lost">ηι συγγραφῆι ἐμοὶ μένῃ ἡ ἐπιβάλλουσα</supplied> 

    <lb n="16"/>τῶι Ἁρπα<unclear cert="high" reason="undefined">γ</unclear><supplied cert="high" reason="lost">άθηι ἐπιβολὴ τῶν φιλανθρώπων</supplied> 

    <lb n="17"/>ὡς καὶ ἐ<unclear cert="high" reason="undefined">π</unclear>᾽ <supplied cert="high" reason="lost">ἄλλ</supplied>ων σύν<unclear cert="high" reason="undefined">η</unclear><supplied cert="high" reason="lost">θές ἐστιν κατὰ</supplied> 

    <lb n="18"/>μηδὲν ἐλαττ<supplied cert="high" reason="lost">ου</supplied>μένο<unclear cert="high" reason="undefined">υ</unclear> <supplied cert="high" reason="lost">μου ἐν τῆι ἐπὶ τὸν</supplied> 

    <lb n="19"/>Ἁρπαγάθην πράξει. 
