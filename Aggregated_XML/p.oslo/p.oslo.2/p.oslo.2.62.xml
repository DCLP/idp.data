    <lb n="1"/>Διοσκουρίδη<supplied cert="high" reason="lost">ς</supplied> <unclear cert="high" reason="undefined">Εὐ</unclear>δαιμ<supplied cert="high" reason="lost">ο</supplied>νίδι τ<unclear cert="high" reason="undefined">ῆ</unclear><supplied cert="high" reason="lost">ι</supplied> 

    <lb n="2"/>γλυκυτά<supplied cert="high" reason="lost">τ</supplied>ηι ἀδελφῇ χαίρειν. 

    <lb n="3"/>ἀσπάζομαί σε, ἀδελφή, διὰ <supplied cert="high" reason="lost">τ</supplied>ούτων μου 

    <lb n="4"/>τῶν γραμμάτων καὶ εὔχομαι πᾶσι τοῖς 

    <lb n="5"/>θεοῖς εὐθέω<supplied cert="high" reason="lost">ς</supplied> ἐπανελθόντα <unclear cert="high" reason="undefined">μ</unclear>ε καταλα-

    <lb n="6"/>βεῖν <add place="supralinear">σε</add> <del rend="erasure">ὡς</del> <subst><add place="inline"><unclear cert="high" reason="undefined">ὑγ</unclear><supplied cert="high" reason="lost">ι</supplied>αίνουσαν</add><del rend="corrected">δει<supplied cert="high" reason="lost">ι</supplied>αινουσαν</del></subst>. <supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ῷ</unclear> <unclear cert="high" reason="undefined">λα</unclear>μπροτ<unclear cert="high" reason="undefined">ά</unclear>τῳ 

    <lb n="7"/>ἡγεμόνι π<supplied cert="high" reason="lost">ρο</supplied><unclear cert="high" reason="undefined">σ</unclear>ῆλ<unclear cert="high" reason="undefined">θο</unclear>ν. <unclear cert="high" reason="undefined">πρ</unclear><supplied cert="high" reason="lost">ο</supplied>σεδέξ<unclear cert="high" reason="undefined">α</unclear>το τὸ <expan>πρ<unclear cert="high" reason="undefined">ᾶγ</unclear><ex cert="high">μα</ex></expan> 

    <lb n="8"/>εἰπὼν ἀκούσεσθαι τῷ κα<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">α</supplied>πλόῳ. καὶ <unclear cert="high" reason="undefined">μ</unclear>έ-

    <lb n="9"/>χρι τῆς Κ<unclear cert="high" reason="undefined">ο</unclear>π<supplied cert="high" reason="lost">τ</supplied>οῦ γείνομ<unclear cert="high" reason="undefined">α</unclear>ι <unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ε</supplied>τὰ τοῦ κρατίστ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">υ</supplied> 

    <lb n="10"/>Σεουήρου <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">αὶ</supplied> ἐκεῖθεν θε<unclear cert="high" reason="undefined">ῶ</unclear>ν θελόντων 

    <lb n="11"/>ἐπανέρχο<supplied cert="high" reason="lost">μ</supplied><unclear cert="high" reason="undefined">αι</unclear> πρὸς σέ. <del rend="erasure"><supplied cert="high" reason="lost">ὁ</supplied> οἶν<unclear cert="high" reason="undefined">ο</unclear></del> ἡγ<unclear cert="high" reason="undefined">οῦ</unclear>μ<supplied cert="high" reason="lost">αι</supplied> 

    <lb n="12"/>ὅτι ὁ οἶνο<supplied cert="high" reason="lost">ς</supplied> τοῦ <app type="editorial"><lem><seg cert="low" part="N">νομ<unclear cert="low" reason="undefined">ά</unclear><supplied cert="low" reason="lost">ρχου</supplied></seg></lem><rdg>νομ<unclear cert="high" reason="undefined">α</unclear><gap extent="4" reason="lost" unit="character"/></rdg></app> τῆς Σιναρὺ 

    <lb n="13"/><choice><corr>πέπρακται</corr><sic>π<unclear cert="high" reason="undefined">ε</unclear>πραται</sic></choice> καλίστ<supplied cert="high" reason="lost">ης τιμῆς</supplied>. εἰ δὲ μή καλί-

    <lb n="14"/><supplied cert="high" reason="lost">στης </supplied><gap extent="unknown" reason="lost" unit="character"/> καὶ ὁ οἶν<supplied cert="high" reason="lost">ος </supplied><gap extent="unknown" reason="lost" unit="character"/> 

