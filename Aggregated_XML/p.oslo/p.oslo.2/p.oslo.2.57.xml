    <lb n="10"/><supplied cert="high" reason="lost">το</supplied>ὺς μισθοὺς, ἐπ<unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">ι</supplied><unclear cert="high" reason="undefined">δ</unclear>ὴ τοῦ <unclear cert="high" reason="undefined">τ</unclear><gap extent="16" reason="lost" unit="character"/> 

