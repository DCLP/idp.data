    <lb n="1"/>Γορπιαίου <num value="23">κγ</num> 

    <lb n="2"/>βασιλ<unclear cert="high" reason="undefined">εύ</unclear>οντος Πτολεμαίου τοῦ Πτολεμαίου καὶ <supplied cert="high" reason="lost">Ἀρσινόης</supplied> 

    <lb n="3"/>θεῶν <unclear cert="high" reason="undefined">Ἀ</unclear>δελφῶν <expan><ex cert="high">ἔτους</ex></expan> <num value="10">ι</num>, ἐφ᾽ ἱερέως Ἀπολλωνίδου τοῦ <supplied cert="high" reason="lost">Μοσχίωνος</supplied> 

    <lb n="4"/>τὸ <num value="2">β</num> <expan><ex cert="high">ἔτος</ex></expan> <supplied cert="high" reason="lost">Ἀ</supplied>λεξάν<unclear cert="high" reason="undefined">δ</unclear>ρου καὶ θεῶν Ἀδελφῶν καὶ θεῶν Εὐ<supplied cert="high" reason="lost">εργετῶν,</supplied> 

    <lb n="5"/>κανη<supplied cert="high" reason="lost">φό</supplied>ρου Ἀρσινόη<unclear cert="high" reason="undefined">ς</unclear> Φιλαδέλφου Μενεκράτειας τ<supplied cert="high" reason="lost">ῆς Φιλάμ</supplied>-

    <lb n="6"/>μονος τὸ <num value="2">β</num> <expan><ex cert="high">ἔτος</ex></expan>, μηνὸς Γορπιαίου <num value="23">κγ</num>, <choice><corr>ἐν</corr><sic>εγ</sic></choice> Κροκο<unclear cert="high" reason="undefined">δ</unclear>ίλω<supplied cert="high" reason="lost">ν πόλει</supplied> 

    <lb n="7"/>τοῦ Ἀρ<supplied cert="high" reason="lost">σ</supplied>ινοίτου, τάδε διέθετο νοῶν <unclear cert="high" reason="undefined">καὶ</unclear> φ<unclear cert="high" reason="undefined">ρον</unclear><supplied cert="high" reason="lost">ῶ</supplied>ν <gap extent="8" reason="lost" unit="character"/> 

    <lb n="8"/>δης Βοι<unclear cert="high" reason="undefined">ώ</unclear>τιος τῶν Πυθαγγέλο<supplied cert="high" reason="lost">υ</supplied> <unclear cert="high" reason="undefined">ἐ</unclear>πιλοχα<supplied cert="high" reason="lost">γὸς</supplied> <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">ληροῦχος</supplied> 

    <lb n="9"/>ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <num value="60">ξ</num> μελίχρους βραχὺς οὐλὴ ἐπ᾽ ἀριστερᾶς <supplied cert="high" reason="lost">ὀ</supplied>φ<supplied cert="high" reason="lost">ρύος</supplied> 

    <lb n="10"/>ἀν<supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">φ</unclear><supplied cert="high" reason="lost">άλ</supplied>ακρος. εἴη <choice><corr>μέν</corr><sic>μεμ</sic></choice> μοι ὑγιαίνοντι αὐτὸν τ<supplied cert="high" reason="lost">ὰ</supplied> 

    <lb n="11"/><supplied cert="high" reason="lost">ἐμα</supplied><unclear cert="high" reason="undefined">υ</unclear>τοῦ διοικεῖν. ἐὰν δέ τι ἀνθρώπινον πάσχω, <supplied cert="high" reason="lost">κατα</supplied>-

    <lb n="12"/><supplied cert="high" reason="lost">λείπω τ</supplied><unclear cert="high" reason="undefined">ὰ</unclear> ὑπάρχοντά μοι πάντα Ἀριστ<gap extent="8" reason="lost" unit="character"/> 

    <lb n="13"/><gap extent="6" reason="lost" unit="character"/>ους ἀστῆι. ἄλλωι δὲ οὐθενὶ οὐθὲν κ<supplied cert="high" reason="lost">αταλεί</supplied>-

    <lb n="14"/><supplied cert="high" reason="lost">πω. ὀφεί</supplied><unclear cert="high" reason="undefined">λ</unclear>ω οὐθενὶ οὐθέν. ἐπιτρόπους δὲ αἱρο<supplied cert="high" reason="lost">ῦμαι</supplied> 

