    <lb n="16"/>καὶ τῶν τ<unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">ύτω</supplied>ν συνα<unclear reason="undefined" cert="low">γω</unclear><supplied reason="lost" cert="low">νιστῶν</supplied><supplied reason="lost" cert="high"> τῶν ἀφιγμένων </supplied><gap reason="lost" extent="unknown" unit="character"/> 

