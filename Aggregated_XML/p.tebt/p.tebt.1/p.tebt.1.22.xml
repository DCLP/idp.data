    <lb n="1"/>Ταῶς κωμάρχης καὶ οἱ πρεσβ<supplied reason="lost" cert="high">ύ</supplied>τερ<supplied reason="lost" cert="high">οι</supplied> 

    <lb n="2"/>Μεγχεῖ χαίρειν.  τῆι <num value="20">κ</num> τοῦ Φαῶφι 

    <lb n="3"/>συντετευχότος σοι <choice><corr>Ἀπύ<supplied reason="omitted" cert="high">γ</supplied>χι<supplied reason="omitted" cert="high">ο</supplied>ς</corr><sic>απυχις</sic></choice> περὶ <unclear reason="undefined" cert="high">ὧ</unclear>ν 

    <lb n="4"/>σοι διεστάμην περὶ τῆς <unclear reason="undefined" cert="high">γ</unclear>ῆς <unclear reason="undefined" cert="high">οπ</unclear><gap reason="lost" extent="1" unit="character"/>ς 

    <lb n="5"/>καὶ διὰ σὲ τὰ πλεῖστα συνκάταινος 

    <lb n="6"/>ἐγενόμην.  πεπομφότες τῆι <num value="21">κα</num> 

    <lb n="7"/>ἐπὶ τὰ σπέρματα τοῦ δὲ Κότυος 

    <lb n="8"/>μὴ βεβουλημένου ἀποδοῦναι τὰ σπέρμα-

    <lb n="9"/>τα <choice><corr>ἀναβαλλομένου</corr><sic>αναβαλλομενος</sic></choice> εἰς τὸν φυλακίτην 

    <lb n="10"/>ἔτι καὶ νῦν πεπόμφαμεν πάλιν τὸν 

    <lb n="11"/><supplied reason="lost" cert="high">Ἀπῦγχ</supplied><unclear reason="undefined" cert="high">ι</unclear>ν.  γράψον ἡμῖν <sic>ἡμῖν</sic> 

    <lb n="12"/>τίς ἀ<unclear reason="undefined" cert="high">δικ</unclear>εῖ.  εἰ δ<unclear reason="undefined" cert="high">ὲ</unclear> <gap reason="lost" extent="9" unit="character"/><unclear reason="undefined" cert="high">ι</unclear> 

    <lb n="13"/>ὀμν<supplied reason="lost" cert="high">ύ</supplied>ομεν τοὺς θεοὺς <choice><corr>ἦ</corr><sic>ει</sic></choice> μὴν μετρῆσ<supplied reason="lost" cert="high">αι</supplied> 

    <lb n="14"/>Κό<supplied reason="lost" cert="high">τ</supplied><unclear reason="undefined" cert="high">υ</unclear>ος τὰ ἐκφόριά σου ἀπολέ<unclear reason="undefined" cert="high">γ</unclear>ον-

    <lb n="15"/>τ<unclear reason="undefined" cert="high">ος</unclear> τόν.  περὶ δὲ τοῦ <choice><corr>φυλ<supplied reason="omitted" cert="high">α</supplied>κίτου</corr><sic>φυλκιτου</sic></choice> 

    <lb n="16"/>ἐὰν <unclear reason="undefined" cert="high">τε</unclear> <choice><corr>διάξῃ</corr><sic>διαξησ<unclear reason="undefined" cert="high">θ</unclear>ε</sic></choice> <del rend="erasure" status="unremarkable">περὶ τοιου</del> 

    <lb n="17"/>ἀντ<supplied reason="lost" cert="high">ι</supplied>ποιούμενος περὶ τῶν σπερμάτων 

    <lb n="18"/>δέσμ<supplied reason="lost" cert="high">ιο</supplied>ν αὐτὸν ἐξαπόστειλον 

    <lb n="19"/>πρὸς ἡμᾶς. 

    <lb n="20"/><milestone rend="paragraphos" unit="undefined"/> <expan>ἔρρωσ<ex cert="high">ο</ex></expan>.  <expan><ex cert="high">ἔτους</ex></expan> <num value="6">ϛ</num> <expan>Φαῶ<ex cert="high">φι</ex></expan> <num value="21">κα</num>. 

    <lb n="21"/>προστ<unclear reason="undefined" cert="high">α</unclear>υσ<unclear reason="undefined" cert="high">ω</unclear>ς 
