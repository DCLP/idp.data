    <lb n="1"/>Μεγχῆς Πετεσούχου κωμογραμματεὺς 

    <lb n="2"/>Κερκεοσίρεως τῆς Π<supplied reason="lost" cert="high">ολ</supplied>έμωνος μερί<supplied reason="lost" cert="high">δος</supplied> 

    <lb n="3"/>τοῦ Ἀρσινοίτου Δωρίωνι Εἰρηναίου <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>τῶν <expan><ex cert="high">πρώτων</ex></expan> <expan>φί<ex cert="high">λων</ex></expan>.  <choice><corr>ὁμολογῶ</corr><sic>ομολογωι</sic></choice> <choice><corr>παρα<supplied reason="omitted" cert="high">με</supplied>μετρῆ<supplied reason="lost" cert="high">σθαι</supplied></corr><sic>παραμετρη</sic></choice> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>παρὰ σοῦ ἐν μὲν τῶι <num value="51">να</num> <expan><ex cert="high">ἔτει</ex></expan> <supplied reason="lost" cert="high">πυροῦ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>ἀρτάβας ἑκατὸν <num value="100"/> μέτρωι δοχικῶ<supplied reason="lost" cert="high">ι</supplied> καὶ <expan><supplied reason="lost" cert="high">με<ex cert="high">τρήσει</ex></supplied></expan> 

    <lb n="7"/>δικαίαι, <expan><ex cert="high">πυρὸν</ex></expan> <choice><corr>νέον</corr><sic>νηον</sic></choice> ἄδολον κεκοσκ<supplied reason="lost" cert="high">ινευμένον</supplied> 

    <lb n="8"/>καὶ φακοῦ ὁμοίως <expan><ex cert="high">ἀρτάβας</ex></expan> <num value="20">κ</num>, ἐρεγμοῦ <supplied reason="lost" cert="high"><num value="13">ιγ</num>, πίσου <num value="13">ιγ</num>,</supplied> 

    <lb n="9"/>πασπερμείου <num value="10">ι</num>, σινάπε<supplied reason="lost" cert="high">ως <num value="4">δ</num>, εὕστρας <num value="1">α</num>,</supplied> 

    <lb n="10"/><expan><ex cert="high">γίνονται</ex></expan> ὀσπρίων <num value="61">ξα</num>, <expan><ex cert="high">πυροῦ</ex></expan> <expan><ex cert="high">ἀρτάβαι</ex></expan> <num value="100">ρ</num>. 

    <lb n="11"/>ἀπὸ δὲ τοῦ <num value="52">νβ</num> <expan><ex cert="high">ἔτους</ex></expan> κατ᾽ ἐν<supplied reason="lost" cert="high">ιαυτὸν <expan>με<ex cert="high">τρήσεις</ex></expan></supplied> 

    <lb n="12"/><expan><ex cert="high">πυροῦ</ex></expan> <expan><ex cert="high">ἀρτάβας</ex></expan> <num value="50">ν</num> καθαρὰς κ<supplied reason="lost" cert="high">εκοσκινευμένας</supplied> 

    <lb n="13"/>καὶ μέτρωι δοχικ<supplied reason="lost" cert="high">ῶι <expan>με<ex cert="high">τρήσει</ex></expan> δικαίαι</supplied> 

    <lb n="14"/>καθότι προγέγραπται <supplied reason="lost" cert="high">ἄνευ δίκης</supplied> 

    <lb n="15"/>καὶ κρίσεως καὶ πάσ<supplied reason="lost" cert="high">ης <app type="BL"><lem resp="1.422"><expan>ἀντιλ<ex cert="high">ογίας</ex></expan></lem><rdg><gap reason="illegible" extent="4" unit="character"/></rdg></app> ἢ τὰ προκεί</supplied>-

    <lb n="16"/>μενα δ<unclear reason="undefined" cert="high">ιπ</unclear>λᾶ <choice><corr>ἢ</corr><sic>ηι</sic></choice> <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ιμὴν ἑκάστης ἀρτάβης</supplied> 

    <lb n="17"/>χαλκοῦ <expan><ex cert="high">δραχμὰς</ex></expan> <num value="2000">Β</num> καὶ <supplied reason="lost" cert="high">ἱερὰς θεοῖς</supplied> 

    <lb n="18"/><choice><corr>Εὐεργέταις</corr><sic>ευγεργεταις</sic></choice> <gap reason="lost" extent="1" unit="character"/><supplied reason="lost" cert="high"> ἡ ἀποχὴ</supplied> 

    <lb n="19"/><choice><corr>ἔστω</corr><sic>εστω<unclear reason="undefined" cert="high">ι</unclear></sic></choice> κυρ<supplied reason="lost" cert="high">ί</supplied>α πα<supplied reason="lost" cert="high">ν</supplied>τα<unclear reason="undefined" cert="high">χ</unclear><supplied reason="lost" cert="high">ῆ ἐπιφερομένη.</supplied> 
