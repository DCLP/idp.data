<placeName cert="high" full="yes">Tebtynis</placeName></head><div lang="grc" org="uniform" part="N" sample="complete" type="edition"><div n="ctr" org="uniform" part="N" sample="complete" type="textpart"><ab part="N">

    <lb n="1"/>Μεγχῆς Πετεσούχου κωμογραμματεὺς 

    <lb n="2"/>Κερκεοσίρεως τῆς Π<supplied cert="high" reason="lost">ολ</supplied>έμωνος μερί<supplied cert="high" reason="lost">δος</supplied> 

    <lb n="3"/>τοῦ Ἀρσινοίτου Δωρίωνι Εἰρηναίου <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>τῶν <expan><ex cert="high">πρώτων</ex></expan> <expan>φί<ex cert="high">λων</ex></expan>.  <choice><corr>ὁμολογῶ</corr><sic>ομολογωι</sic></choice> <choice><corr>παρα<supplied cert="high" reason="omitted">με</supplied>μετρῆ<supplied cert="high" reason="lost">σθαι</supplied></corr><sic>παραμετρη</sic></choice> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>παρὰ σοῦ ἐν μὲν τῶι <num value="51">να</num> <expan><ex cert="high">ἔτει</ex></expan> <supplied cert="high" reason="lost">πυροῦ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/>ἀρτάβας ἑκατὸν <num value="100"/> μέτρωι δοχικῶ<supplied cert="high" reason="lost">ι</supplied> καὶ <expan><supplied cert="high" reason="lost">με<ex cert="high">τρήσει</ex></supplied></expan> 

    <lb n="7"/>δικαίαι, <expan><ex cert="high">πυρὸν</ex></expan> <choice><corr>νέον</corr><sic>νηον</sic></choice> ἄδολον κεκοσκ<supplied cert="high" reason="lost">ινευμένον</supplied> 

    <lb n="8"/>καὶ φακοῦ ὁμοίως <expan><ex cert="high">ἀρτάβας</ex></expan> <num value="20">κ</num>, ἐρεγμοῦ <supplied cert="high" reason="lost"><num value="13">ιγ</num>, πίσου <num value="13">ιγ</num>,</supplied> 

    <lb n="9"/>πασπερμείου <num value="10">ι</num>, σινάπε<supplied cert="high" reason="lost">ως <num value="4">δ</num>, εὕστρας <num value="1">α</num>,</supplied> 

    <lb n="10"/><expan><ex cert="high">γίνονται</ex></expan> ὀσπρίων <num value="61">ξα</num>, <expan><ex cert="high">πυροῦ</ex></expan> <expan><ex cert="high">ἀρτάβαι</ex></expan> <num value="100">ρ</num>. 

    <lb n="11"/>ἀπὸ δὲ τοῦ <num value="52">νβ</num> <expan><ex cert="high">ἔτους</ex></expan> κατ᾽ ἐν<supplied cert="high" reason="lost">ιαυτὸν <expan>με<ex cert="high">τρήσεις</ex></expan></supplied> 

    <lb n="12"/><expan><ex cert="high">πυροῦ</ex></expan> <expan><ex cert="high">ἀρτάβας</ex></expan> <num value="50">ν</num> καθαρὰς κ<supplied cert="high" reason="lost">εκοσκινευμένας</supplied> 

    <lb n="13"/>καὶ μέτρωι δοχικ<supplied cert="high" reason="lost">ῶι <expan>με<ex cert="high">τρήσει</ex></expan> δικαίαι</supplied> 

    <lb n="14"/>καθότι προγέγραπται <supplied cert="high" reason="lost">ἄνευ δίκης</supplied> 

    <lb n="15"/>καὶ κρίσεως καὶ πάσ<supplied cert="high" reason="lost">ης <app type="BL"><lem resp="1.422"><expan>ἀντιλ<ex cert="high">ογίας</ex></expan></lem><rdg><gap extent="4" reason="illegible" unit="character"/></rdg></app> ἢ τὰ προκεί</supplied>-

    <lb n="16"/>μενα δ<unclear cert="high" reason="undefined">ιπ</unclear>λᾶ <choice><corr>ἢ</corr><sic>ηι</sic></choice> <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ιμὴν ἑκάστης ἀρτάβης</supplied> 

    <lb n="17"/>χαλκοῦ <expan><ex cert="high">δραχμὰς</ex></expan> <num value="2000">Β</num> καὶ <supplied cert="high" reason="lost">ἱερὰς θεοῖς</supplied> 

    <lb n="18"/><choice><corr>Εὐεργέταις</corr><sic>ευγεργεταις</sic></choice> <gap extent="1" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἡ ἀποχὴ</supplied> 

    <lb n="19"/><choice><corr>ἔστω</corr><sic>εστω<unclear cert="high" reason="undefined">ι</unclear></sic></choice> κυρ<supplied cert="high" reason="lost">ί</supplied>α πα<supplied cert="high" reason="lost">ν</supplied>τα<unclear cert="high" reason="undefined">χ</unclear><supplied cert="high" reason="lost">ῆ ἐπιφερομένη.</supplied> 
