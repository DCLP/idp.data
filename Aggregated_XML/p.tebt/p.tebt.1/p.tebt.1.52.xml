    <lb n="12"/>ἐνδ<add place="supralinear">ε</add>ὴς οὖσα τῶν ἀνα<unclear reason="undefined" cert="high">γ</unclear><supplied reason="lost" cert="high">καίων </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>καὶ τοῖς περὶ τὸ σῶμ<supplied reason="lost" cert="high">α </supplied><gap reason="lost" extent="4" unit="character"/> 

    <lb n="14"/><unclear reason="undefined" cert="high">ζ</unclear>ομι<unclear reason="undefined" cert="high">ο</unclear>ις <unclear reason="undefined" cert="high">β</unclear>ουλομένη <gap reason="lost" extent="5" unit="character"/> 

    <lb n="15"/>θαι <subst><add place="inline">τὴν</add><del rend="corrected" status="unremarkable">σην</del></subst> σημαινομέν<supplied reason="lost" cert="high">ην καὶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

