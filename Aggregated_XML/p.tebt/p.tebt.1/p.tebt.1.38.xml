    <lb n="1"/>Μεγχῆς κωμογραμ<supplied cert="high" reason="lost">μ</supplied>ατεὺς <supplied cert="high" reason="lost">Κε</supplied>ρ<supplied cert="high" reason="lost">κεοσί</supplied>ρ<supplied cert="high" reason="lost">ε</supplied>ως τῆς Πολέμων<supplied cert="high" reason="lost">ος μερίδος Ὥρωι </supplied><gap extent="10" reason="lost" unit="character"/><supplied cert="high" reason="lost"> χαίρειν.</supplied> 

    <lb n="2"/>ἐν τῶι ἐν Πτολεμαίδι Εὐεργέτιδι συνεσταμένωι <gap extent="30" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τῆς</supplied> 

    <lb n="3"/>κατὰ φύλλον γεωμετρίας προσέπεσέν μοι περὶ τ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ῦ Ἀπολλόδωρον τὸν ἐν τῆι κώμηι ἐλαιο</supplied>-

    <lb n="4"/>πώλην εὑρηκέναι τῆι <num value="11">ια</num> τοῦ ὑ<supplied cert="high" reason="lost">πο</supplied>κ<supplied cert="high" reason="lost">ει</supplied>μένου μ<supplied cert="high" reason="lost">ηνὸς *θρᾶικά τινα παραπωλήσαντα ἔλαιον</supplied> 

    <lb n="5"/>ἐν ὧι καταγίνετα<supplied cert="high" reason="lost">ι</supplied> Πετεσοῦχος σκυτεὺς οἴκ<supplied cert="high" reason="lost">ωι </supplied><gap extent="35" reason="lost" unit="character"/> 

    <lb n="6"/>καὶ τοῦτον παραδόντες Ἀπολλωνίωι τῶι διεξ<supplied cert="high" reason="lost">άγοντι τὰ κατὰ τὴν ἐπιστατεία</supplied><unclear cert="high" reason="undefined">ν</unclear> τῆς <unclear cert="high" reason="undefined">αὐτ</unclear><supplied cert="high" reason="lost">ῆ</supplied><unclear cert="high" reason="undefined">ς</unclear> 

    <lb n="7"/>σὺν τῶι ἐπιτίμωι καταστῆναι ἐφ᾽ οὓς καθήκε<supplied cert="high" reason="lost">ι περὶ ὧν ἐπιδέδωκέν μοι ὁ</supplied> Ἀπολλόδωρος 

    <lb n="8"/>τὴν ὑποκειμένην προσαγγελίαν καλῶς ἔχ<supplied cert="high" reason="lost">ειν ὑπελάβομεν προσανενέγ</supplied>και. 
<milestone rend="paragraphos" unit="undefined"/>
    <lb n="9"/><supplied cert="high" reason="lost">ἔρρωσο.  <expan><ex cert="high">ἔτους</ex></expan></supplied> <num value="4">δ</num> Μεχεὶρ <num value="14">ιδ</num>. 

    <lb n="10"/>παρ᾽ Ἀπολλοδώρου τοῦ ἐξειληφότος τὴν διάθεσι<supplied cert="high" reason="lost">ν καὶ τὸ τ</supplied>έλος τοῦ ἐλαίου 

    <lb n="11"/><unclear cert="high" reason="undefined">τῆς</unclear> <unclear cert="high" reason="undefined">α</unclear>ὐτῆς εἰς τὸ <num value="4">δ</num> <expan><ex cert="high">ἔτος</ex></expan>. τῆς ἐγλήμψεως εἰς τέλος καταλελ<supplied cert="high" reason="lost">ειμμέν</supplied>ης χάριν τῶν 

    <lb n="12"/>παρεισφερόντων εἰς τὴν κώμην καὶ παραπωλούντων Κολ<supplied cert="high" reason="lost">πιτ</supplied>ικὸν ἔλαιον καὶ κίκι, ὅθεν 

    <lb n="13"/>τῆι <num value="11">ια</num> τοῦ Μεχεὶρ <choice><corr>προσαγγελέντος</corr><sic>προσαγγελεντες</sic></choice> μοι Θρᾶικά τινα οὗ <choice><corr>τὸ</corr><sic>τον</sic></choice> <supplied cert="high" reason="lost">ὄνο</supplied>μα <choice><corr>ἀγνοῶ</corr><sic>αγνοωι</sic></choice> τῶν ἐκ Κερκε-

    <lb n="14"/>σήφεως παρεισενηνοχότα ἔλαιον εἰς ὃν ἐνοικεῖ Πετεσο<supplied cert="high" reason="lost">ῦχος σ</supplied>κυτεὺς οἶκον καὶ παρα-

    <lb n="15"/>πωλεῖν Θαήσει τῆι καταγινομένηι ἐν τῆι αὐτῆι οἰκίαι <supplied cert="high" reason="lost">καὶ </supplied><gap extent="7" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ι</unclear>ωι χηνοβοσκῶι 

    <lb n="16"/>καὶ τῆι τούτου θυγατρὶ τῶν ἐκ τῆς αὐτῆς κώμης, εὐθέως <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">αραλαβὼν τὸ</supplied><unclear cert="high" reason="undefined">ν</unclear> ἐπιστάτην καὶ τὸν 


    <lb n="17"/>παρὰ τοῦ ἀρχιφυλακίτου χάριν τοῦ καὶ σὲ μὴ παρεῖναι ἐπὶ <app type="BL"><lem resp="1.423">τῶ<supplied cert="high" reason="lost">ν τόπων</supplied></lem><rdg>τῶ<supplied cert="high" reason="lost">ι ἔργωι</supplied></rdg></app> <supplied cert="high" reason="lost">ἐ</supplied>πὶ τὸν σημαινό-

    <lb n="18"/>μενον τοῦ σκυτέως οἶκον κατέλαβον τὸν Θρᾶικα ἔνδον ὄντα τὸ <num value="4"><unclear cert="high" reason="undefined">δ</unclear></num> <supplied cert="high" reason="lost">ἐπίτ</supplied>ιμον ἐκτετο-

    <lb n="19"/>πισμ<supplied cert="high" reason="lost">έ</supplied><unclear cert="high" reason="undefined">ν</unclear>ο<supplied cert="high" reason="lost">ν, ἔρ</supplied>ευναν δὲ τούτου σὺν αὐτοῖς ποιησάμενος εὗρον ἐν ε<gap extent="4" reason="lost" unit="character"/>ωι <unclear cert="high" reason="undefined">ο</unclear>ν<gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/> 

    <lb n="20"/>αὐτῆι <gap extent="9" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">κ</unclear>ινημένηι ἐλαίου ωσ<unclear cert="high" reason="undefined">ι</unclear><gap extent="10" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ι κ<supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">ὶ</unclear> συμ<unclear cert="high" reason="undefined">μ</unclear><gap extent="3" reason="lost" unit="character"/>ριαν <unclear cert="high" reason="undefined">δ</unclear>η<gap extent="4" reason="illegible" unit="character"/> 

    <lb n="21"/>καὶ ἄλλην <gap extent="37" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ολ</unclear><gap extent="2" reason="illegible" unit="character"/><gap extent="5" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="3" reason="illegible" unit="character"/> <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ὸ</supplied><unclear cert="high" reason="undefined">ν</unclear> 

    <lb n="22"/>Θρᾶικα παρε<gap extent="30" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τοῦ σκυ</supplied>τέως κασ<unclear cert="high" reason="undefined">ῆ</unclear>ι καὶ μηλωταῖς 

    <lb n="23"/><gap extent="44" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ε</supplied>ἰς φυγὴν ὡρμηκέναι, τὸ δὲ 

    <lb n="24"/><supplied cert="high" reason="lost">ἐ</supplied>πίτιμον <gap extent="34" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ὁ προ</supplied><unclear cert="high" reason="undefined">γ</unclear>ε<unclear cert="high" reason="undefined">γραμμ</unclear>ένος Θρᾶιξ οὐ δια 

    <lb n="25"/><gap extent="3" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ν</unclear>ει<gap extent="27" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ὥστ᾽ ἂν ἐπιγεγονένα</supplied>ι μοι βλάβος εἰς <expan>χα<ex cert="high">λκοῦ</ex></expan> <expan><ex cert="high">τάλαντα</ex></expan> <num value="15">ιε</num>. 

    <lb n="26"/>διὸ ἐπιδίδω<supplied cert="high" reason="lost">μι σοι ὅπως περὶ ὑπογραφὴν ποιήσηι</supplied> προσυποτάξαντα 

    <lb n="27"/>ἀντίγραφον <supplied cert="high" reason="lost">οἷς καθήκει </supplied><gap extent="unknown" reason="lost" unit="character"/> 

