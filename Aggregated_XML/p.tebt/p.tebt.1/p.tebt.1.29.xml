    <lb n="1"/><gap extent="unknown" reason="lost" unit="line"/> 

    <lb n="1"/>τοῖς χρηματισταῖς ὧν εἰσαγωγεὺς Δ<unclear cert="high" reason="undefined">ω</unclear><gap extent="4" reason="lost" unit="character"/> 

    <lb n="2"/>παρὰ Πολέμωνος τοῦ καὶ Πετεσού<supplied cert="high" reason="lost">χου</supplied> 

    <lb n="3"/>κωμογραμματέως Κερκεοσίρεω<supplied cert="high" reason="lost">ς τῆς Πο</supplied>-

    <lb n="4"/>λέμωνος μερίδος.  Ἀκουσίλαο<supplied cert="high" reason="lost">ς καὶ Πτο</supplied>-

    <lb n="5"/>λεμαῖος οἱ δύο <num value="2"/> Ἡρακλείδου τῶν ἐ<supplied cert="high" reason="lost">κ τῆς</supplied> 

    <lb n="6"/>κώμης ἀποδέδωκάν μοι παρ᾽ ὑμ<supplied cert="high" reason="lost">ῶν ἀντί</supplied>-

    <lb n="7"/>γραφον ἐντεύξεως <choice><corr>προφερόμεν<supplied cert="high" reason="omitted">οι</supplied></corr><sic>προφερομεν</sic></choice> <unclear cert="high" reason="undefined">ὑ</unclear><supplied cert="high" reason="lost">φελέσ</supplied>-

    <lb n="8"/>θαι με αὐτῶν γῆν, ὑπὲρ ὧν καὶ τυγ<unclear cert="high" reason="undefined">χ</unclear><supplied cert="high" reason="lost">άνω ἀπο</supplied>-

    <lb n="9"/><unclear cert="high" reason="undefined">δε</unclear><supplied cert="high" reason="lost">δωκ</supplied>ὼς <choice><corr>ὑμῖν</corr><sic>ημιν</sic></choice> χ<supplied cert="high" reason="lost">ρ</supplied>ημ<supplied cert="high" reason="lost">ατ</supplied>ισμὸν πα<gap extent="6" reason="lost" unit="character"/> 

    <lb n="10"/>τη<unclear cert="high" reason="undefined">ν</unclear> τῆς τούτων <unclear cert="high" reason="undefined">σ</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="4" reason="lost" unit="character"/><unclear cert="high" reason="undefined">λ</unclear>ης.  κα<unclear cert="high" reason="undefined">θ</unclear><supplied cert="high" reason="lost">εστη</supplied>-

    <lb n="11"/>κότ<supplied cert="high" reason="lost">ο</supplied>ς οὖν τοῦ δέοντος και<supplied cert="high" reason="lost">ρ</supplied>οῦ τῆς <unclear cert="low" reason="undefined">τ</unclear><supplied cert="low" reason="lost">ρύγης</supplied> 

    <lb n="12"/>φρο<supplied cert="high" reason="lost">ν</supplied>τίδος ἱκανῆς φορολογίας <app type="BL"><lem resp="1.423">περὶ <supplied cert="high" reason="lost">ἐμὲ</supplied></lem><rdg>περι<supplied cert="high" reason="lost">σσῆς</supplied></rdg></app> 

    <lb n="13"/>οὔσ<supplied cert="high" reason="lost">η</supplied>ς καὶ διαγραφῆς μοι <choice><corr>ἐπικειμένης</corr><sic>ε<unclear cert="high" reason="undefined">π</unclear>ικειμενηι</sic></choice> <supplied cert="high" reason="lost">τῶν ὑ</supplied>-

    <lb n="14"/>πο<supplied cert="high" reason="lost">κε</supplied>ιμένων <choice><corr>τῆι</corr><sic><unclear cert="high" reason="undefined">της</unclear></sic></choice> <choice><corr>χρείαι</corr><sic>χρ<unclear cert="high" reason="undefined">η</unclear><supplied cert="high" reason="lost">ας</supplied></sic></choice> βασι<unclear cert="high" reason="undefined">λ</unclear><supplied cert="high" reason="lost">ικῶν</supplied> 

    <lb n="15"/>καὶ πυρίνου μερισμοῦ, ἀ<unclear cert="high" reason="undefined">ξι</unclear>ῶ τοῦ και<supplied cert="high" reason="lost">ροῦ αὐ</supplied>-

    <lb n="16"/>το<supplied cert="high" reason="lost">ῦ</supplied> προσδεομένου <app type="BL"><lem resp="1.423">σ<unclear cert="high" reason="undefined">υν</unclear>τάξαι</lem><rdg>ὑποτάξαι</rdg></app> παρεπ<unclear cert="high" reason="undefined">ι</unclear><supplied cert="high" reason="lost">σχεῖν</supplied> 

    <lb n="17"/>τὰ καθ᾽ ἡμᾶς μέχρι <supplied cert="high" reason="lost">το</supplied><unclear cert="high" reason="undefined">ῦ</unclear> ἀπὸ τῆς προ<supplied cert="high" reason="lost">κει</supplied>-

    <lb n="18"/>μένης ἀσχ<unclear cert="high" reason="undefined">ολ</unclear><supplied cert="high" reason="lost">ί</supplied>α<supplied cert="high" reason="lost">ς ἀπολυθῆ</supplied>ναι ἵνα <supplied cert="high" reason="lost">μηδὲν ἦι</supplied> 

    <lb n="19"/><subst><add place="inline">ἐμπόδιον</add><del rend="corrected"><del rend="erasure">α</del>μποδιον</del></subst>. 
