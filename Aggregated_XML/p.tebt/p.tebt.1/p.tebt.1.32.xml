    <lb n="1"/>Ἀρίστιππος Εὐμήλωι χα<supplied reason="lost" cert="high">ί</supplied>ρειν.  τῆς παρὰ Π<supplied reason="lost" cert="high">α</supplied>γκράτ<supplied reason="lost" cert="high">ους</supplied> <supplied reason="lost" cert="high">τοῦ πρὸς τῆι</supplied> 

    <lb n="2"/>συντάξει τῶν κατοίκων ἱππέων γεγραμμένης ἡμῖν <supplied reason="lost" cert="high">ἐπισ</supplied><unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ο</supplied>λῆς 

    <lb n="3"/><supplied reason="lost" cert="high">ὑπό</supplied>κειταί σοι τ<supplied reason="lost" cert="high">ὸ</supplied> ἀντίγραφ<supplied reason="lost" cert="high">ον.  στόχασαι οὖν ὅπως μ</supplied><unclear reason="undefined" cert="high">ὴ</unclear> παρεν<supplied reason="lost" cert="high">ο</supplied>χλή<unclear reason="undefined" cert="high">σ</unclear><supplied reason="lost" cert="high">ῃς</supplied> 

    <lb n="4"/><supplied reason="lost" cert="high">τὸν Ἀσ</supplied>κληπιάδην εἰς τὰς ἐφοδικὰς λειτουργίας. ἔρρωσο.  <expan><ex cert="high">ἔτους</ex></expan> <supplied reason="lost" cert="high"><num value="3">γ</num></supplied><num value="6"><unclear reason="undefined" cert="high">ϛ</unclear></num> Παῦνι <gap reason="lost" extent="2" unit="character"/> 

    <lb n="5"/><supplied reason="lost" cert="high">Παγ</supplied>κράτης Ἀριστίππωι χαίρειν.  ἧς γε<supplied reason="lost" cert="high">γράφ</supplied>αμ<supplied reason="lost" cert="high">ε</supplied>ν ἐπιστολῆς 


    <lb n="6"/><gap reason="lost" extent="6" unit="character"/>ι τῶ<supplied reason="lost" cert="high">ι βα</supplied><unclear reason="undefined" cert="high">σ</unclear>ιλι<unclear reason="undefined" cert="high">κ</unclear>ῶι <unclear reason="undefined" cert="high">γ</unclear>ρα<unclear reason="undefined" cert="high">μμ</unclear><supplied reason="lost" cert="high">α</supplied>τεῖ τὸ ἀντίγραφ<supplied reason="lost" cert="high">ο</supplied>ν <unclear reason="undefined" cert="high">ὑ</unclear>πο<supplied reason="lost" cert="high">τετά</supplied><unclear reason="undefined" cert="high">χα</unclear><supplied reason="lost" cert="high">μεν</supplied> 

    <lb n="7"/><supplied reason="lost" cert="high">ὅπως εἰ</supplied>δῆ<unclear reason="undefined" cert="high">ι</unclear>ς.  <expan><ex cert="high">ἔτους</ex></expan> <num value="36">λϛ</num> Φαμενὼθ <gap reason="illegible" extent="2" unit="character"/> 

    <lb n="8"/><gap reason="lost" extent="6" unit="character"/><supplied reason="lost" cert="high"> ι.</supplied> τῆς παρὰ Σώσου κ<supplied reason="lost" cert="high">αὶ</supplied> <unclear reason="undefined" cert="high">Α</unclear><supplied reason="lost" cert="high">ἰ</supplied>γ<supplied reason="lost" cert="high">ύπ</supplied>του τῶν <unclear reason="undefined" cert="high">δ</unclear><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="7" unit="character"/><unclear reason="undefined" cert="high">ν</unclear> 

    <lb n="9"/><gap reason="lost" extent="9" unit="character"/><supplied reason="lost" cert="high"> προ</supplied>χειρισθέντων ὑπὸ τ<supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">ῦ</unclear> πολιτ<unclear reason="undefined" cert="high">εύ</unclear>ματ<supplied reason="lost" cert="high">ος τῶν Κρητῶν</supplied> 

    <lb n="10"/><supplied reason="lost" cert="high">γεγραμμένης</supplied> ἡμῖν ἐπιστολῆς ὑπὲρ τῶν <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ῶι</supplied> <unclear reason="undefined" cert="high">Ἀσ</unclear>κ<supplied reason="lost" cert="high">ληπιάδηι ἀνηκόντων</supplied> 

    <lb n="11"/><supplied reason="lost" cert="high">ἀντίγραφον ὑ</supplied>πόκειται <supplied reason="lost" cert="high">ὅ</supplied>πως εἰδῆ<unclear reason="undefined" cert="high">ι</unclear>ς μετακείμε<unclear reason="undefined" cert="high">νον</unclear> <unclear reason="undefined" cert="high">α</unclear>ὐ<supplied reason="lost" cert="high">τὸ</supplied><unclear reason="undefined" cert="high">ν</unclear> <gap reason="illegible" extent="4" unit="character"/><gap reason="lost" extent="3" unit="character"/> 

    <lb n="12"/><supplied reason="lost" cert="high">καὶ γένηται ἀ</supplied>κολούθως τοῖς προστεταγμένοις. 

    <lb n="13"/><supplied reason="lost" cert="high">Σῶσος</supplied> καὶ <unclear reason="undefined" cert="high">Α</unclear><supplied reason="lost" cert="high">ἴ</supplied><unclear reason="undefined" cert="high">γ</unclear>υπτος Παγκρ<unclear reason="undefined" cert="high">ά</unclear>τει χαίρειν.  ἐπε<supplied reason="lost" cert="high">ὶ</supplied> πρ<unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">στέτα</supplied><unclear reason="undefined" cert="high">κ</unclear>ται δι᾽ ἡμῶν 

    <lb n="14"/><supplied reason="lost" cert="high">τοὺς</supplied> κατοίκους ἱππεῖς ἐφ<unclear reason="undefined" cert="high">ο</unclear><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="5" unit="character"/><unclear reason="undefined" cert="high">των</unclear><gap reason="lost" extent="2" unit="character"/><unclear reason="undefined" cert="high">α</unclear>φ<gap reason="lost" extent="17" unit="character"/> 

    <lb n="15"/><supplied reason="lost" cert="high">ἐπ</supplied>έσταλκέ μοι Ἀπολλόδωρος <supplied reason="lost" cert="high">τῶ</supplied>ν πρώτ<supplied reason="lost" cert="high">ω</supplied>ν φίλω<unclear reason="undefined" cert="high">ν</unclear> <supplied reason="lost" cert="high">ὁ ἐ</supplied>πι<supplied reason="lost" cert="high">στ</supplied><unclear reason="undefined" cert="high">ά</unclear>τη<unclear reason="undefined" cert="high">ς</unclear> 

    <lb n="16"/><supplied reason="lost" cert="high">καὶ</supplied> γραμματεὺς τῶν κατοίκων ἱππέων ἀπὸ τῶν ἐπικεχωρημένω<supplied reason="lost" cert="high">ν</supplied> 

    <lb n="17"/>τῶι πολιτεύματι τῶν Κρητῶν ἀνδρῶν <num value="500">φ</num> <subst><add place="inline">Ἀσκληπιάδην</add><del rend="corrected" status="unremarkable">ασκληπιαδη<del rend="erasure" status="unremarkable">ς</del></del></subst> 

    <lb n="18"/>Πτολεμαίου Μακεδόνα τῶν κατὰ μερίδα ἐφόδων ἐφ᾽ ὧι ἔχει κλῆ<supplied reason="lost" cert="high">ρον</supplied> 

    <lb n="19"/>περὶ Κερκεοσῖριν <supplied reason="lost" cert="high">τῆς</supplied> Πολέμωνος μερίδος <expan><ex cert="high">ἀρουρῶν</ex></expan> <num value="24">κδ</num>. καλῶς οὖν <choice><corr>ποιήσε<supplied reason="lost" cert="high">ις</supplied></corr><sic>ποησε</sic></choice> 

    <lb n="20"/>καταχωρίσας καὶ <supplied reason="lost" cert="high">λαβὼ</supplied>ν αὐτὸν ἐν τῆι πέμπτηι <expan>ἱπ<ex cert="high">παρχίαι</ex></expan> τῶν <expan><ex cert="high">ἑκατονταρούρων</ex></expan> καὶ τῶ<supplied reason="lost" cert="high">ι</supplied> 

    <lb n="21"/>Ἀπολλοδώρωι προ<supplied reason="lost" cert="high">σανε</supplied>νέ<supplied reason="lost" cert="high">γ</supplied>κας.  ὑποτετά<unclear reason="undefined" cert="high">χ</unclear><supplied reason="lost" cert="high">α</supplied><unclear reason="undefined" cert="high">μ</unclear>εν δὲ κα<supplied reason="lost" cert="high">ὶ</supplied> τὴν εἰκόνα αὐ<supplied reason="lost" cert="high">τοῦ</supplied> 

    <lb n="22"/>καὶ τοῦ υἱοῦ τὸ ὄνομα. 
<milestone rend="paragraphos" unit="undefined"/>
    <lb n="23"/>ἔστιν δὲ ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <num value="22">κβ</num> βραχὺς <expan>μελίχρ<ex cert="high">ως</ex></expan> κλαστὸς <gap reason="lost" extent="20" unit="character"/> 

