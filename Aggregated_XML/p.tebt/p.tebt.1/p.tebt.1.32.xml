    <lb n="1"/>Ἀρίστιππος Εὐμήλωι χα<supplied cert="high" reason="lost">ί</supplied>ρειν.  τῆς παρὰ Π<supplied cert="high" reason="lost">α</supplied>γκράτ<supplied cert="high" reason="lost">ους</supplied> <supplied cert="high" reason="lost">τοῦ πρὸς τῆι</supplied> 

    <lb n="2"/>συντάξει τῶν κατοίκων ἱππέων γεγραμμένης ἡμῖν <supplied cert="high" reason="lost">ἐπισ</supplied><unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ο</supplied>λῆς 

    <lb n="3"/><supplied cert="high" reason="lost">ὑπό</supplied>κειταί σοι τ<supplied cert="high" reason="lost">ὸ</supplied> ἀντίγραφ<supplied cert="high" reason="lost">ον.  στόχασαι οὖν ὅπως μ</supplied><unclear cert="high" reason="undefined">ὴ</unclear> παρεν<supplied cert="high" reason="lost">ο</supplied>χλή<unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">ῃς</supplied> 

    <lb n="4"/><supplied cert="high" reason="lost">τὸν Ἀσ</supplied>κληπιάδην εἰς τὰς ἐφοδικὰς λειτουργίας. ἔρρωσο.  <expan><ex cert="high">ἔτους</ex></expan> <supplied cert="high" reason="lost"><num value="3">γ</num></supplied><num value="6"><unclear cert="high" reason="undefined">ϛ</unclear></num> Παῦνι <gap extent="2" reason="lost" unit="character"/> 

    <lb n="5"/><supplied cert="high" reason="lost">Παγ</supplied>κράτης Ἀριστίππωι χαίρειν.  ἧς γε<supplied cert="high" reason="lost">γράφ</supplied>αμ<supplied cert="high" reason="lost">ε</supplied>ν ἐπιστολῆς 


    <lb n="6"/><gap extent="6" reason="lost" unit="character"/>ι τῶ<supplied cert="high" reason="lost">ι βα</supplied><unclear cert="high" reason="undefined">σ</unclear>ιλι<unclear cert="high" reason="undefined">κ</unclear>ῶι <unclear cert="high" reason="undefined">γ</unclear>ρα<unclear cert="high" reason="undefined">μμ</unclear><supplied cert="high" reason="lost">α</supplied>τεῖ τὸ ἀντίγραφ<supplied cert="high" reason="lost">ο</supplied>ν <unclear cert="high" reason="undefined">ὑ</unclear>πο<supplied cert="high" reason="lost">τετά</supplied><unclear cert="high" reason="undefined">χα</unclear><supplied cert="high" reason="lost">μεν</supplied> 

    <lb n="7"/><supplied cert="high" reason="lost">ὅπως εἰ</supplied>δῆ<unclear cert="high" reason="undefined">ι</unclear>ς.  <expan><ex cert="high">ἔτους</ex></expan> <num value="36">λϛ</num> Φαμενὼθ <gap extent="2" reason="illegible" unit="character"/> 

    <lb n="8"/><gap extent="6" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ι.</supplied> τῆς παρὰ Σώσου κ<supplied cert="high" reason="lost">αὶ</supplied> <unclear cert="high" reason="undefined">Α</unclear><supplied cert="high" reason="lost">ἰ</supplied>γ<supplied cert="high" reason="lost">ύπ</supplied>του τῶν <unclear cert="high" reason="undefined">δ</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="7" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ν</unclear> 

    <lb n="9"/><gap extent="9" reason="lost" unit="character"/><supplied cert="high" reason="lost"> προ</supplied>χειρισθέντων ὑπὸ τ<supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ῦ</unclear> πολιτ<unclear cert="high" reason="undefined">εύ</unclear>ματ<supplied cert="high" reason="lost">ος τῶν Κρητῶν</supplied> 

    <lb n="10"/><supplied cert="high" reason="lost">γεγραμμένης</supplied> ἡμῖν ἐπιστολῆς ὑπὲρ τῶν <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ῶι</supplied> <unclear cert="high" reason="undefined">Ἀσ</unclear>κ<supplied cert="high" reason="lost">ληπιάδηι ἀνηκόντων</supplied> 

    <lb n="11"/><supplied cert="high" reason="lost">ἀντίγραφον ὑ</supplied>πόκειται <supplied cert="high" reason="lost">ὅ</supplied>πως εἰδῆ<unclear cert="high" reason="undefined">ι</unclear>ς μετακείμε<unclear cert="high" reason="undefined">νον</unclear> <unclear cert="high" reason="undefined">α</unclear>ὐ<supplied cert="high" reason="lost">τὸ</supplied><unclear cert="high" reason="undefined">ν</unclear> <gap extent="4" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/> 

    <lb n="12"/><supplied cert="high" reason="lost">καὶ γένηται ἀ</supplied>κολούθως τοῖς προστεταγμένοις. 

    <lb n="13"/><supplied cert="high" reason="lost">Σῶσος</supplied> καὶ <unclear cert="high" reason="undefined">Α</unclear><supplied cert="high" reason="lost">ἴ</supplied><unclear cert="high" reason="undefined">γ</unclear>υπτος Παγκρ<unclear cert="high" reason="undefined">ά</unclear>τει χαίρειν.  ἐπε<supplied cert="high" reason="lost">ὶ</supplied> πρ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">στέτα</supplied><unclear cert="high" reason="undefined">κ</unclear>ται δι᾽ ἡμῶν 

    <lb n="14"/><supplied cert="high" reason="lost">τοὺς</supplied> κατοίκους ἱππεῖς ἐφ<unclear cert="high" reason="undefined">ο</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="5" reason="lost" unit="character"/><unclear cert="high" reason="undefined">των</unclear><gap extent="2" reason="lost" unit="character"/><unclear cert="high" reason="undefined">α</unclear>φ<gap extent="17" reason="lost" unit="character"/> 

    <lb n="15"/><supplied cert="high" reason="lost">ἐπ</supplied>έσταλκέ μοι Ἀπολλόδωρος <supplied cert="high" reason="lost">τῶ</supplied>ν πρώτ<supplied cert="high" reason="lost">ω</supplied>ν φίλω<unclear cert="high" reason="undefined">ν</unclear> <supplied cert="high" reason="lost">ὁ ἐ</supplied>πι<supplied cert="high" reason="lost">στ</supplied><unclear cert="high" reason="undefined">ά</unclear>τη<unclear cert="high" reason="undefined">ς</unclear> 

    <lb n="16"/><supplied cert="high" reason="lost">καὶ</supplied> γραμματεὺς τῶν κατοίκων ἱππέων ἀπὸ τῶν ἐπικεχωρημένω<supplied cert="high" reason="lost">ν</supplied> 

    <lb n="17"/>τῶι πολιτεύματι τῶν Κρητῶν ἀνδρῶν <num value="500">φ</num> <subst><add place="inline">Ἀσκληπιάδην</add><del rend="corrected" status="unremarkable">ασκληπιαδη<del rend="erasure" status="unremarkable">ς</del></del></subst> 

    <lb n="18"/>Πτολεμαίου Μακεδόνα τῶν κατὰ μερίδα ἐφόδων ἐφ᾽ ὧι ἔχει κλῆ<supplied cert="high" reason="lost">ρον</supplied> 

    <lb n="19"/>περὶ Κερκεοσῖριν <supplied cert="high" reason="lost">τῆς</supplied> Πολέμωνος μερίδος <expan><ex cert="high">ἀρουρῶν</ex></expan> <num value="24">κδ</num>. καλῶς οὖν <choice><corr>ποιήσε<supplied cert="high" reason="lost">ις</supplied></corr><sic>ποησε</sic></choice> 

    <lb n="20"/>καταχωρίσας καὶ <supplied cert="high" reason="lost">λαβὼ</supplied>ν αὐτὸν ἐν τῆι πέμπτηι <expan>ἱπ<ex cert="high">παρχίαι</ex></expan> τῶν <expan><ex cert="high">ἑκατονταρούρων</ex></expan> καὶ τῶ<supplied cert="high" reason="lost">ι</supplied> 

    <lb n="21"/>Ἀπολλοδώρωι προ<supplied cert="high" reason="lost">σανε</supplied>νέ<supplied cert="high" reason="lost">γ</supplied>κας.  ὑποτετά<unclear cert="high" reason="undefined">χ</unclear><supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">μ</unclear>εν δὲ κα<supplied cert="high" reason="lost">ὶ</supplied> τὴν εἰκόνα αὐ<supplied cert="high" reason="lost">τοῦ</supplied> 

    <lb n="22"/>καὶ τοῦ υἱοῦ τὸ ὄνομα. 
<milestone rend="paragraphos" unit="undefined"/>
    <lb n="23"/>ἔστιν δὲ ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <num value="22">κβ</num> βραχὺς <expan>μελίχρ<ex cert="high">ως</ex></expan> κλαστὸς <gap extent="20" reason="lost" unit="character"/> 

    <lb n="24"/>υἱὸς Πτολεμαῖος μηνῶν πέντε <num value="5"/> <gap extent="unknown" reason="lost" unit="character"/> 
