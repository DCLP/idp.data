    <lb n="28"/>ἐὰ<supplied cert="high" reason="lost">ν δὲ</supplied> μὴ παραδ<supplied cert="high" reason="lost">ῶ</supplied> <choice><corr>ἀποδώσω</corr><sic><supplied cert="high" reason="lost">α</supplied>ποδοσ<unclear cert="high" reason="undefined">ο</unclear></sic></choice> <choice><corr>ἃς</corr><sic><supplied cert="high" reason="lost">τα</supplied>ς</sic></choice> ἔσχον δραχμὰς 

    <lb n="29"/>τρι<supplied cert="high" reason="lost">ακοσ</supplied>ίας <num value="300"/>, ἀμεταμ<supplied cert="high" reason="lost">ίσθ</supplied>ωτα καὶ ἀναυτούργητα ἐπὶ τὸν 

    <lb n="30"/>πρ<supplied cert="high" reason="lost">οκεί</supplied>μενον χρόν<supplied cert="high" reason="lost">ον, κ</supplied>αὶ <choice><corr>ἐπερωτηθεὶς</corr><sic>επηρωτ<supplied cert="high" reason="lost">η</supplied>θεις</sic></choice> ὡμ<supplied cert="high" reason="lost">ο</supplied>λόγησα. 

    <lb n="31"/>Αὐ<supplied cert="high" reason="lost">ρή</supplied>λιος Δημή<supplied cert="high" reason="lost">τρ</supplied>ιος μεμ<supplied cert="high" reason="lost">ί</supplied>σθωμαι τὰς ἀρούρας 

