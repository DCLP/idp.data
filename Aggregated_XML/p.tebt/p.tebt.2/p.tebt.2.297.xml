    <lb n="5"/><gap extent="2" reason="lost" unit="character"/> τάξ<supplied cert="high" reason="lost">ιν</supplied> <unclear cert="high" reason="undefined">ὁ</unclear> Μαρσισοῦχος <choice><corr>παρακληθεὶς</corr><sic><unclear cert="high" reason="undefined">π</unclear>αρακλη<supplied cert="high" reason="lost">θ</supplied><unclear cert="high" reason="undefined">ις</unclear></sic></choice> <gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">α</unclear>δ<gap extent="5" reason="lost" unit="character"/> 

    <lb n="6"/>τ<gap extent="2" reason="lost" unit="character"/><unclear cert="high" reason="undefined">μα</unclear> κατέστησεν τὸν Ἁρθώτην <unclear cert="high" reason="undefined">ὃν</unclear> ε<unclear cert="high" reason="undefined">λ</unclear>ε<gap extent="2" reason="lost" unit="character"/><unclear cert="high" reason="undefined">τ</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/> 

