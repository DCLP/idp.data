    <lb n="1"/><expan>Αὐ<ex cert="high">ρήλιος</ex></expan> Σύρος <expan>Αὐ<ex cert="high">ρηλίῳ</ex></expan> Ἀ<supplied reason="lost" cert="high">κου</supplied>τᾶτι τῷ 

    <lb n="2"/>ἀδελφῷ χαί<supplied reason="lost" cert="high">ρει</supplied>ν. 

    <lb n="2a"/><gap reason="lost" extent="unknown" unit="character"/><unclear reason="undefined" cert="high">υ</unclear>π<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>καλῶς ποιήσεις <gap reason="lost" extent="3" unit="character"/><unclear reason="undefined" cert="high">ρ</unclear><gap reason="lost" extent="4" unit="character"/><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="1" unit="character"/> 

    <lb n="4"/>ἐπιστολὴν τα<gap reason="lost" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><unclear reason="undefined" cert="high">υ</unclear><gap reason="lost" extent="1" unit="character"/> 

    <lb n="5"/>ἐπὶ ἡ ἀδελφή σ<supplied reason="lost" cert="high">ου νω</supplied>θρεύεται, 

    <lb n="6"/>παραδοὺς τὴν <supplied reason="lost" cert="high">οἰ</supplied>κίαν τ<add place="supralinear">ο</add><subst><add place="inline">ῖς</add><del rend="corrected" status="unremarkable">τ<del rend="erasure" status="unremarkable">η</del>ις</del></subst> 

    <lb n="7"/>περὶ Κρονίων<supplied reason="lost" cert="high">α</supplied> καὶ Σαραπάμ-

    <lb n="8"/>μωνι καὶ τὰ βιβλία τῆς ἀδελ-

    <lb n="9"/><choice><corr>φῆ<supplied reason="omitted" cert="high">ς</supplied></corr><sic>αδελφη</sic></choice> σου. <choice><corr>ἐρεῖς</corr><sic><add place="supralinear"><unclear reason="undefined" cert="high">ε</unclear>ρις</add></sic></choice> Ταωσᾶτι ὕπαγε <del rend="erasure" status="unremarkable">π</del> μετὰ 

    <lb n="10"/>Μέλανος πρὸς Νεμεσᾶν, 

    <lb n="11"/><choice><corr>δέξαι</corr><sic>δεξε</sic></choice> παρ᾽ αὐτοῦ τὰς <expan><ex cert="high">δραχμὰς</ex></expan> <num value="100">ρ</num>. ἀσπά-

    <lb n="12"/>ζομαι τὴν γυναῖκάν μου 

    <lb n="13"/>καὶ τὰ παιδία μου καὶ Σερα-

    <lb n="14"/>πάμμωνα καὶ <unclear reason="undefined" cert="high">Ἀμ</unclear>ατίαν 

    <lb n="15"/>καὶ τοὺς ἐνοίκους πάντες 

    <lb n="16"/>κα<supplied reason="lost" cert="high">τ᾽</supplied> ὄνομα. <choice><corr>Ἡραὶς</corr><sic>ηρααεις</sic></choice> Ἀττι-

    <lb n="17"/>νέου <num value="22">κβ</num> <unclear reason="undefined" cert="high">ε</unclear>ἰκάδι καὶ δευ<supplied reason="omitted" cert="high">τέρᾳ</supplied> μηνός <choice><corr><expan><ex cert="high"><note lang="en" anchored="yes" place="not specified">?</note></ex></expan></corr><sic>δευμην<unclear reason="undefined" cert="high">η</unclear></sic></choice> 

    <lb n="18"/>τέτοκεν θῆλυ, π<unclear reason="undefined" cert="high">ρ</unclear>ὸς σήμε<unclear reason="undefined" cert="high">ρ</unclear>ον 

    <lb n="19"/><choice><corr>κατάκειται</corr><sic>κα<unclear reason="undefined" cert="high">τα</unclear>κιται</sic></choice> καὶ οὐ <choice><corr>δύναμαι</corr><sic>δυναμε</sic></choice> 

    <lb n="20"/>ἐλθεῖν πρὸς <unclear reason="undefined" cert="high">Ἀματί</unclear>αν. 

    <lb n="21"/>νῦν πέμψον Ἀκουτᾶν 

    <lb n="22"/>εὐθέως. 

    <lb n="23"/>ἐρρῶσθαι ὑμ<supplied reason="lost" cert="high">ᾶς</supplied> εὔχομαι. 
