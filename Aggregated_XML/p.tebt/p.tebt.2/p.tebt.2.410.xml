    <lb n="6"/><unclear cert="high" reason="undefined">ἐωνημέν</unclear>ου τῶν <choice><corr>γειτνιωσῶν</corr><sic>γιτνιωσων</sic></choice> 

    <lb n="7"/>αὐτῷ ε<gap extent="3" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/> Λυσιμάχῳ 

    <lb n="8"/>τ<supplied cert="high" reason="lost">ῶ</supplied>ι <supplied cert="high" reason="lost">κωμο</supplied>γραμματεῖ. μν<supplied cert="high" reason="lost">ή</supplied>-

    <lb n="9"/>σθητ<unclear cert="high" reason="undefined">ι</unclear> <unclear cert="high" reason="undefined">ὡ</unclear><supplied cert="high" reason="lost">ς</supplied> <unclear cert="high" reason="undefined">ἐν</unclear> τῷ Τρ<supplied cert="high" reason="lost">ι</supplied>στόμῳ με 

    <lb n="10"/>ἐφιλοτ<supplied cert="high" reason="lost">ι</supplied><unclear cert="high" reason="undefined">μ</unclear>οῦ σὺν ἐμοὶ μεῖναι. 

    <lb n="11"/>ἐρωτῶ σε <subst><add place="inline">ταχύτερον</add><del rend="corrected">χαχυτερον</del></subst> συσ-

    <lb n="12"/><subst><add place="inline">χεῖν</add><del rend="corrected">συ<gap extent="1" reason="illegible" unit="character"/>χειν</del></subst> <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ὸ</supplied> πρᾶγμα ἵνα <num value="24">κδ</num> ἐρχό-

