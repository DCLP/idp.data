    <lb n="14"/><supplied cert="high" reason="lost">ἀνθο</supplied>μολογήσηται περὶ τῆς 

    <lb n="15"/>σπ<supplied cert="high" reason="lost">ο</supplied>υδῆς <unclear cert="high" reason="undefined">τὰ</unclear> <abbr><unclear cert="high" reason="undefined">διπ</unclear></abbr>. <expan>ἔρρω<ex cert="high">σο</ex></expan>. 
<milestone rend="paragraphos" unit="undefined"/>
