    <lb n="1"/><supplied cert="high" reason="lost">Οὐ</supplied>εττίωι <app type="BL"><lem resp="1.427">Τ<unclear cert="high" reason="undefined">ού</unclear><supplied cert="high" reason="lost">ρβων</supplied>ι</lem><rdg>Τ<unclear cert="high" reason="undefined">ου</unclear><gap extent="5" reason="lost" unit="character"/></rdg></app> <unclear cert="high" reason="undefined">τῶι</unclear> 

    <lb n="2"/>κρατίστωι <supplied cert="high" reason="lost">ἐ</supplied>πιστρατ<supplied cert="high" reason="lost">ήγωι</supplied> 

    <lb n="3"/>παρὰ Κρονοῦτο<supplied cert="high" reason="lost">ς</supplied> Ζω<hi rend="diaeresis">ί</hi>λ<supplied cert="high" reason="lost">ου</supplied> 

    <lb n="4"/>τ<supplied cert="high" reason="lost">ο</supplied>ῦ Πετεσούχου ἀπὸ κώμης 

    <lb n="5"/><supplied cert="high" reason="lost">Τε</supplied>βτύνεως Πολέμωνος 

    <lb n="6"/>με<supplied cert="high" reason="lost">ρίδος</supplied> τοῦ Ἀρ<supplied cert="high" reason="lost">σ</supplied>ινοείτου. 

    <lb n="7"/>ἔτι πάλαι, κύριε, τοῦ προγεγραμ-

    <lb n="8"/>μένου μου πατρὸς ἀναδο-

    <lb n="9"/>θέντος εἰς ἐπιτήρησιν γε-

    <lb n="10"/><supplied cert="high" reason="lost">ν</supplied>ηματογραφου<supplied cert="high" reason="lost">μέ</supplied>νων 

    <lb n="11"/><supplied cert="high" reason="lost">ὑ</supplied>παρχόντων καὶ μετὰ τὸν 

