    <lb n="2"/>δ<supplied cert="high" reason="lost">έλ</supplied>φ<supplied cert="high" reason="lost">ου </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"><foreign lang="la">,</foreign> ἱερείας Ἀρσινόης</supplied> 

    <lb n="3"/>Φιλοπάτ<supplied cert="high" reason="lost">ορος </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"><foreign lang="la">,</foreign> μηνὸς</supplied> 

    <lb n="4"/>Δύστρου <gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Θωὺθ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>ἐν Κροκ<supplied cert="high" reason="lost">οδίλων πόλει τοῦ Ἀρσινοΐτου. ἐδάνεισεν</supplied> 

    <lb n="6"/>Ἀπολλώ<supplied cert="high" reason="lost">νιος </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τα</supplied>-

    <lb n="7"/><unclear cert="high" reason="undefined">κτ</unclear><supplied cert="high" reason="lost">ό</supplied>μισθο<supplied cert="high" reason="lost">ς Ἀλεξάνδρωι </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>τῶν οὔπω ἐπ<supplied cert="high" reason="lost">ηγμένων εἰς δῆμον </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>χαλκοῦ δ<unclear cert="low" reason="undefined">οκ</unclear><supplied cert="low" reason="lost">ίμου</supplied><supplied cert="high" reason="lost"> ὀφθαλμοφάνους τάλαντα</supplied> 

    <lb n="10"/>δεκαδύο <num value="12"/> <supplied cert="high" reason="lost">ἄτοκα εἰς </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐπὶ</supplied> 

    <lb n="11"/>ὑποθήκη<supplied cert="low" reason="lost">ι τῶι ὑπάρχοντι αὐτῶι σώματι</supplied> 

    <lb n="12"/>ὧι ὄνομα <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/>σωματίω<unclear cert="high" reason="undefined">ν</unclear> <gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐν τῶι προγεγραμμέ</supplied>-

    <lb n="14"/>νωι χρόν<supplied cert="high" reason="lost">ωι</supplied> <add place="supralinear"><unclear cert="high" reason="undefined">π</unclear>τωκε κα<unclear cert="high" reason="undefined">λ</unclear><gap extent="unknown" reason="lost" unit="character"/></add> <supplied cert="high" reason="lost">τὸ δὲ δάνειον τοῦτο ἀποδότω</supplied> 

    <lb n="15"/>Ἀλέξανδρ<supplied cert="high" reason="lost">ος Ἀπολλωνίωι ἐν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="16"/>ἐὰν δὲ μὴ ἀπ<supplied cert="high" reason="lost">οδῶι καθὰ γέγραπται, ἐξέστω</supplied> 

    <lb n="17"/>Ἀπολλωνίωι <supplied cert="high" reason="lost">ἐπικαταβολὴν ποιήσασθαι</supplied> 

    <lb n="18"/>τῆς ὑποθήκ<supplied cert="low" reason="lost">ης κατὰ τὸ διάγραμμα </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="19"/>αὐτ<unclear cert="high" reason="undefined">ό</unclear>. βεβ<supplied cert="high" reason="lost">αιούτω δὲ Ἀλέξανδρος Ἀπολ</supplied>-

    <lb n="20"/>λωνίωι τὴ<supplied cert="high" reason="lost">ν ὑποθήκην ταύτην καὶ</supplied> 

    <lb n="21"/>παρεχέσθω <supplied cert="high" reason="lost">αὐτὴν ἀνέπαφον καὶ ἀνενε</supplied>-

    <lb n="22"/>χύραστ<supplied cert="high" reason="lost">ο</supplied>ν <supplied cert="high" reason="lost">καὶ ἀνεπιδάνειστον ἄλλου</supplied> 

    <lb n="23"/>δανείου <supplied cert="high" reason="lost">καὶ καθαρὰν ἀπὸ βασιλικῶν. ἐὰν δὲ</supplied> 

    <lb n="24"/>μὴ βε<supplied cert="high" reason="lost">βαιοῖ αὐτὴν ἢ μὴ παρέχηται καθὰ</supplied> 


    <lb n="25"/>γέγραπτ<supplied cert="high" reason="lost">αι ἢ κίνδυνός τις γένηται περὶ τὴν</supplied> 

    <lb n="26"/>ὑποθή<supplied cert="high" reason="lost">κην ταύτην ἤτοι περὶ πᾶσαν ἢ μέ</supplied>-

    <lb n="27"/>ρος αὐ<supplied cert="high" reason="lost">τῆς τρόπωι ὡιποτοῦν, ἀποδότω</supplied> 

    <lb n="28"/>Ἀλέξα<supplied cert="high" reason="lost">νδρος Ἀπολλωνίωι τὸ δάνειον τοῦτο</supplied> 

