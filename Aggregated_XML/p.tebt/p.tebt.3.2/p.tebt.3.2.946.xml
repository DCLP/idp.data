    <lb n="1"/>Πάσιππος Φίλωνι χαίρειν. ε<supplied reason="lost" cert="high">ἰ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>τοῖς θεοῖς πολλὴν χάριν ἔχ<supplied reason="lost" cert="high">ω </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>παρό<unclear reason="undefined" cert="high">ντ</unclear>ι ἐνταῦθα μὴ ἀπο<supplied reason="lost" cert="low">δεδωκέναι με </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>ἐμοῦ δὲ προσαναπεπληρω<supplied reason="lost" cert="high">κότος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>ἀρτάβας <num value="90">ϙ</num> ὥστ᾽ ἀπέχει<supplied reason="lost" cert="high">ν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>Μεσορὴ καὶ ἔτι προέχειν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>α<unclear reason="undefined" cert="high">ποητ</unclear>ου ἀπέχοντος ὁμοίω<supplied reason="lost" cert="high">ς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>ἐληλύθην εἰς Διονυσιάδα κα<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><expan>χα<ex cert="high">λκοῦ</ex></expan> <expan><ex cert="high">δραχμ  </ex></expan> <num value="650">χν</num>, νυνὶ δὲ οὐθενὸς ἀτό<supplied reason="lost" cert="low">που </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>ἐπεὶ οὖν καὶ ὁ Λαμέδοντος <expan>κλ<ex cert="high">ῆρος</ex></expan> <unclear reason="undefined" cert="high">ε</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>διὰ τὸ ἐπιδεῖσθαι σπέρματος <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>σπείρειν ὑπὸ <choice><corr>χεῖρα</corr><sic>χερα</sic></choice>. ἐὰν οὖν σοι <supplied reason="lost" cert="high">φαίνηται </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>τῶι σησάμωι ὅπως μηκ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/>τὸ ἄχυρον οὐχ ὑπομεμενη<supplied reason="lost" cert="high">κ </supplied><gap reason="lost" extent="unknown" unit="character"/> 
