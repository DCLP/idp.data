    <lb n="21"/>τῆς Ἀντικλέους, κανηφόρου Ἀρσινόης Φι<supplied reason="lost" cert="high">λαδέλφου Ἀρσινόης τῆς</supplied> 

    <lb n="22"/>Σόλωνος, μηνὸς Αὐδναίου Αἰγυπτίων δὲ Ἐπεὶ<supplied reason="lost" cert="high">φ πεντεκαιδε</supplied>-

    <lb n="23"/>κάτηι, ἐν Σαμαρείαι τοῦ Ἀρσινοΐτου νομοῦ. ὁ<supplied reason="lost" cert="high">μολογεῖ Θεόδοτος</supplied> 

    <lb n="24"/>Κασσάνδρου Παίων τῶν Φυλέως τακτόμισθος Ἁρ<supplied reason="lost" cert="high">υώτηι Ἁρσεῦτος</supplied> 

    <lb n="25"/><supplied reason="lost" cert="high">καὶ Ὀρσ</supplied><unclear reason="undefined" cert="high">ενού</unclear>φει <unclear reason="undefined" cert="high">Α</unclear><gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">ε</unclear>φ<unclear reason="undefined" cert="high">ε</unclear><gap reason="illegible" extent="2" unit="character"/><unclear reason="undefined" cert="high">ι</unclear>ος κ<unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">ὶ Κ</supplied><unclear reason="undefined" cert="high">ατ</unclear>ύτει Πε<unclear reason="undefined" cert="high">τ</unclear><gap reason="lost" extent="5" unit="character"/><supplied reason="lost" cert="high"> καὶ ἄλλωι</supplied> 

    <lb n="26"/>Κατύτει <unclear reason="undefined" cert="high">Π</unclear><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/>ύμιος <unclear reason="undefined" cert="high">τοῖς</unclear> <unclear reason="undefined" cert="high">τέσσαρσι</unclear><supplied reason="lost" cert="high">ν <num value="4"/> Ἀρσινοΐταις βασιλι</supplied>-

    <lb n="27"/>κοῖς γεωργοῖς παρακεχωρηκ<unclear reason="undefined" cert="high">έ</unclear><supplied reason="lost" cert="high">ναι τὸν ὑπάρχοντα σταθμὸν</supplied> 

    <lb n="28"/><unclear reason="undefined" cert="high">τῶι</unclear> πατρὶ αὐτοῦ ἐν Κερκεσήφ<supplied reason="lost" cert="high">ει </supplied><gap reason="lost" extent="17" unit="character"/> 

    <lb n="29"/><unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">αὶ</supplied> μὴ ἐπελεύσεσθαι μήτε αὐτὸς μήτε τῶν π<supplied reason="lost" cert="high">αρ᾽ αὐτοῦ μη</supplied>-

    <lb n="30"/><supplied reason="lost" cert="high">θε</supplied><unclear reason="undefined" cert="high">ὶς</unclear> μηδὲ ἄλλον ὑποκαθήσειν παρευρέσει <choice><corr>ᾑτινιοῦν</corr><sic>ηιτινιουν</sic></choice>. <supplied reason="lost" cert="high">ἐὰ</supplied><unclear reason="undefined" cert="high">ν</unclear> <unclear reason="undefined" cert="high">δὲ</unclear> <supplied reason="lost" cert="high">ἐ</supplied><unclear reason="undefined" cert="high">πέλ</unclear>-

    <lb n="31"/><supplied reason="lost" cert="high">θη ἢ α</supplied>ὐτὸς ἢ τῶν παρ᾽ αὐτοῦ τις, ἀποτεισάτω Θεόδοτος Ἁρυώ-

    <lb n="32"/><supplied reason="lost" cert="high">τ</supplied>ηι καὶ Ὀρσεν<supplied reason="lost" cert="high">ο</supplied>ύφει καὶ Κατύτει κ<supplied reason="lost" cert="high">αὶ τῶι</supplied> <unclear reason="undefined" cert="high">ἄ</unclear>λλωι Κατύτει ἐπίτιμον 

    <lb n="33"/><supplied reason="lost" cert="high">χαλ</supplied>κοῦ νομίσματος δραχμὰς δισχιλίας <num value="2000"/>, κ<supplied reason="lost" cert="high">α</supplied>ὶ ἡ ἔφοδος αὐτῶι ἄκυ-

    <lb n="34"/><supplied reason="lost" cert="high">ρος ἔσ</supplied>τω. ἡ συνγραφὴ ἥδε κυρία. μάρτυρες Θεόδοτος Ἰάσονος, 

    <lb n="35"/><supplied reason="lost" cert="high">Διόφαντος Θεοδότου, Ἱεροοῦς Τιμο</supplied>θέου, Μίλων Ζ<unclear reason="undefined" cert="high">ω</unclear><supplied reason="lost" cert="high">σ</supplied>ί<unclear reason="undefined" cert="high">μου</unclear>, Δημοκράτ<unclear reason="undefined" cert="high">ης</unclear> 

