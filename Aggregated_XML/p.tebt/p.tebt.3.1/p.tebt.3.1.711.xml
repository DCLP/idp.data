<placeName cert="high" full="yes">Tebtynis</placeName></head><div lang="grc" org="uniform" part="N" sample="complete" type="edition"><div n="r" org="uniform" part="N" sample="complete" type="textpart"><ab part="N">

    <lb n="1"/>Θέων τοῖς ἐν Ὀξ<supplied cert="high" reason="lost">υρύγ</supplied>χοις φύλα<supplied cert="high" reason="lost">ξι</supplied> 

    <lb n="2"/>καὶ <supplied cert="high" reason="lost">το</supplied>ῖς γεωργο<supplied cert="high" reason="lost">ῖς</supplied> χ<supplied cert="high" reason="lost">αί</supplied>ρ<supplied cert="high" reason="lost">ει</supplied>ν. κατ<supplied cert="high" reason="lost">έλαβον</supplied> 

    <lb n="3"/>διὰ π<supplied cert="high" reason="lost">λ</supplied>ειόνων Πετωῦν τ<unclear cert="high" reason="undefined">ο</unclear><gap extent="4" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/> 

    <lb n="4"/>κωμάρχην τῆς κώμης <choice><corr>λιποτελῆ</corr><sic>λειποτελη</sic></choice> <supplied cert="high" reason="lost">ὄντα</supplied> 

    <lb n="5"/>καὶ <add place="supralinear">ἀτοποῦντα</add>, μεγάλως δὲ <choice><corr>καθυστερηκότα</corr><sic>καθυστηρ<unclear cert="high" reason="undefined">η</unclear>κοτα</sic></choice> ἐν <supplied cert="high" reason="lost">τῶι</supplied> 

    <lb n="6"/>διεσταμένωι πρὸς αὐτὸ<unclear cert="high" reason="undefined">ν</unclear> κεφαλαίωι οὗ <supplied cert="high" reason="lost">δεῖ</supplied> 

    <lb n="7"/>δοθῆναι ἀπὸ προχειρισμοῦ σίτ<supplied cert="high" reason="lost">ο</supplied>υ <gap extent="2" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/> 

    <lb n="8"/>καὶ διειλκυκότα ἕως τῆς <num value="20">κ</num> το<supplied cert="high" reason="lost">ῦ </supplied><gap extent="3" reason="lost" unit="character"/> 

    <lb n="9"/>ἀπολ<supplied cert="high" reason="lost">ε</supplied>λοιπέναι πλείω τοῦ ἡμίσους. <unclear cert="high" reason="undefined">ὅθ</unclear><supplied cert="high" reason="lost">εν</supplied> 

    <lb n="10"/>ἀναδραμόντες τα<unclear cert="high" reason="undefined">τ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/>γενέσθαι ἐν τούτοις <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="12"/>καὶ ἐν τοῖς πρότερον <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/>τὰ τῆς χρείας ἐκπλη<supplied cert="high" reason="lost">ρ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἵνα</supplied> 

    <lb n="14"/>μηθὲν ἐλάσσωμα <gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τῶι</supplied> 

    <lb n="15"/>προγεγραμμένωι γέ<supplied cert="high" reason="lost">νηται </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="16"/><supplied cert="high" reason="lost">γ</supplied>ενόμενοι ἐπιδ<gap extent="3" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="17"/>εἰς <unclear cert="high" reason="undefined">τὸ</unclear> <num value="44">μ<unclear cert="high" reason="undefined">δ</unclear></num> <supplied cert="high" reason="lost"><expan><ex cert="low">ἔτος</ex></expan></supplied> <unclear cert="high" reason="undefined">μ</unclear>όνον τὰ <unclear cert="high" reason="undefined">δ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="18"/>παραδοθῆναι ἐπακ<supplied cert="high" reason="lost">ολουθ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="19"/>δειξα<unclear cert="high" reason="undefined">με</unclear><supplied cert="high" reason="lost">ν</supplied><gap extent="2" reason="lost" unit="character"/> τυγχανε<gap extent="unknown" reason="lost" unit="character"/> 
<milestone rend="paragraphos" unit="undefined"/>
