    <lb n="16"/><supplied reason="lost" cert="high">σταλῆι</supplied>, μελέτω δὲ 

    <lb n="17"/><gap reason="lost" extent="8" unit="character"/><gap reason="illegible" extent="1" unit="character"/>θεν παρα 

    <lb n="18"/><gap reason="lost" extent="11" unit="character"/><unclear reason="undefined" cert="high">μ</unclear>ας δε 

    <lb n="19"/><gap reason="lost" extent="6" unit="character"/><supplied reason="lost" cert="high"> Ζηνόδ</supplied><unclear reason="undefined" cert="high">ωρ</unclear>ον 

    <lb n="20"/><gap reason="lost" extent="12" unit="character"/><unclear reason="undefined" cert="high">ν</unclear> κ<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

