    <lb n="4"/><unclear cert="high" reason="undefined">ι</unclear><gap extent="4" reason="illegible" unit="character"/><gap extent="3" reason="illegible" unit="character"/>υ<unclear cert="high" reason="undefined">τ</unclear><gap extent="3" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ις</unclear> τῶι <expan>θη<ex cert="high">σαυρῷ</ex></expan> 

