    <lb n="1"/><gap extent="unknown" reason="lost" unit="line"/> 

    <lb n="1"/>τὴν <unclear cert="high" reason="undefined">π</unclear>αρ<unclear cert="high" reason="undefined">ά</unclear>δειξ<supplied cert="high" reason="lost">ι</supplied>ν ὧν ἐμν<supplied cert="high" reason="lost">ήσα</supplied><unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ο <expan><ex cert="high">ἀρουρῶν</ex></expan>  </supplied><gap extent="18" reason="lost" unit="character"/> 

    <lb n="2"/>ἀπὸ τῶν ἐπικεχω<unclear cert="high" reason="undefined">ρ</unclear>ημένων εἰδ<supplied cert="high" reason="lost">ῶν  </supplied><gap extent="16" reason="lost" unit="character"/> 

    <lb n="3"/>ὑπολόγου περὶ τ<unclear cert="high" reason="undefined">ὰ</unclear>ς ὑπ<supplied cert="high" reason="lost">ο</supplied>σημ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ιν</supplied><unclear cert="high" reason="undefined">ο</unclear>μένας <supplied cert="low" reason="lost">κώμας</supplied><supplied cert="high" reason="lost">  στοχασάμενοι</supplied> 

    <lb n="4"/>τοῦ μηθὲν ἐν τούτοις ἀγνοηθῆ<supplied cert="high" reason="lost">ναι μηδὲ πρὸς χάριν οἰκο</supplied>-

    <lb n="5"/>νομηθῆναι, τῶν δὲ παραδειχθη<supplied cert="high" reason="lost">σομένων <expan><ex cert="high">ἀρουρῶν</ex></expan> πέμψαθ᾽ ἡμῖν</supplied> 

    <lb n="6"/>τὰς σχηματογραφίας δισσὰς εὐσήμ<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">ς, ἐπιθέντες τὰ μέτρα</supplied> 

    <lb n="7"/>καὶ γειτνίας ὅπως τὰ <choice><corr>ἄλλα</corr><sic>ταλλα</sic></choice> οἰκονομ<unclear cert="high" reason="undefined">η</unclear><supplied cert="high" reason="lost">θῆι καθάπερ ἐπέσταλται.</supplied> 

    <lb n="8"/><supplied cert="high" reason="lost">ἔρρωσθε. </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>Εἰρηναῖος Ὥρωι χαίρειν.  τῆς πρὸς Ἀ<supplied cert="high" reason="lost">σκληπιάδην</supplied> 

    <lb n="10"/>ἐπιστολῆς ἀντίγραφον ὑπόκειται. <gap extent="12" reason="lost" unit="character"/> 

    <lb n="11"/>ἀκολούθως καὶ ὑποπαραδειχθη<unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">ομένων <expan><ex cert="high">ἀρουρῶν</ex></expan> αἱ σχηματο</supplied>-

    <lb n="12"/>γραφίαι πεμφθήτωσαν ἡμῖν εὐσ<supplied cert="low" reason="lost"><unclear cert="high" reason="undefined">ή</unclear>μως.  ἔρρωσο. </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/>Ἀσκληπιάδει.  τοῦ μετενη<supplied cert="high" reason="lost">νεγμένου ἡμῖν  </supplied><gap extent="11" reason="lost" unit="character"/> 

    <lb n="14"/>ὑπὲρ τῆς ἐπικεχωρημέ<supplied cert="high" reason="lost">νης </supplied><gap extent="unknown" reason="lost" unit="character"/> 

