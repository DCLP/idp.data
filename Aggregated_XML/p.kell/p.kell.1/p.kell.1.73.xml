    <lb n="32"/>κυρίωι μου ἀδελφῷ <g type="x"/> Παμοῦρι 

    <lb n="33"/>Ψάις <choice><corr>Τρυφάνους</corr><sic>τρυφανης</sic></choice> </ab></div></div><div n="meta.33327" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.33327" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" notBefore="301" notAfter="400">IV</date></p></div><div n="meta.33327" type="commentary" subtype="mentionedDates" org="uniform" part="N" sample="complete"><head>Erwähnte Daten</head></div><div n="meta.33327" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">P.Kell.</title><biblScope type="volume">1</biblScope><biblScope type="numbers">73</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">33327</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0143</series>; <biblScope type="volume">1</biblScope>; <biblScope type="numbers">73</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0143;1;73" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.33327" type="bibliography" subtype="corrections" org="uniform" part="N" sample="complete"><head>BL-Einträge nach BL-Konkordanz</head><bibl type="BL" default="NO"><biblScope type="volume">XI</biblScope>, S. <biblScope type="pages">103</biblScope></bibl></div><div n="meta.33327" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p><bibl type="illustration" default="NO">Pl. 73</bibl></p></div><div n="meta.33327" type="history" subtype="locations" org="uniform" part="N" sample="complete"><p><placeName type="ancientFindspot" cert="high" full="yes">Kellis</placeName> (<geogName type="ancientRegion" cert="high" full="yes">Oasis Magna</geogName>)</p></div></body></text></TEI.2>