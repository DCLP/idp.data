    <lb n="2"/><unclear cert="high" reason="undefined">Ψάι</unclear><supplied cert="high" reason="lost">ς</supplied> <choice><corr>Τρυφάνους</corr><sic><unclear cert="high" reason="undefined">τρυφ</unclear>ανης</sic></choice> χαίρειν. ἔσχον πα-

    <lb n="3"/><supplied cert="high" reason="lost">ρὰ</supplied> <unclear cert="high" reason="undefined">σ</unclear>οῦ κατ᾽ ἐντολὴν ψάιτος Κέλε 

    <lb n="4"/>Πα<unclear cert="high" reason="undefined">τ</unclear>σῖ<supplied cert="high" reason="lost">ρε</supplied> χαλκοῦ <gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">δ</unclear><gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">σ</unclear>ιον <hi rend="supraline">ἓν</hi> <num value="1"/> καὶ 

