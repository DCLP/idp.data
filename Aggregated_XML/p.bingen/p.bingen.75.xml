    <lb n="8"/>ως καὶ μηδένα κωλ<supplied cert="high" reason="lost">ύοντα </supplied><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="9"/>ὡς πρόκειται δούλη<unclear cert="high" reason="undefined">ν</unclear><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀποφε-</supplied>

