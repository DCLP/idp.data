    <lb n="11"/>θεσθαι ἃς ἔχομεν <supplied cert="high" reason="lost">ἀσ</supplied>φαλείας τ<unclear cert="high" reason="undefined">ῶ</unclear>ν ἀρ[ουρῶν <note lang="en" place="not specified">?</note> <gap extent="unknown" reason="illegible" unit="character"/> <app type="alternative"><lem/><rdg><supplied cert="high" reason="lost">συναλλα-</supplied></rdg></app>]

    <lb n="12"/>γμάτων ἀπηλλάχθαι καὶ Διοσκόρου ῥ<supplied cert="low" reason="lost">ήτορος  </supplied><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="13"/>δυνήσοντ<supplied cert="high" reason="lost">αι</supplied> πάντες οἱ <choice><corr>ἐμφερόμενοι</corr><sic>ενφερομενοι</sic></choice>.

    <lb n="14"/>καὶ τὰ πάλαι καὶ τὰ <supplied cert="high" reason="lost">ὑπ᾽</supplied> ἐμοῦ τῶν ἀναλω<supplied cert="low" reason="lost">μάτων  </supplied><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="15"/>παρηγγελμένων τ<gap extent="2" reason="illegible" unit="character"/> ωμη <gap extent="unknown" reason="lost" unit="character"/>

    <lb n="16"/>δεσμὸν κ<unclear cert="high" reason="undefined">α</unclear>ὶ ἰδίωμα ἔχει καὶ ς<gap extent="unknown" reason="lost" unit="character"/></ab></div></div><div n="meta.78046" part="N" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.78046" part="N" subtype="textDate" type="commentary"><p><date notAfter="200" notBefore="176" type="textDate">Ende II (?)</date></p></div><div n="meta.78046" part="N" subtype="mentionedDates" type="commentary"><head>Erwähnte Daten</head><p>Z. 2-3: <date type="mentioned" value="0166-03-02">2. März 166 (Jahr unsicher)</date></p><p>Z. 2-3: <date type="mentioned" value="0198-03-02">2. März 198 (Jahr unsicher)</date></p></div><div n="meta.78046" part="N" subtype="principalEdition" type="bibliography"><listBibl><bibl subtype="principal" type="publication"><title level="s" type="abbreviated">P.Bingen</title><biblScope type="numbers">78</biblScope></bibl><bibl type="Trismegistos"><biblScope type="numbers">78046</biblScope></bibl><bibl type="DDbDP"><series>0254</series>; ; <biblScope type="numbers">78</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0254;;78" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.78046" part="N" subtype="illustrations" type="bibliography"><p><bibl type="illustration">Pl. 49</bibl></p></div><div n="meta.78046" part="N" subtype="locations" type="history"><p><placeName cert="high" type="ancientFindspot">Oxyrhynchos</placeName></p></div></body></text></TEI.2>