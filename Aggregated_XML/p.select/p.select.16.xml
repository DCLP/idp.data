    <lb n="13"/><supplied cert="high" reason="lost">οἴνου κνίδια </supplied><gap extent="8" reason="lost" unit="character"/><foreign lang="la">.</foreign> <unclear cert="high" reason="undefined">π</unclear>αρέξω <unclear cert="high" reason="undefined">δ</unclear>ὲ <hi rend="diaeresis">ὑ</hi>μῖν ἐν τῇ <choice><corr>πέμπτῃ</corr><sic>πεντε</sic></choice> καὶ εἰκάδι τοῦ Ἐπεὶφ μηνὸς 

    <lb n="14"/><supplied cert="high" reason="lost">λογῷ συνηθείας οἴνου ἀγ</supplied>γεῖον <choice><corr cert="low"/><sic><supplied cert="high" reason="lost">αγ</supplied>γιον</sic></choice> ἕν <num value="1"/> καὶ πινάκιον ἕν <num value="1"/> μεστὸν διαφόρων βρ<unclear cert="high" reason="undefined">ωμ</unclear>άτων 

    <lb n="15"/><gap extent="20" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="3" reason="illegible" unit="character"/> καὶ δελφάκια δύο <num value="2"/> εὐάρεστα ἄξια ἀργυρίου ταλάντων 

    <lb n="16"/>[ <gap extent="12" reason="illegible" unit="character"/> <unclear cert="high" reason="undefined">δισκάρια</unclear>] μεστὰ <choice><corr>διαφόρων</corr><sic>διαφορα</sic></choice> <choice><corr>βρωμάτων</corr><sic>βρωματα</sic></choice> <choice><corr>ἔχοντα</corr><sic>εχοντες</sic></choice> τυροὺς διακοσίους <num value="200"/> 

    <lb n="17"/><gap extent="20" reason="lost" unit="character"/><unclear cert="high" reason="undefined">τ</unclear>εσσεράκοντα ἀφ᾽ οἵου θέλητε τούτους <choice><corr>τρεφομένους</corr><sic>τρεφομενον</sic></choice> 

    <lb n="18"/><gap extent="19" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τ</supplied>ὸ γράμμα ταύτης τῆς μισθώσεως κύριον καὶ βέβαιον καὶ 

    <lb n="19"/>[<expan>ἐπερ<ex cert="high">ωθητεὶς</ex></expan> <expan>ὡμολ<ex cert="high">όγησα</ex></expan>. <handShift new="m2"/> <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> <expan>Παῦλ<ex cert="high">ος</ex></expan> Σαρα]πίωνος ὁ <expan>προκ<ex cert="high">είμενος</ex></expan> μεμίσθωμαι ὡς <expan>πρόκ<ex cert="high">ειται</ex></expan>. <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> Τα<supplied cert="high" reason="lost">υ</supplied><unclear cert="high" reason="undefined">ρ</unclear>ῖνος Γενν<unclear cert="high" reason="undefined">αδί</unclear>ου 

