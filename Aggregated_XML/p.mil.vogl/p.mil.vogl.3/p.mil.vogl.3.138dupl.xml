    <lb n="20"/><supplied reason="lost" cert="high">Πτολλαρί</supplied>ωνος καὶ ἑτέρ<supplied reason="lost" cert="high">ων, πάν</supplied><unclear reason="undefined" cert="high">τ</unclear>ων ἀνυπολόγων 

    <lb n="21"/><supplied reason="lost" cert="high">καὶ ἀκινδύνω</supplied>ν, ἄγοντες τὰ <supplied reason="lost" cert="high">τῶν</supplied> ἀρουρῶν ἔργα 

