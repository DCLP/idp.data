    <lb n="9"/>σ<supplied reason="lost" cert="high">αρα</supplied> <num value="4"/> <supplied reason="lost" cert="high">ἀ</supplied>πὸ τ<unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">ῦ ἐνεστῶτος δεκά</supplied>τ<supplied reason="lost" cert="high">ου ἔτους τὰς</supplied> 

    <lb n="10"/><supplied reason="lost" cert="high">ὑπα</supplied>ρχού<unclear reason="undefined" cert="high">σα</unclear><supplied reason="lost" cert="high">ς ὑμῖν, τῷ μ</supplied>ὲν Π<supplied reason="lost" cert="high">τολλαρίωνι</supplied> 

