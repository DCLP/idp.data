    <lb n="18"/><supplied reason="lost" cert="high">κατ᾽ ἔτος σὺν αἷς</supplied> λήμψομαι σπερ<supplied reason="lost" cert="high">μ</supplied>άτω<supplied reason="lost" cert="high">ν</supplied> 

    <lb n="19"/><supplied reason="lost" cert="high">πυροῦ ἀρτάβαις</supplied> ὀκτώ <num value="8"/>, τοῦ παντ<supplied reason="lost" cert="high">ὸς κατ᾽ ἔ</supplied>-

