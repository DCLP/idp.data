    <lb n="1"/><g type="stauros"/> ἐν ὀνόματι τοῦ κυρίου καὶ 

    <lb n="2"/><expan>δεσπό<ex cert="high">του</ex></expan> Ἰησοῦ <expan>Χ<ex cert="high">ριστο</ex></expan>ῦ τοῦ θεοῦ καὶ 

    <lb n="3"/><expan>σωτῆρ<ex cert="high">ος</ex></expan> ἡμῶν, βασιλείας τοῦ 

    <lb n="4"/>ἡμῶν <expan>δεσπ<ex cert="high">ότο</ex></expan>υ <expan>Φλ<ex cert="high">αουίου</ex></expan> Μαυρικίου

    <lb n="5"/>Τιβερίου τοῦ <expan>αἰων<ex cert="high">ίου</ex></expan> Αὐγούστου 

    <lb n="6"/>ἔτους <num value="21">κα</num> Χοίαχ <num value="24">κδ</num> <num value="6">ϛ</num> <expan>ἰν<ex cert="high">δικτίονος</ex></expan> ἐν <expan>Ἀρ<ex cert="high">σινόῃ</ex></expan>. 

    <lb n="7"/>Αὐρήλιος <hi rend="diaeresis">Ἰ</hi>ωάννῃ υἱὸς 

    <lb n="8"/>Παύλου ἀπὸ κώμης Φιλοξένου 


    <lb n="9"/>τοῦ Ἀρσινο<hi rend="diaeresis">ί</hi>του νομοῦ τῷ αἰδε-

    <lb n="10"/>σίμῳ <hi rend="diaeresis">Ἰ</hi>ωάννῃ τῷ <expan>πραγμα<ex cert="high">τικῷ</ex></expan> 

    <lb n="11"/>πιστικῷ ἀποθήκης ἁγίου 

    <lb n="12"/>Σεργίου ἐμπόρου μένοντι ἐν 

    <lb n="13"/>ἐνταῦθα τῷ Ἀρσινο<hi rend="diaeresis">ί</hi>τῃ <expan>χ<ex cert="high">αίρειν</ex></expan>. 

    <lb n="14"/>ὁμολογῶ ἐσχηκέναι <app type="BL"><lem resp="1.192">με</lem><rdg>ὁμοῦ</rdg></app> 

    <lb n="15"/>καὶ νῦν παρὰ σοῦ διὰ χειρὸς 

    <lb n="16"/><gap extent="9" reason="lost" unit="character"/><unclear cert="high" reason="undefined">κον</unclear><gap extent="6" reason="lost" unit="character"/> 

    <lb n="17"/><gap extent="unknown" reason="lost" unit="line"/> 
