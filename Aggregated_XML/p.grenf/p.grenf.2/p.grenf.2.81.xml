    <lb n="1"/>μετὰ τὴν <hi rend="diaeresis">ὑ</hi>πατείαν τῶν δεσποτῶν ἡμῶν 

    <lb n="2"/>Ἀρκαδίου καὶ Ὁνωρίου τῶν αἰωνίων Αὐγούστων 

    <lb n="3"/>Παῦνι <num value="1">α</num>. 

    <lb n="4"/>Αὐρήλιοι Βίκτωρ Σευήρου καὶ Κόλλουθος 

    <lb n="5"/>Μεσουήριος ἀμφότεροι ἀ<supplied reason="lost" cert="high">π</supplied>ὸ Ἀντινόου 

    <lb n="6"/>πόλεως τῆς λαμπροτάτης <app type="BL"><lem resp="1.191">κεφαλαιωταὶ τοῦ</lem><rdg>κεφαλαιοτάτου</rdg></app> 

    <lb n="7"/>ἡγενομικο<supplied reason="lost" cert="high">ῦ</supplied> πολυκώπου τοῦ ὑπὸ 

    <lb n="8"/>Ἀπίωνα ἀρ<supplied reason="lost" cert="high">χι</supplied>κυβερνήτην Αὐρηλίῳ 

    <lb n="9"/>Σεινούθῃ <supplied reason="lost" cert="high">Βίκ</supplied>τορος ἀπὸ Ἑρμοῦ πόλεως 

    <lb n="10"/>καταμένο<supplied reason="lost" cert="high">ντι</supplied> ἐν Πανὸ<supplied reason="lost" cert="high">ς</supplied> πόλει ἐρέτῃ τοῦ 

    <lb n="11"/>αὐτοῦ πολυκ<supplied reason="lost" cert="high">ώ</supplied>που χαίρ<supplied reason="lost" cert="high">ει</supplied>ν. δέδωκας 

    <lb n="12"/>κατὰ τὴν συ<supplied reason="lost" cert="high">ν</supplied>ήθειαν το<supplied reason="lost" cert="high">ῦ σ</supplied>οῦ πατρὸς 

    <lb n="13"/><supplied reason="lost" cert="high">τ</supplied>οὺς συμπ<supplied reason="lost" cert="high">εφ</supplied>ωνη<supplied reason="lost" cert="high">μέν</supplied>ους μι<supplied reason="lost" cert="high">σ</supplied>θοὺς 

    <lb n="14"/>ὑπὲρ <hi rend="diaeresis">ἰ</hi>δ<supplied reason="lost" cert="high">ί</supplied>ας σου <supplied reason="lost" cert="high">κεφ</supplied>αλῆς <supplied reason="lost" cert="high">κανόν</supplied>ος τῆς παρελθού-

    <lb n="15"/>σης π<supplied reason="lost" cert="high">ρώτης</supplied> ἰνδι<supplied reason="lost" cert="high">κτίονος</supplied> χωρ<supplied reason="lost" cert="high">ο</supplied>ῦντας 

    <lb n="16"/>εἰς <choice><corr>ναυτιλίαν</corr><sic>να<supplied reason="lost" cert="high">υστιλε</supplied>ιαν</sic></choice> τ<supplied reason="lost" cert="high">οῦ αὐτοῦ</supplied> <choice><corr>πολυκώπου</corr><sic>πωλ<supplied reason="lost" cert="high">υ</supplied>κωπου</sic></choice> 

    <lb n="17"/>τῷ τὴ<supplied reason="lost" cert="high">ν ἀντὶ</supplied> σοῦ χ<supplied reason="lost" cert="high">ώραν ἀπ</supplied>οπληροῦντι, 

    <lb n="18"/>καὶ διὰ τ<supplied reason="lost" cert="high">οῦτο τ</supplied>αύτην τ<supplied reason="lost" cert="high">ὴν ἀπ</supplied>οχὴν ἐξεδό<supplied reason="lost" cert="high">μ</supplied>εθά 

    <lb n="19"/>σοι ε<supplied reason="lost" cert="high">ἰς ἀσφ</supplied>άλειαν <supplied reason="lost" cert="high">κυρί</supplied>αν καὶ βεβαία<supplied reason="lost" cert="high">ν</supplied>, 

    <lb n="20"/>καὶ <expan>ἐπ[ερ<ex cert="high">ωτηθέντες</ex></expan> <expan>ὡ]μολ<ex cert="high">ογήσαμεν</ex></expan>. <supplied reason="lost" cert="high">Αὐρή</supplied>λιος Βίκτωρ <supplied reason="lost" cert="high">Σευήρ</supplied>-

    <lb n="21"/>ου ἐ<supplied reason="lost" cert="high">ξέδωκ</supplied>α τὴ<supplied reason="lost" cert="high">ν ἀπ</supplied>οχὴν ὡς <expan>πρ[όκ<ex cert="high">ειται</ex>]</expan>. 

    <lb n="22"/>Αὐρ<supplied reason="lost" cert="high">ήλιος Κόλλουθος </supplied><choice><corr><supplied reason="lost" cert="high">Με</supplied>σουήριος</corr><sic><supplied reason="lost" cert="high">με</supplied>σουηρις</sic></choice> ὁ π<supplied reason="lost" cert="high">ρ</supplied>οκείμενος ἐξ<supplied reason="lost" cert="high">έδω</supplied>-

    <lb n="23"/><supplied reason="lost" cert="high">κα τὴν ἀποχὴ</supplied>ν ὡς πρόκειται. 
