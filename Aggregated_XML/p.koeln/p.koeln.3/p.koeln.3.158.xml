    <lb n="20"/>νομισμάτια δύο <expan>ζυγ<ex cert="high">ῷ</ex></expan> 

    <lb n="21"/>Ἀρσινο<hi rend="diaeresis">ί</hi>του, <expan><ex cert="high">γίνεται</ex></expan> <expan>χρ<ex cert="high">υσοῦ</ex></expan> <expan>νο<ex cert="high">μισμάτια</ex></expan> <num value="2">β</num> <g type="unintelligible"/>, 

