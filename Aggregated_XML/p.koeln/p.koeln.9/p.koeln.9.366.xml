    <lb n="5"/><subst><add place="inline"><unclear reason="undefined" cert="high">τούτ</unclear>ων</add><del rend="corrected" status="unremarkable"><gap reason="illegible" extent="4" unit="character"/>ων</del></subst>, τὸ ἐπιβάλλον αὐτῶι <unclear reason="undefined" cert="high">μέ</unclear>ρος <supplied reason="lost" cert="high">ὧν ἐδάνεισεν</supplied>

    <lb n="6"/>αὐτ<unclear reason="undefined" cert="high">ῷ</unclear> τε καὶ Θέωνι Θέωνος τοῦ ὀγδόου καὶ τρι<unclear reason="undefined" cert="high">α</unclear>κ<unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">στοῦ</supplied>

    <lb n="7"/>ἔτους Τῦβι πέμπτηι διάγων ἐν Ἀλεξανδρε<supplied reason="lost" cert="high">ίᾳ ἐπὶ τῶν</supplied>

    <lb n="8"/>μνημόνων χαλκοῦ ταλάντων δεκαπέντ<supplied reason="lost" cert="high">ε <num value="15"/></supplied>,

    <lb n="9"/>καὶ μηθὲν ἐνκαλεῖ<unclear reason="undefined" cert="high">ν</unclear> μηδ᾽ ἐνκαλέσειν μ<supplied reason="lost" cert="high">ηδὲ</supplied>

    <lb n="10"/><supplied reason="lost" cert="high">ἐ</supplied>πελεύσεσθαι Δημήτριον Ἀγαθίνωι μη<unclear reason="undefined" cert="high">δ</unclear><supplied reason="lost" cert="high">ὲ τοῖς</supplied>

    <lb n="11"/>παρ᾽ αὐτοῦ περὶ τοῦ δανείου τ<unclear reason="undefined" cert="high">ῶ</unclear>ν <unclear reason="undefined" cert="high">δ</unclear>εκαπέντ<supplied reason="lost" cert="high">ε <num value="15"/></supplied>

    <lb n="12"/>ταλάντων μηδὲ <unclear reason="undefined" cert="high">π</unclear>ερὶ μέρους αὐτῶν μ<unclear reason="undefined" cert="high">ηδ</unclear><supplied reason="lost" cert="high">ὲ</supplied>

