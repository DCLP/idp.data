    <lb n="14"/>εγγύης τοῦ δανείου <add place="supralinear">τὴν δὲ πρ<unclear reason="undefined" cert="high">ᾶ</unclear>ξιν εἶναι Δημ<supplied reason="lost" cert="high">ητρίῳ</supplied> παρὰ τοῦ Θέωνος τῶν λοιπῶν τ<unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">λά</supplied><unclear reason="undefined" cert="high">ντων</unclear> <unclear reason="undefined" cert="high">ἓ</unclear><supplied reason="lost" cert="high">ξ <num value="6"/></supplied> <app type="alternative"><lem><gap reason="illegible" extent="2" unit="character"/>ο<unclear reason="undefined" cert="high">λ</unclear><gap reason="lost" extent="unknown" unit="character"/></lem><rdg><app type="alternative"><lem><gap reason="illegible" extent="2" unit="character"/>ο<unclear reason="undefined" cert="high">ν</unclear><gap reason="lost" extent="unknown" unit="character"/></lem><rdg><gap reason="illegible" extent="2" unit="character"/>ο<unclear reason="undefined" cert="high">δ</unclear><gap reason="lost" extent="unknown" unit="character"/></rdg></app></rdg></app></add>. ἐὰν δὲ Δημήτριος παρ<supplied reason="lost" cert="high">α</supplied>-

    <lb n="15"/>συγγραφῶν ἐπέλθῃ ἐπι<unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">αλῶ</supplied><unclear reason="undefined" cert="high">ν</unclear> <supplied reason="lost" cert="high">αὐτῷ</supplied> <unclear reason="undefined" cert="high">πε</unclear>ρ<unclear reason="undefined" cert="high">ὶ</unclear> <supplied reason="lost" cert="high">τῶν</supplied>

    <lb n="16"/>προγεγραμμένων ἢ <unclear reason="undefined" cert="high">ἄλ</unclear><supplied reason="lost" cert="high">λος</supplied> <unclear reason="undefined" cert="high">τις</unclear> <supplied reason="lost" cert="high">τῶν</supplied> <unclear reason="undefined" cert="high">παρ</unclear>᾽ <supplied reason="lost" cert="high">αὐτοῦ</supplied>,

    <lb n="17"/>ἡ μὲν ἔφοδος αὐτῷ τε καὶ π<unclear reason="undefined" cert="high">α</unclear>ντὶ τῷ <supplied reason="lost" cert="high">περὶ τούτου</supplied>

    <lb n="18"/>ἐπελθ<supplied reason="lost" cert="high">ό</supplied>ντι ἄκυρος ἔστω καὶ προσαποτ<supplied reason="lost" cert="high">εισάτω</supplied>

    <lb n="19"/>Δημήτριος Ἀγαθίνῳ ἐπίτιμον χαλκοῦ ν<supplied reason="lost" cert="high">ο</supplied>-

    <lb n="20"/><supplied reason="lost" cert="high">μίσμα</supplied><unclear reason="undefined" cert="high">το</unclear><supplied reason="lost" cert="high">ς</supplied> τάλαντα εἴκοσι <num value="20"/> καὶ τὸ βλάβος <unclear reason="undefined" cert="high">κα</unclear><supplied reason="lost" cert="high">ὶ εἰς τὸ</supplied>

