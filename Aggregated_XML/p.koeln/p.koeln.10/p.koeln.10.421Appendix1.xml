    <lb n="23"/><gap reason="lost" extent="7" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="3" unit="character"/><gap reason="illegible" extent="1" unit="character"/>ον ἐκ <unclear reason="undefined" cert="high">δα</unclear>κρ<unclear reason="undefined" cert="high">ύω</unclear><supplied reason="lost" cert="high">ν </supplied><gap reason="lost" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/>σων καὶ μεγάλων πνευστιῶν

    <lb n="24"/>καὶ τού<unclear reason="undefined" cert="high">τ</unclear>ους <gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="3" unit="character"/>ρουντ<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="2" unit="character"/>κ<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="2" unit="character"/><unclear reason="undefined" cert="high">ς</unclear><gap reason="illegible" extent="4" unit="character"/> πεποι<unclear reason="undefined" cert="high">ήκα</unclear>σι καὶ μὴ

