    <lb n="5"/>π<unclear reason="undefined" cert="high">ρά</unclear>γμ<unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">τα</supplied> <unclear reason="undefined" cert="high">κ</unclear>αὶ <choice><corr>κατεστήσατό</corr><sic>καταστησατ<unclear reason="undefined" cert="high">ο</unclear></sic></choice> με ἐπάνω τ<gap reason="illegible" extent="1" unit="character"/>ε<unclear reason="undefined" cert="high">α</unclear><gap reason="illegible" extent="2" unit="character"/><unclear reason="undefined" cert="high">ουπρ</unclear><gap reason="illegible" extent="1" unit="character"/>μ<gap reason="illegible" extent="5" unit="character"/>λο<gap reason="illegible" extent="2" unit="character"/> <supplied reason="lost" cert="high">τῷ</supplied> συντεθέν<unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ι</supplied>

    <lb n="6"/>παρ᾽ αὐτοῦ διαθηκιμαίῳ γράμματι καὶ ὁρισθέν μοι π<unclear reason="undefined" cert="high">α</unclear>ρ<unclear reason="undefined" cert="high">ὰ</unclear>  ροειρημένου μου

    <lb n="7"/>ἀδελφοῦ τὸ κτῆμα κλήρου Ψινεΐου σὺν δικαίοις πᾶσι καὶ τὰς ἄλλας αὐτοῦ ἀρούρ<unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">ς</supplied>

    <lb n="8"/><supplied reason="lost" cert="high">δ</supplied>ι<unclear reason="undefined" cert="high">ακ</unclear>ε<unclear reason="undefined" cert="high">ιμ</unclear>έ<unclear reason="undefined" cert="high">νας</unclear> <supplied reason="omitted" cert="high">ἐν</supplied> <expan>κλήρ<ex cert="high">ῳ</ex></expan> <unclear reason="undefined" cert="high">Ν</unclear>ε<unclear reason="undefined" cert="high">μ</unclear>πκῆ<unclear reason="undefined" cert="high">το</unclear>ς καὶ <choice><corr>πάσας τὰς προσόδους</corr><sic>παν<unclear reason="undefined" cert="high">τα</unclear> τα προσοδα</sic></choice> τούτων διοικήσασθαι

    <lb n="9"/>δικ<unclear reason="undefined" cert="high">α</unclear>ίως <sic>παρ᾽ ἐ<unclear reason="undefined" cert="high">μο</unclear>ῦ</sic> καὶ ἀναλί<supplied reason="lost" cert="high">σ</supplied><unclear reason="undefined" cert="high">κ</unclear>ειν τὴν κατ<gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="2" unit="character"/> <unclear reason="undefined" cert="high">π</unclear>ρόσ<unclear reason="undefined" cert="high">οδον</unclear> <supplied reason="lost" cert="high">π</supplied><unclear reason="undefined" cert="high">ρ</unclear><supplied reason="lost" cert="high">ὸς τ</supplied><unclear reason="undefined" cert="high">ὰς</unclear> ς

    <lb n="10"/>προσφορὰς καὶ ἀγάπας καὶ μ<gap reason="illegible" extent="5" unit="character"/><gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="1" unit="character"/> <unclear reason="undefined" cert="high">πλη</unclear>ρῶσ<unclear reason="undefined" cert="high">α</unclear>ι τὰ ἑα<unclear reason="undefined" cert="high">υ</unclear>τοῦ δημόσια καὶ τ<unclear reason="undefined" cert="high">ὰ</unclear>

    <lb n="11"/>χρεοφ<unclear reason="undefined" cert="high">λ</unclear>ήματα π<unclear reason="undefined" cert="high">ρ</unclear><supplied reason="lost" cert="high">ὸ</supplied>ς ὅλον τὸν χρόνον <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ῆς</supplied> <unclear reason="undefined" cert="high">ἐ</unclear><supplied reason="lost" cert="high">μ</supplied><unclear reason="undefined" cert="high">ῆς</unclear> ζωῆς <unclear reason="undefined" cert="high">πρ</unclear><supplied reason="lost" cert="high">ὸς</supplied> τ<unclear reason="undefined" cert="high">ὴ</unclear>ν <unclear reason="undefined" cert="high">π</unclear>ίστιν

    <lb n="12"/>τῆς αὐτοῦ ἐγγράφου βουλήσε<unclear reason="undefined" cert="high">ως</unclear> καὶ <unclear reason="undefined" cert="high">ε</unclear><gap reason="illegible" extent="3" unit="character"/>ω<gap reason="illegible" extent="8" unit="character"/><unclear reason="undefined" cert="high">εῖσ</unclear>θαι παρ᾽ ἐμοῦ

    <lb n="13"/>ἀκαταφρονήτως. καὶ ἐπειδὴ πρὸ ὀλίγου τοῦ χρόνου <choice><corr>τὸ <expan>προκ<ex cert="high">είμενον</ex></expan> κτῆμα</corr><sic>του προκ κτηματος</sic></choice>

    <lb n="14"/>ἐπορθήθη καὶ ἀπελύθη ἐν καιρῷ τῆς στενοσίας καὶ μοχθηρίας, οὐκ ἐδυνή-

    <lb n="15"/><unclear reason="undefined" cert="high">θην</unclear> ομ<unclear reason="undefined" cert="high">ε</unclear>ῖναι τὸ βάρος τῆς αὐτουργίας <supplied reason="omitted" cert="high">καὶ</supplied> τὰ δημόσια καὶ τὴν φροντίδα

    <lb n="16"/>τῆς <choice><corr>ὑπουργίας</corr><sic>υποργειας</sic></choice> τῆς μνήμης <unclear reason="undefined" cert="high">τοῦ</unclear> <expan><unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ρο</supplied><unclear reason="undefined" cert="high">κ</unclear><ex cert="high">ειμένου</ex></expan> μακ<unclear reason="undefined" cert="high">α</unclear>ριωτάτου μου ἀδελφοῦ

    <lb n="17"/>καὶ τὰ χρεοφλήματα ἐν καιρο<unclear reason="undefined" cert="high">ῖ</unclear>ς <unclear reason="undefined" cert="high">μο</unclear>χ<supplied reason="lost" cert="high">θηρο</supplied>ῖς καὶ ἀβρόχοις, μάλ<unclear reason="undefined" cert="high">ι</unclear>στα δὲ τὴν ἐμὴν

    <lb n="18"/><choice><corr>γεραιότητα</corr><sic>γερ<supplied reason="lost" cert="high">α</supplied>ιοτητην</sic></choice> καὶ ἀδυναστ<unclear reason="undefined" cert="high">ίαν</unclear> <sic><unclear reason="undefined" cert="high">μου</unclear></sic> μενο<unclear reason="undefined" cert="high">ύσ</unclear>η, <choice><corr>ἐκάθισα</corr><sic>εκ<unclear reason="undefined" cert="high">αθ</unclear>εισ<unclear reason="undefined" cert="high">α</unclear></sic></choice> δὲ ἐμ<unclear reason="undefined" cert="high">αυτὸ</unclear>ν <unclear reason="undefined" cert="high">ὡς</unclear> μᾶλλον

    <lb n="19"/>σύμφερον ἐπιλέξα<supplied reason="omitted" cert="high">ι</supplied> <choice><corr>ἱκανοὺς ἄνδρας</corr><sic>ικανοι ανδρες</sic></choice> εἰς τὸ ἀναγκαῖον καὶ <choice><corr>εὐσεβὲς</corr><sic>ευσεβον</sic></choice> ἔργον

    <lb n="20"/>κατὰ τὴν δύναμιν τῆς γεγενημένης μοι παρὰ τοῦ <expan>προκ<ex cert="high">ειμένου</ex></expan> μακαριωτάτου μου

    <lb n="21"/>ἀδελφοῦ Ἑρμαουῶτος <supplied reason="omitted" cert="high">  </supplied>, <choice><corr>ηὗρον</corr><sic>ηυρεθη</sic></choice> δὲ τὸν τιμιώτατον καὶ ἐλε<supplied reason="omitted" cert="high">ειν</supplied>ότερον Βησαρίωνα

