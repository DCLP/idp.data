    <lb n="5"/>λογῶ <supplied cert="high" reason="lost">ἐσχηκ</supplied>έναι παρ᾽ ὑμῶν 

    <lb n="6"/>το<unclear cert="high" reason="undefined">ὺ</unclear>ς <unclear cert="high" reason="undefined">τε</unclear><supplied cert="high" reason="lost">σσα</supplied><unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ά</supplied>κοντα πέντε <num value="45"/> 

