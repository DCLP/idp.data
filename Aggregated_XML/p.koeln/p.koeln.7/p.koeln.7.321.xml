    <lb n="6"/>ἀπὸ τῆς <supplied cert="high" reason="lost">αὐ</supplied>τῆς κώμης <expan><ex cert="high">καὶ</ex></expan> ἔστιν <gap extent="2" reason="lost" unit="character"/> ἑξῆς ὑ<unclear cert="high" reason="undefined">π</unclear>οδ<gap extent="1" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐ</supplied>πείπερ τοῦ προγεγραμ<supplied cert="high" reason="lost">μένου </supplied><gap extent="14" reason="lost" unit="character"/> 

    <lb n="7"/><gap extent="2" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/>ιαν τίθεσθαι καὶ ἐνθεῖναι τ<unclear cert="high" reason="undefined">ο</unclear><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/>μ<gap extent="4" reason="illegible" unit="character"/> <unclear cert="high" reason="undefined">εἰς</unclear> <unclear cert="high" reason="undefined">τὸ</unclear> <unclear cert="high" reason="undefined">μ</unclear>έσον <expan>αὐτ<ex cert="high">ῶν</ex></expan>, ἐκ τούτου π<unclear cert="high" reason="undefined">α</unclear>ραλαβ<supplied cert="high" reason="lost">εῖν </supplied><gap extent="11" reason="lost" unit="character"/> 

    <lb n="8"/>ἐν τῷ <choice><corr>οἴκῳ</corr><sic>οικου</sic></choice> <expan>αὐτ<ex cert="high">οῦ</ex></expan> μετὰ τῶν καταλειφθέντων <expan>αὐτ<ex cert="high">ῷ</ex></expan> οἱωνδήποτε πραγμάτ<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">ν κινη</supplied><unclear cert="high" reason="undefined">τῶν</unclear> 

    <lb n="9"/>ἀκινήτων αὐτοκ<supplied cert="high" reason="lost">ινήτ</supplied><unclear cert="high" reason="undefined">ων</unclear> <gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><unclear cert="high" reason="undefined">κ</unclear>ο<gap extent="3" reason="illegible" unit="character"/> <supplied cert="high" reason="lost">ἀ</supplied>ρ<unclear cert="high" reason="undefined">ο</unclear>υ<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ῶν</supplied> σπορίμων <expan>γῃδ<ex cert="high">ίων</ex></expan> καὶ κτην<supplied cert="high" reason="lost">ῶν </supplied><gap extent="10" reason="lost" unit="character"/> 

    <lb n="10"/>καὶ <choice><corr>ἐπαύλεων</corr><sic>επαυλαιων</sic></choice> καὶ ἄλλων διακειμένων ἔν <choice><corr>τε</corr><sic>δε</sic></choice> <choice><corr>τῷ</corr><sic>δω</sic></choice> ἀγρῷ <expan>α<supplied cert="high" reason="lost">ὐ</supplied>τ<ex cert="high">οῦ</ex></expan> κ<unclear cert="high" reason="undefined">αὶ</unclear> ἐν τ<supplied cert="high" reason="lost">ῇ κώμῃ </supplied><gap extent="8" reason="lost" unit="character"/> 

    <lb n="11"/>καὶ δι᾽ ἣν ἐπείσθησαν γενέσθαι ἀμφότεροι εἰς <expan>ἑαυτ<ex cert="high">οὺς</ex></expan> τὴν παροῦσαν ἀντισύγγ<supplied cert="high" reason="lost">ραφον ὁ</supplied>μολογίαν 

    <lb n="12"/><g type="stauros"/> τῆς υἱοθεσίας <g type="stauros"/> δι᾽ ἧς ὁμολογοῦσιν ὁ μὲν <choice><corr>εἰρημένος</corr><sic>ειρημενον</sic></choice> <choice><corr>Σέργιος</corr><sic>σεργιο<unclear cert="high" reason="undefined">ν</unclear></sic></choice> τὸν προγεγραμμέ<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">ον </supplied><gap extent="5" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ν</unclear> <abbr>αὐ<unclear cert="high" reason="undefined">τ</unclear></abbr> ιγν<gap extent="2" reason="illegible" unit="character"/>ο<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/>ἴδιον καὶ <choice><corr>πρωτότοκον</corr><sic>προτοτοκον</sic></choice> <expan>αὐτ<ex cert="high">οῦ</ex></expan> υἱὸν τὴν πᾶσαν τῶν ἀκινήτων <expan>αὐτ<ex cert="high">οῦ</ex></expan> πραγμάτων ἐργα<supplied cert="high" reason="lost">σίαν ἐκτε</supplied>λέσαι <supplied cert="high" reason="lost">καὶ</supplied> 

    <lb n="14"/>τὴν ἅπασαν τῶν ἁπάντων <expan>αὐτ<ex cert="high">οῦ</ex></expan> πραγμάτων συντέλειαν <choice><corr>δεόντως</corr><sic>δεοντων</sic></choice> κ<supplied cert="high" reason="lost">αὶ ἀκαταφ</supplied>ρονήτ<supplied cert="high" reason="lost">ως</supplied> 

    <lb n="15"/>καὶ <subst><add place="inline">ἀκαταγνώστως</add><del rend="corrected" status="unremarkable">ακαταγνω<del rend="erasure" status="unremarkable">ω</del>στως</del></subst>· καὶ τὸ <choice><corr>περισσευόμενον</corr><sic>περισευομενον</sic></choice> <expan>αὐτ<ex cert="high">οῖς</ex></expan> <choice><corr>ἐτησίως</corr><sic>εντησιως</sic></choice> ὁμοθυμ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">δὸν </supplied><choice><corr><supplied cert="high" reason="lost">δαπ</supplied><unclear cert="high" reason="undefined">α</unclear>νήσου<unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">ιν</supplied></corr><sic><supplied cert="high" reason="lost">δαπ</supplied><unclear cert="high" reason="undefined">α</unclear>νησω<unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">ιν</supplied></sic></choice>, 

    <lb n="16"/>ἐπείπερ εἰς μίαν <num value="1"/> <choice><corr>ὁμοζωΐαν</corr><sic>ομοζωειαν</sic></choice> <choice><corr>ἀποζήσουσιν</corr><sic>αποζωησουσιν</sic></choice> μετ᾽ <choice><corr>ἀλλήλων</corr><sic>αταλλ<unclear cert="high" reason="undefined">η</unclear>λους</sic></choice>. εἰ δὲ <unclear cert="high" reason="undefined">α</unclear><gap extent="7" reason="lost" unit="character"/><unclear cert="high" reason="undefined">σ</unclear>εις 

