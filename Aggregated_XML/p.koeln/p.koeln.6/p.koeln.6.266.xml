    <lb n="1"/>Ἀπολλώνιος Πετεύρει χαίρειν. εἰ τῶι τε 

    <lb n="2"/>σώματι ὑγιαίνεις καὶ ἐν τοῖς ἄλλοις 

    <lb n="3"/>κατὰ λόγον ἀπαλλάσσεις εἴη ἂν ὡς εὔ-

    <lb n="4"/>χομαι. ὑγίαινον δὲ καὶ αὐτός. ἀπαγ-

    <lb n="5"/>γείλαντός μοι Σωστράτου μέμφεσθαί σε 

    <lb n="6"/>κ<gap extent="3" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="3" reason="illegible" unit="character"/>ασ<unclear cert="high" reason="undefined">φη</unclear><gap extent="3" reason="illegible" unit="character"/>ο<gap extent="2" reason="illegible" unit="character"/>χαι<gap extent="2" reason="illegible" unit="character"/>σσε<gap extent="1" reason="illegible" unit="character"/> 

    <lb n="7"/>Ἀπολλωνίο<unclear cert="high" reason="undefined">υ</unclear> <gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ε</unclear>ιχον 

    <lb n="8"/>οὔτε ἀπιστ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>ποιούμενον <unclear cert="high" reason="undefined">η</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/>ἔχοντος εἰ με<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/>πεπλημεληκ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="12"/>εχοιμιαν σοι με<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/>τρόπον διατελ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="14"/><gap extent="1" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ε</unclear> ἀνεστὼς κα<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="15"/>τῆι εὐνοίᾳ δ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="16"/>ὑποστειλα<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="17"/>τρις καθαρα<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="18"/>τοὺς ἐν τῶ<supplied cert="high" reason="lost">ι </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="19"/>χρόνος ονπ<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="20"/>μηνυτὴ<unclear cert="high" reason="undefined">ς</unclear> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="21"/>παραδεδω<unclear cert="high" reason="undefined">κ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="22"/>μεθ᾽ ὅρκου <unclear cert="high" reason="undefined">α</unclear><gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="23"/>εἴ τι <add place="supralinear">ἤμην</add> τῶν εγ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="24"/>τινος ἂν <add place="supralinear">τω</add> αγ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="25"/>πειραθῆναι <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="26"/>περιβαλεῖν <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="27"/>παραδρομ<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="28"/>περὶ τούτω<unclear cert="high" reason="undefined">ν</unclear> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="29"/>οὐ τῆς τυχ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ύσης </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="30"/>τὸ τυχὸν <unclear cert="high" reason="undefined">α</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 
</ab></div><div n="v" part="N" type="textpart"><ab part="N">

    <lb n="1,msup"/>Πετεύρει 

    <lb n="1"/><gap extent="unknown" reason="lost" unit="character"/>ρως τὰ μέγιστα 

    <lb n="2"/><gap extent="unknown" reason="lost" unit="character"/> πρός με ἔχοντα 

    <lb n="3"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">μ</unclear>ενος ἀποστείλω 

    <lb n="4"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">σ</unclear><gap extent="2" reason="illegible" unit="character"/>αι γράψον μοι 

    <lb n="5"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> πρὸς ὑμᾶς </ab></div></div><div n="meta.3196" part="N" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.3196" part="N" subtype="textDate" type="commentary"><p><date notAfter="-0205" notBefore="-0221" type="textDate">221 - 205 v.Chr.</date></p></div><div n="meta.3196" part="N" subtype="general" type="commentary"><p>Gehört zum Archiv des Oikonomos Apollonios.</p></div><div n="meta.3196" part="N" subtype="principalEdition" type="bibliography"><listBibl><bibl subtype="principal" type="publication"><title level="s" type="abbreviated">P.Köln</title><biblScope type="volume">6</biblScope><biblScope type="numbers">266</biblScope></bibl><bibl type="Trismegistos"><biblScope type="numbers">3196</biblScope></bibl><bibl type="DDbDP"><series>0144</series>; <biblScope type="volume">6</biblScope>; <biblScope type="numbers">266</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0144;6;266" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.3196" part="N" subtype="illustrations" type="bibliography"><p><bibl type="illustration">Tafel XXXVII</bibl></p></div><div n="meta.3196" part="N" subtype="locations" type="history"><p><geogName cert="high" type="nome">Arsinoites</geogName>, <geogName cert="high" key="Aegyptus" type="ancientRegion">Egypt</geogName></p></div><div n="meta.3196" part="N" type="figure"><p><figure href="http://www.uni-koeln.de/phil-fak/ifa/NRWakademie/papyrologie/Karte/VI_266.html"><figDesc>Abbildung im Internet</figDesc></figure></p></div></body></text></TEI.2>