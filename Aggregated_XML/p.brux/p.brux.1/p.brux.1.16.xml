    <lb n="1"/><handShift new="m20"/> <num value="105">ρε</num> 

    <lb n="1"/><handShift new="m6"/> Ἀπίωνι <expan>βασιλ<ex cert="high">ικῷ</ex></expan> <expan>γρ<ex cert="high">αμματεῖ</ex></expan> Προσωπείτου 

    <lb n="2"/>παρὰ <expan>Παντβ<ex cert="high">εῦτος</ex></expan> καὶ <choice><corr>Τιθοεννήσιος</corr><sic>τιθοενησις</sic></choice> καὶ Παλακρήους 

    <lb n="3"/>καὶ <choice><corr>Ἁροννήσιος</corr><sic>αροννησις</sic></choice> τῶν <num value="4">δ</num> <expan>Πετῶτ<ex cert="high">ος</ex></expan> 

    <lb n="4"/>Πνεφερῶτος. 

    <lb n="5"/><expan>ἀπογρ<ex cert="high">αφόμεθα</ex></expan> κατὰ τὰ ὑπὸ Καλουεισίου 

    <lb n="6"/>Στατιανοῦ τοῦ <choice><corr>λαμπρο<supplied reason="omitted" cert="high">τά</supplied>του</corr><sic>λαμπροτου</sic></choice> 

    <lb n="7"/>ἡγεμόνος προστεταγμένα 

    <lb n="8"/>εἰς τὴν ἐπ᾽ ἀγαθοῖς <choice><corr>γινομένην</corr><sic>γεινομενος</sic></choice> 

    <lb n="9"/><expan>κ<ex cert="high">ατ᾽</ex></expan> <expan>οἰ<ex cert="high">κίαν</ex></expan> <expan>ἀπογρ<ex cert="high">αφὴν</ex></expan> τὰ ὑπάρχοντα 

    <lb n="10"/>ἐν τῇ κώμῃ <expan>Θελβ<ex cert="high">όνθον</ex></expan> <expan>Σί<ex cert="high">φθα</ex></expan> 

    <lb n="11"/><expan>οἰκ<ex cert="high">ίας</ex></expan> καὶ <expan>ψιλ<ex cert="high">οῦ</ex></expan> <expan>τόπ<ex cert="high">ου</ex></expan> <add place="supralinear"><expan>ἄλλο<ex cert="high">υ</ex></expan></add> Θαειήους <expan>Ψεναμούν<ex cert="high">ιος</ex></expan> 

    <lb n="12"/><expan>πήχ<ex cert="high">εων</ex></expan> <num value="100">ρ</num>, ἐν <choice><corr>οἷς</corr><sic>ης</sic></choice> <choice><corr>ἐλαιούργιον</corr><sic>ελεουργιον</sic></choice>, 

    <lb n="13"/><expan>Παντβ<ex cert="high">εῦτος</ex></expan> <expan>μ<ex cert="high">έρη</ex></expan> <num value="2">β</num> καὶ <choice><corr>Τιθοεννήσιος</corr><sic>τιθοενησιος</sic></choice> <expan>μ<ex cert="high">έρος</ex></expan> <num value="1">α</num> 

    <lb n="14"/>καὶ <choice><corr>Παλακρήους</corr><sic>παλακρης</sic></choice> <expan>μ<ex cert="high">έρος</ex></expan> <num value="1">α</num> καὶ Ἁροννήσιος <expan>μ<ex cert="high">έρος</ex></expan> <num value="1">α</num> 

    <lb n="15"/>τῶν <num value="4">δ</num> <expan>Πετῶτ<ex cert="high">ος</ex></expan> Πνεφερῶτος, 

    <lb n="16"/>ἐν οἷς οὐδείς,  καὶ Πνεφερῶτος 

    <lb n="17"/><expan>ψιλ<ex cert="high">ῶν</ex></expan> <expan>τόπ<ex cert="high">ων</ex></expan> <expan>πήχ<ex cert="high">εων</ex></expan> <num value="668">χξη</num> <num value="1/2">𐅵</num> τῶν <expan>α<ex cert="high">ὐτῶν</ex></expan> 

    <lb n="18"/><expan>προγεγρα<ex cert="high">μμένων</ex></expan> τὸ <expan>ἐπιβάλ<ex cert="high">λον</ex></expan> ἑκάστῳ <expan>μ<ex cert="high">έρος</ex></expan> 

    <lb n="19"/>καὶ <expan>Πετῶ<ex cert="high">τος</ex></expan> <supplied reason="omitted" cert="high"><expan>ψιλ<ex cert="high">οῦ</ex></expan></supplied> <expan>τόπ<ex cert="high">ου</ex></expan> τῶν <expan>α<ex cert="high">ὐτῶν</ex></expan> <sic>καὶ</sic> <expan>προγεγ<ex cert="high">ραμμένων</ex></expan> 

    <lb n="20"/>καὶ <supplied reason="omitted" cert="high">τὸ</supplied> <expan>ἐπιβάλ<ex cert="high">λον</ex></expan> <choice><corr>ἑκάστῳ</corr><sic>εκαστη</sic></choice> <expan>μ<ex cert="high">έρους</ex></expan> <num value="1/2">𐅵</num> καὶ 

    <lb n="21"/>τὸ <expan>λοιπ<ex cert="high">ὸν</ex></expan> <expan>μ<ex cert="high">έρος</ex></expan> <num value="1/2">𐅵</num> <expan>Θερμούθ<ex cert="high">ιος</ex></expan> <expan>Πνεφερ<unclear reason="undefined" cert="high">οτιθ</unclear><ex cert="high">οήους</ex></expan> 

    <lb n="22"/>κατὰ τὸ <num value="1/4">δ</num> μέρος καὶ <choice><corr>Νουάτιος</corr><sic>νουατις</sic></choice> <expan>Παλαχ<ex cert="high">ρήους</ex></expan> 

    <lb n="23"/>ἀπὸ <expan>κληρ<ex cert="high">ονομίας</ex></expan> <expan>Πνεφερῶτ<ex cert="high">ος</ex></expan> <expan>Πνεφ<ex cert="high">ερῶτος</ex></expan> <sic>καὶ</sic> τὸ <expan>λοιπ<ex cert="high">ὸν</ex></expan> 

    <lb n="24"/>μέρος <num value="1/4">δ</num>          ἐν οἷς οὐδείς. 

    <lb n="25"/><expan><ex cert="high">ἔτους</ex></expan> <num value="14">ιδ</num> Αὐτοκράτορος <supplied reason="omitted" cert="high">Καίσαρος Μάρκου Αὐρηλίου</supplied> <choice><corr>Ἀντωνίνο<supplied reason="lost" cert="high">υ</supplied></corr><sic>αντωνεινο<supplied reason="lost" cert="high">υ</supplied></sic></choice> 

    <lb n="26"/>Σεβαστοῦ Ἀρμενιακοῦ 

    <lb n="27"/>Μηδικοῦ <choice><corr>Παρθικοῦ</corr><sic>παρ<del rend="erasure" status="unremarkable">τ</del>θικου</sic></choice> <choice><corr>Γερμανι<supplied reason="omitted" cert="high">κ</supplied>οῦ</corr><sic>γερμανιου</sic></choice> 

    <lb n="28"/>Μεγίστου, Ἐπεὶφ <num value="25">κε</num>. 

    <lb n="29"/><handShift new="m3"/> <expan>Παντβ<ex cert="high">εῦς</ex></expan> καὶ Τοθοεννῆσις 

    <lb n="30"/>καὶ <choice><corr>οἱ</corr><sic>τω</sic></choice> <expan>ἀδελ<ex cert="high">φοὶ</ex></expan> <expan>ἐπιδεδώκ<ex cert="high">αμεν</ex></expan> ὡς <expan>π<ex cert="high">ρόκειται</ex></expan>. 

    <lb n="31"/>Ἀγαθὸς <expan>Δαί<ex cert="high">μων</ex></expan> <expan>Σαίφιο<ex cert="high">ς</ex></expan> <expan>ἔγρ<ex cert="high">αψα</ex></expan> 

    <lb n="32"/>ὑπὲρ <expan>α<ex cert="high">ὐτῶν</ex></expan> μὴ <choice><corr>εἰδότων</corr><sic>ειδοδες</sic></choice> <expan>γρ<ex cert="high">άμματα</ex></expan>. 

    <lb n="33"/><handShift new="m21"/> δι᾽ <abbr>Ἁρποκ</abbr> <expan>σε<ex cert="high">λίδος</ex></expan> <num value="42">μβ</num>. </ab></div><div n="meta.16053" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.16053" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" value="0174-07-19" notBefore="174" notAfter="174">19. Juli 174</date></p></div><div n="meta.16053" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Rückseite: P.Brux. I  21.</p></div><div n="meta.16053" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">P.Brux.</title><biblScope type="volume">1</biblScope><biblScope type="numbers">16</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">16053</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0085</series>; <biblScope type="volume">1</biblScope>; <biblScope type="numbers">16</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0085;1;16" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.16053" type="bibliography" subtype="corrections" org="uniform" part="N" sample="complete"><head>BL-Einträge nach BL-Konkordanz</head><bibl type="BL" default="NO"><biblScope type="volume">X</biblScope>, S. <biblScope type="pages">31</biblScope></bibl></div><div n="meta.16053" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p><bibl type="illustration" default="NO">P.Lugd. Bat. V nach S. 186</bibl><bibl type="illustration" default="NO">Egyptian Treasures in Europe, Vol. 2 (Inv. E.7616)</bibl><bibl type="illustration" default="NO">Brosius, Ancient Archives, S. 250</bibl></p></div><div n="meta.16053" type="bibliography" su