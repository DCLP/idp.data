    <lb n="12"/><expan>οἰκ<ex cert="high">ίαν</ex></expan> καὶ <expan>ψιλ<ex cert="high">οὺς</ex></expan> <expan>τόπ<ex cert="high">ους</ex></expan> Ἀμμωνίο<supplied cert="high" reason="lost">υ</supplied> 

