    <lb n="2"/>σημασία· εἰς <choice><corr>τὸ</corr><sic>τον</sic></choice> <num value="1">α</num> μέρος <expan>Ἀντιν<ex cert="high">όου</ex></expan> τῆς Ἑπτανομ-

    <lb n="3"/>ίας <gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="2" unit="character"/> </ab></div></div><div n="meta.31506" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.31506" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" notBefore="201" notAfter="250">1. Hälfte III</date></p></div><div n="meta.31506" type="commentary" subtype="mentionedDates" org="uniform" part="N" sample="complete"><head>Erwähnte Daten</head></div><div n="meta.31506" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">P.Laur.</title><biblScope type="volume">1</biblScope><biblScope type="numbers">20</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">31506</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0147</series>; <biblScope type="volume">1</biblScope>; <biblScope type="numbers">20</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0147;1;20" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.31506" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p><bibl type="illustration" default="NO">Tav. XX</bibl></p></div><div n="meta.31506" type="history" subtype="locations" org="uniform" part="N" sample="complete"><p><placeName type="ancientFindspot" cert="high" full="yes">Antinoopolis</placeName></p></div></body></text></TEI.2>