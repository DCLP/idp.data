    <lb n="1"/>Σαραπί<unclear reason="undefined" cert="high">ω</unclear>ν Βε<unclear reason="undefined" cert="high">λλήο</unclear>υς 

    <lb n="2"/>ὁρμοφύλακος τῆς <expan>Καινῆ<ex cert="high">ς</ex></expan> 

    <lb n="3"/>ὁ ἡγούμενος τοῖς ἐν 

    <lb n="4"/>τῆ<unclear reason="undefined" cert="high">ι</unclear> <unclear reason="undefined" cert="high">Κα</unclear>ινῆι χειρισταῖς 

    <lb n="5"/>καὶ οἱ ἄλλοι <choice><corr>πρεσβύτεροι</corr><sic>προσβυτεροι</sic></choice> 

    <lb n="6"/><supplied reason="lost" cert="high">Δ</supplied><unclear reason="undefined" cert="high">ι</unclear>ονυσίωι τῶι σιτο-

    <lb n="7"/><supplied reason="lost" cert="high">λο</supplied>γοῦντι τὸ περὶ <expan>Ὀξύρυ<ex cert="high">γχα</ex></expan> 

    <lb n="8"/><supplied reason="lost" cert="high">ἐρ</supplied>γ<unclear reason="undefined" cert="high">α</unclear>στήριον χαίρειν· 

    <lb n="9"/><choice><corr><supplied reason="lost" cert="high">ὁ</supplied>μολογοῦ<supplied reason="omitted" cert="high">με</supplied>ν</corr><sic><supplied reason="lost" cert="high">ο</supplied>μολογουν</sic></choice> ἔχειν παρὰ σοῦ 

    <lb n="10"/><supplied reason="lost" cert="high">π</supplied>υρῶν ἀρτάβας <choice><corr>εἴκοσι</corr><sic>εικουσι</sic></choice>, 

    <lb n="11"/><supplied reason="lost" cert="high"><expan><ex cert="high">γίνονται</ex></expan></supplied> πυρῶν <expan>ἀ<ex cert="high">ρτάβαι</ex></expan> <num value="20">κ</num>, φόρετρον 

    <lb n="12"/><num value="20"><unclear reason="undefined" cert="high">κ</unclear></num> <expan>ἀ<ex cert="high">ρταβῶν</ex></expan>, καὶ οὐ μὴ ἅψω<unclear reason="undefined" cert="high">μεν</unclear> 

    <lb n="13"/><choice><corr><supplied reason="lost" cert="high">φο</supplied>ρέτρου</corr><sic><supplied reason="lost" cert="high">φο</supplied><unclear reason="undefined" cert="high">ρε</unclear>τρ<unclear reason="undefined" cert="high">ο</unclear>ν</sic></choice> ἐὰν μὴ κουφί-

    <lb n="14"/><supplied reason="lost" cert="high">σ</supplied><unclear reason="undefined" cert="high">ω</unclear>μεν αὐτὰς μέτρω<unclear reason="undefined" cert="high">ι</unclear> 

    <lb n="15"/><unclear reason="undefined" cert="high">τ</unclear>ῶι πρὸς τὸ χαλκοῦν. 

    <lb n="16"/>ἐὰν δὲ μὴ ἀποτείσω-

    <lb n="17"/>μεν, <choice><corr>μετρήσομεν</corr><sic>μετρησωμεν</sic></choice> 

    <lb n="18"/>πυρῶν <expan>ἀ<ex cert="high">ρτάβας</ex></expan> <num value="40">μ</num>. 
