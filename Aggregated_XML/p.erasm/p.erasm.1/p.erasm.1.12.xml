    <lb n="1"/>Πετεαρμώτης Ὥρου 

    <lb n="2"/>ὁ ἡγούμενος τοῖς ἐν τῆι 

    <lb n="3"/>Καινῇ νωτοφόροις καὶ οἱ 

    <lb n="4"/>ἄλλοι <choice><corr>πρεσβύτεροι</corr><sic>πρυσβυτεροι</sic></choice> 

    <lb n="5"/>Διονυσίωι τῶι σιτολογοῦ<unclear reason="undefined" cert="high">ν</unclear>-

    <lb n="6"/>τι τὸ περὶ Ὀξύρυγχα <expan>ἐργα<ex cert="high">στήριον</ex></expan> 

    <lb n="7"/>χαίρειν· ὁμολογοῦμεν 

    <lb n="8"/>ἔχειν παρὰ <unclear reason="undefined" cert="high">σο</unclear>ῦ ἀπὸ 

    <lb n="9"/>τῶν γενη<unclear reason="undefined" cert="high">μ</unclear>άτων 

    <lb n="10"/>τοῦ <num value="29">κθ</num> <expan><ex cert="high">ἔτους</ex></expan> ὑπὲρ φ<unclear reason="undefined" cert="high">ο</unclear>-

    <lb n="11"/>ρέτρου ἐν προχρήσει 

    <lb n="12"/>πυρῶν ἀρτάβας εἴκο<unclear reason="undefined" cert="high">σι</unclear>, 

    <lb n="13"/><expan><ex cert="high">γίνονται</ex></expan> πυρῶν <expan>ἀ<ex cert="high">ρτάβαι</ex></expan> <num value="20">κ</num> μέτρωι 

    <lb n="14"/>τῶι πρὸς <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ὸ</supplied> χαλκοῦν 

    <lb n="15"/>καὶ οὐ μὴ ἅ<unclear reason="undefined" cert="high">ψ</unclear>ωμαι φορέτρ<unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">υ</supplied> 

    <lb n="16"/>ἐὰν μὴ κουφίσω<unclear reason="undefined" cert="high">μαι</unclear> <unclear reason="undefined" cert="high">αὐ</unclear><supplied reason="lost" cert="high">τὰς</supplied> 

    <lb n="17"/>ἀπὸ τῶν ἐπιβαλλομ<supplied reason="lost" cert="high">έ</supplied><unclear reason="undefined" cert="high">ν</unclear><supplied reason="lost" cert="high">ων</supplied> 

    <lb n="18"/>μοι <choice><corr>φορέτρων</corr><sic>φορετρον</sic></choice>. ἐὰν δὲ <unclear reason="undefined" cert="high">μὴ</unclear> 

    <lb n="19"/><choice><corr>ἀποτείσωμεν</corr><sic>απο<unclear reason="undefined" cert="high">τεισ</unclear>ο<unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">ε</supplied><unclear reason="undefined" cert="high">ν</unclear></sic></choice>, <unclear reason="undefined" cert="high">μετ</unclear><supplied reason="lost" cert="high">ρήσομεν</supplied> 

    <lb n="20"/><supplied reason="lost" cert="high">πυρῶν <expan>ἀ<ex cert="high">ρτάβας</ex></expan> <num value="40">μ</num></supplied>. 
</ab></div><div type="textpart" n="v" org="uniform" part="N" sample="complete"><ab part="N">

