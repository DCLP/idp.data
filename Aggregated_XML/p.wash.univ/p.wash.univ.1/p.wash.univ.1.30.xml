    <lb n="25"/>κ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ὶ</supplied> <unclear cert="high" reason="undefined">ε</unclear>ὐθέως σοι κομισθήσεται 

    <lb n="26"/><app type="editorial"><lem>ἠγ<supplied cert="high" reason="lost">ο</supplied>ράκειν</lem><rdg><gap extent="3" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ρ</unclear>ακειν</rdg></app> <unclear cert="high" reason="undefined">ἱμ</unclear>άτιον 

    <lb n="26/`27"/><app xml:space="preserve"><lem>προπάλαιον</lem><rdgGrp type="ed"><app type="editorial"><lem/><rdg><subst><add place="inline">πρὸ παλαίου</add><del rend="corrected"><unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">λ</unclear>αιον</del></subst></rdg></app><subst><add place="inline">πρὸ παλαίου</add><del rend="corrected"><unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">λ</unclear>αιον</del></subst></rdgGrp></app> 

    <lb n="27"/><hi rend="diaeresis">ἵ</hi>να πέμψω <unclear cert="high" reason="undefined">σ</unclear>οι αὐ-

    <lb n="28"/>τὸ διὰ Ὑακίνθου.  ἀντί<supplied cert="high" reason="lost">γ</supplied><unclear cert="high" reason="undefined">ρ</unclear>αψον 

    <lb n="29"/>δέ μοι εἴπ<unclear cert="high" reason="undefined">ε</unclear>ρ <app type="editorial"><lem>χρῄζεις</lem><rdg>χρή<unclear cert="high" reason="undefined">σ</unclear>εις</rdg></app> ἄ<supplied cert="high" reason="lost">λλ</supplied><unclear cert="high" reason="undefined">ο</unclear>υ 

    <lb n="30"/>καὶ εὐθέως σοι κομισθήσ<unclear cert="high" reason="undefined">ε</unclear>-

    <lb n="31"/>ται. ἐρρῶσθαί <unclear cert="high" reason="undefined">σε</unclear> <unclear cert="high" reason="undefined">εὔχ</unclear>ομαι. 

    <lb n="32"/>ἀσπάζομαί σε, ἄδε<unclear cert="high" reason="undefined">λφε</unclear> 

