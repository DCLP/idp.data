    <lb n="4"/><unclear reason="undefined" cert="high">Φα</unclear><supplied reason="lost" cert="high">ρ</supplied><unclear reason="undefined" cert="high">μο</unclear>ῦθι <num value="10">ι</num> <supplied reason="lost" cert="high">ἐν Πτολεμαίδι Εὐε</supplied><unclear reason="undefined" cert="high">ρ</unclear>γέτιδι τοῦ Ἀ<unclear reason="undefined" cert="high">ρ</unclear>σι<unclear reason="undefined" cert="high">νοείτου</unclear> ν<supplied reason="lost" cert="high">ο</supplied>μ<supplied reason="lost" cert="high">οῦ. ὁμολογεῖ Αὐρήλιος Τίτος Οὐαλέριος Γαιανὸς Τίτου Οὐαλερίου 

Στρατιανοῦ τοῦ καὶ Σερήνου</supplied> 

    <lb n="5"/>μητρ<unclear reason="undefined" cert="high">ὸς</unclear> υ<unclear reason="undefined" cert="high">λ</unclear><supplied reason="lost" cert="high">ίας Ἀντινοεὺς καὶ</supplied> <unclear reason="undefined" cert="high">ὡ</unclear>ς <expan>χρηματίζ<ex cert="high">ει</ex></expan> ὡ<unclear reason="undefined" cert="high">ς</unclear> <expan><ex cert="high">ἐτῶν</ex></expan> <num value="55">ν<unclear reason="undefined" cert="high">ε</unclear></num> ο<supplied reason="lost" cert="high">ὐλὴ </supplied><gap reason="lost" extent="20" unit="character"/><supplied reason="lost" cert="high">   Αὐρηλίᾳ Ταπάειτι Παπνούθιος μητρὸς Θαείσας ἀπὸ κώμης Φιλαδελ</supplied>-

    <lb n="6"/>φίας τ<unclear reason="undefined" cert="high">ῆς</unclear> α<unclear reason="undefined" cert="high">κλ</unclear><supplied reason="lost" cert="high">ε</supplied>ί<supplied reason="lost" cert="high">δου μερίδος</supplied> χωρὶς κυρίου χρηματιζο<supplied reason="lost" cert="high">ύσῃ κατὰ τὰ Ῥωμαίων ἔθη τέκνων δικαίῳ πεπρακέναι αὐτῇ τὸν ὁμολογοῦντα κατὰ τήνδε</supplied> 

    <lb n="7"/><unclear reason="undefined" cert="high">τὴν</unclear> <choice><corr>ὁμολογίαν</corr><sic><unclear reason="undefined" cert="high">ομ</unclear><supplied reason="lost" cert="high">ο</supplied>λ<unclear reason="undefined" cert="high">ογ</unclear>ε<supplied reason="lost" cert="high">ι</supplied><unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">ν</supplied></sic></choice> <unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">αὶ διὰ τῆς τ</supplied>ῶν ἐνκτ<unclear reason="undefined" cert="high">ήσεων</unclear> βιβ<supplied reason="lost" cert="high">λ</supplied>ι<supplied reason="lost" cert="high">οθήκης ἀπὸ τοῦ νῦν ἐπὶ τὸν ἅπαντα χρόνον τὴν ὑπάρχουσαν αὐτῷ ἐν τῇ αὐτῇ κώμῃ</supplied> 

    <lb n="8"/>Φι<unclear reason="undefined" cert="high">λα</unclear>δ<supplied reason="lost" cert="high">ελ</supplied><unclear reason="undefined" cert="high">φ</unclear>ί<unclear reason="undefined" cert="high">ᾳ</unclear> οἰ<unclear reason="undefined" cert="high">κία</unclear>ν μο<unclear reason="undefined" cert="high">ν</unclear><supplied reason="lost" cert="high">όστεγο</supplied>ν <expan><ex cert="high">πρότερον</ex></expan> ὄντος μυ<unclear reason="undefined" cert="high">λα</unclear>ίου ἐν ᾧ ὕ<unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">αυλις καταπεπτωκυῖα καὶ αὐτὸ καὶ τὰ περὶ αὐτὸ ἐπὶ τοῖς τῆς αὐτῆς οἰκίας 
μέτροις καὶ πηχισμοῖς καὶ θε</supplied>-

    <lb n="9"/>μελ<unclear reason="undefined" cert="high">ί</unclear>οι<unclear reason="undefined" cert="high">ς</unclear> καὶ <choice><corr>τείχεσι</corr><sic>τι<unclear reason="undefined" cert="high">χε</unclear>σι</sic></choice> κα<supplied reason="lost" cert="high">ὶ</supplied> <unclear reason="undefined" cert="high">φω</unclear><supplied reason="lost" cert="high">σφο</supplied>ρίαις καὶ εἰσόδοις καὶ ἐξόδοι<unclear reason="undefined" cert="high">ς</unclear> <supplied reason="lost" cert="high">καὶ ταῖς πάσαις χρήσεσι καὶ δικαίοις πᾶσι κατὰ τὴν ἐξ ἀρχῆς 
καὶ μέχρι νῦν συνήθειαν ἧς γείτο</supplied>-

    <lb n="10"/>νες <unclear reason="undefined" cert="high">κα</unclear>θὼς ἐξ <unclear reason="undefined" cert="high">σ</unclear>υμφ<supplied reason="lost" cert="high">ώνο</supplied><unclear reason="undefined" cert="high">υ</unclear> <supplied reason="lost" cert="high">ὐτοὶ ὑ</supplied>πηγόρευσαν νότου καὶ λιβ<unclear reason="undefined" cert="high">ὸ</unclear><supplied reason="lost" cert="high">ς  </supplied><gap reason="lost" extent="15" unit="character"/><supplied reason="lost" cert="high"> βορρᾶ Ἀρίωνος Σαραπίωνος οἰκία ἀπηλιώτου ῥύμη δημοσία καὶ ἀπέχειν</supplied> 

    <lb n="11"/>τὸν <unclear reason="undefined" cert="high">ὁμ</unclear>ολογοῦν<unclear reason="undefined" cert="high">τ</unclear>α <unclear reason="undefined" cert="high">Τ</unclear>ί<unclear reason="undefined" cert="high">τ</unclear>ον <supplied reason="lost" cert="high">Οὐα</supplied>λέριον Γαιανὸν παρὰ τῆ<supplied reason="lost" cert="high">ς Ταπάειτος τὴν συμπεφωνημένην τιμὴν πᾶσαν ἐκ πλήρους ἀργυρίου δρα</supplied>-

    <lb n="12"/>χμὰς <choice><corr>τρισχιλίας</corr><sic>τρισχειλιας</sic></choice>, <expan>γί<ex cert="high">νονται</ex></expan> <expan><ex cert="high">δραχμαὶ</ex></expan> <num value="3000">Γ</num>, π<supplied reason="lost" cert="high">αρ</supplied>αχρῆμα διὰ χειρὸς καὶ βεβ<supplied reason="lost" cert="high">αιώσειν καὶ αὐτὸν τὸν ὁμολογοῦντα καὶ τοὺς 
παρ᾽ αὐτοῦ τῇ Ταπάειτι καὶ τοῖς παρ᾽ αὐτῆς</supplied> 

    <lb n="13"/>τὸ πεπραμένον αὐτῇ ὡ<supplied reason="lost" cert="high">ς π</supplied><unclear reason="undefined" cert="high">ρ</unclear>όκειται οἰκίδιον πάσῃ βεβ<supplied reason="lost" cert="high">αιώσει ὃ καὶ παρέξεται ἀνέπαφον καὶ ἀνενεχύραστον καὶ <choice><corr>ἀνεπιδάνειστον</corr><sic>ανεπιδανιστον</sic></choice> καὶ καθαρὸν ἀ</supplied>-

    <lb n="14"/>πὸ παντὸς <choice><corr>ὀφειλήματος</corr><sic>οφιληματο<unclear reason="undefined" cert="high">ς</unclear></sic></choice> δημοσίου τε καὶ ἰδιωτικοῦ κ<supplied reason="lost" cert="high">αὶ πάσης ἐμποιήσεως καὶ μηδένα κωλύοντα τὴν Ταπάειν μηδὲ τοὺς</supplied> 

    <lb n="15"/>παρ᾽ αὐτῆς κυριεύοντας <unclear reason="undefined" cert="high">τοῦ</unclear> ὐτοῦ καὶ πε<unclear reason="undefined" cert="high">π</unclear>ρακέναι α<unclear reason="undefined" cert="high">ὐ</unclear><supplied reason="lost" cert="high">τὸν τὸν ὁμολογοῦντα οὐ μόνον τὸ οἰκίδιον ἀλλὰ καὶ τὰ ἐξ αὐτοῦ ἀπὸ νῦν περιγινόμενα ἀποφέρεσθαι</supplied> 

    <lb n="16"/>εἰς τὸ ἴδιον καὶ ἐξουσίαν <unclear reason="undefined" cert="high">ἔ</unclear>χειν <choice><corr>διοικεῖν</corr><sic>διοικιν</sic></choice> καὶ <choice><corr>οἰκονομεῖν</corr><sic>οικονομιν</sic></choice> π<supplied reason="lost" cert="high">ερὶ αὐτοῦ ὡς ἐὰν αἵρηται ἐφ᾽ ᾧ καὶ μὴ ἐπιπορεύεσθαι 
αὐτὸν τὸν ὁμολογοῦντα μηδὲ ἄλ</supplied>-

    <lb n="17"/>λον ὑπὲρ αὐτοῦ μηδένα κατὰ μηδένα τρόπον ἀλλὰ κα<supplied reason="lost" cert="high">ὶ τὸν ἐπελευσόμενον ἢ ἐμποιησόμενον καθ᾽ ὁντιναοῦν τρόπον αὐτὸν ἀποστή</supplied>-

    <lb n="18"/><choice><corr>σειν</corr><sic>αποστησιν</sic></choice> παραχρῆμα τοῖς ἰδίοις δαπανήμασι ἔτι καὶ προσ<unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">ποτείσειν τὴν τιμὴν μεθ᾽ ἡμιολίας καὶ τὰ ἀναλώματα διπλᾶ χωρὶς τοῦ μένειν κύρια</supplied> 

    <lb n="19"/>καὶ τὰ προγε<unclear reason="undefined" cert="high">γ</unclear>ραμμένα. περὶ τοῦ ταῦτα οὕτως ὀρθ<supplied reason="lost" cert="high">ῶς καὶ καλῶς γεγονέναι ἐπερωτηθεὶς ὡμολόγησεν</supplied>. 

    <lb n="20"/><handShift new="m2"/> Τίτος Οὐαλέριος Γαιανὸς <choice><corr>ὁμολογῶ</corr><sic>ομολογο</sic></choice> <choice><corr>πεπρακέναι</corr><sic>παιπρακεναι</sic></choice> <supplied reason="lost" cert="high">Αὐρηλίᾳ Ταπάειτι τὸ ὕπαρχόν μοι ἐν κώμῃ Φιλαδελφίᾳ 
οἰκίδιον ἐν ᾧ ὕπαυλις </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ὃ καὶ παρέξω καθαρὸν ὡς πρό</supplied>-

    <lb n="21"/><choice><corr>κειται</corr><sic><subst><add place="inline"><supplied reason="lost" cert="high">προ</supplied>κινται</add><del rend="corrected" status="unremarkable"><supplied reason="lost" cert="high">προ</supplied>κιν<gap reason="illegible" extent="2" unit="character"/>ι</del></subst></sic></choice> καὶ ἀπ<unclear reason="undefined" cert="high">έ</unclear>σχον τὰς τῆς τ<unclear reason="undefined" cert="high">ιμ</unclear>ῆς δραχμὰς τρι<unclear reason="undefined" cert="high">σ</unclear><supplied reason="lost" cert="high">χιλίας <num value="3000"/> ὡς πρόκειται. <handShift new="m3"/> Αὐρηλία Ταπάεις γέγονε</supplied> 

    <lb n="22"/>εἴς με ἡ πρᾶσ<unclear reason="undefined" cert="high">ις</unclear> ὡ<unclear reason="undefined" cert="high">ς</unclear> <expan>π<unclear reason="undefined" cert="high">ρό</unclear>κ<ex cert="high">ειται</ex></expan>. Αὐρήλιος Ἥ<unclear reason="undefined" cert="high">ρ</unclear>ων Ἀφ<unclear reason="undefined" cert="high">ικων</unclear>ίου ἔγραψ<supplied reason="lost" cert="high">α ὑπὲρ αὐτῆς ἀγραμμάτου. <handShift new="m4"/> <expan>Αὐρή<ex cert="high">λιος</ex></expan> Ὡρίων <expan>βουλ<ex cert="high">ευτὴς</ex></expan> <expan>βιβλ<ex cert="high">ιοφύλαξ</ex></expan> 
