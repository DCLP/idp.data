    <lb n="1"/>ἀπὸ τῆς μεγάλης <gap extent="30" reason="lost" unit="character"/> 

    <lb n="2"/>κυριας Αὐρηλίῳ <gap extent="25" reason="lost" unit="character"/><supplied cert="high" reason="lost">  ἀπὸ τῆς</supplied> 

    <lb n="3"/><unclear cert="high" reason="undefined">Ὀξυρ</unclear>υγχιτῶν <supplied cert="high" reason="lost">πόλεως χαίρειν. ὁμολογῶ παρειλη</supplied>-

    <lb n="4"/><unclear cert="high" reason="undefined">φ</unclear>έναι παρὰ σοῦ καὶ <unclear cert="high" reason="undefined">ἐ</unclear><supplied cert="high" reason="lost">μβεβλῆσθαι </supplied><gap extent="20" reason="lost" unit="character"/> 

    <lb n="5"/>ἐν ὅρμῳ τῆς Ὀξυρ<unclear cert="high" reason="undefined">υ</unclear><supplied cert="high" reason="lost">γχιτῶν πόλεως  </supplied><gap extent="17" reason="lost" unit="character"/> 

    <lb n="6"/>δεκαπέντε <num value="15"/> ναύλου αὐτῶν χ<unclear cert="high" reason="undefined">ρυ</unclear><supplied cert="high" reason="lost">σοῦ </supplied><gap extent="17" reason="lost" unit="character"/> 

    <lb n="7"/>ἐπὶ τῷ με τὸν γόμον ἀποκομίσ<unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">σθαι εἰς τὴν <expan>λαμπρ<ex cert="high">οτάτην</ex></expan> Ἀλεξάν</supplied>-

    <lb n="8"/>δρειαν καὶ παραδοῦναί σοι ἄδολ<supplied cert="high" reason="lost">ον καθαρὸν ἀκακούργητον</supplied> 

    <lb n="9"/>ἀπὸ πάσης ναυτικῆς κακουργίας <gap extent="1" reason="illegible" unit="character"/><gap extent="20" reason="lost" unit="character"/><supplied cert="high" reason="lost">  καὶ</supplied> 

    <lb n="10"/>ἅμα τῇ <choice><corr>ἀπογομώσει</corr><sic>απογομωσι</sic></choice> <choice><corr>λαβεῖν</corr><sic>λαβιν</sic></choice> με παρ<supplied cert="high" reason="lost">ὰ σοῦ </supplied><gap extent="20" reason="lost" unit="character"/> 

    <lb n="11"/><expan><ex cert="high">τάλαντα</ex></expan> <num value="8">η</num> ναυλωτικῆς. ἐπερωτηθε<supplied cert="high" reason="lost">ὶς ὡμολόγησα</supplied>. 

    <lb n="12"/>Αὐρήλιος Κολλοῦθος Πάτος <gap extent="20" reason="lost" unit="character"/> 

