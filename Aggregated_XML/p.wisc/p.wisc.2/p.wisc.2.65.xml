    <lb n="1"/>ἀπὸ τῆς μεγάλης <gap reason="lost" extent="30" unit="character"/> 

    <lb n="2"/>κυριας Αὐρηλίῳ <gap reason="lost" extent="25" unit="character"/><supplied reason="lost" cert="high">  ἀπὸ τῆς</supplied> 

    <lb n="3"/><unclear reason="undefined" cert="high">Ὀξυρ</unclear>υγχιτῶν <supplied reason="lost" cert="high">πόλεως χαίρειν. ὁμολογῶ παρειλη</supplied>-

    <lb n="4"/><unclear reason="undefined" cert="high">φ</unclear>έναι παρὰ σοῦ καὶ <unclear reason="undefined" cert="high">ἐ</unclear><supplied reason="lost" cert="high">μβεβλῆσθαι </supplied><gap reason="lost" extent="20" unit="character"/> 

    <lb n="5"/>ἐν ὅρμῳ τῆς Ὀξυρ<unclear reason="undefined" cert="high">υ</unclear><supplied reason="lost" cert="high">γχιτῶν πόλεως  </supplied><gap reason="lost" extent="17" unit="character"/> 

    <lb n="6"/>δεκαπέντε <num value="15"/> ναύλου αὐτῶν χ<unclear reason="undefined" cert="high">ρυ</unclear><supplied reason="lost" cert="high">σοῦ </supplied><gap reason="lost" extent="17" unit="character"/> 

    <lb n="7"/>ἐπὶ τῷ με τὸν γόμον ἀποκομίσ<unclear reason="undefined" cert="high">ε</unclear><supplied reason="lost" cert="high">σθαι εἰς τὴν <expan>λαμπρ<ex cert="high">οτάτην</ex></expan> Ἀλεξάν</supplied>-

    <lb n="8"/>δρειαν καὶ παραδοῦναί σοι ἄδολ<supplied reason="lost" cert="high">ον καθαρὸν ἀκακούργητον</supplied> 

    <lb n="9"/>ἀπὸ πάσης ναυτικῆς κακουργίας <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="20" unit="character"/><supplied reason="lost" cert="high">  καὶ</supplied> 

    <lb n="10"/>ἅμα τῇ <choice><corr>ἀπογομώσει</corr><sic>απογομωσι</sic></choice> <choice><corr>λαβεῖν</corr><sic>λαβιν</sic></choice> με παρ<supplied reason="lost" cert="high">ὰ σοῦ </supplied><gap reason="lost" extent="20" unit="character"/> 

    <lb n="11"/><expan><ex cert="high">τάλαντα</ex></expan> <num value="8">η</num> ναυλωτικῆς. ἐπερωτηθε<supplied reason="lost" cert="high">ὶς ὡμολόγησα</supplied>. 

