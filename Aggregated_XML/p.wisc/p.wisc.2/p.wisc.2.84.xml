    <lb n="22"/><choice><corr>λαβοῦσα</corr><sic>λαβω<unclear reason="undefined" cert="high">ν</unclear></sic></choice> <unclear reason="undefined" cert="high">ταῦ</unclear>τα γρά<supplied reason="lost" cert="high">μ</supplied>ματα 

