    <lb n="1"/><supplied cert="high" reason="lost">Σεμπ</supplied><unclear cert="high" reason="undefined">ρώ</unclear>νιος   Σα<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ορνί</supplied>λῳ τῷ ἀδελ<unclear cert="high" reason="undefined">φ</unclear>ῷ 

    <lb n="2"/>χαίρειν. 

    <lb n="3"/><choice><corr>ἐκομισάμην</corr><sic><unclear cert="high" reason="undefined">ε</unclear>κομεισαμην</sic></choice> σου δύο <num value="2"/> <choice><corr>ἐπιστολάς</corr><sic><supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">π</unclear>ειστολας</sic></choice>, <choice><corr>μίαν</corr><sic>μειαν</sic></choice> μὲν πε-

    <lb n="4"/><unclear cert="high" reason="undefined">ρ</unclear>ὶ ὧν ἐδήλωσα τῷ ἀ<supplied cert="high" reason="lost">δ</supplied>ελφῷ Μαξίμῳ, ἑτέραν δὲ 

    <lb n="5"/>περὶ τῆς κυρίας ἡμῶ<unclear cert="high" reason="undefined">ν</unclear> μητρὸς ὡς κινδυνευσά-

    <lb n="6"/><choice><corr>σης</corr><sic>κεινδυνευ<supplied cert="high" reason="lost">ο</supplied>υσασης</sic></choice> αὐτῆς καὶ ἀκμὴν <choice><corr>διακατεχομένης</corr><sic><unclear cert="high" reason="undefined">δ</unclear>ειακατεχομενη<add place="supralinear">ς</add></sic></choice> τῇ ν<unclear cert="high" reason="undefined">ω</unclear>θρί-

    <lb n="7"/>ᾳ. <choice><corr>γίγνωσκε</corr><sic>γεινωσκε</sic></choice> οὖν, ἀδ<unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">λ</supplied>φέ, ἱκανῶς με ἀγων<supplied cert="high" reason="lost">ι</supplied>ᾶν 

    <lb n="8"/>ἅμα μηδὲ τὰς <choice><corr>νύκτας</corr><sic>νυκκτ<supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">ς</unclear></sic></choice> κοιμώμενον <choice><corr>ἄχρις</corr><sic>αχρε<supplied cert="high" reason="lost">ι</supplied>ς</sic></choice> οὗ 

    <lb n="9"/>μοι δηλώσῃς πῶς δι<supplied cert="high" reason="lost">ά</supplied>γει ἐν τούτῳ τῷ ἀέρι. <unclear cert="high" reason="undefined">μ</unclear>ὴ λη-

    <lb n="10"/>ρήσεις ἄχρι οὗ εὕρῃς τὸ<unclear cert="high" reason="undefined">ν</unclear> πρὸς ἐμὲ καταπλέοντα. 

    <lb n="11"/>παρηγόρ<unclear cert="high" reason="undefined">η</unclear>σον τὸν ἀ<unclear cert="high" reason="undefined">δ</unclear>ελφὸν Μ<unclear cert="high" reason="undefined">ά</unclear>ξιμον ὡς ἐὰν δύ-

    <lb n="12"/>νῃ. ἔγραψ<supplied cert="high" reason="lost">α</supplied> <unclear cert="high" reason="undefined">κ</unclear>αὶ Οὐαλερίῳ τῷ ἀδελφῷ περὶ τούτου. ἐλπί-

    <lb n="13"/>ζω δ<unclear cert="high" reason="undefined">ὲ</unclear> <supplied cert="high" reason="lost">ὡς</supplied> καὶ <choice><corr>αὐτὸς</corr><sic>καυτος</sic></choice> οὐ<unclear cert="high" reason="undefined">κ</unclear> <unclear cert="high" reason="undefined">ἀ</unclear>γνοεῖ π<unclear cert="high" reason="undefined">ῶς</unclear> χαλεπῶ<unclear cert="high" reason="undefined">ς</unclear> 

    <lb n="14"/>περὶ <gap extent="4" reason="lost" unit="character"/>ς <choice><corr>διάγομεν</corr><sic>διαγωμ<unclear cert="high" reason="undefined">ε</unclear>ν</sic></choice> ἀλλὰ τί ἐ<unclear cert="high" reason="undefined">κ</unclear> τούτου ὠφε-

    <lb n="15"/><choice><corr>λῆσαι</corr><sic>ω<unclear cert="high" reason="undefined">φε</unclear>λησε</sic></choice> <unclear cert="high" reason="undefined">δυν</unclear>ηθησόμεθ<unclear cert="high" reason="undefined">α</unclear>; οὐ γὰρ μόνος αὐτῆς ἐ<unclear cert="high" reason="undefined">σ</unclear>-

    <lb n="16"/>τερή<supplied cert="high" reason="lost">θη ἀλ</supplied>λὰ καὶ πάντες ἡμεῖς. <choice><corr>δι<supplied cert="high" reason="omitted">ὸ</supplied></corr><sic>δι</sic></choice> ο<unclear cert="high" reason="undefined">ὖ</unclear>ν ὡς μη-

    <lb n="17"/>δὲν δυ<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">αμέ</supplied>νας ποίησαι <supplied cert="high" reason="lost">ἀ</supplied>λλ᾽ οὐ<unclear cert="high" reason="undefined">δ</unclear>ὲ <supplied cert="high" reason="lost">ὀ</supplied><unclear cert="high" reason="undefined">φ</unclear>έ-

    <lb n="18"/>λῃς τὰ <unclear cert="high" reason="undefined">ἀν</unclear><supplied cert="high" reason="lost">θ</supplied>ρ<supplied cert="high" reason="lost">ώ</supplied>πινα φρονεῖν <choice><corr>ἐξαιρέτως</corr><sic>ε<unclear cert="high" reason="undefined">ξ</unclear>ερετως</sic></choice> 

    <lb n="19"/>ἐν <choice><corr>τοιούτῳ</corr><sic>τυουτω</sic></choice> <choice><corr>καιρῷ</corr><sic>κερω</sic></choice>. <choice><corr>ἔρ<supplied cert="high" reason="omitted">ρ</supplied>ωσό</corr><sic>ερω<unclear cert="high" reason="undefined">σ</unclear>ο</sic></choice> μοι. 
</ab></div><div n="r,3" part="N" type="textpart"><ab part="N">

    <lb n="20"/>Σεμ<unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ρώνιος</supplied>        <unclear cert="high" reason="undefined">Σατ</unclear><supplied cert="high" reason="lost">ο</supplied>ρ<supplied cert="high" reason="lost">νίλᾳ</supplied> 

    <lb n="21"/>τῇ <choice><corr>μητρὶ</corr><sic><unclear cert="high" reason="undefined">μ</unclear>ητ<supplied cert="high" reason="lost">ρ</supplied><unclear cert="high" reason="undefined">ει</unclear></sic></choice>     χα<unclear cert="high" reason="undefined">ί</unclear><supplied cert="high" reason="lost">ρ</supplied><unclear cert="high" reason="undefined">ε</unclear>ιν. 

    <lb n="22"/><choice><corr>λαβοῦσα</corr><sic>λαβω<unclear cert="high" reason="undefined">ν</unclear></sic></choice> <unclear cert="high" reason="undefined">ταῦ</unclear>τα γρά<supplied cert="high" reason="lost">μ</supplied>ματα 

    <lb n="23"/>αὐτῆ<unclear cert="high" reason="undefined">ς</unclear> ὥρας δήλω<supplied cert="high" reason="lost">σ</supplied>όν μοι 

    <lb n="24"/>πῶς δι<unclear cert="high" reason="undefined">ά</unclear><supplied cert="high" reason="lost">γ</supplied>εις, οὐκ ὀλί<unclear cert="high" reason="undefined">γως</unclear> <unclear cert="high" reason="undefined">γὰρ</unclear> 

    <lb n="25"/>ἀγωνι<unclear cert="high" reason="undefined">ῶ</unclear> <choice><corr>μέχρι</corr><sic><unclear cert="high" reason="undefined">μ</unclear>εχρει</sic></choice> <unclear cert="high" reason="undefined">οὗ</unclear> μάθω 

    <lb n="26"/>τὰ περὶ σοῦ. περὶ γ<unclear cert="high" reason="undefined">ὰρ</unclear> <unclear cert="high" reason="undefined">τ</unclear>οῦ ἀ-

    <lb n="27"/>δελφοῦ <supplied cert="high" reason="lost">μ</supplied>ου Μαξί<unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ου</supplied> <unclear cert="high" reason="undefined">νῦν</unclear> 

    <lb n="28"/>σοι γράψ<supplied cert="high" reason="lost">ω</supplied> ὅπως α<unclear cert="high" reason="undefined">ὐ</unclear><supplied cert="high" reason="lost">τ</supplied>ὸ<unclear cert="high" reason="undefined">ν</unclear> πα-

    <lb n="29"/>ρηγορή<supplied cert="high" reason="lost">σ</supplied>ῃς. οὐκ ι<add place="supralinear"><gap extent="1" reason="illegible" unit="character"/></add>δ<gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> ἐ<unclear cert="high" reason="undefined">κ</unclear> πε-

    <lb n="30"/><choice><corr>ρισσοῦ</corr><sic>περεισσου</sic></choice> <gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ω</unclear>κω γάρ, <unclear cert="high" reason="undefined">φ</unclear><supplied cert="high" reason="lost">η</supplied><unclear cert="high" reason="undefined">σ</unclear>ιν, αὐ-

    <lb n="31"/>τὰ τὰ υ<unclear cert="high" reason="undefined">δ</unclear><gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">α</unclear> ἀλλὰ ἡ <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ῦ</unclear> <choice><corr>καιροῦ</corr><sic>κερου</sic></choice> 

    <lb n="32"/><choice><corr>ἀκαιρία</corr><sic>ακερια</sic></choice> οὐκ ἐᾷ με <gap extent="1" reason="illegible" unit="character"/>ι<gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ε</unclear>νό-

    <lb n="33"/>μενος μὴ ἐκ τῆς λ<unclear cert="high" reason="undefined">ύπ</unclear>ης 

    <lb n="34"/>εἰς ἄλλο τι καὶ <choice><corr>αὐτὸς</corr><sic>καυτος</sic></choice> <choice><corr><supplied cert="high" reason="lost">ἀπ</supplied>οτραπῇ</corr><sic><supplied cert="high" reason="lost">απ</supplied><unclear cert="high" reason="undefined">ο</unclear>τ<add place="supralinear">ρ</add>α<del rend="erasure">ρ</del>πη</sic></choice>. 

    <lb n="35"/>οἶδα γὰρ ὅτι καὶ σὺ αὐτὴ ὑπὲρ 

    <lb n="36"/><choice><corr>αὐτοῦ</corr><sic>αυτον</sic></choice> λυπῇ ἀλλά, <unclear cert="high" reason="undefined">δέ</unclear>σπ<supplied cert="high" reason="lost">οι</supplied><unclear cert="high" reason="undefined">να</unclear>, 

    <lb n="37"/>νῆψον διὰ τὰ ἀδέλφιά μου 

    <lb n="38"/>καὶ <choice><corr>το<supplied cert="high" reason="omitted">ῦ</supplied>το</corr><sic>τοτο</sic></choice> <choice><corr>ἔδει</corr><sic>εδι</sic></choice> ὅτι ἴσως π<supplied cert="high" reason="lost">εφ</supplied><unclear cert="high" reason="undefined">ίλ</unclear>η-

    <lb n="39"/>κε <supplied cert="high" reason="lost">τὰ</supplied>ς θυγατέρ<supplied cert="high" reason="lost">ας</supplied> καὶ <gap extent="3" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/> 

    <lb n="40"/>ἀδελφὴν Σιβό<supplied cert="high" reason="lost">ρ</supplied><unclear cert="high" reason="undefined">ραν</unclear> <supplied cert="high" reason="lost">ἀ</supplied>λλὰ 

    <lb n="41"/>τί δυνάμεθα πρὸ<unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">ὃ</unclear> <supplied cert="high" reason="lost">ὁ θεὸς</supplied> ο<supplied cert="high" reason="lost">ὐ</supplied> 

    <lb n="42"/>δύναται; ἔρρωσό μοι. 
</ab></div><div n="v" part="N" type="textpart"><ab part="N">

    <lb n="43"/><expan><supplied cert="high" reason="lost">ἀπ</supplied><unclear cert="high" reason="undefined">όδ</unclear><ex cert="high">ος</ex></expan> <supplied cert="high" reason="lost">Ο</supplied><unclear cert="high" reason="undefined">ὐ</unclear>α<unclear cert="high" reason="undefined">λε</unclear>ρί<unclear cert="high" reason="undefined">ωι</unclear> <unclear cert="high" reason="undefined">ἀδε</unclear><supplied cert="high" reason="lost">λφῶ</supplied><unclear cert="high" reason="undefined">ι</unclear> <supplied cert="high" reason="lost">καὶ Σατορ</supplied>νίλωι ἀδελφῶι <expan>π<ex cert="high">αρὰ</ex></expan> <unclear cert="high" reason="undefined">Σεμπ</unclear><supplied cert="high" reason="lost">ρωνίου </supplied><gap extent="1" reason="lost" unit="character"/> </ab></div></div><div n="meta.26689" part="N" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.26689" part="N" subtype="textDate" type="commentary"><p><date notAfter="200" notBefore="176" type="textDate">spätes II</date></p></div><div n="meta.26689" part="N" subtype="general" type="commentary"><p>Neuedition: Chapa, Letters of Condolence, 4. Gehört zum Happy - Family - Archiv.</p></div><div n="meta.26689" part="N" subtype="principalEdition" type="bibliography"><listBibl><bibl subtype="principal" type="publication"><title level="s" type="abbreviated">P.Wisc.</title><biblScope type="volume">2</biblScope><biblScope type="numbers">84</biblScope></bibl><bibl type="Trismegistos"><biblScope type="numbers">26689</biblScope></bibl><bibl type="DDbDP"><series>0226</series>; <biblScope type="volume">2</biblScope>; <biblScope type="numbers">84</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0226;2;84" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.26689" part="N" subtype="corrections" type="bibliography"><head>BL-Einträge nach BL-Konkordanz</head><listBibl><bibl type="BL"><biblScope type="volume