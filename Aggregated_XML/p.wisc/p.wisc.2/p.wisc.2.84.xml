    <lb n="1"/><supplied reason="lost" cert="high">Σεμπ</supplied><unclear reason="undefined" cert="high">ρώ</unclear>νιος   Σα<unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ορνί</supplied>λῳ τῷ ἀδελ<unclear reason="undefined" cert="high">φ</unclear>ῷ 

    <lb n="2"/>χαίρειν. 

    <lb n="3"/><choice><corr>ἐκομισάμην</corr><sic><unclear reason="undefined" cert="high">ε</unclear>κομεισαμην</sic></choice> σου δύο <num value="2"/> <choice><corr>ἐπιστολάς</corr><sic><supplied reason="lost" cert="high">ε</supplied><unclear reason="undefined" cert="high">π</unclear>ειστολας</sic></choice>, <choice><corr>μίαν</corr><sic>μειαν</sic></choice> μὲν πε-

    <lb n="4"/><unclear reason="undefined" cert="high">ρ</unclear>ὶ ὧν ἐδήλωσα τῷ ἀ<supplied reason="lost" cert="high">δ</supplied>ελφῷ Μαξίμῳ, ἑτέραν δὲ 

    <lb n="5"/>περὶ τῆς κυρίας ἡμῶ<unclear reason="undefined" cert="high">ν</unclear> μητρὸς ὡς κινδυνευσά-

    <lb n="6"/><choice><corr>σης</corr><sic>κεινδυνευ<supplied reason="lost" cert="high">ο</supplied>υσασης</sic></choice> αὐτῆς καὶ ἀκμὴν <choice><corr>διακατεχομένης</corr><sic><unclear reason="undefined" cert="high">δ</unclear>ειακατεχομενη<add place="supralinear">ς</add></sic></choice> τῇ ν<unclear reason="undefined" cert="high">ω</unclear>θρί-

    <lb n="7"/>ᾳ. <choice><corr>γίγνωσκε</corr><sic>γεινωσκε</sic></choice> οὖν, ἀδ<unclear reason="undefined" cert="high">ε</unclear><supplied reason="lost" cert="high">λ</supplied>φέ, ἱκανῶς με ἀγων<supplied reason="lost" cert="high">ι</supplied>ᾶν 

    <lb n="8"/>ἅμα μηδὲ τὰς <choice><corr>νύκτας</corr><sic>νυκκτ<supplied reason="lost" cert="high">α</supplied><unclear reason="undefined" cert="high">ς</unclear></sic></choice> κοιμώμενον <choice><corr>ἄχρις</corr><sic>αχρε<supplied reason="lost" cert="high">ι</supplied>ς</sic></choice> οὗ 

    <lb n="9"/>μοι δηλώσῃς πῶς δι<supplied reason="lost" cert="high">ά</supplied>γει ἐν τούτῳ τῷ ἀέρι. <unclear reason="undefined" cert="high">μ</unclear>ὴ λη-

    <lb n="10"/>ρήσεις ἄχρι οὗ εὕρῃς τὸ<unclear reason="undefined" cert="high">ν</unclear> πρὸς ἐμὲ καταπλέοντα. 

    <lb n="11"/>παρηγόρ<unclear reason="undefined" cert="high">η</unclear>σον τὸν ἀ<unclear reason="undefined" cert="high">δ</unclear>ελφὸν Μ<unclear reason="undefined" cert="high">ά</unclear>ξιμον ὡς ἐὰν δύ-

    <lb n="12"/>νῃ. ἔγραψ<supplied reason="lost" cert="high">α</supplied> <unclear reason="undefined" cert="high">κ</unclear>αὶ Οὐαλερίῳ τῷ ἀδελφῷ περὶ τούτου. ἐλπί-

    <lb n="13"/>ζω δ<unclear reason="undefined" cert="high">ὲ</unclear> <supplied reason="lost" cert="high">ὡς</supplied> καὶ <choice><corr>αὐτὸς</corr><sic>καυτος</sic></choice> οὐ<unclear reason="undefined" cert="high">κ</unclear> γνοεῖ π<unclear reason="undefined" cert="high">ῶς</unclear> χαλεπῶ<unclear reason="undefined" cert="high">ς</unclear> 

    <lb n="14"/>περὶ <gap reason="lost" extent="4" unit="character"/>ς <choice><corr>διάγομεν</corr><sic>διαγωμ<unclear reason="undefined" cert="high">ε</unclear>ν</sic></choice> ἀλλὰ τί ἐ<unclear reason="undefined" cert="high">κ</unclear> τούτου ὠφε-

    <lb n="15"/><choice><corr>λῆσαι</corr><sic>ω<unclear reason="undefined" cert="high">φε</unclear>λησε</sic></choice> <unclear reason="undefined" cert="high">δυν</unclear>ηθησόμεθ<unclear reason="undefined" cert="high">α</unclear>; οὐ γὰρ μόνος αὐτῆς ἐ<unclear reason="undefined" cert="high">σ</unclear>-

    <lb n="16"/>τερή<supplied reason="lost" cert="high">θη ἀλ</supplied>λὰ καὶ πάντες ἡμεῖς. <choice><corr>δι<supplied reason="omitted" cert="high">ὸ</supplied></corr><sic>δι</sic></choice> ο<unclear reason="undefined" cert="high">ὖ</unclear>ν ὡς μη-

    <lb n="17"/>δὲν δυ<unclear reason="undefined" cert="high">ν</unclear><supplied reason="lost" cert="high">αμέ</supplied>νας ποίησαι <supplied reason="lost" cert="high">ἀ</supplied>λλ᾽ οὐ<unclear reason="undefined" cert="high">δ</unclear>ὲ <supplied reason="lost" cert="high">ὀ</supplied><unclear reason="undefined" cert="high">φ</unclear>έ-

    <lb n="18"/>λῃς τὰ <unclear reason="undefined" cert="high">ἀν</unclear><supplied reason="lost" cert="high">θ</supplied>ρ<supplied reason="lost" cert="high">ώ</supplied>πινα φρονεῖν <choice><corr>ἐξαιρέτως</corr><sic>ε<unclear reason="undefined" cert="high">ξ</unclear>ερετως</sic></choice> 

    <lb n="19"/>ἐν <choice><corr>τοιούτῳ</corr><sic>τυουτω</sic></choice> <choice><corr>καιρῷ</corr><sic>κερω</sic></choice>. <choice><corr>ἔρ<supplied reason="omitted" cert="high">ρ</supplied>ωσό</corr><sic>ερω<unclear reason="undefined" cert="high">σ</unclear>ο</sic></choice> μοι. 
</ab></div><div type="textpart" n="r,3" org="uniform" part="N" sample="complete"><ab part="N">

