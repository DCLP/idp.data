    <lb n="1"/>Αἰλίῳ Ἡρακλείτῳ βοηθῷ Αἰλίου Ἐγλέκτου 

    <lb n="2"/>ἐπιτρόπου το<unclear reason="undefined" cert="high">ῦ</unclear> κυρίου Καίσαρος 

    <lb n="3"/><unclear reason="undefined" cert="high">πα</unclear>ρὰ Πτολεμαίου Διοδώρου τοῦ καὶ Διοσκόρου 

    <lb n="4"/><unclear reason="undefined" cert="high">μι</unclear>σθωτοῦ δρυμοῦ κώμης Θεαδελφείας. τ<unclear reason="undefined" cert="high">ῶ</unclear>ν 

    <lb n="5"/>σ<unclear reason="undefined" cert="high">υ</unclear>ν<unclear reason="undefined" cert="high">ή</unclear>θων ὑδάτων δεόντων κατελθεῖν 

    <lb n="6"/>ε<unclear reason="undefined" cert="high">ἰ</unclear>ς τὸν προκιμένον δρυμὸν ἕως πλήσθῃ, μ<unclear reason="undefined" cert="high">έ</unclear>χρι 

    <lb n="7"/>νῦν οὐκ <subst><add place="inline">ἔσχεν</add><del rend="corrected" status="unremarkable">σσχεν</del></subst>, ἀλλὰ κινδυνεύει ἀποξηρανθῆ-

    <lb n="8"/><unclear reason="undefined" cert="high">ν</unclear><supplied reason="lost" cert="high">α</supplied>ι, <subst><add place="inline">ὅθεν</add><del rend="corrected" status="unremarkable">ττεν</del></subst> ἀξιῶ σε ἐπιστεῖλαι τῷ αἰγιαλοφύλακι 

    <lb n="9"/><unclear reason="undefined" cert="high">γ</unclear><supplied reason="lost" cert="high">ράμ</supplied><unclear reason="undefined" cert="high">μ</unclear>ατα <sic>σε</sic>, ὅτι, ἐὰν <choice><corr>ἀμέλειά</corr><sic>αμελια</sic></choice> τις γένηται τῶν ὑδά-

    <lb n="10"/><supplied reason="lost" cert="high">τ</supplied><unclear reason="undefined" cert="high">ω</unclear>ν καὶ <unclear reason="undefined" cert="high">μ</unclear>ὴ πλ<unclear reason="undefined" cert="high">ή</unclear>σθῃ καθὼς ἔθος ἐστί, οὐ μό-

    <lb n="11"/><supplied reason="lost" cert="high">ν</supplied><unclear reason="undefined" cert="high">ο</unclear>ν τῷ ἑξῆς <choice><corr>ἔτει</corr><sic>ετι</sic></choice> βλάβος <choice><corr>ἐπακολουθεῖ</corr><sic>επακολου<unclear reason="undefined" cert="high">θ</unclear>ι</sic></choice>, τοῦ <unclear reason="undefined" cert="high">γὰρ</unclear> 

    <lb n="12"/><supplied reason="lost" cert="high">δρυμοῦ μισ</supplied>θωθέντος <unclear reason="undefined" cert="high">ἀ</unclear>λλὰ ἔτη τρ<unclear reason="undefined" cert="high">ί</unclear>α <num value="3"/> ἄφορος 

    <lb n="13"/><supplied reason="lost" cert="high">γίνεται</supplied>. <unclear reason="undefined" cert="high">ἀ</unclear>ξιῶ οὖν, κύ<unclear reason="undefined" cert="high">ρ</unclear><supplied reason="lost" cert="high">ι</supplied><unclear reason="undefined" cert="high">ε</unclear>, φανερὸν <supplied reason="lost" cert="high">τ</supplied>ῷ αἰγιαλοφύλα-

    <lb n="14"/><supplied reason="lost" cert="high">κι ποιῆσα</supplied><unclear reason="undefined" cert="high">ι</unclear>, ἵν᾽ ἐὰν ἕν<unclear reason="undefined" cert="high">ε</unclear><supplied reason="lost" cert="high">κ</supplied><unclear reason="undefined" cert="high">α</unclear> <choice><corr>ἀμελείας</corr><sic>αμελιας</sic></choice> β<unclear reason="undefined" cert="high">λ</unclear>άβος τι γένηται, 

    <lb n="15"/><gap reason="lost" extent="7" unit="character"/><supplied reason="lost" cert="high">  μ</supplied><unclear reason="undefined" cert="high">ὲ</unclear> πρὸς αὐτ<unclear reason="undefined" cert="high">ὸ</unclear>ν <choice><corr><supplied reason="lost" cert="high">ἔχει</supplied>ν</corr><sic><supplied reason="lost" cert="high">εχι</supplied>ν</sic></choice> λόγον ἐπ<unclear reason="undefined" cert="high">ὶ</unclear> τοῦ κρατίσ-

    <lb n="16"/><supplied reason="lost" cert="high">τοῦ ἐπ</supplied><unclear reason="undefined" cert="high">ι</unclear>τρόπου. <expan><ex cert="high">ἔτους</ex></expan> <num value="8">η</num> Ἁθὺρ <num value="7">ζ</num>. 

    <lb n="17"/><supplied reason="lost" cert="high"><handShift new="m2"/> </supplied><gap reason="lost" extent="unknown" unit="character"/>. Αἴλιος <expan>Ἡ<unclear reason="undefined" cert="high">ρ</unclear>ά<hi rend="superscript">κ</hi><ex cert="high">λειτος</ex></expan> <unclear reason="undefined" cert="high">Ἀπ</unclear>ολλωνίῳ <expan>αἰγιαλοφύλα<hi rend="superscript">κ</hi><ex cert="high">ι</ex></expan> 

    <lb n="18"/><supplied reason="lost" cert="high">τῷ</supplied> <unclear reason="undefined" cert="high">φ</unclear>ιλτάτῳ <unclear reason="undefined" cert="high">χ</unclear><supplied reason="lost" cert="high">αίρει</supplied><unclear reason="undefined" cert="high">ν</unclear>. <unclear reason="undefined" cert="high">ο</unclear>ὐ προσεδόκησα καὶ περὶ <unclear reason="undefined" cert="high">τοῦ</unclear> 

    <lb n="19"/><supplied reason="lost" cert="high">δρυ</supplied><unclear reason="undefined" cert="high">μ</unclear>οῦ <choice><corr>ἀξιωθήσεσθαι</corr><sic>αξι<unclear reason="undefined" cert="high">ω</unclear>ιθησ<unclear reason="undefined" cert="high">ε</unclear>σθαι</sic></choice>, ἵνα παράσχῃ αὐτῷ 

    <lb n="20"/><supplied reason="lost" cert="high">τὰ α</supplied><unclear reason="undefined" cert="high">ὐ</unclear>τάρ<unclear reason="undefined" cert="high">κ</unclear>η <unclear reason="undefined" cert="high">ὕ</unclear>δατ<unclear reason="undefined" cert="high">α</unclear> ὡς ὁ <unclear reason="undefined" cert="high">μισ</unclear>θωτὴς α<unclear reason="undefined" cert="high">ὐτ</unclear><supplied reason="lost" cert="high">οῦ</supplied> 

    <lb n="21"/><supplied reason="lost" cert="high">Πτολε</supplied>μαῖος ἠξίωσ<unclear reason="undefined" cert="high">ε</unclear>ν. ἀκόλουθο<unclear reason="undefined" cert="high">ν</unclear> <unclear reason="undefined" cert="high">γ</unclear>ὰρ ἦν 

    <lb n="22"/><supplied reason="lost" cert="high">τῇ ἐ</supplied><unclear reason="undefined" cert="high">π</unclear>ιμελ<unclear reason="undefined" cert="high">εί</unclear>ᾳ <unclear reason="undefined" cert="high">σ</unclear>ου <supplied reason="lost" cert="high">κ</supplied><unclear reason="undefined" cert="high">α</unclear>ὶ τούτου πρὶν ἀ<unclear reason="undefined" cert="high">ξ</unclear><supplied reason="lost" cert="high">ι</supplied><unclear reason="undefined" cert="high">ῶσ</unclear>αί τινα 

    <lb n="23"/><supplied reason="lost" cert="high">φρον</supplied><unclear reason="undefined" cert="high">τί</unclear>σαι. εἰ δέ <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ι μ</supplied><unclear reason="undefined" cert="high">έ</unclear>χ<supplied reason="lost" cert="high">ρι</supplied> νῦν ἐμποδὼν ἐγένετο 

    <lb n="24"/><supplied reason="lost" cert="high">ἢ ἄλλ</supplied><unclear reason="undefined" cert="high">ο</unclear> <unclear reason="undefined" cert="high">τι</unclear> τῶν μᾶλλ<supplied reason="lost" cert="high">ό</supplied>ν <unclear reason="undefined" cert="high">σο</unclear>ι δοκούντων ἐπείγειν, ὑπέρ-

    <lb n="25"/><supplied reason="lost" cert="high">θεσι</supplied><unclear reason="undefined" cert="high">ν</unclear> <unclear reason="undefined" cert="high">τ</unclear>ῇ πλη<unclear reason="undefined" cert="high">ρώ</unclear>σει <unclear reason="undefined" cert="high">το</unclear>ῦ δρυμοῦ παρέσχεν ἀλλὰ 

    <lb n="26"/><supplied reason="lost" cert="high">καὶ νῦν</supplied>, <unclear reason="undefined" cert="high">ἀ</unclear>δελφέ, φρόντισον, ὥστε τὰ συνήθη ὕδατα 

    <lb n="27"/><supplied reason="lost" cert="high">αὐτῷ</supplied> παρασχεθῆναι. ἐρρῶσθαί σε <expan>εὔχο<ex cert="high">μαι</ex></expan> φίλτατ<unclear reason="undefined" cert="high">ε</unclear>. 

    <lb n="28"/><supplied reason="lost" cert="high"><expan><ex cert="high">ἔτους</ex></expan></supplied> <num value="8"><unclear reason="undefined" cert="high">η</unclear></num> <expan>Ἀντω<ex cert="high">νίνου</ex></expan> <unclear reason="undefined" cert="high">Ἁθ</unclear>ὺρ <num value="7">ζ</num>. </ab></div><div n="meta.13710" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.13710" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" value="0144-11-03" notBefore="144" notAfter="144">3. Nov. 144</date></p></div><div n="meta.13710" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Kopie: P.Wisc. I  35.</p></div><div n="meta.13710" type="commentary" subtype="mentionedDates" org="uniform" part="N" sample="complete"><head>Erwähnte Daten</head><p>Z. 1-16: <date type="mentioned" value="0144-11-03">3. Nov. 144</date></p></div><div n="meta.13710" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">P.Wisc.</title><biblScope type="volume">1</biblScope><biblScope type="numbers">34</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">13710</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0226</series>; <biblScope type="volume">1</biblScope>; <biblScope type="numbers">34dupl</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0226;1;34dupl" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.13710" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p><bibl type="illustration" default="NO">Plate XIII</bibl><bibl type="illustration" default="NO">Boswinkel - Sijpesteijn, Greek Papyri, 20</bibl></p></div><div n="meta.13710" type="hi