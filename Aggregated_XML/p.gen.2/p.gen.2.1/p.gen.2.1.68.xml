    <lb n="1"/>προσεφ<unclear reason="undefined" cert="high">ώ</unclear><supplied reason="lost" cert="high">ν</supplied>ησεν καὶ <choice><corr>ὡμολόγησεν</corr><sic>ομολογησεν</sic></choice> Αὐ<supplied reason="lost" cert="high">ρή</supplied>λιος Περγά<unclear reason="undefined" cert="high">μ</unclear>ιος Κυρίλλο<unclear reason="undefined" cert="high">υ</unclear>

    <lb n="2"/>ἀπὸ κώ<unclear reason="undefined" cert="high">μη</unclear>ς Φιλ<supplied reason="lost" cert="high">αδελ</supplied>φίας <unclear reason="undefined" cert="high">τ</unclear>οῦ Ἀ<unclear reason="undefined" cert="high">ρσ</unclear>ινοιτ<supplied reason="lost" cert="high">ο</supplied>υ νομοῦ. ὁμολογῶ

    <lb n="3"/>ὁ <choice><corr>προκείμενος</corr><sic>προκιμ<unclear reason="undefined" cert="high">εν</unclear><supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">ς</unclear></sic></choice> Περγάμ<supplied reason="lost" cert="high">ιο</supplied>ς· <choice><corr>ἐδεξάμην</corr><sic>ε<supplied reason="lost" cert="high">δ</supplied>αιξαμην</sic></choice> παρὰ <choice><corr>τοῦ</corr><sic>τω</sic></choice> <del rend="erasure" status="unremarkable">α</del> <choice><corr>πριαμένου</corr><sic>πριαμενω</sic></choice>

    <lb n="4"/>Ὀλ <expan><unclear reason="undefined" cert="high">Κού</unclear>[<ex cert="high">ειήους</ex></expan>] ἀπὸ τῆ<unclear reason="undefined" cert="high">ς</unclear> αὐτῆ<unclear reason="undefined" cert="high">ς</unclear> κώμης τὸ μέρος Κυρίλλου

    <lb n="5"/>τοῦ ἡ<unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">ετέρ</supplied>ου ἀδελφοῦ Ὀλ πάντα τὰ <choice><corr>ὑποτείνοντα αὐτῷ</corr><sic>συντινοντα αυτου</sic></choice>

    <lb n="6"/>ἐκ τοῦ <unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ατρ</supplied>ός· πρόβατ<unclear reason="undefined" cert="high">α</unclear> ὀνύχινα <choice><corr>τέλεια τεσσαράκοντα</corr><sic>τελια τεσσερακοντα</sic></choice> δύο,

    <lb n="7"/><expan>πρόβ<ex cert="high">ατα</ex></expan> <num value="42">μ<supplied reason="lost" cert="high">β</supplied></num>, ἀρνία ἕνδεκα <sic><add place="supralinear">ἕνα</add></sic>, <expan>ἀρ<ex cert="high">νία</ex></expan> <num value="11"><unclear reason="undefined" cert="high">ι</unclear>α</num>, <choice><corr>αἶγας τέσσαρας</corr><sic>εγας τεσσαρες</sic></choice>, <choice><corr>αἶγας</corr><sic>εγ</sic></choice> <num value="4">δ</num>,

    <lb n="8"/>χίμαρον <unclear reason="undefined" cert="high">ἕν</unclear>α <expan><unclear reason="undefined" cert="high">χ</unclear><ex cert="high">ίμαρον</ex></expan> <num value="1">α</num>, μικρὰ <unclear reason="undefined" cert="high">δ</unclear>[ύο, <expan>μ<ex cert="high">ικρὰ</ex></expan>] <num value="2">β</num>, χρυσίου ὁλοκοττίνους

    <lb n="9"/>ὀκτώ, <expan>χρ<ex cert="high">υσίου</ex></expan> <expan>ν<unclear reason="undefined" cert="high">ο</unclear>μ<ex cert="high">ίσματα</ex></expan> <num value="8">η</num>, σάκ<supplied reason="lost" cert="high">κον</supplied> κοῦφον <choice><corr>ἕνα</corr><sic>εναν</sic></choice> <num value="1"/>, δίφρον <choice><corr>ἕνα</corr><sic>εναν</sic></choice> <num value="1"/>

    <lb n="10"/>καὶ <choice><corr>τὸ συντεῖνον</corr><sic>τα <unclear reason="undefined" cert="high">συ</unclear><supplied reason="lost" cert="high">ντ</supplied><unclear reason="undefined" cert="high">ι</unclear>νον</sic></choice> α<unclear reason="undefined" cert="high">ὐ</unclear>τοῦ <choice><corr>ἑρματικὸν κράββατον</corr><sic>ερματι<unclear reason="undefined" cert="high">κ</unclear>ων χραβακτων</sic></choice> ἕν <num value="1"/>,

    <lb n="11"/>μέγαν δίφρον, ἐπὶ παρουσίᾳ <unclear reason="undefined" cert="high">Διδ</unclear>ύμου καὶ <choice><corr>Παύλου</corr><sic>παυλω</sic></choice> πρεσβυτέρου

    <lb n="12"/>τῆς α<unclear reason="undefined" cert="high">ὐτ</unclear>ῆς κώμης, καὶ οὐδένα λόγον ἔχω πρὸς τ<unclear reason="undefined" cert="high">ὸν</unclear>

    <lb n="13"/>Αὐρήλι<supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">ν</unclear> Ὀλ, καὶ <expan>ἐπερ<ex cert="high">ωτηθεὶς</ex></expan> ὡμολό<unclear reason="undefined" cert="high">γ</unclear>ησα. Αὐρήλιος Περγάμιος

    <lb n="14"/>ὁ <choice><corr>προκείμενος</corr><sic>προκιμενος</sic></choice> ἐδεξάμην τὸ μέρος Κυ<unclear reason="undefined" cert="high">ρ</unclear>ίλλου παρὰ <choice><corr>τοῦ Αὐρηλίου</corr><sic>τω αυρηλιω</sic></choice>

    <lb n="15"/>Ὀλ πάντα τὰ <choice><corr>συντείνοντα</corr><sic>συντι<unclear reason="undefined" cert="high">ν</unclear><supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">ν</unclear>τα</sic></choice> α<unclear reason="undefined" cert="high">ὐ</unclear><supplied reason="lost" cert="high">τ</supplied><unclear reason="undefined" cert="high">ῷ</unclear>, εἴτ<supplied reason="lost" cert="high">ε </supplied><choice><corr><supplied reason="lost" cert="high">π</supplied>ρόβατον</corr><sic><supplied reason="lost" cert="high">π</supplied>ροβ<supplied reason="lost" cert="high">ατ</supplied>ων</sic></choice>, εἴτε χ<unclear reason="undefined" cert="high">ρ</unclear><supplied reason="lost" cert="high">υ</supplied>σίον,

    <lb n="16"/><choice><corr>εἴτε ἱματισμόν</corr><sic>ειται ει<supplied reason="lost" cert="high">ματι</supplied><unclear reason="undefined" cert="high">σ</unclear>μον</sic></choice>, <choice><corr>εἴτε</corr><sic>ειται</sic></choice> χα<supplied reason="lost" cert="high">μαι</supplied><unclear reason="undefined" cert="high">ε</unclear>ύν<unclear reason="undefined" cert="high">η</unclear><supplied reason="lost" cert="high">ν</supplied>, καὶ οὐδένα λ<supplied reason="lost" cert="high">ό</supplied>γον ἔχω πρός <choice><corr>σε</corr><sic>σαι</sic></choice>

    <lb n="17"/><gap reason="lost" extent="6" unit="character"/><unclear reason="undefined" cert="high">υ</unclear>. Αὐρήλι<supplied reason="lost" cert="high">ο</supplied>ς Κλαμάσιο<supplied reason="lost" cert="high">ς το</supplied><unclear reason="undefined" cert="high">ῦ</unclear> Σαραπάμμω<unclear reason="undefined" cert="high">ν</unclear><supplied reason="lost" cert="high">ο</supplied>ς ἔγραψα <unclear reason="undefined" cert="high">ὑ</unclear><supplied reason="lost" cert="high">π</supplied>ὲρ αὐ-

    <lb n="18"/><supplied reason="lost" cert="high">τῶν<expan>ἀγραμμ<ex cert="high">άτων</ex></expan>τ</supplied>ὸ <choice><corr>σωμάτιον</corr><sic>σοματιον</sic></choice> γρ<supplied reason="lost" cert="high">άψα</supplied>ς. Α<unclear reason="undefined" cert="high">ὐρ</unclear><supplied reason="lost" cert="high">ήλ</supplied>ι<unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">ς Κλα</supplied>μάσιο<supplied reason="lost" cert="high">ς</supplied> <unclear reason="undefined" cert="high">ἔ</unclear><supplied reason="lost" cert="high">γ</supplied>ραψα ὑπὲρ <unclear reason="undefined" cert="high">αὐ</unclear>-

    <lb n="19"/><supplied reason="lost" cert="high">τῶν ἀγραμμ</supplied>άτων καὶ <unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">αρτ</supplied><unclear reason="undefined" cert="high">υ</unclear>ρῶ ὡς <unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ρόκ</supplied>ειται.

    <lb n="20"/><supplied reason="lost" cert="high">μετὰ τ</supplied>ὴν <choice><corr>ὑπατείαν Εὐχαιρίου</corr><sic>υπατιαν <supplied reason="lost" cert="high">ε</supplied><unclear reason="undefined" cert="high">υ</unclear>χειριου</sic></choice> καὶ <choice><corr>Συαγρίου</corr><sic>σοι<supplied reason="lost" cert="high">αγ</supplied><unclear reason="undefined" cert="high">ρ</unclear>ιου</sic></choice> τῶν λα<supplied reason="lost" cert="high">μ</supplied>προτάτω<unclear reason="undefined" cert="high">ν</unclear>

    <lb n="21"/><gap reason="lost" extent="9" unit="character"/> Παχὼν <num value="13">ιγ</num> τῆς <num value="11">ια</num> <expan>ἰν<unclear reason="undefined" cert="high">δ</unclear><ex cert="high">ικτίωνος</ex></expan> ἐν Φιλα<supplied reason="lost" cert="high">δ</supplied>ελφίᾳ.</ab></div><div n="meta.11243" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.11243" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" value="0382-05-08" notBefore="382" notAfter="382">8. Mai 382</date></p></div><div n="meta.11243" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Gehört zum notariellen Archiv aus Philadelphia. Zu Z. 9 vgl. AnalPap 14-15, 2002-2003, S. 387.</p></div><div n="meta.11243" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">P.Gen.</title><biblScope type="volume">1</biblScope><biblScope type="fascicle"> (2e éd.)</biblScope><biblScope type="numbers">68</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">11243</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0266</series>; <biblScope type="volume">1</biblScope>; <biblScope type="numbers">68</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0266;1;68" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.11243" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p><bibl type="illustration" default="NO">pl. XLVIII</bibl></p></div><div n="meta.11243" type="history" subtype="locations" org="uniform" part="N" sample="complete"><p><placeName type="ancientFindspot" cert="high" full="yes">Philadelphia</placeName> (<geogName type="nome" cert="high" full="yes">Arsinoite