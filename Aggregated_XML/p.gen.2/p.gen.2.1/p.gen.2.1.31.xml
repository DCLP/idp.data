    <lb n="9"/>γράμματα. ἐ<unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ε</supplied><unclear reason="undefined" cert="high">ὶ</unclear> δὲ μετὰ τὴν <unclear reason="undefined" cert="high">κ</unclear>ρίσιν τῇ αὐτῇ <choice><corr>αὐθαδείᾳ</corr><sic>αυθαδια</sic></choice>

    <lb n="10"/>χρ<unclear reason="undefined" cert="high">ώ</unclear>μενος ὁ Διόσκορος, καταφρονῶν μου τῆς χ<unclear reason="undefined" cert="high">ηρ</unclear>εί-

    <lb n="11"/>ας, <supplied reason="omitted" cert="high">τὰ</supplied> ὑπάρχοντά μου ἀ<unclear reason="undefined" cert="high">λ</unclear>όγως διεμίσθωσε γεωργ<unclear reason="undefined" cert="high">ο</unclear>ῖς

    <lb n="12"/>καὶ τ<unclear reason="undefined" cert="high">ῶν</unclear> <unclear reason="undefined" cert="high">μὲν</unclear> <choice><corr>ἐκ</corr><sic>εγ</sic></choice> μ<unclear reason="undefined" cert="high">έρ</unclear><supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">υ</unclear><supplied reason="lost" cert="high">ς</supplied> ἐκφόρ<unclear reason="undefined" cert="high">ι</unclear>α ἔλαβεν <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">οῦ</supplied> <num value="9"><unclear reason="undefined" cert="high">θ</unclear></num> <expan><ex cert="high">ἔτους</ex></expan>, τῶν

    <lb n="13"/>δ<unclear reason="undefined" cert="high">ὲ</unclear> <supplied reason="lost" cert="high">πα</supplied><unclear reason="undefined" cert="high">ρα</unclear>κατέσχον διὰ στρατηγοῦ παρὰ γ<unclear reason="undefined" cert="high">ε</unclear>ωργοῖς ἄχρι

    <lb n="14"/><unclear reason="undefined" cert="high">κ</unclear>ρίσεως, παραγγελείς τε ὁ Διόσκορος διὰ <unclear reason="undefined" cert="high">σ</unclear>τρατηγ<unclear reason="undefined" cert="high">ι</unclear><supplied reason="lost" cert="high">κο</supplied><unclear reason="undefined" cert="high">ῦ</unclear>

