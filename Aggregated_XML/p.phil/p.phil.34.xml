    <lb n="19"/>ε<gap extent="4" reason="illegible" unit="character"/><gap extent="3" reason="illegible" unit="character"/>σλυ<gap extent="3" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">τ</unclear>α 

