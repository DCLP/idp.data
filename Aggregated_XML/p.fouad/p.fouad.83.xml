    <lb n="2"/>Φοιβάμμωνα βάπτισαι καὶ <unclear cert="high" reason="undefined">χ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>καὶ κελλαρικῶν, καὶ καταξί<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">σ</supplied><unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ν </supplied><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>ἔκπεμψαι διὰ ταχέων εἴπερ οὐ προῄρηται π<unclear cert="high" reason="undefined">ρ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>καταλαβεῖν ἡμᾶς, ἔκπεμψαι δὲ παραυτὰ καὶ <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/>με<unclear cert="high" reason="undefined">τ</unclear>ὰ τῶν κελλαρικῶν καὶ τ<unclear cert="high" reason="undefined">ω</unclear><gap extent="1" reason="lost" unit="character"/> <gap extent="1" reason="illegible" unit="character"/>εναμε<unclear cert="high" reason="undefined">ν</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/>Φοιβάμμωνι. <hi rend="diaeresis">ὑ</hi>γιαίνομεν δὲ πάντες σὺν Θε<supplied cert="high" reason="lost">ῷ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>τὸ σὸν μέγεθος προσφθέγγεται δὲ αὐτὸ καὶ <unclear cert="high" reason="undefined">ο</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>Φοιβάμμων· προσφθέγγομαι δὲ <unclear cert="high" reason="undefined">καὶ</unclear> τὸν κύριόν <unclear cert="high" reason="undefined">μ</unclear><supplied cert="low" reason="lost">ου </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/>ἀδελφὸν Παῦλον· προσφθέγγεται δὲ <unclear cert="high" reason="undefined">τ</unclear>ὴν αὐτοῦ λαμ<supplied cert="high" reason="lost">πρότητα ὁ</supplied> 

    <lb n="11"/>κύριός μου Φοιβάμμων. 

