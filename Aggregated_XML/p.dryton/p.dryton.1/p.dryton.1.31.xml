    <lb n="1"/><gap desc="vestiges" extent="unknown" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἵνα μηθὲν</supplied>

    <lb n="2"/><unclear cert="high" reason="undefined">ἐν</unclear> <unclear cert="high" reason="undefined">το</unclear>ύτοις <unclear cert="high" reason="undefined">ἀ</unclear>γν<supplied cert="high" reason="lost">ο</supplied>ηθῆι <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="3"/><unclear cert="high" reason="undefined">Ἑ</unclear>ρμίαι τῶν διαδόχων καὶ ἐπιμ<unclear cert="high" reason="undefined">ελητῆι</unclear>, <supplied cert="high" reason="lost">πα</supplied><unclear cert="high" reason="undefined">ρ</unclear><supplied cert="low" reason="lost">ὰ <unclear cert="high" reason="undefined">Δ</unclear></supplied><unclear cert="high" reason="undefined">ρ</unclear><supplied cert="low" reason="lost">ύτωνος τοῦ Παμφίλου </supplied><gap extent="6" reason="lost" unit="character"/><supplied cert="high" reason="lost"> παρεφεδρε</supplied><unclear cert="high" reason="undefined">ύ</unclear>οντος Κρητὸς δήμου <unclear cert="high" reason="undefined">Φ</unclear><supplied cert="low" reason="lost">ιλωτερείου </supplied><gap extent="unknown" reason="lost" unit="character"/>.

    <lb n="4"/><unclear cert="high" reason="undefined">Ἐ</unclear>πεὶ Πατσε<unclear cert="high" reason="undefined">οῦ</unclear>τος <expan>κωμογρ<ex cert="high">αμματέως</ex></expan> <expan>Πα<unclear cert="high" reason="undefined">θύ</unclear>ρ<ex cert="high">εως</ex></expan> καθ᾽ ἱδίαν μῆνιν ἀναγράψαντός με παρὰ τὸ <supplied cert="high" reason="lost">καθῆ</supplied><unclear cert="high" reason="undefined">κ</unclear>ον ἐν τοῖς δυνα<supplied cert="high" reason="lost">μένοις </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> <expan><ex cert="high">τάλαντα</ex></expan></supplied>

