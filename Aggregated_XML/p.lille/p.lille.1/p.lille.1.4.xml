    <lb n="16"/><app type="editorial"><lem>τοῦ <expan>συ<ex cert="high">μβόλου</ex></expan></lem><rdg>Τοῦθ</rdg></app> τοῦ παρὰ Θεογένους εἰς τὴν <app type="BL"><lem resp="9.122"><num value="2">β</num> <expan><ex cert="high">ἑξά</ex>μη<ex cert="high">νον</ex></expan></lem><rdg><expan>β<ex cert="high">ασιλικὴν</ex></expan> <expan>σ<ex cert="high">ύν</ex>τ<ex cert="high">αξιν</ex></expan></rdg></app> τοῦ <num value="5">ε</num> <expan><ex cert="high">ἔτους</ex></expan> <expan>μά<ex cert="high">λιστα</ex></expan> <expan>σύ<ex cert="high">νταξον</ex></expan> 

    <lb n="17"/>ἀποδοῦναι <app type="BL"><lem resp="9.122">Μεγαλλεῖ</lem><rdg>Μεταλλεῖ</rdg></app> τῶι ἡγεμόνι <expan><ex cert="high">δραχμὰς</ex></expan> <num value="42">μβ</num> τῆς <num value="2">β</num> <add place="supralinear">διπ<unclear reason="undefined" cert="high">λ</unclear>ῆς</add> τ<unclear reason="undefined" cert="high">ο</unclear><gap reason="illegible" extent="2" unit="character"/> <num value="13">ι<unclear reason="undefined" cert="high">γ</unclear></num> 

