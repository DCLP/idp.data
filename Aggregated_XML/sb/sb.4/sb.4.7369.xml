    <lb n="1"/>χμγ 

    <lb n="2"/><g type="stauros"/> ὑπατείας <unclear cert="high" reason="undefined">Φ</unclear>λ<supplied cert="high" reason="lost">α</supplied>υ<unclear cert="high" reason="undefined">ί</unclear><supplied cert="high" reason="lost">ου Πα</supplied>ύλου <unclear cert="high" reason="undefined">καὶ</unclear> <unclear cert="high" reason="undefined">Μο</unclear>σχιανοῦ, Θὼθ [<gap extent="1" reason="illegible" unit="character"/>, <num value="6">ϛ</num>] <expan>ἰνδ<ex cert="high">ικτίονος</ex></expan>. 

    <lb n="3"/>Ταυρίνῳ <supplied cert="high" reason="lost">τῷ</supplied> <unclear cert="high" reason="undefined">ἐλλο</unclear><supplied cert="high" reason="lost">γιμωτά</supplied>τῳ πρεσ<unclear cert="high" reason="undefined">β</unclear>υ<unclear cert="high" reason="undefined">τ</unclear>έρῳ καθολικῆς ἐ<unclear cert="high" reason="undefined">κκ</unclear>λησίας Ἑρμουπόλεως τ<unclear cert="high" reason="undefined">ῆ</unclear>ς λαμπροτάτης, 

    <lb n="4"/>υἱῷ <supplied cert="high" reason="lost">τ</supplied>οῦ τῆ<supplied cert="high" reason="lost">ς μακαρίας μν</supplied><unclear cert="high" reason="undefined">ή</unclear>μ<unclear cert="high" reason="undefined">η</unclear><supplied cert="high" reason="lost">ς</supplied> Ἰω<supplied cert="high" reason="lost">άν</supplied>νου <expan>παρ<ex cert="high">ὰ</ex></expan> Ἀπολλῶ<supplied cert="high" reason="lost">το</supplied>ς Ἰσαίο<supplied cert="high" reason="lost">υ</supplied>, μητρὸς Ἰόνης, ἀμπελουργοῦ 

    <lb n="5"/>ἀπὸ κώμης <gap extent="5" reason="lost" unit="character"/>κύρε<gap extent="2" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τοῦ</supplied> Ἑρμουπολί<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ου</supplied> ν<supplied cert="high" reason="lost">ομ</supplied><unclear cert="high" reason="undefined">οῦ</unclear>, κ<supplied cert="high" reason="lost">αὶ</supplied> <unclear cert="high" reason="undefined">Ἀ</unclear>πολλ<supplied cert="high" reason="lost">ῶ</supplied>τος Ἰσ<supplied cert="high" reason="lost">ι</supplied>δ<unclear cert="high" reason="undefined">ώ</unclear>ρου μητρὸς 

    <lb n="6"/>Εἰρήνης ἀπὸ τῆ<supplied cert="high" reason="lost">ς αὐτῆς</supplied> <unclear cert="high" reason="undefined">κώμης</unclear>, <unclear cert="high" reason="undefined">καὶ</unclear> <supplied cert="high" reason="lost">αὐ</supplied>τοῦ ἀμπελου<supplied cert="high" reason="lost">ργ</supplied>οῦ. <supplied cert="high" reason="lost">ὁ</supplied>μολογο<supplied cert="high" reason="lost">ῦ</supplied><unclear cert="high" reason="undefined">μ</unclear>εν ἀ<unclear cert="high" reason="undefined">λλ</unclear>ηλέγγυοι <unclear cert="high" reason="undefined">εἰς</unclear> <expan><unclear cert="high" reason="undefined">ἔ</unclear>κτ<ex cert="high">ισιν</ex></expan> 

    <lb n="7"/>καὶ ἀλλήλ<unclear cert="high" reason="undefined">ων</unclear> <unclear cert="high" reason="undefined">ἀνάδ</unclear>οχοι ἑκου<unclear cert="high" reason="undefined">σ</unclear>ίως καὶ αὐθαιρέτω<supplied cert="high" reason="lost">ς</supplied> μεμι<unclear cert="high" reason="undefined">σθῶ</unclear>σθαι παρ<unclear cert="high" reason="undefined">ὰ</unclear> <unclear cert="high" reason="undefined">τ</unclear>ῆς σῆς 

    <lb n="8"/>εὐλαβείας ἐπὶ <unclear cert="high" reason="undefined">διετῆ</unclear> χρόν<supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ν</unclear>, λογιζόμενον ἀπὸ καρπῶν τῆ<supplied cert="high" reason="lost">ς</supplied> εὐτυχοῦς ἑ<unclear cert="high" reason="undefined">βδόμ</unclear>ης <expan>ἰνδ<ex cert="high">ικτίονος</ex></expan>, 

    <lb n="9"/>τὸ ὑπάρχον α<unclear cert="high" reason="undefined">ὐ</unclear><supplied cert="high" reason="lost">τῇ</supplied> χωρί<supplied cert="high" reason="lost">ο</supplied>ν ἀμπελικὸν ζωόφυτον ἐν συστάσει ἀ<unclear cert="high" reason="undefined">ρ</unclear>ουρῶν, ὅσων ἐστίν, 

    <lb n="10"/>σὺν φυτοῖς πᾶσι <supplied cert="high" reason="lost">κα</supplied>ὶ φοίνιξι, διακείμενον ἐν πεδιάδι τῆς αὐτῆς ἡμῶν κώμης 

    <lb n="11"/>πρὸς ἀμπελουργικὴν ἡμῶν ἐργασίαν καὶ πᾶσαν ἐπιμέλειαν καὶ φιλοκαλίαν. <unclear cert="high" reason="undefined">σ</unclear>κάψομεν 

    <lb n="12"/>δὲ τὸ αὐτὸ χωρ<supplied cert="high" reason="lost">ί</supplied>ον πέμπτον κατ᾽ ἔτος, ἐφ᾽ ᾧτε τὴν σὴν εὐλάβειαν <choice><corr>δέξασθαι</corr><sic>δεξασσθαι</sic></choice> ἥμισυ μέρος 

    <lb n="13"/>παντὸς τοῦ ἐκ<unclear cert="high" reason="undefined">β</unclear>ησομένου οἴνου καὶ παντοίων φυτῶν καὶ φοινίκων καὶ <choice><corr>ἡμᾶς</corr><sic>εμε</sic></choice> μετὰ 

    <lb n="14"/>ὑδροπαρόχ<unclear cert="high" reason="undefined">ου</unclear> <unclear cert="high" reason="undefined">τὸ</unclear> ἄλλο ἥμισυ μέρος, ἀνθ᾽ ὧν ποιούμεθα καμάτων, λαβεῖν δὲ τὴν σὴν 

    <lb n="15"/>εὐλάβειαν κατ᾽ ἔ<unclear cert="high" reason="undefined">τος</unclear> οὐδὲν ἧττον λόγῳ πρωτολήνου ἀπὸ τοῦ κοινοῦ πίθου οἴνου 

    <lb n="16"/>κνίδια ὀγδο<unclear cert="high" reason="undefined">ήκον</unclear>τα <num value="80"/>. παρέξομεν δὲ κατ᾽ ἔτος ἐν τῷ καιρῷ τῶν τρυγῶν δελφάκιον ἄξιον 

    <lb n="17"/>ἀργυρίου ταλάντω<unclear cert="high" reason="undefined">ν</unclear> <choice><corr>ἑξακισχιλίαν</corr><sic><supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">ξ</unclear>ακισχειλ<unclear cert="high" reason="undefined">ι</unclear>ων</sic></choice> <num value="6000"/> <unclear cert="high" reason="undefined">κα</unclear>ὶ πινάκιον μεστὸν διαφόρων βρωμάτων <choice><corr>ἄξιον</corr><sic>αξιων</sic></choice> ἀργυρίου 

    <lb n="18"/>ταλάντων <choice><corr>τετρακισχιλίων</corr><sic>τετρακισχειλιων</sic></choice> <num value="4000"/>, <unclear cert="high" reason="undefined">δη</unclear>λονότι τῶν καρπῶν τῶν φοινίκων στελλομένων ἡμῖν παρεχο-

    <lb n="19"/>μένοις τῇ σῇ εὐλαβείᾳ ὑπὲρ ἐκ<supplied cert="high" reason="lost">φ</supplied>ο<unclear cert="high" reason="undefined">ρ</unclear>ίω<unclear cert="high" reason="undefined">ν</unclear> αὐτῶν κατ᾽ ἔτος χρυσοῦ νομισματίου τρίτον <num value=" 1/3 "/> κατὰ τὴν σὴν ἀλλαγήν. 

    <lb n="20"/>μισθούμεθα οὐδὲν ἧττ<supplied cert="high" reason="lost">ον </supplied><gap extent="3" reason="lost" unit="character"/>το<gap extent="4" reason="illegible" unit="character"/> νεόφυτον τὸ ἥμισυ τὸ καὶ <choice><corr>συνημμένον</corr><sic>συνη<add place="supralinear">μ</add>μενου</sic></choice> τῷ μεγάλῳ χωρίῳ, 

    <lb n="21"/><unclear cert="high" reason="undefined">ὥστε</unclear> <unclear cert="high" reason="undefined">ἡμᾶ</unclear>ς τὴν πᾶσαν <unclear cert="high" reason="undefined">ἀ</unclear>μπελ<supplied cert="high" reason="lost">ουργ</supplied>ικὴ<supplied cert="high" reason="lost">ν</supplied> <unclear cert="high" reason="undefined">ἐ</unclear><supplied cert="high" reason="lost">ργα</supplied>σίαν ποιήσασθα<supplied cert="high" reason="lost">ι</supplied> ἀ<supplied cert="high" reason="lost">μ</supplied>έμπ<supplied cert="high" reason="lost">τ</supplied>ως κατὰ μίμησιν τοῦ μεγάλου σου 

    <lb n="22"/>ἀμπελικοῦ χωρίου <unclear cert="high" reason="undefined">ἐ</unclear><supplied cert="high" reason="lost">π</supplied><unclear cert="high" reason="undefined">ὶ</unclear> <unclear cert="high" reason="undefined">τ</unclear>ῷδε· <supplied cert="high" reason="lost">τὴν</supplied> σ<unclear cert="high" reason="undefined">ὴν</unclear> <supplied cert="high" reason="lost">εὐ</supplied><unclear cert="high" reason="undefined">λ</unclear>άβειαν λαβεῖ<supplied cert="high" reason="lost">ν</supplied> καὶ ἀπὸ τούτου τὸ ἥμισυ παντὸ<supplied cert="high" reason="lost">ς τ</supplied>οῦ ἐκβησο-

    <lb n="23"/>μένου οἴνου καὶ ἡμᾶς μετὰ το<supplied cert="high" reason="lost">ῦ ὑδ</supplied><unclear cert="high" reason="undefined">ροπ</unclear>α<unclear cert="high" reason="undefined">ρ</unclear>ό<unclear cert="high" reason="undefined">χ</unclear>ου τὸ ἄλλο ἥμισυ <supplied cert="high" reason="lost">μέρος ὑ</supplied>πὲρ <unclear cert="high" reason="undefined">τῶ</unclear>ν <supplied cert="high" reason="lost">κ</supplied><unclear cert="high" reason="undefined">α</unclear>μάτ<unclear cert="high" reason="undefined">ων</unclear> <sic><unclear cert="high" reason="undefined">ματων</unclear></sic> καὶ <supplied cert="high" reason="lost">λαβεῖν αὐ</supplied><unclear cert="high" reason="undefined">τὴ</unclear>ν 

    <lb n="24"/><supplied cert="high" reason="lost">ἀπὸ</supplied> τοῦ κ<supplied cert="high" reason="lost">ο</supplied>ινοῦ πίθ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">υ</supplied> ἐπὶ μό<supplied cert="high" reason="lost">νη</supplied>ς <supplied cert="high" reason="lost">τῆς εἰσιού</supplied>σης ἑβδ<supplied cert="high" reason="lost">όμ</supplied>η[ς <expan>ἰνδ<ex cert="high">ικτίονος</ex></expan>] λ<supplied cert="high" reason="lost">όγ</supplied>ῳ <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ρ</supplied>ω<supplied cert="high" reason="lost">το</supplied><unclear cert="high" reason="undefined">λή</unclear>ν<supplied cert="high" reason="lost">ο</supplied>υ οἴ<supplied cert="high" reason="lost">νου κν</supplied><unclear cert="high" reason="undefined">ίδια</unclear> 

    <lb n="25"/><supplied cert="high" reason="lost">τεσ</supplied>σεράκοντα <num value="40"/>. παρ<unclear cert="high" reason="undefined">έξο</unclear>μεν δὲ <supplied cert="high" reason="lost">πρ</supplied><unclear cert="high" reason="undefined">ὸ</unclear>ς το<unclear cert="high" reason="undefined">ῖ</unclear>ς <unclear cert="high" reason="undefined">ἐκβ</unclear>ησ<supplied cert="high" reason="lost">ο</supplied>μένοις καὶ <unclear cert="high" reason="undefined">ὑπ</unclear>ὲρ συνηθείας τοῦ νεοφύτου δελφ<unclear cert="high" reason="undefined">ά</unclear>κιον ἄξιον 

    <lb n="26"/><supplied cert="high" reason="lost">ἀ</supplied>ργυρίου ταλάντω<supplied cert="high" reason="lost">ν</supplied> <choice><corr>πεντακισχιλίων</corr><sic>πε<supplied cert="high" reason="lost">ντα</supplied>κισ<unclear cert="high" reason="undefined">χει</unclear>λιων</sic></choice> <num value="5000"/>, <supplied cert="high" reason="lost">καὶ πι</supplied>νάκιον ὁμοίως ἄξιον ἀργυρί<supplied cert="high" reason="lost">ου</supplied> ταλάντω<supplied cert="high" reason="lost">ν</supplied> <choice><corr>τρισχιλίων</corr><sic>τρεισχειλιων</sic></choice>,  μετὰ δὲ 

    <lb n="27"/>τὴν ἑβδόμην <expan>ἰν<unclear cert="high" reason="undefined">δι</unclear>κ<ex cert="high">τίονα</ex></expan> <supplied cert="high" reason="lost">ἐπ</supplied>ὶ τῷ ἡ<unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ᾶ</supplied>ς π<supplied cert="high" reason="lost">αρέχει</supplied>ν ὅ<unclear cert="high" reason="undefined">λας</unclear> <unclear cert="high" reason="undefined">τὰς</unclear> <supplied cert="high" reason="lost">συ</supplied>νηθείας κατὰ <unclear cert="high" reason="undefined">μί</unclear><supplied cert="high" reason="lost">μη</supplied>σιν τοῦ μεγάλου χωρίου. ἡ μίσθωσις 

    <lb n="28"/>κυρία καὶ βεβαία καὶ <expan><unclear cert="high" reason="undefined">ἐπε</unclear>[ρ<ex cert="high">ωτηθέντες</ex></expan> <expan>ὡμο]λ<ex cert="high">ογήσαμεν</ex></expan> <gap extent="4" reason="illegible" unit="character"/> <handShift new="m2"/> Ἀπολλ<supplied cert="high" reason="lost">ὼς</supplied> Ἰσαίο<supplied cert="high" reason="lost">υ</supplied> καὶ Ἀπολλὼς Ἰ<unclear cert="high" reason="undefined">σι</unclear>δώρου <choice><corr>οἱ</corr><sic>ο</sic></choice> <expan>προκ<ex cert="high">είμενοι</ex></expan> <choice><corr>μεμισθώμεθα</corr><sic>μεμισθ<supplied cert="high" reason="lost">ωμ</supplied>αι</sic></choice>, ὡς <expan>πρόκ<ex cert="high">ειται</ex></expan>. <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> <sic>Αὐρη</sic> Βίκτορ ἀπὸ <expan>Ἑρ<ex cert="high">μουπόλεως</ex></expan> 

    <lb n="29"/><supplied cert="high" reason="lost">ἔ</supplied>γραψα ὑπὲρ <choice><corr>αὐτῶν</corr><sic>αυτου</sic></choice> <expan>γρ<ex cert="high">άμματα</ex></expan> μὴ <unclear cert="high" reason="undefined">ε</unclear>ἰδ<unclear cert="high" reason="undefined">ότ</unclear><supplied cert="high" reason="lost">ων </supplied><gap extent="4" reason="lost" unit="character"/> <note anchored="yes" lang="en" place="not specified">vestig<gap extent="15" reason="illegible" unit="character"/></note> <handShift new="m3"/> <expan>μαρ<ex cert="high">τυρῶ</ex></expan> τῇ μισθώσει ἀκούσας 
παρὰ <choice><corr>τῶν</corr><sic>του</sic></choice> <choice><corr>θεμένων</corr><sic>θεμενου</sic></choice>. <handShift new="m4"/> <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> <g type="stauros"/> <unclear cert="high" reason="undefined">Ἀν</unclear>δρέ<unclear cert="high" reason="undefined">ας</unclear> Μέλανος 

    <lb n="30"/><expan><unclear cert="high" reason="undefined">π</unclear>ρεσβύτερ<ex cert="high">ος</ex></expan> <expan>Ἑρμουπόλ<ex cert="high">εως</ex></expan> μαρτυρῶ τῇ μισθώσει ἀκούσας π<supplied cert="high" reason="lost">αρ</supplied>ὰ <choice><corr>τῶν</corr><sic>του</sic></choice> <choice><corr>θεμένων</corr><sic>θεμενου</sic></choice> <g type="stauros"/> <handShift new="m5"/> <g type="chirho"/> δι᾽ <expan>ἐμ<ex cert="high">οῦ</ex></expan> <expan>Παύλ<ex cert="high">ου</ex></expan> <expan>ἐγ<ex cert="high">ράφη</ex></expan>. 

