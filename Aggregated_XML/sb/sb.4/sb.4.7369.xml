    <lb n="6"/>Εἰρήνης ἀπὸ τῆ<supplied reason="lost" cert="high">ς αὐτῆς</supplied> <unclear reason="undefined" cert="high">κώμης</unclear>, <unclear reason="undefined" cert="high">καὶ</unclear> <supplied reason="lost" cert="high">αὐ</supplied>τοῦ ἀμπελου<supplied reason="lost" cert="high">ργ</supplied>οῦ. <supplied reason="lost" cert="high">ὁ</supplied>μολογο<supplied reason="lost" cert="high">ῦ</supplied><unclear reason="undefined" cert="high">μ</unclear>εν ἀ<unclear reason="undefined" cert="high">λλ</unclear>ηλέγγυοι <unclear reason="undefined" cert="high">εἰς</unclear> <expan><unclear reason="undefined" cert="high">ἔ</unclear>κτ<ex cert="high">ισιν</ex></expan> 

