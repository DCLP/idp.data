    <lb n="1"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>αυται κ<supplied cert="high" reason="lost">αὶ</supplied> ἐν ἄλλαις αἰτίαις <unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ε</supplied>ίζοσιν ε<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/><gap extent="unknown" reason="lost" unit="character"/> περὶ ὧν δι<supplied cert="high" reason="lost">α</supplied>τελοῦμεν γράφοντες, ὅπω<supplied cert="high" reason="lost">ς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/><gap extent="unknown" reason="lost" unit="character"/>νται, πλεῖον δ᾽ οὐθὲν γίνεται. <space dim="horizontal" extent="unknown" unit="character"/> 

    <lb n="4"/><space dim="horizontal" extent="unknown" unit="character"/>  ἔρρωσο. 

    <lb n="5"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ὁ </supplied><choice><corr><supplied cert="high" reason="lost">διοικ</supplied>ητὴ<supplied cert="high" reason="omitted">ς</supplied></corr><sic><supplied cert="high" reason="lost">διοικ</supplied>ητη</sic></choice> τοῖς ἄλλοις διοικηταῖς. <space dim="horizontal" extent="unknown" unit="character"/> 

    <lb n="6"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> Πτολεμαῖος Ἰμούθηι χαίρειν. τῶν π<supplied cert="high" reason="lost">εμφ</supplied>-

