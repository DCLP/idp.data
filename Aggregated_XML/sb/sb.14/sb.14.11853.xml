    <lb n="1"/>Πρόκλος Σαραπίωνι τῷ φιλτάτῳ πλεῖστα χαίρειν. 

    <lb n="2"/>εὖ ποιήσεις ἀπο<unclear cert="high" reason="undefined">κα</unclear>ταστήσας Τερεντίῳ τὰς δύο <num value="2"/> ἀρτάβας τοῦ 

    <lb n="3"/><choice><corr>σίτου</corr><sic>σειτου</sic></choice> ἃς κατέ<unclear cert="high" reason="undefined">λ</unclear>ειψα παρὰ Πάσειτι γεωργῶι; <unclear cert="high" reason="undefined">κ</unclear>αὶ περὶ πάντων 

    <lb n="4"/>τῶν πρα<unclear cert="high" reason="undefined">γ</unclear>μάτ<unclear cert="high" reason="undefined">ων</unclear> <unclear cert="high" reason="undefined">ἡ</unclear>μῶν μελησάτω σοι. καὶ <subst><add place="inline">γράψον</add><del rend="corrected" status="unremarkable">γρα<del rend="erasure" status="unremarkable">σ<gap extent="1" reason="illegible" unit="character"/></del>ψον</del></subst> μοι 

    <lb n="5"/>καὶ περὶ τῆς σω<unclear cert="high" reason="undefined">τ</unclear>ηρίας σου ἵνα ἀμέριμνος <choice><corr>ὦ</corr><sic>ωμαι</sic></choice>. πε<unclear cert="high" reason="undefined">ρὶ</unclear> <unclear cert="high" reason="undefined">δὲ</unclear> <unclear cert="high" reason="undefined">τῶν</unclear> 

    <lb n="6"/>ἐργα<unclear cert="high" reason="undefined">τ</unclear>ῶν τῆς κοιλάδος προσεχόντως <unclear cert="high" reason="undefined">ἐ</unclear>πιστα<supplied cert="high" reason="lost">λή</supplied>τω Χαυνῇ. 

    <lb n="7"/>μελησάτω σοι περὶ τῆς οἰκονομίας ἧς κα<unclear cert="high" reason="undefined">τέ</unclear><supplied cert="high" reason="lost">λει</supplied><unclear cert="high" reason="undefined">ψ</unclear>α <del rend="erasure" status="unremarkable">τ<unclear cert="high" reason="undefined">ω</unclear></del> παρὰ τ<unclear cert="high" reason="undefined">ῷ</unclear> 

    <lb n="8"/>νομικῷ ἐν τῇ Σεβαστῇ ἀγορᾷ. <choice><corr>λίαν</corr><sic>λειαν</sic></choice> δέ σε ἡ θυγάτηρ σου μέ<unclear cert="high" reason="undefined">μ</unclear>-

    <lb n="9"/>φετ<unclear cert="high" reason="undefined">α</unclear>ι ὅτι ἕως σήμερο<unclear cert="high" reason="undefined">ν</unclear> ο<unclear cert="high" reason="undefined">ὐ</unclear>κ ἔγρα<supplied cert="high" reason="lost">ψα</supplied><unclear cert="high" reason="undefined">ς</unclear> <choice><corr>ἡμῖν</corr><sic>η<unclear cert="high" reason="undefined">μ</unclear>ει<supplied cert="high" reason="lost">ν</supplied></sic></choice> <supplied cert="high" reason="lost">π</supplied>ε<unclear cert="high" reason="undefined">ρὶ</unclear> τῆς σω<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">η</supplied>-

    <lb n="10"/>ρία<unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">σ</unclear>ου. ἐὰν ὁ καιρ<unclear cert="high" reason="undefined">ὸ</unclear>ς γένηται τοῦ φόρου μ<unclear cert="high" reason="undefined">οι</unclear><gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">τ</unclear>ωνος σοι <add place="supralinear"><supplied cert="high" reason="lost">σ</supplied><unclear cert="high" reason="undefined">ὺ</unclear></add> καὶ 

    <lb n="11"/>Ἀλ<unclear cert="high" reason="undefined">ᾶ</unclear>ς ὁ ἰατρὸς <add place="supralinear">καὶ Ε<gap extent="1" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/>ρ ὁ ἀδε<unclear cert="high" reason="undefined">λφ</unclear>ὸς</add> λαβόντες παρὰ τοῦ γεωργοῦ δότε <choice><corr>αὐτὸ<supplied cert="high" reason="omitted">ν</supplied></corr><sic>αυτο</sic></choice> Λόγγῳ, 

    <lb n="12"/>λαβόντες <gap extent="unknown" reason="lost" unit="character"/> <unclear cert="high" reason="undefined">παρ</unclear>᾽ αὐτ<supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ῦ</unclear> ἀποχὴν ἔνγραπ<unclear cert="high" reason="undefined">τ</unclear>ον. ἐὰν δὲ μὴ θέ<unclear cert="high" reason="undefined">λῃ</unclear> <unclear cert="high" reason="undefined">τὰ</unclear> <choice><corr>δ<supplied cert="high" reason="lost">είγ</supplied>-

    <lb n="13"/>ματα</corr><sic><unclear cert="high" reason="undefined">δ</unclear><supplied cert="high" reason="lost">ιγ</supplied>ματα</sic></choice> τῶν <gap extent="3" reason="lost" unit="character"/>ε<gap extent="1" reason="illegible" unit="character"/>δ<unclear cert="high" reason="undefined">ε</unclear><gap extent="4" reason="illegible" unit="character"/> τω<unclear cert="high" reason="undefined">δοτ</unclear>ε<unclear cert="high" reason="undefined">ι</unclear>α<gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">τ</unclear>α. ἔχει δὲ ὁ γεωρ<unclear cert="high" reason="undefined">γὸς</unclear> Πάι<unclear cert="high" reason="undefined">ς</unclear> <gap extent="2" reason="illegible" unit="character"/>τ<gap extent="4" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/>. 
