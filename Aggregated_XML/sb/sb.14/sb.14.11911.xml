    <lb n="1"/>Ἀ<unclear reason="undefined" cert="high">π</unclear>ιδίᾳ Ἀ<supplied reason="lost" cert="high">πολλωναρίῳ καὶ ὡς</supplied> 

    <lb n="2"/>χρηματίζε<supplied reason="lost" cert="high">ι ἀπὸ τοῦ </supplied><gap reason="lost" extent="5" unit="character"/> 

    <lb n="3"/>νομοῦ Σιμοίρεως τό<supplied reason="lost" cert="high">πων,</supplied> 

