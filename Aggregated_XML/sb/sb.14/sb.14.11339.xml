    <lb n="3" rend="inverse"/> <supplied cert="high" reason="lost">χαίρ</supplied>ειν <choice><corr>ὁμο<supplied cert="high" reason="omitted">λο</supplied>γῶ</corr><sic>ομογω</sic></choice> 

