    <lb n="1"/>ευ<gap reason="illegible" extent="4" unit="character"/> <space extent="unknown" unit="character" dim="horizontal"/> <gap reason="illegible" extent="2" unit="character"/> 

    <lb n="2"/>πράκτορσι χαίρε<supplied reason="lost" cert="high">ιν</supplied>. ἐ<unclear reason="undefined" cert="high">π</unclear>ὶ <unclear reason="undefined" cert="high">τ</unclear>ῶι δι<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>τὴν εἰσαγωγὴν τοῦ σιτικοῦ στεφ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>γίνεσθαι πλοῖα δὲ προσπεπλ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>ἐπὶ τὴν τοῦ πυροῦ ἐμβολὴν διὰ τὴν 

    <lb n="6"/>ἐν τῆι πόλει περὶ τὴς σιταρχία<supplied reason="lost" cert="high">ς</supplied> δυσχωρίαν 

    <lb n="7"/>ἠναγκάσμεθα ἀποσπάσαντες ἑαυτῶν 

    <lb n="8"/>Ἀριστόνικον τῶν ἰδίων πέμψαι ἔχο<supplied reason="lost" cert="high">ντα</supplied> 

    <lb n="9"/>ὑπομνηματισμὸν <gap reason="lost" extent="3" unit="character"/> ἐν ὑμῖν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>κεφαλαίου καὶ μὴ ἀποστησόμενον 

    <lb n="11"/>μέχρι τοῦ διὰ νυκτὸς καὶ ἡμέρας 

    <lb n="12"/>ἐκπληρωθῆναι τοὺς δὲ ὀλιγω<supplied reason="lost" cert="high">ρή</supplied><unclear reason="undefined" cert="high">σ</unclear>οντας 

    <lb n="13"/>ἐξαποστέλλειν ἐφ᾽ ἡμᾶς ἀπὸ δὲ τῆς 

    <lb n="14"/>ἐψηφισμένης ὑπὸ τῶν ἀνδρῶν κοινῆς 

    <lb n="15"/>δαπάνης καθ᾽ ἃς εἴχετε πικροτέρας 

    <lb n="16"/>διαστολὰς ἅμα τοῖς βασιλικοῖς μετρεῖν τῶι 

    <lb n="17"/>σιτιλόγ<supplied reason="lost" cert="high">ω</supplied>ι μηδενὶ <supplied reason="lost" cert="high">καθ᾽ ὁ</supplied>ντινοῦν τρόπον 

    <lb n="18"/><gap reason="illegible" extent="1" unit="character"/>ρ<gap reason="lost" extent="5" unit="character"/><gap reason="illegible" extent="1" unit="character"/> μηδὲν ἄνευ τοῦ παρ᾽ ἡμῶν 

    <lb n="19"/>ἐπισταλῆναι τι. 

    <lb n="20"/>ἔρρωσθε <expan><ex cert="high">ἔτους</ex></expan> <num value="24">κδ</num> Χοιὰχ <num value="15">ιε</num>. 
