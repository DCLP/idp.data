    <lb n="1"/><handShift new="m1"/> Δῖος <expan>γρα<ex cert="high">μματεὺς</ex></expan> <expan>σεση<ex cert="high">μείωμαι</ex></expan> <num value="15">ιε</num> <expan><ex cert="high">ἔτους</ex></expan> Μεσορή. 

    <lb n="2"/><handShift new="m2"/> Δείῳ τῷ καὶ Ἀπολλων<supplied cert="high" reason="lost">ίῳ καὶ</supplied> 

    <lb n="3"/>Ηρώδῃ τῷ καὶ Διογένι 

    <lb n="4"/><expan>γεγυ<ex cert="high">μνασιαρχηκόσι</ex></expan> <expan>βιβλ<ex cert="high">ιοφύλαξι</ex></expan> <expan>ἐνκτ<ex cert="high">ήσεων</ex></expan> <expan>Ἀρσι<ex cert="high">νοίτου</ex></expan> 

    <lb n="5"/>παρὰ Ἀλεξ<unclear cert="high" reason="undefined">ί</unclear>ωνος τοῦ <expan>Πνε<unclear cert="high" reason="undefined">φ</unclear><supplied cert="high" reason="lost">ερῶ<ex cert="high">τος</ex></supplied></expan> 

    <lb n="6"/>τοῦ Διονυσίου τῶν ἀπὸ κώμ<supplied cert="high" reason="lost">ης</supplied> 

    <lb n="7"/><expan>Θεαδελ<ex cert="high">φείας</ex></expan>. κατὰ τὰ ὑπὸ τ<supplied cert="high" reason="lost">οῦ</supplied> 

    <lb n="8"/>κρατίστου <supplied cert="low" reason="lost"><expan>ἡγεμ<ex cert="high">όνος</ex></expan> <expan>Φλ<ex cert="high">αουίου</ex></expan> Τιτιανοῦ </supplied> 

    <lb n="9"/>κελευσθέντα ἀπογράφ<unclear cert="high" reason="undefined">ομαι</unclear> 

    <lb n="10"/>τὸ ὑπάρχον μο ἐ<supplied cert="high" reason="lost">π</supplied>ὶ τὴν 

    <lb n="11"/>κώμην ἥμισυ μέρος οἰκίας 

    <lb n="12"/>καὶ αὐλῆς. ἐὰν δέ τι κατ᾽ αὐτῆς 

    <lb n="13"/>ἐξοικονομῶ π<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ότε</supplied><unclear cert="high" reason="undefined">ρ</unclear>ον 

    <lb n="14"/><choice><corr>ἀποδείξω</corr><sic>αποδιξω</sic></choice> ὑπάρχειν. δ<unclear cert="high" reason="undefined">ι</unclear>ὸ <expan>ἐπιδίδ<ex cert="high">ωμι</ex></expan> 

