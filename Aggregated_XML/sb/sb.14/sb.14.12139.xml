    <lb n="1"/><supplied cert="high" reason="lost">λ</supplied>εχθέντων τοῦ τε ὑπομνηματισμοῦ τοῦ ἀρχ<supplied cert="high" reason="lost">ιδ</supplied>ικα<supplied cert="high" reason="lost">σ</supplied>τοῦ 

