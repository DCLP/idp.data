    <lb n="1"/>Πτολεμαίωι <expan>βασιλ<ex cert="high">ικῷ</ex></expan> <expan>γρα<ex cert="high">μματεῖ</ex></expan> <expan>Ἀρσι<ex cert="high">νοίτου</ex></expan> <expan>Πολέμ<ex cert="high">ωνος</ex></expan> <expan>μερίδ<ex cert="high">ος</ex></expan> 

    <lb n="2"/><expan>διαδεχο<ex cert="high">μένῳ</ex></expan> καὶ τὰ κατὰ τὴν <expan>στρα<ex cert="high">τηγίαν</ex></expan> 

