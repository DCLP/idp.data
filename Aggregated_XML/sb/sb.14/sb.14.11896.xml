    <lb n="1"/>καὶ σήμερον ἐπ᾽ ἐμοὶ <unclear cert="high" reason="undefined">κ</unclear>αὶ <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="1"/><gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/> κατὰ τουτ<gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>ἐπὶ παρουσία<unclear cert="high" reason="undefined">ι</unclear> Μαξίμου <num value="3">γ</num><gap extent="1" reason="lost" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/><gap extent="1" reason="lost" unit="character"/><unclear cert="high" reason="undefined">α</unclear>σ<gap extent="1" reason="lost" unit="character"/>ς λ<unclear cert="high" reason="undefined">ο</unclear>γ<unclear cert="high" reason="undefined">ι</unclear>ωτάτῳ σχο<unclear cert="high" reason="undefined">λ</unclear><supplied cert="high" reason="lost">αστι</supplied>κῳ <unclear cert="high" reason="undefined">καὶ</unclear> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>πόλεως τούτου ἕνεκεν ὁμολο<unclear cert="high" reason="undefined">γ</unclear><supplied cert="high" reason="lost">ῶ ἑπομνύμενος τὴν θείαν</supplied> 

    <lb n="5"/>καὶ οὐράνιον τύχην τῶν τὰ πάντα νι<supplied cert="high" reason="lost">κώντων δεσποτῶν</supplied> 

    <lb n="6"/>ἡμῶν Θεοδοσίου καὶ Οὐαλ<supplied cert="high" reason="lost">ε</supplied>ντινιανοῦ <supplied cert="high" reason="lost">τῶν αἰωνίων</supplied> 

    <lb n="7"/>Αὐγούστων μηδένα λόγον ἔχειν <unclear cert="high" reason="undefined">πρ</unclear><supplied cert="high" reason="lost">ὸς σὲ περὶ ταύτης</supplied> 

    <lb n="8"/>τῆς προφάσεως κ<supplied cert="high" reason="lost">αὶ μὴ</supplied> ἐγκαλεῖν σοι ἐντεῦ<supplied cert="high" reason="lost">θεν μήτε</supplied> 

    <lb n="9"/><choice><corr>ἐγκαλέσειν</corr><sic>εγκαλεσιν</sic></choice> <choice><corr>μητ᾽</corr><sic>μεθ</sic></choice> ἐμ<supplied cert="high" reason="lost">ὲ μήθ᾽ ἕ</supplied>να τῶν παρ᾽ ἐμοῦ σοι <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/>διὰ τὸ εἰληφέναι μ<unclear cert="high" reason="undefined">ε</unclear> νῦν <gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> πάσης ὑγι<gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/>ἐξέδωκά σοι ταύτην τὴν διάλυσιν πρὸς ἀσφά<supplied cert="high" reason="lost">λειάν σου· καὶ</supplied> 

    <lb n="12"/><expan>ἐπερ<ex cert="high">ωτηθεὶς</ex></expan> <expan>ὡμολ<ex cert="high">όγησα</ex></expan>. <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> Σίμων <hi rend="diaeresis">Ἰ</hi>σακίου <supplied cert="high" reason="lost">πεποίημαι τὴν</supplied> 

    <lb n="13"/>διάλυσιν καὶ οὐκ ἐγκ<supplied cert="high" reason="lost">αλ</supplied>ῶ οὔτε ἐγκαλέσω <supplied cert="high" reason="lost">σοι καὶ ὤμοσα</supplied> 

    <lb n="14"/>τὸν θεῖον ὅρκον ὡς <expan>πρόκ<ex cert="high">ειται</ex></expan>. <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> Ταυρῖνος Ε<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="15"/>ἀξιωθεὶς ἔγραψα ὑπὲρ αὐτοῦ παρόντος <expan>γράμμ<ex cert="high">ατα</ex></expan> <unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ὴ εἰδότος.</supplied> 

    <lb n="16"/><gap extent="4" reason="illegible" unit="character"/>ων <g type="stauros"/> δι᾽ ἐμοῦ Φοιβάμμωνος ἐγράφη. 
