    <lb n="1"/>Αὐρηλίᾳ <unclear reason="undefined" cert="high">Δ</unclear><supplied reason="lost" cert="high">ημη</supplied>τρίᾳ <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ῇ καὶ Ἀμμωνί</supplied>ᾳ καὶ ὡς <expan>χρηματίζ<ex cert="high">ει</ex></expan> ἀπὸ 

    <lb n="2"/>Ἑρμοῦ π<supplied reason="lost" cert="high">όλε</supplied><unclear reason="undefined" cert="high">ω</unclear>ς τῆς λ<unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">μπροτάτη</supplied><unclear reason="undefined" cert="high">ς</unclear> 

    <lb n="3"/>παρὰ Αὐ<unclear reason="undefined" cert="high">ρ</unclear><supplied reason="lost" cert="high">ηλίου Σα</supplied><unclear reason="undefined" cert="high">ρ</unclear>απίωνος Αρ<gap reason="lost" extent="3" unit="character"/><gap reason="illegible" extent="1" unit="character"/>τίου καταγινομένου 

    <lb n="4"/>ἐν κώμ<supplied reason="lost" cert="high">ῃ Μα</supplied><unclear reason="undefined" cert="high">γδ</unclear>ώλων Βουκόλων τοῦ Ἑρμοπολίτου. 

    <lb n="5"/>ὁμολογ<unclear reason="undefined" cert="high">ῶ</unclear> <supplied reason="lost" cert="high">ἐξει</supplied><unclear reason="undefined" cert="high">λ</unclear>ηφέναι παρὰ σοῦ πρὸς μίσθωσιν ἐπὶ τετρα-

    <lb n="6"/>ετῆ χρόνο<supplied reason="lost" cert="high">ν λογ</supplied><unclear reason="undefined" cert="high">ι</unclear>ζόμενον ἀπὸ τοῦ ὄντος μηνὸς Μεχεὶρ 

    <lb n="7"/><num value="5">ε</num> νέας ἰν<supplied reason="lost" cert="high">δικτί</supplied><unclear reason="undefined" cert="high">ω</unclear>νος, καρπῶν τῆς εὐτυχοῦς ἕκτη<unclear reason="undefined" cert="high">ς</unclear> έας ἤτοι 

    <lb n="8"/><num value="20">κ</num> ἰνδικτί<unclear reason="undefined" cert="high">ω</unclear><supplied reason="lost" cert="high">νο</supplied>ς τὸν ὑπάρχοντά σοι περὶ τ<unclear reason="undefined" cert="high">ὴν</unclear> αὐτὴν κώμην 

    <lb n="9"/>Μαγδώλων Βουκόλων <subst><add place="inline"><choice><corr>ἐλαιωνοφοινικοπαράδεισον</corr><sic>ελαιωνοφοινικοπαραδισον</sic></choice></add><del rend="corrected" status="unremarkable">ελαιονοφοινικοπαραδεισον</del></subst> <add place="supralinear"><unclear reason="undefined" cert="high">ἣ</unclear>ν  προ<unclear reason="undefined" cert="high">εῖ</unclear>χον</add> ἐπὶ 

    <lb n="10"/>τῷ πρὸς ἀλλήλους συμπεφωνημένῳ φόρῳ τῷ μὲν πρώ-

    <lb n="11"/>τῳ ἐνιαυτῷ ἐλαιῶν <choice><corr>μελα<supplied reason="omitted" cert="high">ι</supplied>νῶν</corr><sic>μελανω<unclear reason="undefined" cert="high">ν</unclear></sic></choice> <supplied reason="lost" cert="high">ἀρ</supplied>τάβην μίαν ἥμισυ <num value="1  1/2 "/> καὶ 

    <lb n="12"/>φοινίκων <choice><corr>καρυωτῶν</corr><sic>κα<unclear reason="undefined" cert="high">ροι</unclear>ωτων</sic></choice> καὶ πατ<unclear reason="undefined" cert="high">η</unclear><supplied reason="lost" cert="high">τῶν</supplied> κατὰ τὸ ἥμισυ ἀρτάβην 

    <lb n="13"/>μίαν <num value="1"/> καὶ <choice><corr>σφυρίδα</corr><sic>σπυρ<supplied reason="lost" cert="high">ιδα</supplied></sic></choice> <supplied reason="lost" cert="high">ἐννεάπλοκο</supplied>ν καὶ ἄλλην πεντάπλοκον, 

    <lb n="14"/>ἐπὶ δὲ τὸν ἑξῆ<supplied reason="lost" cert="high">ς τριετῆ χόνον κατ᾽ ἔ</supplied>τος ἐλαιῶν ἀρτάβας δύο <num value="2"/> 

    <lb n="15"/>καὶ φοινίκων <supplied reason="lost" cert="high">ξηρῶν καὶ π</supplied>ατητῶν ἀρτάβην μίαν <num value="1"/> καὶ ἀργυρίου 

    <lb n="16"/>Σεβαστῶν <unclear reason="undefined" cert="high">ν</unclear><supplied reason="lost" cert="high">ομίσματο</supplied>ς τάλαντα <supplied reason="lost" cert="high">ἓ</supplied><unclear reason="undefined" cert="high">ξ</unclear> <num value="6"/> καὶ τὰ αὐτὰ <choice><corr>σφυρίδια</corr><sic>σπυριδια</sic></choice> δύο <num value="2"/>, 

    <lb n="17"/>ἅπερ προ<unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">είμενα ἀπ</supplied><unclear reason="undefined" cert="high">ο</unclear>δώσω ἐν τῷ δέοντι καιρῷ ἕκαστον 

    <lb n="18"/>ὡς πρόκει<unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">αι ἀνυπερθ</supplied>έτως καὶ τὴν δέουσαν γεουχικὴν 

    <lb n="19"/><hi rend="diaeresis">ὑ</hi>πηρε<supplied reason="lost" cert="high">σίαν </supplied><gap reason="lost" extent="9" unit="character"/><unclear reason="undefined" cert="high">ω</unclear>ς ποι<supplied reason="lost" cert="high">ή</supplied>σομαι ἀπὸ τῶν ὄντων <choice><corr>ἐκεῖσε</corr><sic>εκισε</sic></choice> 

    <lb n="20"/>παντοίων <supplied reason="lost" cert="high">ἐπὶ δὲ τῷ</supplied> <unclear reason="undefined" cert="high">τ</unclear>έλει τοῦ χρόνου παραδώσω σοι τὸν αὐτὸν 

    <lb n="21"/><choice><corr>παράδεισον</corr><sic>παραδισον</sic></choice> <unclear reason="undefined" cert="high">ἐ</unclear><supplied reason="lost" cert="high">ν τῇ κρατί</supplied>στῃ ἐπιμελείᾳ καθαρὸν ἀπὸ <choice><corr>θρύων</corr><sic>θροιων</sic></choice> καὶ 

    <lb n="22"/><choice><corr>δείσης</corr><sic>δισης</sic></choice> πάση<supplied reason="lost" cert="high">ς καὶ τὰ φυτ</supplied><unclear reason="undefined" cert="high">ὰ</unclear> εὐθαλοῦντα καὶ τὰ <choice><corr>τείχη</corr><sic>τιχη</sic></choice> συν<unclear reason="undefined" cert="high">εσ</unclear>τῶτα ὡς παρέ-

    <lb n="23"/>λαβον καὶ τὸ <unclear reason="undefined" cert="high">ἀ</unclear>ρ<supplied reason="lost" cert="high">γύρι</supplied>ον, τῶν δημοσίων τελεσμάτων πάντων ὄν-

    <lb n="24"/>των πρὸς σ<supplied reason="lost" cert="high">ὲ τὴν</supplied> γεοῦχον. ἡ μίσθωσις κυρία καὶ <expan>ἐπερωτηθ<ex cert="high">εὶς</ex></expan> <expan>ὡμολόγ<ex cert="high">ησα</ex></expan>. 

    <lb n="25"/><hi rend="diaeresis">ὑ</hi>πατείας <unclear reason="undefined" cert="high">Π</unclear><supplied reason="lost" cert="high">απίου Π</supplied>ακατιανοῦ τοῦ λαμπροτάτου ἐπάρχου τοῦ 

    <lb n="26"/>ἱεροῦ <supplied reason="lost" cert="high">πραιτωρίου κα</supplied><unclear reason="undefined" cert="high">ὶ</unclear> Μεκιλίου Ἱλαριανοῦ τοῦ λαμπροτάτου 

    <lb n="27"/><supplied reason="lost" cert="high">Μεχεὶρ </supplied><gap reason="lost" extent="2" unit="character"/><supplied reason="lost" cert="high"> Αὐρήλιος Σαρ</supplied>απίων ὁ προκείμενος μεμίσθωμαι ὡς <expan>πρόκ<ex cert="high">ειται</ex></expan>. 

    <lb n="28"/><handShift new="m2"/> [ <gap reason="illegible" extent="unknown" unit="character"/> <expan>ἔγρ<ex cert="high">αψα</ex></expan> <expan>ὑ<ex cert="high">πὲρ</ex></expan> αὐτοῦ <expan>γρ]ά<ex cert="high">μματα</ex></expan> μὴ <expan><ex cert="high">εἰδότος</ex></expan>. </ab></div><div n="meta.18176" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.18176" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" notBefore="0332-01-27" notAfter="0332-02-25">27. Jan. - 25. Febr. 332</date></p></div><div n="meta.18176" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Gehört zum Dossier der Aurelia Demetria alias Ammonia.</p></div><div n="meta.18176" type="commentary" subtype="mentionedDates" org="uniform" part="N" sample="complete"><head>Erwähnte Daten</head></div><div n="meta.18176" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">SB</title><biblScope type="volume">14</biblScope><biblScope type="numbers">11711</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">18176</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0239</series>; <biblScope type="volume">14</biblScope>; <biblScope type="numbers">11711</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0239;14;11711" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.18176" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p><bibl type="illustration" default="NO">P.Vindob.Bosw. III und IV</bibl></p></div><div n="meta.18176" type="bibliography" subtype="otherPublications" org="uniform" part="N" sample="complete"><head>Andere Publikation</head><bibl type="pub