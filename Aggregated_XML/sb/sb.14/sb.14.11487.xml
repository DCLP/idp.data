    <lb n="21"/><supplied cert="high" reason="lost">τὸν Ψοῖφ</supplied><unclear cert="high" reason="undefined">ι</unclear><supplied cert="high" reason="lost">ν, ὃν καὶ</supplied> <unclear cert="high" reason="undefined">ἔχε</unclear><supplied cert="high" reason="lost">ι</supplied><unclear cert="high" reason="undefined">ν</unclear> παρὰ <unclear cert="high" reason="undefined">το</unclear>ῦ <unclear cert="high" reason="undefined">Εὐ</unclear><supplied cert="high" reason="lost">τύχου</supplied> 

