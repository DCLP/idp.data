    <lb n="1"/><supplied reason="lost" cert="high">χαίρ</supplied><unclear reason="undefined" cert="high">ο</unclear>ις κύριέ μου Θέων <expan>π<ex cert="high">αρὰ</ex></expan> Χαιρήμονος. 

    <lb n="2"/><supplied reason="lost" cert="high">ἔγρ</supplied><unclear reason="undefined" cert="high">α</unclear>ψα σοι ὡς <choice><corr>εἰληφὼς</corr><sic><hi rend="diaeresis">ι</hi>ληφως</sic></choice> <choice><corr>τιμὴν</corr><sic>τει<unclear reason="undefined" cert="high">μ</unclear>η<add place="supralinear">ν</add></sic></choice> δύο <num value="2"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/> ταλάντων πίσσης παρὰ τοῦ 

    <lb n="4"/><supplied reason="lost" cert="high">υἱο</supplied>ῦ του κεραμέως, παρόντος καὶ 

    <lb n="5"/><supplied reason="lost" cert="high">Π</supplied><unclear reason="undefined" cert="high">α</unclear>μφίλου. μὴ <choice><corr>ἀντειπόντος</corr><sic>αντιποντος</sic></choice> <supplied reason="lost" cert="high">ο</supplied>ὖ<unclear reason="undefined" cert="high">ν</unclear> 

    <lb n="6"/><supplied reason="lost" cert="high">τ</supplied>οῦ κεραμέως, <choice><corr>ἀπέστη</corr><sic>απεσστη</sic></choice> ὡς τη-

    <lb n="7"/><gap reason="lost" extent="1" unit="character"/>ης ἐρχομένου παραλαβεῖν 

    <lb n="8"/><supplied reason="lost" cert="high">τὴ</supplied>ν πίσσαν. ἐλθόντος οὖν 

    <lb n="9"/><supplied reason="lost" cert="high">το</supplied>ῦ Σερήν<supplied reason="lost" cert="high">ου</supplied> <subst><add place="inline">ἑτ<add place="supralinear">οί</add>μου</add><del rend="corrected" status="unremarkable">ετυμου</del></subst> ἐπὶ τὴν 

