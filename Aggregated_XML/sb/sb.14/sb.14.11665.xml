    <lb n="1"/>Ἀχιλλεὺς Θέωνι τ<supplied cert="high" reason="lost">ῷ ἀδελφῷ</supplied> 

    <lb n="2"/>χαίρειν.   <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>ὅτι με οὐδὲ ὁ χρόν<supplied cert="high" reason="lost">ος τῆς ἀπο</supplied>-

    <lb n="4"/>δημίας ἐπιλαθέ<unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">θαι σου πε</supplied>-

    <lb n="5"/>ποίηκεν δειγνυ<gap extent="1" reason="illegible" unit="character"/><gap extent="8" reason="lost" unit="character"/> 

    <lb n="6"/>ἀπὸ τῶν τοῦ ἀδελ<unclear cert="high" reason="undefined">φ</unclear><supplied cert="high" reason="lost">οῦ γραμμά</supplied>-

    <lb n="7"/>των ἐν οἷς σε πολλ<unclear cert="high" reason="undefined">άκ</unclear><supplied cert="high" reason="lost">ις προσα</supplied>-

    <lb n="8"/>γορεύω μετὰ τῶν <supplied cert="high" reason="lost">ἐμῶν πάντων·</supplied> 

    <lb n="9"/>ἐδήλωσα δὲ καὶ ν<supplied cert="high" reason="lost">ῦν </supplied><gap extent="7" reason="lost" unit="character"/> 

    <lb n="10"/>τὸν τοῦ Πανφίλου <gap extent="5" reason="lost" unit="character"/><supplied cert="high" reason="lost"> πεμ</supplied>-

    <lb n="11"/>ψάμενος μὲν καὶ α<gap extent="5" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀπο</supplied>-

    <lb n="12"/>δημῆσαι ἐμοὶ σὺν <gap extent="8" reason="lost" unit="character"/> 

    <lb n="13a"/><handShift new="m2"/> τησάμενον δὲ τοῦτο 

    <lb n="13"/><del rend="erasure" status="unremarkable">ματε</del> ἀξιώσας ων<gap extent="9" reason="lost" unit="character"/> 

    <lb n="14"/>τησαι παρ᾽ ἐμοῦ <gap extent="1" reason="illegible" unit="character"/><gap extent="9" reason="lost" unit="character"/> 

    <lb n="15"/>τως <unclear cert="high" reason="undefined">ὡ</unclear>ς οὐκ <unclear cert="high" reason="undefined">ὀ</unclear>ν<unclear cert="high" reason="undefined">τ</unclear><gap extent="10" reason="lost" unit="character"/> 

    <lb n="16"/>ως ἀλλὰ Θέωνος <gap extent="1" reason="illegible" unit="character"/><gap extent="9" reason="lost" unit="character"/> 

    <lb n="17"/>λεως ἐπισταμενο<gap extent="8" reason="lost" unit="character"/> 

    <lb n="18"/>εὔνοιαν τὴν ἐμὴν <gap extent="3" reason="lost" unit="character"/><supplied cert="high" reason="lost"> πολλά</supplied>-


    <lb n="19"/>κις γράφε καὶ ὅσα βούλ<supplied cert="high" reason="lost">ῃ· εὔ</supplied>-

    <lb n="20"/>χομαι δὲ σέ τε καὶ <gap extent="1" reason="illegible" unit="character"/><gap extent="7" reason="lost" unit="character"/> 

    <lb n="21"/>μηκέτι μόνον <unclear cert="high" reason="undefined">ἀλ</unclear><supplied cert="high" reason="lost">λὰ </supplied><gap extent="7" reason="lost" unit="character"/> 

    <lb n="22"/>ἤδη καὶ διὰ τὸν <gap extent="10" reason="lost" unit="character"/> 

    <lb n="23"/>λίας γενέσθαι αδ<gap extent="8" reason="lost" unit="character"/> 

    <lb n="24"/>πρὸς αὐτὸν απ<gap extent="11" reason="lost" unit="character"/> 

    <lb n="25"/>τάξιν τὴν ἐμ<supplied cert="high" reason="lost">ὴν </supplied><gap extent="10" reason="lost" unit="character"/> 

    <lb n="26"/><handShift new="m2"/> γάρου καὶ τὰ <hi rend="diaeresis">ὑ</hi>ελὰ κα<supplied cert="high" reason="lost">ὶ </supplied><gap extent="6" reason="lost" unit="character"/> 

    <lb n="27"/>καὶ αὐτὸν ἔθελον <gap extent="1" reason="illegible" unit="character"/><gap extent="7" reason="lost" unit="character"/> 

    <lb n="28"/>νησασθαι προσα<unclear cert="high" reason="undefined">γο</unclear><supplied cert="high" reason="lost">ρεύω τὸν πα</supplied>-

    <lb n="29"/>τέρα Πάμφιλον κα<supplied cert="high" reason="lost">ὶ </supplied><gap extent="7" reason="lost" unit="character"/> 

    <lb n="30"/>τας με τοῦ πολὺ ε<gap extent="1" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/><supplied cert="high" reason="lost"> θυγα</supplied>-

    <lb n="31"/>τέρας εὐτυχεῖς. ἔρ<supplied cert="high" reason="lost">ρωσο, ἄδελ</supplied>-

    <lb n="32"/>φε, εὐτυχῶν <supplied cert="high" reason="lost">σὺν παντὶ</supplied> 

    <lb n="33"/>γένε<unclear cert="high" reason="undefined">ι</unclear>.     <gap extent="unknown" reason="lost" unit="character"/> 
