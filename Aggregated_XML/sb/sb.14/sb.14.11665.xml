    <lb n="22"/>ἤδη καὶ διὰ τὸν <gap reason="lost" extent="10" unit="character"/> 

    <lb n="23"/>λίας γενέσθαι αδ<gap reason="lost" extent="8" unit="character"/> 

    <lb n="24"/>πρὸς αὐτὸν απ<gap reason="lost" extent="11" unit="character"/> 

    <lb n="25"/>τάξιν τὴν ἐμ<supplied reason="lost" cert="high">ὴν </supplied><gap reason="lost" extent="10" unit="character"/> 

    <lb n="26"/><handShift new="m2"/> γάρου καὶ τὰ <hi rend="diaeresis">ὑ</hi>ελὰ κα<supplied reason="lost" cert="high">ὶ </supplied><gap reason="lost" extent="6" unit="character"/> 

    <lb n="27"/>καὶ αὐτὸν ἔθελον <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="7" unit="character"/> 

    <lb n="28"/>νησασθαι προσα<unclear reason="undefined" cert="high">γο</unclear><supplied reason="lost" cert="high">ρεύω τὸν πα</supplied>-

    <lb n="29"/>τέρα Πάμφιλον κα<supplied reason="lost" cert="high">ὶ </supplied><gap reason="lost" extent="7" unit="character"/> 

    <lb n="30"/>τας με τοῦ πολὺ ε<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="3" unit="character"/><supplied reason="lost" cert="high"> θυγα</supplied>-

    <lb n="31"/>τέρας εὐτυχεῖς. ἔρ<supplied reason="lost" cert="high">ρωσο, ἄδελ</supplied>-

