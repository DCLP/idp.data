    <lb n="1"/>Ἀχιλλεὺς Θέωνι τ<supplied reason="lost" cert="high">ῷ ἀδελφῷ</supplied> 

    <lb n="2"/>χαίρειν.   <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>ὅτι με οὐδὲ ὁ χρόν<supplied reason="lost" cert="high">ος τῆς ἀπο</supplied>-

    <lb n="4"/>δημίας ἐπιλαθέ<unclear reason="undefined" cert="high">σ</unclear><supplied reason="lost" cert="high">θαι σου πε</supplied>-

    <lb n="5"/>ποίηκεν δειγνυ<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="8" unit="character"/> 

    <lb n="6"/>ἀπὸ τῶν τοῦ ἀδελ<unclear reason="undefined" cert="high">φ</unclear><supplied reason="lost" cert="high">οῦ γραμμά</supplied>-

    <lb n="7"/>των ἐν οἷς σε πολλ<unclear reason="undefined" cert="high">άκ</unclear><supplied reason="lost" cert="high">ις προσα</supplied>-

    <lb n="8"/>γορεύω μετὰ τῶν <supplied reason="lost" cert="high">ἐμῶν πάντων·</supplied> 

    <lb n="9"/>ἐδήλωσα δὲ καὶ ν<supplied reason="lost" cert="high">ῦν </supplied><gap reason="lost" extent="7" unit="character"/> 

    <lb n="10"/>τὸν τοῦ Πανφίλου <gap reason="lost" extent="5" unit="character"/><supplied reason="lost" cert="high"> πεμ</supplied>-

    <lb n="11"/>ψάμενος μὲν καὶ α<gap reason="lost" extent="5" unit="character"/><supplied reason="lost" cert="high"> ἀπο</supplied>-

    <lb n="12"/>δημῆσαι ἐμοὶ σὺν <gap reason="lost" extent="8" unit="character"/> 

    <lb n="13a"/><handShift new="m2"/> τησάμενον δὲ τοῦτο 

    <lb n="13"/><del rend="erasure" status="unremarkable">ματε</del> ἀξιώσας ων<gap reason="lost" extent="9" unit="character"/> 

    <lb n="14"/>τησαι παρ᾽ ἐμοῦ <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="9" unit="character"/> 

    <lb n="15"/>τως <unclear reason="undefined" cert="high">ὡ</unclear>ς οὐκ <unclear reason="undefined" cert="high">ὀ</unclear>ν<unclear reason="undefined" cert="high">τ</unclear><gap reason="lost" extent="10" unit="character"/> 

    <lb n="16"/>ως ἀλλὰ Θέωνος <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="9" unit="character"/> 

    <lb n="17"/>λεως ἐπισταμενο<gap reason="lost" extent="8" unit="character"/> 

    <lb n="18"/>εὔνοιαν τὴν ἐμὴν <gap reason="lost" extent="3" unit="character"/><supplied reason="lost" cert="high"> πολλά</supplied>-


    <lb n="19"/>κις γράφε καὶ ὅσα βούλ<supplied reason="lost" cert="high">ῃ· εὔ</supplied>-

    <lb n="20"/>χομαι δὲ σέ τε καὶ <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="7" unit="character"/> 

