    <lb n="1"/>βασιλευόντων Πτολεμαίου τοῦ Πτολεμαίου καὶ βασιλίσσης Κλεοπάτρας τῆς ἀδελφῆς κ<unclear reason="undefined" cert="high">αὶ</unclear> <supplied reason="lost" cert="high">α</supplied><unclear reason="undefined" cert="high">σιλίσσης</unclear> ε<unclear reason="undefined" cert="high">ο</unclear>πάτρα<supplied reason="lost" cert="high">ς τῆς γυναικὸς</supplied>

    <lb n="2"/>θεῶν Εὐεργετῶν ἔτους <choice><corr><num value="38">λη</num></corr><sic>ηλ</sic></choice>· Μεχεὶρ <num value="1">α</num>. κατεμετρήθη σταθμὸς ἐν τῆι ἐκτισ<unclear reason="undefined" cert="high">μένηι</unclear> ι Εὐεργέτι<supplied reason="lost" cert="high">δι ὑπὸ Βοήθου τοῦ</supplied>

    <lb n="3"/>συγγενο<unclear reason="undefined" cert="high">ῦ</unclear>ς καὶ ἐπιστρατήγου καὶ στρατηγοῦ τῆς <unclear reason="undefined" cert="high">Θ</unclear>ηβαίδος καὶ κτίστου διὰ Πα<unclear reason="undefined" cert="high">ν</unclear><supplied reason="lost" cert="high">ί</supplied><unclear reason="undefined" cert="high">σκου</unclear> οδότου καὶ ἀντ<supplied reason="lost" cert="high">ιγραφέως</supplied>

    <lb n="4"/><add place="supralinear">Τανούπει Τφεώφιος τῆ<unclear reason="undefined" cert="high">ι</unclear> διατεταγμένῃ ἐν τῆι τετραγώνωι καπηλίδι</add> εἰ<add place="supralinear">ς</add> οἰ<unclear reason="undefined" cert="high">κ</unclear>ητήριον μὴ τασ<unclear reason="undefined" cert="high">σο</unclear>μένωι ἐμβαδὸν μηδ᾽ εἰς τὸ ἐγκύκλιον τέλος μηδ᾽ ἄλλ<unclear reason="undefined" cert="high">ο</unclear>  ἁπλῶς, ἀλλ᾽ ἔχ<supplied reason="lost" cert="high">ειν κυρίως τὴν</supplied>

    <lb n="5"/>κτῆσιν ἰδιόκτητον· ἐξέσται δ᾽ αὐτῶι ἀπ<supplied reason="lost" cert="high">ο</supplied>λείπειν ἐγγό<supplied reason="lost" cert="high">ν</supplied>οις καὶ τῇ ἰδίᾳ <unclear reason="undefined" cert="high">γυναικὶ</unclear>   οῖς ἔγγιστα γένο<supplied reason="lost" cert="high">υς ἢ οἷς ἐὰν αἱρῆται</supplied>

    <lb n="6"/><add place="supralinear">ἐν τῆι πόλει ἐν <expan>στρα<ex cert="high">τείᾳ</ex></expan> φερ<unclear reason="undefined" cert="high">ο</unclear>μ<unclear reason="undefined" cert="high">έ</unclear>νων</add> τούτων ὁμ<unclear reason="undefined" cert="high">ο</unclear>ί<unclear reason="undefined" cert="high">ω</unclear>ς μηδὲν <unclear reason="undefined" cert="high">τ</unclear>ασσομένων μήτ<unclear reason="undefined" cert="high">ε</unclear> τῶν μεταλαμ<supplied reason="lost" cert="high">β</supplied>ανόντων μήτε τῶν <unclear reason="undefined" cert="high">παραχ</unclear>ωρούντων κατὰ <supplied reason="lost" cert="high">μηδεμίαν</supplied>

    <lb n="7"/>παρεύρεσιν ἀκολούθως τοῖς ὑπὸ κτίστου ἐπικεχωρη<unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">έ</supplied><unclear reason="undefined" cert="high">ν</unclear>οις πηχῶν <unclear reason="undefined" cert="high">σ</unclear>τερεῶν δύ<unclear reason="undefined" cert="high">ο</unclear>, <expan><ex cert="high">γίνονται</ex></expan> <expan>π<ex cert="high">ήχεις</ex></expan> <num value="2">β</num>, ὧν μέτ<supplied reason="lost" cert="high">ρα νότου ἐπὶ βορρᾶ <expan>π<ex cert="high">ήχεις</ex></expan> <num value="20">κ</num>,</supplied>

    <lb n="8"/>λιβὸς ἐπ᾽ ἀπηλιώτην <expan>π<ex cert="high">ήχεις</ex></expan> <num value="10">ι</num>· γείτονες <unclear reason="undefined" cert="high">νότ</unclear>ου <handShift new="m2"/>Πετοσίρ<supplied reason="lost" cert="high">ιο</supplied><unclear reason="undefined" cert="high">ς</unclear> <choice><corr>χρυσοχόου</corr><sic>χρυσοχ<unclear reason="undefined" cert="high">ωο</unclear>υ</sic></choice>, βορρᾶ οἱ <expan><unclear reason="undefined" cert="high">λ</unclear>ο<ex cert="high"><unclear reason="undefined" cert="high">ιποὶ</unclear></ex></expan> τόποι, ἀπ<supplied reason="omitted" cert="high">η</supplied>λιώτου <supplied reason="lost" cert="high">στοά, λιβὸς</supplied> ῥύ<unclear reason="undefined" cert="high">μη</unclear> <expan>π<ex cert="high">ηχῶν</ex></expan> <num value="10">ι</num>.
