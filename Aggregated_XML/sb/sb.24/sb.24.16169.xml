    <lb n="1"/>ἔτους <choice><corr><num value="562">φξβ</num></corr><sic>βξφ</sic></choice> ἀπέδο<supplied reason="omitted" cert="high">ψο</supplied> Αβσαλμας Αβιδρωδακου τὴν ὑπάρχου-

    <lb n="2"/>σαν αὐτῷ δούλην ὀνόματι Ιμμεδαβου <expan>ἀ<unclear reason="undefined" cert="high">ρ</unclear><ex cert="low">γυρώνητον</ex></expan>· ἐπρίατο <unclear reason="undefined" cert="high">Σα</unclear><supplied reason="lost" cert="high">μ</supplied><unclear reason="undefined" cert="high">σ</unclear><supplied reason="lost" cert="high">α</supplied><unclear reason="undefined" cert="high">ι</unclear><supplied reason="lost" cert="high">ος</supplied>

    <lb n="3"/><supplied reason="lost" cert="high">Τε</supplied><unclear reason="undefined" cert="high">ιατος</unclear> <choice><corr>Ἀβουρηνὸς</corr><sic><unclear reason="undefined" cert="high">αβουρ</unclear>ηνου</sic></choice> <expan><ex cert="high">δηναρίων</ex></expan> <num value="700">ψ</num>.

    <lb n="4"/>ἐπὶ ὑπάτων τῶν κυρίων ἡμῶν Αὐτοκρατόρων Καισάρων Τραι-

    <lb n="5"/>ανοῦ Δεκίου Αὐγούστου τὸ <num value="3">γ</num> καὶ Δεκίου καὶ Κυίντου υἱῶν αὐτοῦ

    <lb n="6"/>τ<unclear reason="undefined" cert="high">ῶν</unclear> ν Εὐ<unclear reason="undefined" cert="high">σε</unclear>βῶν ἀνικήτων Σεβαστῶν ἡγεμονίας, κατὰ δὲ

    <lb n="7"/><unclear reason="undefined" cert="high">τὸν</unclear> πρ<unclear reason="undefined" cert="high">ό</unclear>τερον ἀριθμὸν ἔτους <choice><corr><num value="562">φξβ</num></corr><sic>βξφ</sic></choice> μηνὸς <choice><corr>Αὐδναίου <num value="27">κζ</num></corr><sic>αιδυναιου ζκ</sic></choice> ἐν Βηφου-

    <lb n="8"/>ραιᾳ κώμῃ ἐπὶ τῶν ὑπογεγραμμένων μαρτύρων. ἀπέδοτο

    <lb n="9"/><unclear reason="undefined" cert="high">Α</unclear>βσα<unclear reason="undefined" cert="high">λ</unclear>μας Αβιδρωδακου <del rend="erasure" status="unremarkable">κώμης</del> κώμης Βηαθαγαης τῆς Ἀ-

    <lb n="10"/>βουρηνῆς π<unclear reason="undefined" cert="high">ε</unclear>ριχώρου Θεγαναβων τὴν ὑπάρχουσαν αὐτῷ δούλην

    <lb n="11"/>ἀρ<supplied reason="lost" cert="high">γ</supplied><unclear reason="undefined" cert="high">υ</unclear>ρώνητον ὀνόματι Ιμμεδαβου ἢ καὶ εἴ τινι ἑτέρῳ ὀνόματι κα-

    <lb n="12"/><unclear reason="undefined" cert="high">λεῖται</unclear>  <choice><corr>κληθήσεται</corr><sic>κληθησηται</sic></choice>, <choice><corr>γένει</corr><sic>γενι</sic></choice> περιχώραν Νεσιβει, οὖσα<supplied reason="omitted" cert="high">ν</supplied> ὡς ἐτῶν δεκατριῶ<unclear reason="undefined" cert="high">ν</unclear> <num value="13"/>

    <lb n="13"/><expan><unclear reason="undefined" cert="high">μ</unclear><ex cert="high">ικρῷ</ex></expan> <expan><unclear reason="undefined" cert="high">π</unclear><ex cert="high">λέον</ex></expan> <expan><unclear reason="undefined" cert="high">ἥ</unclear><ex cert="high">ττω</ex></expan>, λευκόχρουν, <choice><corr>στρογγυλοπρόσωπον</corr><sic>στρονγυλοπροσωπον</sic></choice>, εὔοφρυν, εὐόφθαλμον,

    <lb n="14"/><choice><corr>εὐθύρινα</corr><sic>ευθυρεινα</sic></choice>, ἐωνημένην <unclear reason="undefined" cert="high">δὲ</unclear> αὐτῷ κατὰ καταγραφὴν ἣν ἐπέδειξεν

    <lb n="15"/>γ<supplied reason="lost" cert="high">εγεν</supplied><unclear reason="undefined" cert="high">η</unclear>μ<unclear reason="undefined" cert="high">έ</unclear>ν<unclear reason="undefined" cert="high">η</unclear>ν ἐν <choice><corr>Σεπτιμίᾳ</corr><sic>σεπτιμμια</sic></choice> κολωνίᾳ <choice><corr>μητροπόλει</corr><sic>μητροπολι</sic></choice> Νεσιβει τῇ πρὸ

    <lb n="16"/><unclear reason="undefined" cert="high">πέντε</unclear> <num value="5"/> Καλανδῶν Μαίων παρὰ <choice><corr>Σεπτιμίου Σατορνίλου</corr><sic>σεπτιμμιου σατορνειλου</sic></choice> Πρόκλου

    <lb n="17"/><choice><corr>Νεσιβηνοῦ</corr><sic><unclear reason="undefined" cert="high">νεσ</unclear>ειβηνου</sic></choice>, ἣν καὶ τὴν πρ<unclear reason="undefined" cert="high">ο</unclear>τέρ<unclear reason="undefined" cert="high">α</unclear>ν <choice><corr>συγγραφὴν</corr><sic>συνγραφην</sic></choice> ἔδωκεν ὁ ἀποδόμενος

    <lb n="18"/><unclear reason="undefined" cert="high">τῷ</unclear> <supplied reason="lost" cert="high">νη</supplied><unclear reason="undefined" cert="high">μ</unclear>ένῳ <unclear reason="undefined" cert="high">εἰς</unclear> λ<unclear reason="undefined" cert="high">ει</unclear>α<unclear reason="undefined" cert="high">ν</unclear> <gap reason="illegible" extent="2" unit="character"/><unclear reason="undefined" cert="high">ια</unclear>ν ἐξ ἐπιτροπικῆς <choice><corr>δοθείσης</corr><sic>δοθεισας</sic></choice> τῷ αὐτῷ <choice><corr>Σατορ-

    <lb n="19"/>νίλ<supplied reason="lost" cert="high">ῳ</supplied></corr><sic>σατορνειλ<supplied reason="lost" cert="high">ω</supplied></sic></choice><gap reason="lost" extent="13" unit="character"/><unclear reason="undefined" cert="high">αε</unclear><gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">ν</unclear>μους<gap reason="lost" extent="3" unit="character"/><supplied reason="lost" cert="high"/>· ἐπρίατο τὴν αὐτ<unclear reason="undefined" cert="high">ὴν</unclear> ούλην Σαμ-

    <lb n="20"/>σαιος Τ<unclear reason="undefined" cert="high">ε</unclear>ιατος τῶν ἀπ<unclear reason="undefined" cert="high">ὸ</unclear> <supplied reason="lost" cert="high">κ</supplied>ώ<unclear reason="undefined" cert="high">μη</unclear>ς Βαναθσαμσων <unclear reason="undefined" cert="high">τῆ</unclear>ς Ἀβουρηνῆς

    <lb n="21"/><choice><corr>τιμῆς</corr><sic><unclear reason="undefined" cert="high">τ</unclear>ειμης</sic></choice> ἀργυρίου δηνα<unclear reason="undefined" cert="high">ρ</unclear>ίων <choice><corr>ἑπτακοσίων</corr><sic><unclear reason="undefined" cert="high">ε</unclear>πτακοσσιων</sic></choice> <num value="700"/>, ἣν <choice><corr>τιμὴν</corr><sic>τειμην</sic></choice> κομισάμ<unclear reason="undefined" cert="high">ε</unclear>-

    <lb n="22"/><unclear reason="undefined" cert="high">νο</unclear>ς <unclear reason="undefined" cert="high">ὁ</unclear> ἀ<unclear reason="undefined" cert="high">πο</unclear>δ<unclear reason="undefined" cert="high">ό</unclear>μ<unclear reason="undefined" cert="high">ε</unclear>νος παρὰ τοῦ ἐωνημένου παρέδωκεν αὐτῷ τὴν αὐ-

    <lb n="23"/>τὴν <unclear reason="undefined" cert="high">δού</unclear>λην εἰς τὸ ἔχει<unclear reason="undefined" cert="high">ν</unclear> αὐτὴν καὶ κτᾶσθαι, χρᾶσθαι, πωλεῖν, διοικεῖν τρό-

    <lb n="24"/><unclear reason="undefined" cert="high">πῳ</unclear>   ἂν <unclear reason="undefined" cert="high">αἱ</unclear>ρ<unclear reason="undefined" cert="high">ῆ</unclear>ται καὶ ἀνε<unclear reason="undefined" cert="high">δέ</unclear>ξα<unclear reason="undefined" cert="high">το</unclear>  ἀποδόμενος ὅπως ἐάν τις <choice><corr>ἐμποιηθῇ</corr><sic>ενποιηθη</sic></choice>

    <lb n="25"/>ἢ <choice><corr>ἐγκαλέσῃ</corr><sic>ενκα<unclear reason="undefined" cert="high">λ</unclear>εση</sic></choice> <unclear reason="undefined" cert="high">πε</unclear>ρὶ τῆς <choice><corr>πεπραμένης</corr><sic><unclear reason="undefined" cert="high">π</unclear>επραμμενης</sic></choice> δούλης ἢ μέρους αὐτῆς, αὐτὸν

    <lb n="26"/><unclear reason="undefined" cert="high">στ</unclear>άντα δι<unclear reason="undefined" cert="high">εκδ</unclear>ικήσειν καὶ <unclear reason="undefined" cert="high">κ</unclear>αθαροποιήσειν καὶ παρα<unclear reason="undefined" cert="high">δ</unclear>ώσειν τ<unclear reason="undefined" cert="high">ῷ</unclear> ωνημ<unclear reason="undefined" cert="high">έ</unclear>-

    <lb n="27"/>νῳ· ἐὰν <unclear reason="undefined" cert="high">δ</unclear>ὲ μ<unclear reason="undefined" cert="high">ὴ</unclear> <choice><corr>καθαροποιήσῃ</corr><sic>καθαροποιησει</sic></choice>, ἀποτίσει ἣν εἴληφεν <choice><corr>τιμὴν</corr><sic>τειμ<supplied reason="lost" cert="high">ην</supplied></sic></choice> <unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">α</supplied>ὶ <unclear reason="undefined" cert="high">τὸ</unclear> <choice><corr>γινό-

    <lb n="28"/>μενον</corr><sic><supplied reason="lost" cert="high">γ</supplied>εινομενον</sic></choice> β<unclear reason="undefined" cert="high">λάβο</unclear>ς <gap reason="illegible" extent="8" unit="character"/> <unclear reason="undefined" cert="high">τὴν</unclear> ὴν ταύτην εἶναι <unclear reason="undefined" cert="high">καὶ</unclear> ς <choice><corr>κυρίαν</corr><sic><unclear reason="undefined" cert="high">κυρ</unclear>ειαν</sic></choice>

    <lb n="29"/>εἰς τὸν ἅπαντα χρόνον· <choice><corr>πίστει</corr><sic>πιστι</sic></choice> ἐπηρώτησεν Σαμσαιος ὁ ἐωνη-

    <lb n="30"/>μένος, <choice><corr>πίστει</corr><sic>πιστι</sic></choice> ε<unclear reason="undefined" cert="high">ὐ</unclear>δ<unclear reason="undefined" cert="high">οκ</unclear><supplied reason="lost" cert="high">ῶν</supplied> ὡμολόγησεν Αβσαλμας Αβιδρω<unclear reason="undefined" cert="high">δακ</unclear>ου ὁ ἀποδό-

    <lb n="31"/><unclear reason="undefined" cert="high">μενο</unclear>ς.

    <lb n="32"/><gap reason="illegible" extent="3" unit="character"/><gap reason="lost" extent="25" unit="character"/><gap reason="illegible" extent="4" unit="character"/>ε<gap reason="illegible" extent="2" unit="character"/> ἔγ<unclear reason="undefined" cert="high">ραψα</unclear> <supplied reason="lost" cert="high">ὑπὲρ αὐτοῦ μὴ εἰ</supplied>δότος

    <lb n="33"/><supplied reason="lost" cert="high">τὰ γράμματα.</supplied>

