    <lb n="3"/>ἀπὸ τῶν <supplied cert="high" reason="lost">ὑπα</supplied>ρ<unclear cert="high" reason="undefined">χ</unclear><supplied cert="high" reason="lost">ό</supplied><unclear cert="high" reason="undefined">ν</unclear>τ<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> διακειμένων ἐπὶ</supplied>

    <lb n="4"/>τῆσδε τῆς πόλε<unclear cert="high" reason="undefined">ως</unclear> <supplied cert="high" reason="lost">ἐπ᾽ ἀμφό</supplied><unclear cert="high" reason="undefined">δο</unclear><supplied cert="high" reason="lost">υ</supplied> <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="5"/><unclear cert="high" reason="undefined">ε</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><unclear cert="high" reason="undefined">λ</unclear>η<unclear cert="high" reason="undefined">σ</unclear>ο<gap extent="10" reason="lost" unit="character"/><gap extent="5" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="6"/>ἐπὶ βορρᾶ με<supplied cert="high" reason="lost">τὰ π</supplied>αν<supplied cert="high" reason="lost">τὸς αὐτ</supplied><unclear cert="high" reason="undefined">ῆ</unclear><supplied cert="high" reason="lost">ς τοῦ</supplied> δικαίου καὶ χρηστη-

    <lb n="7"/>ρίων καὶ παρασχεῖν με λόγῳ <choice><corr>ἐνοικίου</corr><sic>ενοικειου</sic></choice> αὐτῆς καθ᾽ ἕ-

    <lb n="8"/>καστον ἐνιαυτὸν χρυσοῦ κεράτ<unclear cert="high" reason="undefined">ι</unclear>α δεκαοκτώ,

    <lb n="9"/><expan>γί<ex cert="high">νεται</ex></expan> <expan>χρ<ex cert="high">υσοῦ</ex></expan> <expan>κερ<ex cert="high">άτια</ex></expan> <num value="18">ιη</num>, ὅπερ <choice><corr>ἐνοίκιον</corr><sic>ενοικειον</sic></choice> <unclear cert="high" reason="undefined">ἀπο</unclear>δώσω κατ᾽ ἔτος

    <lb n="10"/>δι᾽ <choice><corr>ἑξαμήνου τὸ</corr><sic>εξαμενου τω</sic></choice> καὶ ὁπόταν <choice><corr>βουληθείητε</corr><sic>βουληθ<unclear cert="high" reason="undefined">ε</unclear>ιηται</sic></choice>

    <lb n="11"/><choice><corr>παραδώσω</corr><sic>παραδοσω</sic></choice> τὴν νομὴ<supplied cert="high" reason="lost">ν τ</supplied><unclear cert="high" reason="undefined">ῆ</unclear>ς αὐτῆς <choice><corr>οἰκίας</corr><sic>οικειας</sic></choice>

    <lb n="12"/>ἀβλαβῆ ὡς καὶ <choice><corr>παρέλαβα</corr><sic>παρελαβαμεν</sic></choice>.

    <lb n="13"/>ἁπλῆ φραφεῖ<unclear cert="high" reason="undefined">σ</unclear>α καὶ <expan>ἐπερ<ex cert="high">ωτηθεῖσα</ex></expan> <expan>ὡμ<supplied cert="high" reason="lost">ο</supplied>λ<ex cert="high">όγησα</ex></expan>. <handShift new="m2"/>Α<supplied cert="high" reason="lost">ὐ</supplied>ρηλία Σοφία

    <lb n="14"/>θυγάτηρ Παύλ<unclear cert="high" reason="undefined">ου</unclear> <choice><corr>ἡ προγεγραμμένη</corr><sic><unclear cert="high" reason="undefined">ο</unclear> προγ<unclear cert="high" reason="undefined">εγρ</unclear>αμμενος</sic></choice>

    <lb n="15"/>πεποίημαι τὴν μίσθωσιν καὶ ἀποδώσω

    <lb n="16"/>τὸ ἐνοίκιον καὶ <expan>ἐπερ<ex cert="high">ωτηθεῖσα</ex></expan> <expan><unclear cert="high" reason="undefined">ὡμολ</unclear><ex cert="high">όγησα</ex></expan> ὡς <expan>πρόκ<ex cert="high">ειται</ex></expan>.

    <lb n="17"/>Αὐρήλι<supplied cert="high" reason="lost">ο</supplied>ς Φοιβάμμων <unclear cert="high" reason="undefined">υἱὸς</unclear> Ἰωσῆφ

    <lb n="18"/>ἀξιωθεὶς ἔγρα<unclear cert="high" reason="undefined">ψ</unclear>α ὑπὲρ <choice><corr>αὐτῆς</corr><sic>αυτου</sic></choice> ἀγραμμάτου

    <lb n="19"/>οὔσης.

    <lb n="20"/><g type="tachygraphic marks"/>
