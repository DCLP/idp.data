    <lb n="1"/><supplied reason="lost" cert="high">Αὐρήλιος Διόσκορος </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> τ</supplied><unclear reason="undefined" cert="high">οῦ</unclear> Διοσκόρο<unclear reason="undefined" cert="high">υ</unclear> ὡς <unclear reason="undefined" cert="high">ἐτῶν</unclear>

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> Αὐ</supplied><unclear reason="undefined" cert="high">ρ</unclear>ηλίῳ Χαιρήμονι

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ὡς ἐτῶν τ</supplied>εσσεράκοντα ἑπτὰ <num value="47"/>

    <lb n="4"/><supplied reason="lost" cert="high">οὐλὴ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> παρακεχωρηκέναι τὸν ὁμολογοῦντα τῷ Αὐ</supplied><unclear reason="undefined" cert="high">ρ</unclear>ηλίῳ Χαιρήμονι κατὰ

    <lb n="5"/><supplied reason="lost" cert="high">τήνδε τὴν ὁμολογίαν ἀπὸ τοῦ νῦν ἐπὶ τὸν ἅπαντα χρόνον τὰς</supplied> <unclear reason="undefined" cert="high">ὑ</unclear>παρχούσας αὐτῷ

    <lb n="6"/><supplied reason="lost" cert="high">κλήρου κατοικικοῦ ἀρούρας </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> περ</supplied>ὶ κώμην Κερκεσοῦχα

    <lb n="7"/><supplied reason="lost" cert="high">τῆς Ἡρακλείδου μερίδος τοῦ Ἀρσινοίτου νομοῦ ἢ ὅσαι ἐ</supplied><unclear reason="undefined" cert="high">ὰ</unclear>ν ὦσι ἐν τόπῳ Τκι<unclear reason="undefined" cert="high">ε</unclear>ν

    <lb n="8"/><supplied reason="lost" cert="high">ὧν γείτονες καθὼς ἐκ συμφώνου ὑπηγόρευσαν νότου </supplied><gap reason="lost" extent="unknown" unit="character"/>ν<unclear reason="undefined" cert="high">ο</unclear>ς σιτικὰ ἐδάφη

    <lb n="9"/><gap reason="lost" extent="unknown" unit="character"/>ρες καὶ ἀπέχειν ὁμοῦ καὶ

    <lb n="10"/><supplied reason="lost" cert="high">νῦν τὸν ὁμολογοῦντα παρὰ τοῦ Αὐρηλίου Χαιρήμονος τὸ κε</supplied>φάλαιον πᾶν ἐκ πλή-

    <lb n="11"/><supplied reason="lost" cert="high">ρους ἀργυρίου δραχμὰς </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> διὰ χειρ</supplied>ὸς καὶ <choice><corr>βεβαιώσει</corr><sic>βε<unclear reason="undefined" cert="high">βαι</unclear>ωσι</sic></choice> αὐτῷ

    <lb n="12"/><supplied reason="lost" cert="high">τῷ πριαμένῳ καὶ τοῖς παρ᾽ αὐτοῦ τὰς <expan>παρακεχωρ<ex cert="high">ημένας</ex></expan> ἀρούρας πάσῃ βεβ</supplied><unclear reason="undefined" cert="high">α</unclear>ι<unclear reason="undefined" cert="high">ώσει</unclear> ἃς καὶ παρέξε-

    <lb n="13"/><supplied reason="lost" cert="high">ται ἀνεπάφους καὶ ἀνενεχυράστους καὶ ἀνεπιδανείστους καὶ καθαρὰ</supplied><unclear reason="undefined" cert="high">ς</unclear> ἀπὸ παντὸς ὀφλ<unclear reason="undefined" cert="high">ή</unclear>-

    <lb n="14"/><supplied reason="lost" cert="high">ματος δημοσίου καὶ ἀπὸ μὲν </supplied><gap reason="lost" extent="unknown" unit="character"/><unclear reason="undefined" cert="high">ω</unclear>ν καὶ ἐπιγραφῶν κα<unclear reason="undefined" cert="high">ὶ</unclear>

    <lb n="15"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἀπὸ τ</supplied>ῶν ἔμπροσθεν χρό<unclear reason="undefined" cert="high">ν</unclear>-

    <lb n="16"/><supplied reason="lost" cert="high">ων μέχρι τῆς ἐνεστώσης ἡμέρας </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἀπὸ δὲ ἰδιω</supplied><unclear reason="undefined" cert="high">τ</unclear>ικῶν καὶ πάσης <choice><corr>ἐμ-

    <lb n="17"/><supplied reason="lost" cert="high">ποιήσεως</supplied></corr><sic>εν<supplied reason="lost" cert="high">ποιησεως</supplied></sic></choice><supplied reason="lost" cert="high"> διὰ παντὸς καὶ μηδένα ἀπὸ τοῦ νῦν κωλύειν τὸν Αὐρήλιον Χ</supplied><unclear reason="undefined" cert="high">α</unclear>ιρήμονα ἀλλὰ <unclear reason="undefined" cert="high">μὴ</unclear>

    <lb n="18"/><supplied reason="lost" cert="high">ἄλλον ὑπὲρ αὐτοῦ πράσσοντα κυριεύειν καὶ οἰκονομεῖν. περὶ δ</supplied>ὲ ταῦτα ὀρθῶς καὶ

    <lb n="19"/><supplied reason="lost" cert="high">καλῶς γεγενῆσθαι ἐπερωτηθεὶς ὡμολόγησα. Αὐρήλιος Διόσ</supplied>κορο<unclear reason="undefined" cert="high">ς</unclear> καὶ ὡς χρηματί-

    <lb n="20"/><supplied reason="lost" cert="high">ζω παρακεχώρηκα τῷ Αὐρηλίῳ Χαιρήμονι περὶ κώμην Κερκεσ</supplied>οῦ<unclear reason="undefined" cert="high">χ</unclear>α κλήρου κατοικι<unclear reason="undefined" cert="high">κο</unclear>ῦ

    <lb n="21"/><supplied reason="lost" cert="high">ἀρούρας </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> καὶ ἀπέσχον παρ᾽ αὐτοῦ τὸ παραχ</supplied>ωρη<supplied reason="lost" cert="high">τ</supplied>ικὸν ἀργυρίου δρα<unclear reason="undefined" cert="high">χμ</unclear>ὰς

    <lb n="22"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> καθὼς</supplied> <unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ρόκε</supplied>ιται. Αὐρήλιος Χαι<unclear reason="undefined" cert="high">ρή</unclear>μω<unclear reason="undefined" cert="high">ν</unclear>

    <lb n="23"/><supplied reason="lost" cert="high">ἠγόρακα παρ᾽ αὐτοῦ καθὼς πρόκειται.</supplied>
