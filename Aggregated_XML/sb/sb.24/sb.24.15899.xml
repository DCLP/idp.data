    <lb n="1"/>ἐν ὀνόματι τῆς ἁγίας καὶ ὁμοουσί[ου τριάδος πατρὸς <expan>κ<ex cert="high">αὶ</ex></expan>] 

    <lb n="2"/>υἱοῦ <expan>κ<ex cert="high">αὶ</ex></expan> ἁγίου πνεύματος <expan>κ<ex cert="high">αὶ</ex></expan> τῆς δεσποίνη[ς ἡμῶν τῆς θεοτόκου <expan>κ<ex cert="high">αὶ</ex></expan>] 

    <lb n="3"/>πάντων τῶν <expan>ἁγίω<ex cert="high">ν</ex></expan>, βασιλείας τοῦ <expan>δεσπό<ex cert="high">του</ex></expan> [ἡμῶν <expan>Φλ<ex cert="high">αουίου</ex></expan> Φωκᾶ τοῦ <expan>αἰων<ex cert="high">ίου</ex></expan>] 

    <lb n="4"/>Αὐγούστου, ἔτους <num value="6">ϛ</num>, Μεσορὴ <num value="15">ιε</num>, <num value="12">ιβ</num> <expan>ἰν<ex cert="high">δικτίωνος</ex></expan>, [ἐπ᾽ <expan>Ἀρσι<ex cert="high">νοιτῶν πόλεως</ex></expan>]. 

    <lb n="5"/><handShift new="m2"/>τόδε τὸ κομπρόμισσον ποιοῦν<supplied reason="lost" cert="high">ται πρὸς ἀλλήλους</supplied> 

    <lb n="6"/>ἑκουσίᾳ γνώμῃ ἐκ μὲν τοῦ ἑνὸς μ<supplied reason="lost" cert="high">έρους Αὐρήλιος Κοσμᾶς</supplied>

    <lb n="7"/>ἐπικείμενος Στρατηγίου τοῦ πανευ<supplied reason="lost" cert="high">φήμου πατρικίου</supplied> 

    <lb n="8"/>υἱὸς τοῦ τῆς μακαρίας μνήμης Ἀπο[ <gap reason="illegible" extent="unknown" unit="character"/> ἀπὸ τῆς <expan>Ἀρσι<ex cert="high">νοιτῶν</ex></expan> πόλεως]

    <lb n="9"/>ἐκ δὲ τοῦ ἑτέρου μέρους Αὐρηλία Μ<unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="low">ροῦς θυγάτηρ Τι<unclear reason="undefined" cert="high">μ</unclear></supplied><gap reason="lost" extent="5" unit="character"/>

    <lb n="10"/>χωρ<supplied reason="lost" cert="high">ὶς</supplied> <unclear reason="undefined" cert="high">κυρί</unclear>ου ἀνδρ<unclear reason="undefined" cert="high">ὸς</unclear> <unclear reason="undefined" cert="high">χ</unclear><supplied reason="lost" cert="high">ρ</supplied>η<supplied reason="lost" cert="high">μ</supplied><unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">τ</supplied>ί<unclear reason="undefined" cert="high">ζ</unclear><supplied reason="lost" cert="high">ουσα ἀπὸ τῆς αὐτῆς</supplied>

    <lb n="11"/>πόλεω<supplied reason="lost" cert="high">ς </supplied><gap reason="lost" extent="unknown" unit="character"/> 
