    <lb n="4"/>παρὰ <choice><corr>Αἰλίου</corr><sic>αιλιω</sic></choice> Συρίωνος οὐετρανοῦ τῶν ἐντίμως ἀπολυ-

    <lb n="5"/>θέντων· τοῦ <choice><corr><add place="supralinear">παρ</add>ειμένου</corr><sic><add place="supralinear">παρ</add>ειμενω</sic></choice> μ<unclear cert="high" reason="undefined">ο</unclear>ι μοναχοῦ <choice><corr>χειρογράφου</corr><sic>δι<unclear cert="high" reason="undefined">αγρ</unclear>αφ<unclear cert="high" reason="undefined">ου</unclear></sic></choice> <unclear cert="high" reason="undefined">ἀ</unclear>π<supplied cert="high" reason="lost">ο-</supplied>

    <lb n="6"/>χῆς ἀντίγραφον <supplied cert="high" reason="lost">ὑ</supplied>π<supplied cert="high" reason="lost">ό</supplied>κει<unclear cert="high" reason="undefined">ται</unclear>· <space dim="horizontal" extent="unknown" unit="character"/>

    <lb n="7"/>Αὐρήλιος Σαραπόδω<supplied cert="high" reason="lost">ρ</supplied>ος γυμνασιαρχήσας β<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">υ</supplied><unclear cert="high" reason="undefined">λ</unclear>ευτὴς <expan><unclear cert="high" reason="undefined">Ἡ</unclear><supplied cert="high" reason="lost">ρακλ</supplied>εοπ<ex cert="high">όλεως</ex></expan>

    <lb n="8"/>Αἰλίῳ <choice><corr>Συρίωνι</corr><sic>συριω</sic></choice> οὐε<supplied cert="high" reason="lost">τρ</supplied>ανῷ <choice><corr>γεουχοῦντι</corr><sic>γεοχουντος</sic></choice> ἐν κώμῃ <choice><corr>Ἀγκυρώνων</corr><sic>ανκυρωνων</sic></choice>

    <lb n="9"/>διὰ Νικήτου ἀπελε<supplied cert="high" reason="lost">υ</supplied>θέρου· ὁμολογῶ ἐκδεδόσθαι <choice><corr>σοι</corr><sic>σε</sic></choice> τὴν

    <lb n="10"/>ἀποχὴν καὶ ἀπεσχηκέναι  παρ<supplied cert="high" reason="lost">ὰ</supplied> σοῦ ἃς <choice><corr>ἐδάνεισά</corr><sic>εδανισα</sic></choice> σοι τῷ <subst><add place="inline">Συρί<add place="supralinear">ω</add>νι</add><del rend="corrected">συριονι</del></subst>

    <lb n="11"/>διὰ χιρογράφων δύο <num value="2"/> <supplied cert="high" reason="lost"><expan><ex cert="high">δραχμὰς</ex></expan></supplied> <gap extent="2" reason="illegible" unit="character"/> <supplied cert="high" reason="lost">καὶ μή σο</supplied>ι <choice><corr>ἐγκαλεῖν</corr><sic>ενκαλειν</sic></choice> καὶ μὴ <choice><corr>ἐγκαλέσειν</corr><sic>ενκαλεσειν</sic></choice>

    <lb n="12"/><supplied cert="high" reason="lost">μ</supplied>ηδὲ τούτων μηδὲ <supplied cert="high" reason="lost">ἐπελεύσε</supplied><unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">θαι</supplied> περὶ ἄλλ<unclear cert="high" reason="undefined">ου</unclear> ἁπαξαπλ<supplied cert="high" reason="lost">ῶς</supplied>

    <lb n="13"/><supplied cert="high" reason="lost">μηδ</supplied>ενὸ<supplied cert="high" reason="lost">ς πράγματος </supplied><gap extent="unknown" reason="lost" unit="character"/>π<unclear cert="high" reason="undefined">εν</unclear><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="14"/><gap extent="1" reason="illegible" unit="line"/>

    <lb n="15"/><gap extent="unknown" reason="lost" unit="line"/>

    <lb n="15"/><gap extent="20" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> τοῦτο δ<unclear cert="high" reason="undefined">ὲ</unclear> <supplied cert="high" reason="lost">τὸ αὐθεντικὸν χειρόγραφον</supplied>

    <lb n="16"/><supplied cert="high" reason="lost">βουλόμενος ἐν δημοσ</supplied><unclear cert="high" reason="undefined">ί</unclear>ῳ γενέσθαι δίδ<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">μι τῇ πόλει διὰ τὸ μοναχὸν εἶναι</supplied>

    <lb n="17"/><supplied cert="high" reason="lost">τὰς ὁρισθείσας <expan><ex cert="high">δραχμὰς</ex></expan> <num value="12">ιβ</num></supplied> ὑπὲρ εκατου<unclear cert="high" reason="undefined">τ</unclear><gap extent="15" reason="lost" unit="character"/><supplied cert="high" reason="lost"> καὶ τὸ ὑπὲρ</supplied>

    <lb n="18"/><supplied cert="high" reason="lost">τοῦ τιμήματος το</supplied>ύτου <choice><corr>τέλος</corr><sic>τελους</sic></choice> κ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ὶ</supplied> ἀξι<supplied cert="high" reason="lost">ῶ ἀναλαβόντας τὸ παρειμένον</supplied>

    <lb n="19"/><supplied cert="high" reason="lost">μοναχὸν <choice><corr>χειρόγραφον</corr><sic>διαγραφον</sic></choice> τ</supplied>ῆς ἀποχῆς παρ᾽ ἐ<supplied cert="high" reason="lost">μοῦ <expan>ὑπογρ<ex cert="high">αμμένον</ex></expan> ὑπ᾽ ἐμοῦ περὶ τοῦ</supplied>

    <lb n="20"/><supplied cert="high" reason="lost">τὴν ὑπογραφὴν εἶν</supplied>αι τὸ ἰδιόγραφον <unclear cert="high" reason="undefined">Α</unclear><supplied cert="high" reason="lost">ὐρηλίου Σαραποδώρου γυμνα-</supplied>

    <lb n="21"/><supplied cert="high" reason="lost"><expan>σιαρχ<ex cert="high">ήσαντος</ex></expan> <expan>βουλ<ex cert="high">ευτοῦ</ex></expan> </supplied><choice><corr><supplied cert="high" reason="lost">συγκα</supplied>ταχωρίσαι τῷδε</corr><sic><supplied cert="high" reason="lost">συγκα</supplied><unclear cert="high" reason="undefined">τ</unclear>αχωρισ<unclear cert="high" reason="undefined">ε</unclear> τωτε</sic></choice> τῷ ὑ<unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ομνήματι, τὸ μὲν αὐθεντικὸν</supplied>

    <lb n="22"/><supplied cert="high" reason="lost">εἰς τὴν Ἁδριανὴν</supplied> β<supplied cert="high" reason="lost">ιβ</supplied>λιοθήκην, <expan>τούτ<unclear cert="high" reason="undefined">ο</unclear><ex cert="high">υ</ex></expan> δὲ <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ὸ ἴσον ὑπόμνημα καὶ εἰς</supplied>

    <lb n="23"/><supplied cert="high" reason="lost">τὴν τοῦ Ναναίου</supplied> πρ<supplied cert="high" reason="lost">ὸ</supplied>ς τὸ μένειν μοι τὰ ἀπ᾽ <supplied cert="high" reason="lost">αὐτοῦ πρὸς <expan>Αὐρ<ex cert="high">ήλιον</ex></expan> Σαραπόδωρον γυμνα-</supplied>

