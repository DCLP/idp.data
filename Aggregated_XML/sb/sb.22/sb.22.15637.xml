    <lb n="2"/>τὴ<unclear cert="high" reason="undefined">ν</unclear> <unclear cert="high" reason="undefined">κ</unclear>ώμ<unclear cert="high" reason="undefined">ην</unclear> <gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/>

