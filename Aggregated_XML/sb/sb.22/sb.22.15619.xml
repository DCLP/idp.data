    <lb n="1"/><gap desc="vestiges" extent="unknown" reason="illegible" unit="character"/> <supplied cert="high" reason="lost">τῶ</supplied>ν ὑπα<unclear cert="high" reason="undefined">ρ</unclear>χόντω<supplied cert="high" reason="lost">ν</supplied> <unclear cert="high" reason="undefined">μοι</unclear><gap desc="vestiges" extent="unknown" reason="illegible" unit="character"/>

    <lb n="2"/><unclear cert="high" reason="undefined">κατ</unclear>ὰ τή<unclear cert="high" reason="undefined">ν</unclear>δε τὴν ἁπλῆ<supplied cert="high" reason="lost">ν</supplied> <choice><corr>ἔγγραφον ἀσφάλειαν</corr><sic>εγγραφη<unclear cert="high" reason="undefined">ν</unclear> α<supplied cert="high" reason="lost">σφ</supplied>αλιαν</sic></choice> <unclear cert="high" reason="undefined">ἰ</unclear>διωτι<unclear cert="high" reason="undefined">κ</unclear>ῆ<unclear cert="high" reason="undefined">ς</unclear> τῆς χορὶς θρου ταχ<unclear cert="high" reason="undefined">αι</unclear> <choice><corr>πάσης ἀκαθαρσίας</corr><sic>πα<unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">ν</unclear> αγαθαρσιαν</sic></choice>

    <lb n="3"/><gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ι</unclear>ω<gap extent="2" reason="illegible" unit="character"/>ν ἰδιοτη<unclear cert="high" reason="undefined">τ</unclear><gap extent="2" reason="lost" unit="character"/> καὶ <choice><corr>τὸ</corr><sic>τω</sic></choice> ἕ<unclear cert="high" reason="undefined">κ</unclear>τον μου μέρος <choice><corr>εἴτε ψιλῶν τόπων εἴτε χρηστηρίων εἴτε λάκκου εἴτε πάντων

    <lb n="4"/>δενδρικῶν</corr><sic><supplied cert="high" reason="lost">ι</supplied>δε ψιλον τοπον ιδε χρυστηριων ιδε λακκον ιδεπ<unclear cert="high" reason="undefined">α</unclear>ντα <unclear cert="high" reason="undefined">δ</unclear>εντρικα</sic></choice> καὶ <choice><corr>ἐλθὸν</corr><sic>ελθων</sic></choice> εἰ<unclear cert="high" reason="undefined">ς</unclear> <choice><corr>ἐμὲ</corr><sic>εμα<unclear cert="high" reason="undefined">ι</unclear></sic></choice> <unclear cert="high" reason="undefined">ἀπὸ</unclear>κλ<unclear cert="high" reason="undefined">η</unclear>ρονομίας το<supplied cert="high" reason="lost">ῦ </supplied><choice><corr><supplied cert="high" reason="lost">ἀ</supplied>ποιχομένου</corr><sic><supplied cert="high" reason="lost">α</supplied>νουχομενον</sic></choice> μου πατρὸς Κολλούθου εἰς <choice><corr>πρακτορίαν</corr><sic>πρακτωρι<unclear cert="high" reason="undefined">ας</unclear></sic></choice>

    <lb n="5"/>κώμ<unclear cert="high" reason="undefined">η</unclear>ς <choice><corr>Ἐρείθεως</corr><sic>ερηθεως</sic></choice> Τ<unclear cert="high" reason="undefined">ο</unclear>κ<gap extent="2" reason="illegible" unit="character"/>ς <choice><corr>τὸ ἕκτον</corr><sic>των εκτων</sic></choice> μου μ<unclear cert="high" reason="undefined">έ</unclear>ρος <choice><corr>ἀπηλιώτου</corr><sic>απιλιω<supplied cert="high" reason="lost">του</supplied></sic></choice> οἱ ἀπὸ <choice><corr>Ἀλαβαστρίνης</corr><sic>αλαβαστρινι</sic></choice>, λιβὸς οἱ ἀπὸ Τοοὺ <gap desc="vestiges" extent="unknown" reason="illegible" unit="character"/>

    <lb n="6"/>βορρᾶ <sic>α</sic> ἄπα <choice><corr>Ἁτρείους μονάζοντος</corr><sic>ατρης μονασον</sic></choice>, νότου επτα οἱ ἀπὸ Τοοὺ <supplied cert="high" reason="lost">κ</supplied>αὶ ἅπερ ἐνταῦθα ἀπέσχον παρὰ σ<unclear cert="high" reason="undefined">ο</unclear>ῦ τὴν τιμὴν

    <lb n="7"/><unclear cert="high" reason="undefined">ἐκ</unclear> πλήρους καθὼς πρὸς ἀλλήλους <choice><corr>συμπεφωνήμεθα</corr><sic>σ<unclear cert="high" reason="undefined">υ</unclear>νπεφονιμονου</sic></choice> <unclear cert="high" reason="undefined">χρ</unclear>υσοῦ νομισμάτια ὀκτώ, <expan>γί<ex cert="high">νεται</ex></expan> <expan>νο<ex cert="high">μισμάτια</ex></expan> <num value="8">η</num>,

    <lb n="8"/>κυριεύειν καὶ δεσποτ<sic>ι</sic>εύειν ἐπὶ τοῦ <choice><corr>νῦν</corr><sic>νην</sic></choice> ἐπὶ τ<unclear cert="high" reason="undefined">ὸν</unclear><choice><corr>ἅπαντα</corr><sic>απαατα</sic></choice> χρόνον, διὰ <choice><corr>τοῦτο πεποίηκά</corr><sic>τουτω πε<supplied cert="high" reason="lost">π</supplied><unclear cert="high" reason="undefined">οι</unclear>κα</sic></choice> σ<unclear cert="high" reason="undefined">οι</unclear>

    <lb n="9"/>ταύτην τὴν <choice><corr>πρᾶσιν</corr><sic>πρασι</sic></choice> εἰς <choice><corr>ἀσφάλειαν κυρίαν</corr><sic>ασφαλια κυριε</sic></choice> καὶ <choice><corr>βεβαία</corr><sic>βεβαι</sic></choice> καὶ <sic>καὶ</sic> <choice><corr>ἐπεροτηθεῖσα ὡμολόγησα</corr><sic>επεροτηθις ομολογεισα</sic></choice>. Αὐρη<unclear cert="high" reason="undefined">λ</unclear><supplied cert="high" reason="lost">ία </supplied><gap extent="2" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">α</unclear>νὴ

    <lb n="10"/>Κολλούθου η῾ <choice><corr>προκειμένη</corr><sic>προκιμενη</sic></choice> <supplied cert="high" reason="omitted">ὁμολογῶ</supplied><choice><corr>πεπρακέναι</corr><sic>πεπρακενε</sic></choice> σοι <choice><corr>τὸ ὑπάρχον</corr><sic>τ<unclear cert="high" reason="undefined">πα</unclear>ρχοντων</sic></choice> μοι <choice><corr>ἕκτον</corr><sic>εκτων</sic></choice> μ<unclear cert="high" reason="undefined">έρος</unclear> <choice><corr>ψιλῶν τόπων</corr><sic><unclear cert="high" reason="undefined">ψ</unclear><supplied cert="high" reason="lost">ιλον τοπ</supplied><unclear cert="high" reason="undefined">ο</unclear>ν</sic></choice>

    <lb n="11"/>καὶ ἀ<unclear cert="high" reason="undefined">π</unclear>έσχον τὴν τιμὴν ἐκ πλήρους <expan>χρ<ex cert="high">υσοῦ</ex></expan> <expan>νο<ex cert="high">μισμάτια</ex></expan> <num value="8">η</num> καὶ<choice><corr>στοιχεῖ</corr><sic>στυχ<supplied cert="high" reason="lost">ει</supplied>μ</sic></choice> μοι π<unclear cert="high" reason="undefined">άν</unclear>τα τὰ ἐγγ<supplied cert="high" reason="lost">εγραμμένα</supplied>.

    <lb n="12"/>Αὐρήλιος Ἀροοῦς ἀπὸ Θελβόνθεως <choice><corr>ἀξιωθεὶς</corr><sic>αξιωθις</sic></choice> ἔγραψα ὑπὲρ αὐτῆ<supplied cert="high" reason="omitted">ς</supplied> γράμματα <choice><corr>μὴ</corr><sic>μο<unclear cert="high" reason="undefined">ι</unclear></sic></choice> <unclear cert="high" reason="undefined">εἰδυ</unclear><supplied cert="high" reason="lost">ίας.</supplied>
