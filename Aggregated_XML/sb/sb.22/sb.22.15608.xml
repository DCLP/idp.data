    <lb n="16"/><gap extent="2" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/>ρ<gap extent="2" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>προ<unclear cert="high" reason="undefined">ς</unclear><gap extent="1" reason="illegible" unit="character"/> τῶν καὶ οὕ<unclear cert="high" reason="undefined">τ</unclear>ως

    <lb n="17"/><del rend="erasure"><unclear cert="high" reason="undefined">τ</unclear><gap extent="1" reason="illegible" unit="character"/>ν<gap extent="1" reason="illegible" unit="character"/>α<gap extent="1" reason="illegible" unit="character"/><add place="supralinear">υ</add></del><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>τη<gap extent="1" reason="lost" unit="character"/><supplied cert="high" reason="lost"> π</supplied>ρ<unclear cert="high" reason="undefined">ὸ</unclear>ς <unclear cert="high" reason="undefined">τ</unclear>ὴν τότε μοι

    <lb n="18"/>πα<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">οῦ</supplied><unclear cert="high" reason="undefined">σ</unclear>αν ὥρα<unclear cert="high" reason="undefined">ν</unclear> <gap extent="1" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/>ημι γὰρ <add place="supralinear">καὶ</add> παρὰ

    <lb n="19"/>τῇ σῇ εἰλικρινίᾳ εἰ <choice><corr>χρεία γένοιτο</corr><sic>χρια γ<unclear cert="high" reason="undefined">ει</unclear>νοιτο</sic></choice>

    <lb n="20"/>καὶ δύο <num value="2"/> εὐγενεστά<unclear cert="high" reason="undefined">τους</unclear> μ<unclear cert="high" reason="undefined">άρ</unclear>τυρας

    <lb n="21"/>ε<unclear cert="high" reason="undefined">ἰς</unclear> τοῦτο <del rend="erasure">ευγε<unclear cert="high" reason="undefined">νε</unclear></del> <expan>Φλα<ex cert="high">ουίους</ex></expan> <add place="supralinear">Ἥρωνά <unclear cert="high" reason="undefined">τ</unclear>ε τοῦ νομοῦ καὶ Ἀπολλώνιον <expan>ὀφφ<ex cert="high">ικιάλιον</ex></expan> τῆς <expan>ἡγεμο<ex cert="high">νικῆς</ex></expan> <expan>τάξ<ex cert="high">εως</ex></expan></add> κατηνάγκασεν γάρ με

    <lb n="22"/>ἀνόμως καὶ βιαίως κατὰ τὴν <num value="16">ιϛ</num> τοῦ <expan><supplied cert="high" reason="lost">π</supplied>αρελθ<ex cert="high">όντος</ex></expan>

    <lb n="23"/><expan>μη<ex cert="high">νὸς</ex></expan> Θὼθ <del rend="erasure">τῆς <expan>νυνὶὑπ<ex cert="high">α</ex></expan>τεί<supplied cert="high" reason="lost">ας</supplied></del>
</ab></div><div n="v" part="N" type="textpart"><ab part="N">

