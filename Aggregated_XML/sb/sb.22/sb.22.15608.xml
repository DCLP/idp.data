    <lb n="1"/>Κ<unclear cert="high" reason="undefined">αικι</unclear>λίω<unclear cert="high" reason="undefined">ι</unclear> <supplied cert="high" reason="lost">Κωνσ</supplied><unclear cert="high" reason="undefined">ο</unclear>υλτίωι τῶι δι<unclear cert="high" reason="undefined">α</unclear>σημ<supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">τ</unclear>άτωι ἐπάρχωι Αἰ<unclear cert="high" reason="undefined">γύπτ</unclear>ου

    <lb n="2"/><unclear cert="high" reason="undefined">παρ</unclear>ὰ Αὐρη<unclear cert="high" reason="undefined">λ</unclear><supplied cert="high" reason="lost">ί</supplied><unclear cert="high" reason="undefined">ου</unclear> <supplied cert="high" reason="lost">Ν</supplied><unclear cert="high" reason="undefined">ε</unclear>ιλάμμωνος τοῦ καὶ Ἱ<unclear cert="high" reason="undefined">έ</unclear>ρ<unclear cert="high" reason="undefined">α</unclear>κος καὶ ὡς <expan>χρη<ex cert="high">ματίζει</ex></expan>. Ἀσκληπιάδης ἔχων

    <lb n="3"/>περὶ <add place="supralinear">ἑ</add>αυτὸ<add place="supralinear">ν</add> στρατιω<unclear cert="high" reason="undefined">τ</unclear>ικὸν σχ<unclear cert="high" reason="undefined">ῆ</unclear>μα, <sic>δη</sic> λέγω <add place="supralinear">δὴ</add> τὸ τῶν κρατίστων, ἀνεσκεύασεν πρᾶγμα ἀλογώτατον

    <lb n="4"/>κατ᾽ ἐμ<supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ῦ</unclear> ἀντὶ ε<unclear cert="high" reason="undefined">ὐ</unclear><supplied cert="high" reason="lost">π</supplied><unclear cert="high" reason="undefined">ο</unclear>ιείας καὶ μὴ φέρων <del rend="erasure">κα<unclear cert="high" reason="undefined">τ</unclear>ηναγκάσθην υΠ᾽ αὐτοῦ</del> <add place="supralinear"><del rend="erasure">παρ᾽ αὐτ<unclear cert="high" reason="undefined">ὰ</unclear> τὴν βα</del></add> τὴν βα<del rend="erasure">κ</del>ναυσότητα τοῦ <add place="supralinear">τοιούτου</add> ἀνδρὸς

    <lb n="5"/><choice><corr>εἶξα</corr><sic>ιξ<unclear cert="high" reason="undefined">α</unclear></sic></choice> τῇ <choice><corr>ἀπαιτήσει</corr><sic><unclear cert="high" reason="undefined">απαι</unclear>τ<unclear cert="high" reason="undefined">η</unclear>σι</sic></choice> αὐτοῦ π<supplied cert="high" reason="lost">ρ</supplied>ὸς τὴν <add place="supralinear">τότε μ<unclear cert="high" reason="undefined">ο</unclear>ι</add> παροῦσαν ὥραν <add place="supralinear">οὕτω γάρ με κατηνάγκασεν <unclear cert="high" reason="undefined">β</unclear>ιαίως</add> κατὰ τὴν ἑκκε-

    <lb n="6"/>δεκάτ<unclear cert="high" reason="undefined">η</unclear>ν τοῦ π<unclear cert="high" reason="undefined">αρ</unclear>ελθ<add place="supralinear">όν</add>τος μηνὸς Θὼθ <del rend="erasure">πι</del> <unclear cert="high" reason="undefined">γ</unclear>ρ<unclear cert="high" reason="undefined">ά</unclear>ψασθαι αὐτῷ ἀδότων <supplied cert="high" reason="lost">ἀρ</supplied>γυρίων ταλάν-

    <lb n="7"/>των πέντε κα<unclear cert="high" reason="undefined">ὶ</unclear> <unclear cert="high" reason="undefined">τ</unclear>ριάκοντα <num value="35"/>. <choice><corr>ἐπεὶ</corr><sic>επι</sic></choice> οὖν νόμος διαγορεύει τὰ τοιαῦτα ἄδοτα γραμμάτια

    <lb n="8"/>ἀνίσ<unclear cert="high" reason="undefined">χυ</unclear>ρα <unclear cert="high" reason="undefined">ε</unclear>ἶν<unclear cert="high" reason="undefined">α</unclear>ι, διὰ τοῦτο δέομαι τῆς σῆς ἀρετῆς ὅπως κελεύσῃς πρῶτον μὲν

    <lb n="9"/>κ<unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">ῖ</supplied>σ<unclear cert="high" reason="undefined">θαί</unclear> μο<unclear cert="high" reason="undefined">υ</unclear> <unclear cert="high" reason="undefined">τ</unclear>ὰ βιβλία ἐν τῇ σῇ τοῦ ἐμοῦ δεσπότου <choice><corr>τάξει</corr><sic>ταξι</sic></choice> πρὸς ἀσφάλε<supplied cert="high" reason="lost">ια</supplied><unclear cert="high" reason="undefined">ν</unclear> <add place="supralinear"><supplied cert="high" reason="lost">τ</supplied>ὴν ἡμετέραν</add>, <choice><corr>ἔπειτα</corr><sic><unclear cert="high" reason="undefined">ε</unclear>πιτα</sic></choice>

    <lb n="10"/><supplied cert="high" reason="lost">δὲ</supplied> <unclear cert="high" reason="undefined">διακοῦσαι</unclear> ἐνταῦθα ἐν τῇ λαμπροτάτῃ πόλει τῶν Ἀλεξανδρέων ἐ<unclear cert="high" reason="undefined">ξ</unclear> ἀναγκα<unclear cert="high" reason="undefined">ίας</unclear>

    <lb n="11"/><gap extent="6" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ρ</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ου</unclear>, ἐ<unclear cert="high" reason="undefined">ὰν</unclear> <unclear cert="high" reason="undefined">δ</unclear>ὲ <del rend="erasure">θε<unclear cert="high" reason="undefined">λ</unclear>ῃ</del> <add place="supralinear"><unclear cert="high" reason="undefined">κελεύσῃ</unclear></add> σου <unclear cert="high" reason="undefined">τὸ</unclear> <choice><corr>μεγαλεῖον</corr><sic>μεγαλιον</sic></choice>, ἀποδοῦναι τὸ κ<supplied cert="high" reason="lost">ατ</supplied>ὰ βία<unclear cert="high" reason="undefined">ν</unclear>

    <lb n="12"/><supplied cert="high" reason="lost">γεγραμμέν</supplied>ον γραμμάτι<supplied cert="high" reason="lost">ο</supplied>ν, <add place="supralinear">ἵν᾽ ὦμην ε<supplied cert="high" reason="lost">ὐ</supplied>εργετημένος ὑπὸ τῆς <supplied cert="high" reason="lost">εὐεργ</supplied>ε<supplied cert="high" reason="lost">σ</supplied>ίας <gap extent="3" reason="illegible" unit="character"/></add> καὶ τούτου τυχὼν εἰς ἀεί σοι

    <lb n="13"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="5" reason="illegible" unit="character"/>τάτας χά<supplied cert="high" reason="lost">ρι</supplied>τας ὁ<unclear cert="high" reason="undefined">μολ</unclear><supplied cert="high" reason="lost">ογ</supplied><unclear cert="high" reason="undefined">ήσ</unclear>ω.

    <lb n="14"/>διευ<supplied cert="high" reason="lost">τύχ</supplied>ει.
</ab></div><div n="r,2" part="N" type="textpart"><ab part="N">

