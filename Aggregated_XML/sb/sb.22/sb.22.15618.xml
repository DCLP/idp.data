    <lb n="2"/><supplied reason="lost" cert="high">Αὐρηλία Ἰουλιανὴ θυγάτηρ Ὕμιος τοῦ δεῖνα μητρὸς τῆς δεῖνα ἀπὸ κώμης Ἀλαβ</supplied><unclear reason="undefined" cert="high">αστρί</unclear><supplied reason="lost" cert="high">ν</supplied><unclear reason="undefined" cert="high">η</unclear><supplied reason="lost" cert="high">ς τοῦ Ἀντινοίτου</supplied> νομ<supplied reason="lost" cert="high">ου Αὐ</supplied>ρη<unclear reason="undefined" cert="high">λ</unclear>ί<unclear reason="undefined" cert="high">ῳ</unclear> <choice><corr>Σερήνῳ</corr><sic>σε<supplied reason="lost" cert="high">ρη</supplied><unclear reason="undefined" cert="high">ν</unclear>ος</sic></choice> <unclear reason="undefined" cert="high">ν</unclear><supplied reason="lost" cert="high">αύτ</supplied><unclear reason="undefined" cert="high">ῃ</unclear> <note lang="en" anchored="yes" place="not specified">?</note>

    <lb n="3"/><supplied reason="lost" cert="high">ἀπὸ τῆς αὐτῆς κώμη</supplied><unclear reason="undefined" cert="high">ς</unclear> χαίρειν. <supplied reason="lost" cert="high">ὁμ</supplied>ολ<supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">γ</unclear><supplied reason="lost" cert="high">ῶ πεπ</supplied><unclear reason="undefined" cert="high">ρ</unclear>ακέναι σ<supplied reason="lost" cert="high">οι</supplied> κα<supplied reason="lost" cert="high">ὶ κ</supplied><unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">ταγεγρ</supplied><unclear reason="undefined" cert="high">αφ</unclear>ηκέναι σοι <supplied reason="lost" cert="high">ἀπὸ τοῦ νῦν κ</supplied>υριευτικῶς <unclear reason="undefined" cert="high">ε</unclear><supplied reason="lost" cert="high">ἰς</supplied> <unclear reason="undefined" cert="high">τ</unclear>ὸν ἀεὶ ἅπ<supplied reason="lost" cert="high">α</supplied>ντα χρόν<supplied reason="lost" cert="high">ο</supplied>ν <sic>σου</sic>

    <lb n="4"/><supplied reason="lost" cert="high">τὸ ὑπάρχον</supplied> μοι καὶ <choice><corr>ἐλθὸν</corr><sic>ελθων</sic></choice> εἰς ἐμὲ <unclear reason="undefined" cert="high">ἀ</unclear><supplied reason="lost" cert="high">πὸ</supplied> <choice><corr>δικαίου</corr><sic>δοκαιου</sic></choice> κληρο<supplied reason="lost" cert="high">νο</supplied><unclear reason="undefined" cert="high">μ</unclear>ίας τοῦ πατρός μου πρότερον τῶν ἀπὸ κώμης <unclear reason="undefined" cert="high">Τ</unclear>ερύθε<unclear reason="undefined" cert="high">ω</unclear><supplied reason="lost" cert="high">ς</supplied> Ἐβύ<add place="supralinear">θ</add>εω<unclear reason="undefined" cert="high">ς</unclear> τοῦ <sic>ερ</sic>

    <lb n="5"/><unclear reason="undefined" cert="high">Ἑ</unclear>ρμουπ<unclear reason="undefined" cert="high">ο</unclear>λίτου <supplied reason="lost" cert="high">ν</supplied>ομοῦ περὶ πρακτορίαν κ<unclear reason="undefined" cert="high">ώ</unclear><supplied reason="lost" cert="low">μης </supplied> <space extent="unknown" unit="character" dim="horizontal"/> ὅσου ἐστὶν ἀρουρῶν σὺν <choice><corr>τῇ χερσασπόρῳ</corr><sic>τω χερσασπορου</sic></choice> κα<supplied reason="lost" cert="high">ὶ σ</supplied>ὺν τῷ λά<unclear reason="undefined" cert="high">κ</unclear>κῳ

    <lb n="6"/>καὶ<choice><corr>παντοίοις δένδροις</corr><sic>παντοιων δενδρων</sic></choice>, οὗ <choice><corr>γείτωνες</corr><sic>γιτωνες</sic></choice> καθὼς ἡ <choice><corr>πωλοῦσα ὑπηγόρευσε</corr><sic>πολου<unclear reason="undefined" cert="high">ν</unclear>τα υπηγορευσαμεν</sic></choice> βορρᾶ <choice><corr>Δίου</corr><sic>διος</sic></choice> Πευὼτ ἀπὸ Ἀλαβαστρίνης, λι<supplied reason="lost" cert="high">β</supplied>ὸς κοιν<unclear reason="undefined" cert="high">ο</unclear>ῦ

    <lb n="7"/>τῆς κώμης Τευὼ <choice><corr>Νεανίσκων</corr><sic>νεανισκου</sic></choice> τοῦ Ἑρμοπολίτου νομοῦ, <unclear reason="undefined" cert="high">ν</unclear>ό<unclear reason="undefined" cert="high">τ</unclear>ου <choice><corr>Βήκιος</corr><sic>βηκις</sic></choice> Ὕμιος ἀπὸ τῆς αὐτῆς <supplied reason="lost" cert="high">Τ</supplied>ευώ, ἀπηλιώτου τοῦ λάκ<supplied reason="lost" cert="high">κ</supplied>ου <choice><corr>γεωργῶν</corr><sic>γε<unclear reason="undefined" cert="high">ωρ</unclear>γου</sic></choice>

    <lb n="8"/>συνόδου κώμης Ἀλαβαστρίνης, τιμῆς καὶ παραχωρ<supplied reason="lost" cert="high">η</supplied>τικοῦ <choice><corr>τῶν</corr><sic>του</sic></choice> πρὸς ἀλλήλους συμπεφωνημένων χρυσοῦ <choice><corr>νομισματίων εὐστάθμων</corr><sic>νομισ<unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">α</supplied>τια ευ<unclear reason="undefined" cert="high">σ</unclear><supplied reason="lost" cert="high">τα</supplied>θμα</sic></choice>

    <lb n="9"/>ἀριθμῷ εἴκοσι πέντε, <expan><ex cert="high">γίνεται</ex></expan> <expan>χρυ<ex cert="high">σοῦ</ex></expan> <expan>νο<ex cert="high">μισμάτια</ex></expan> <num value="25">κε</num>, <choice><corr>ἅπερ</corr><sic>ασπερ</sic></choice> αὐτόθι <unclear reason="undefined" cert="high">ἀ</unclear>πέσχη<unclear reason="undefined" cert="high">κ</unclear>α παρὰ σοῦ τὴν τιμὴν ἐκ πλ<supplied reason="omitted" cert="high">ή</supplied>ρους διὰ χειρός, <choice><corr>κρατεῖν</corr><sic>κρατιν</sic></choice>οὖν σε καί κυριεύειν

    <lb n="10"/>σὺν τέκνοις καὶ ἐγκόνοις καὶ <choice><corr>τοῖς</corr><sic>της</sic></choice> παρ᾽ <choice><corr>ἐμοῦ</corr><sic>εμη</sic></choice> <unclear reason="undefined" cert="high">π</unclear>αραλημψομέ<unclear reason="undefined" cert="high">ν</unclear><supplied reason="lost" cert="high">ο</supplied>ις <choice><corr>τὸ πεπραμένον σοι</corr><sic>των πεπραμενων συ</sic></choice> ὑπ᾽ <choice><corr>ἐμοῦ</corr><sic>εμη</sic></choice> ὅσου ἐστὶν ἀρουρῶν σὺν τῷ χέρσ<supplied reason="lost" cert="high">ῳ</supplied> καὶ τῷ

    <lb n="11"/>λάκκῳ <supplied reason="omitted" cert="high">καὶ</supplied> σὺν τῇ <choice><corr>αὐλυδρίῳ</corr><sic>α<unclear reason="undefined" cert="high">υ</unclear>ληδριω</sic></choice> <unclear reason="undefined" cert="high">ὡς</unclear> <choice><corr>πρόκειται</corr><sic>προκιται</sic></choice> εἰ<unclear reason="undefined" cert="high">ς</unclear> τὸν ἀεὶ χρόνον καὶ τὰ ἐξ αὐτῶν περιγιγνόμενα ἀπὸ κανό<unclear reason="undefined" cert="high">ν</unclear><supplied reason="lost" cert="high">ο</supplied>ς τῆς εὐτυχοῦς ἑνδε<unclear reason="undefined" cert="high">κά</unclear>της

    <lb n="12"/>ἰνδικτίονος, τελοῦντα δὲ τὰ ὑπὲρ αὐτῶν τελούμενα ἐτήσια δημόσια τελέσματα πάντα <sic>πάντα</sic> παντοῖα ἀπὸ κανόνος τῆς ε<supplied reason="lost" cert="high">ὐ</supplied>τυχοῦς

    <lb n="13"/>ἑνδεκάτης <expan><unclear reason="undefined" cert="high">ἰ</unclear>νδικτί<ex cert="high">ονος</ex></expan> καί <choice><corr>σοι εἴη</corr><sic>συ ιη</sic></choice> καὶ <choice><corr>ἐξουσία</corr><sic>εξουσι<unclear reason="undefined" cert="high">α</unclear>ν</sic></choice> χρ<unclear reason="undefined" cert="high">ᾶσ</unclear>θαι καὶ <choice><corr>γεωργεῖν</corr><sic>γεωργουντα</sic></choice> καὶ <choice><corr>διοικεῖν</corr><sic>διοικουντα</sic></choice> περὶ αὐτοῦ καὶ ἑτέροις <choice><corr>παραχωρεῖν</corr><sic>παραχωριν</sic></choice> ᾧ ἐ<unclear reason="undefined" cert="high">ὰν</unclear> αἱρῇ τρό-

    <lb n="14"/>πῳ ἀνεμποδίστως ἐφ᾽ ὃ καὶ μὴ ἐπιπορεύεσθαί <choice><corr><app type="alternative"><lem>σέ με</lem><rdg>ἐμε μηδὲ</rdg></app></corr><sic>σε μη</sic></choice> <sic><unclear reason="undefined" cert="high">μη</unclear></sic> ἄλλον τινὰ ὑπὲρ <choice><corr>ἐμοῦ</corr><sic>σου</sic></choice> τρόπῳ μηδενί, ἀλλὰ καὶ ἐπάναγκες <choice><corr>παρέξομαί</corr><sic>παρεξομε</sic></choice> <supplied reason="lost" cert="high">σ</supplied>οι <choice><corr>τοῦτο</corr><sic>τουτου</sic></choice>

    <lb n="15"/>βέβαιον ἀπὸ πάντων πάσῃ <choice><corr>βεβαιώσει</corr><sic>βαιβεωσω</sic></choice> καὶ καθαρ<unclear reason="undefined" cert="high">ὸ</unclear><supplied reason="lost" cert="high">ν</supplied> ἀπό <choice><corr>τε</corr><sic>δη</sic></choice> δημοσία<supplied reason="omitted" cert="high">ς</supplied> καὶ <sic>οι</sic> ἰδιωτικῆς <choice><corr>ὀφειλῆς</corr><sic>οφιλης</sic></choice> καὶ κατοχῆς καὶ οὑντινοσοῦν

    <lb n="16"/>ἄλλου διὰ παντὸς καὶ ἀπὸ παντὸς καὶ πάντα τὸν ἐπελευσόμενον ἢ ἐμποιησόμενον τούτου χάριν ἀποστήσω παραχ<supplied reason="lost" cert="high">ρ</supplied>ῆμα <choice><corr>τοῖς</corr><sic>της</sic></choice> ἰδ<unclear reason="undefined" cert="high">ί</unclear><supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">ι</unclear>ς <choice><corr>μου</corr><sic>σου</sic></choice> ἀνα-

    <lb n="17"/>λώμασι καὶ <choice><corr>δαπανήμασι</corr><sic>δ<unclear reason="undefined" cert="high">α</unclear>πανηματα</sic></choice> καὶ διπλώνω τὴν τιμὴν ὡς ἴδιον χρέως. περὶ δὲ τοῦ ταῦτα οὕτως ὀρθῶς καλῶς <choice><corr>πεπρᾶχθαι</corr><sic>πεπρακχθαι</sic></choice> <sic>καὶ</sic> ἐπερωτη-

    <lb n="18"/>θεῖσ<supplied reason="omitted" cert="high">α</supplied> ὡμολόγησα. ἡ πρᾶσις κυρία πανταχοῦ ἐπιφερομέ<unclear reason="undefined" cert="high">νη</unclear> πρὸς <choice><corr>ἀσφάλειαν</corr><sic>ασφαλιαν</sic></choice>. <handShift new="m2"/> Αὐρηλία <supplied reason="omitted" cert="high">Ἰουλια</supplied>νὴ <choice><corr>θυγάτηρ</corr><sic>θυκατρος</sic></choice> Ὕμιος ἡ προκειμένη

    <lb n="19"/>πέπρακα <choice><corr>τὸ</corr><sic>δ<unclear reason="undefined" cert="high">ο</unclear></sic></choice> προκείμενον <choice><corr>γεώργιον</corr><sic>γεωρικο<unclear reason="undefined" cert="high">ν</unclear></sic></choice> ὡς περι<supplied reason="lost" cert="high">έ</supplied>χει σὺν τῷ λάκκῳ καὶ <choice><corr>ἀπέσχον</corr><sic>απεσχυ</sic></choice> τὴν τιμὴν ἐκ <supplied reason="omitted" cert="high">π</supplied>λήρου<supplied reason="lost" cert="high">ς</supplied>

    <lb n="20"/>χρυσοῦ <choice><corr>νομισμάτια</corr><sic>νομεισματια</sic></choice> εἴκοσι πέντε καὶ β<unclear reason="undefined" cert="high">ε</unclear>βαιώσω ὡς πρόκειται. Αὐρήλιος Ἀντωνῖνος Οὐαλεντ<supplied reason="lost" cert="high">ι</supplied>νόου

    <lb n="21"/>πρεσβύτερος <choice><corr>ἀξιωθεὶς ἔγραψα</corr><sic>αξιωθι<add place="supralinear">ς</add> εκρψα</sic></choice> ὑπὲρ αὐτῆς <unclear reason="undefined" cert="high">γ</unclear>ράμμα<supplied reason="omitted" cert="high">τα</supplied> <choice><corr>μὴ εἰδυίας</corr><sic>μοι ει<unclear reason="undefined" cert="high">τ</unclear>ηειοις</sic></choice> καὶ μαρτυρῶ.</ab></div><div n="meta.47267" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.47267" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" notBefore="0427" notAfter="0428">427 - 428 (?)</date></p><p><date type="textDate" notBefore="0412" notAfter="0413">412 - 413 (?)</date></p></div><div n="meta.47267" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Alternativdatierung: 412 - 413.</p></div><div n="meta.47267" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">SB</title><biblScope type="volume">22</biblScope><biblScope type="numbers">15618</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">47267</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0239</series>; <biblScope type="volume">22</biblScope>; <biblScope type="numbers">15618</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0239;22;15618" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.47267" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p><bibl type="illustration" default="NO">JRA 5, 1992, Ende des Bandes</bibl></p></div><div n="meta.47267" type="history" subtype="locations" org="uniform" part="N" sample="complete"><p><placeName type="ancientFindspot" cert="high" full="yes">Alaba