    <lb n="1"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="2"/>σε ἐρχόμενον πρὸς τ<gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">αυ</unclear>τα καὶ ε<gap extent="1" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/><unclear cert="high" reason="undefined">εω</unclear><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="3"/>θον ἐνταῦθα. νῦν <supplied cert="high" reason="lost">δὲ ἅπα</supplied><unclear cert="high" reason="undefined">ξ</unclear> καὶ δεύτε<unclear cert="high" reason="undefined">ρο</unclear>ν ἐδε<unclear cert="high" reason="undefined">ξ</unclear>ά<unclear cert="high" reason="undefined">μη</unclear>ν <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> γ</supplied>ρ<supplied cert="high" reason="lost">άμμα-</supplied>

    <lb n="4"/>τα δηλ<supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ῦ</unclear>ντά μοι κ<gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ερχόμενον· ἀλλ᾽ <choice><corr>εἰ</corr><sic>ι</sic></choice> καὶ τὰ μάλιστα <unclear cert="high" reason="undefined">ἐλ</unclear>ύπη-

    <lb n="5"/>σέν με ἡ σὴ ἀπουσί<unclear cert="high" reason="undefined">α</unclear> <gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">αξε</unclear>ω ταῦτα ἐν <choice><corr>μέρει</corr><sic>μερι</sic></choice>. τοῦτο δέ σοι παρατίθημι

    <lb n="6"/>ἀξιῶν καὶ δεόμεν<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ς</supplied> <gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">αι</unclear> <unclear cert="high" reason="undefined">τῇ</unclear> θυγατρὶ <unclear cert="high" reason="undefined">τ</unclear>ῇ ἐμοῦ ἀδελφῇ Θεονίλλᾳ δι<unclear cert="high" reason="undefined">κ</unclear>αι

    <lb n="7"/><choice><corr>παραβάλλειν</corr><sic>παραβαλλιν</sic></choice> καθ᾽ ἑκάσ<supplied cert="high" reason="lost">την ὥραν καὶ </supplied><choice><corr><supplied cert="high" reason="lost">πα</supplied>ραινεῖν</corr><sic><supplied cert="high" reason="lost">πα</supplied><unclear cert="high" reason="undefined">ρ</unclear>αινιν</sic></choice> αὐτὴν τὰ εἰκότα ὅπως μηδα<supplied cert="high" reason="lost">μ</supplied><unclear cert="high" reason="undefined">ῶ</unclear>ς

    <lb n="8"/>λυπηθῇ. οἶδα γὰρ α<supplied cert="high" reason="lost">ὐτὸς καὶ </supplied><choice><corr><supplied cert="high" reason="lost">πέ</supplied>πεισμαι</corr><sic><supplied cert="high" reason="lost">πε</supplied>πισμαι</sic></choice> ὅτι οἱ κοινοὶ ἡμῶν <choice><corr>γονεῖς</corr><sic>γ<supplied cert="high" reason="lost">ο</supplied>νις</sic></choice> <unclear cert="high" reason="undefined">κατ</unclear>α-

    <lb n="9"/>βλέπουσιν αὐτὴν κ<supplied cert="high" reason="lost">αίπερ ἅπαξ δ</supplied><unclear cert="high" reason="undefined">ὲ</unclear> καὶ δεύτερον ἐδεήθην αὐτῶν διὰ γρ<supplied cert="high" reason="lost">αμ</supplied>μά-

    <lb n="10"/>των τὴν αὐτὴ<unclear cert="high" reason="undefined">ν</unclear> <choice><corr>ἐπιμέλειαν</corr><sic><unclear cert="high" reason="undefined">ἐπι</unclear><supplied cert="high" reason="lost">με</supplied><unclear cert="high" reason="undefined">λι</unclear>αν</sic></choice> <supplied cert="high" reason="lost">παρέ</supplied><unclear cert="high" reason="undefined">χ</unclear>ειν <unclear cert="high" reason="undefined">α</unclear>ὐ<unclear cert="high" reason="undefined">τῇ</unclear> καὶ πάντα τὰ εἰωθότα π<supplied cert="high" reason="lost">ο</supplied>ι-

    <lb n="11"/>ῆσαι τῶν λοχίων. οἶδεν γὰρ ὁ θε<unclear cert="high" reason="undefined">ὸ</unclear><supplied cert="high" reason="lost">ς ὅ</supplied><unclear cert="high" reason="undefined">τ</unclear>ι ἐβουλόμην καὶ τὰ μύρα κ<supplied cert="high" reason="lost">αὶ τὰ ἄ</supplied><unclear cert="high" reason="undefined">λ</unclear>λα

