    <lb n="6"/>Διδύμου <expan>δι<ex cert="high">ὰ</ex></expan> <expan>το<ex cert="high">ῦ</ex></expan> <expan>υἱο<ex cert="high">ῦ</ex></expan> <expan><ex cert="high">μοναρτάβου</ex></expan> <expan>κατοί<ex cert="high">κων</ex></expan> <expan>Τεβτύν<unclear cert="high" reason="undefined">ε</unclear>ω<ex cert="high">ς</ex></expan> <expan><ex cert="high">πυροῦ</ex></expan> <expan>μέ<ex cert="high">τρῳ</ex></expan> <expan>δη<ex cert="high">μοσίῳ</ex></expan> <expan>ἔπα<unclear cert="high" reason="undefined">ι</unclear><ex cert="high">τον</ex></expan>

