    <lb n="1"/>κυρίῳ μ<unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">υ</supplied> ἀδελ<unclear reason="undefined" cert="high">φ</unclear>ῷ Ἀ<unclear reason="undefined" cert="high">μ</unclear>μωνιανῷ

    <lb n="2"/>Γ<unclear reason="undefined" cert="high">ε</unclear><supplied reason="lost" cert="high">ρ</supplied>όντιος χαίρειν·

    <lb n="3"/>ἐν τῇ χθὲς <unclear reason="undefined" cert="high">ἦλ</unclear>θον εἰς τὴν Τακόνα καὶ ἐσχόλα-

    <lb n="4"/>σα τῇ κατασ<unclear reason="undefined" cert="high">τ</unclear>άσει τῶν ἄλλων <choice><corr>λειτουργιῶν</corr><sic>λιτουργιων</sic></choice> ἵνα

    <lb n="5"/>δυνηθῇ ἡ <unclear reason="undefined" cert="high">ἀ</unclear><supplied reason="lost" cert="high">π</supplied><unclear reason="undefined" cert="high">α</unclear>ίτησις προχωρῆσαι. θαυμάζω

    <lb n="6"/>δὲ πῶς οὐ<supplied reason="lost" cert="high">δ</supplied><unclear reason="undefined" cert="high">ε</unclear>ίς μοι ἤ<unclear reason="undefined" cert="high">ν</unclear>εγκε γράμματά σου

    <lb n="7"/>καὶ ἀθυμῶ τ<unclear reason="undefined" cert="high">ού</unclear>του ἕνεκα, ἐπειδὴ οὐκ οἶδα

    <lb n="8"/>πῶς διάκ<unclear reason="undefined" cert="high">ε</unclear><supplied reason="lost" cert="high">ι</supplied>ται ἡ οἰκία ἢ τὰ καθ᾽ ἡμᾶς καὶ

    <lb n="9"/>κατὰ τὴν π<supplied reason="lost" cert="high">όλιν.</supplied> σπούδασον οὖν ἔχων πολλοὺς

    <lb n="10"/>ἐρχομένο<unclear reason="undefined" cert="high">υ</unclear><supplied reason="lost" cert="high">ς εἰ</supplied>ς τὴν Τακόνα ἢ τοῦ κονδου-

    <lb n="11"/>κτορίου ἢ <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ῆς</supplied> <unclear reason="undefined" cert="high">κ</unclear>ώμης ἀντιγράψαι μοι περὶ

    <lb n="12"/>π<unclear reason="undefined" cert="high">ά</unclear><supplied reason="lost" cert="high">ντ</supplied>ων. <supplied reason="lost" cert="high">περ</supplied>ὶ δὲ τῶν ἀπαιτήσε<unclear reason="undefined" cert="high">ων</unclear> ἐὰν

    <lb n="13"/> γαρ<gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="5" unit="character"/><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="14"/>τα<gap reason="illegible" extent="2" unit="character"/> <supplied reason="lost" cert="high">Π</supplied><unclear reason="undefined" cert="high">ε</unclear>τερμουθι<gap reason="lost" extent="unknown" unit="character"/>

    <lb n="15"/>τε τῶν ἄλλων ω<gap reason="lost" extent="unknown" unit="character"/>

    <lb n="16"/><gap reason="illegible" extent="3" unit="character"/><gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="2" unit="character"/>οιετο<gap reason="lost" extent="unknown" unit="character"/>

    <lb n="17"/>Ταπο<unclear reason="undefined" cert="high">ῦ</unclear>τος <expan>ἐργ<ex cert="high">άται</ex></expan> <num value="20">κ</num> ὁ<unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">οίως </supplied><gap reason="lost" extent="unknown" unit="character"/>

