    <lb n="1"/><g type="stauros"/> <supplied reason="lost" cert="high">ἐν ὀνόματ</supplied>ι <supplied reason="lost" cert="high">τοῦ κυρίου καὶ δεσπότου</supplied>

    <lb n="2"/>Ἰ<supplied reason="lost" cert="high">η</supplied><unclear reason="undefined" cert="high">σ</unclear>οῦ Χριστ<unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">ῦ το</supplied><unclear reason="undefined" cert="high">ῦ</unclear> <supplied reason="lost" cert="high">οῦ κα</supplied><unclear reason="undefined" cert="high">ὶ</unclear> <supplied reason="lost" cert="high">σωτῆρος ἡμῶν</supplied>

    <lb n="3"/>β<supplied reason="lost" cert="high">α</supplied>σιλείας το<unclear reason="undefined" cert="high">ῦ</unclear> <expan>εὐσεβ<ex cert="high">εστάτου</ex></expan> ἡμῶ<supplied reason="lost" cert="high">ν δεσπ</supplied>ότου <expan><supplied reason="lost" cert="high">Φλ<ex cert="high">αουίου</ex></supplied></expan>

    <lb n="4"/><unclear reason="undefined" cert="high">Ἡ</unclear><supplied reason="lost" cert="high">ρ</supplied><unclear reason="undefined" cert="high">α</unclear>κλείου τοῦ αἰωνίου <expan><supplied reason="lost" cert="high">Αὐγούσ<ex cert="high">του</ex></supplied></expan> καὶ

    <lb n="5"/>Α<unclear reason="undefined" cert="high">ὐ</unclear><supplied reason="lost" cert="high">τ</supplied><unclear reason="undefined" cert="high">ο</unclear>κράτορ<unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">ς</supplied> <unclear reason="undefined" cert="high">ἔτ</unclear><supplied reason="lost" cert="high">ο</supplied>υς <num value="29">κθ</num> <unclear reason="undefined" cert="high">Ἐ</unclear>πεὶ<unclear reason="undefined" cert="high">φ</unclear> <num value="19">ιθ</num>

    <lb n="6"/>ἀρχ<supplied reason="lost" cert="high">ῇ</supplied> <num value="13">ιγ</num> <expan>ἰν<ex cert="high">δικτίωνος</ex></expan> ἐν <expan>Ἀρ<ex cert="high">σινοιτῶν πόλει</ex></expan>·

    <lb n="7"/>Αὐρήλιοι Γε<supplied reason="lost" cert="high">ώ</supplied>ργιος ἀ<unclear reason="undefined" cert="high">να</unclear>φά<unclear reason="undefined" cert="high">λα</unclear>κρος υἱὸς

    <lb n="8"/>Φοιβάμμωνος καὶ Σαμβ<unclear reason="undefined" cert="high">ᾶς</unclear> υἱὸς <unclear reason="undefined" cert="high">Π</unclear>εκυσίου

    <lb n="9"/>ἀπὸ κώμης Ἡρ<unclear reason="undefined" cert="high">α</unclear>κλείας τοῦ <unclear reason="undefined" cert="high">Ἀρ</unclear>σιν<supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">ί</unclear>του νο<unclear reason="undefined" cert="high">μ</unclear>οῦ

    <lb n="10"/>Αὐρηλίοις Φοιβάμμωνι τῷ καὶ <unclear reason="undefined" cert="high">Π</unclear>ατόκῳ

    <lb n="11"/>υἱῷ Κοσμᾶ καὶ <choice><corr>ὁμωνύμῳ</corr><sic>ομονυμω</sic></choice> Γεωργ<unclear reason="undefined" cert="high">ί</unclear>ῳ <expan>Φοιβ<ex cert="high">άμμωνος</ex></expan>

    <lb n="12"/>ἀπὸ τῆς Ἀρσινοιτῶν πόλεως <expan>χ<ex cert="high">αίρειν</ex></expan>· <expan>ὁμολογοῦ<unclear reason="undefined" cert="high">μ</unclear><ex cert="high">εν</ex></expan>

    <lb n="13"/>ἐξ ἀλληλεγγύης ἐκμεμισθωκέναι ὑμ<unclear reason="undefined" cert="high">ῖ</unclear>ν

    <lb n="14"/>τὰς διαφερούσας <choice><corr>ἡμῖν</corr><sic>υμιν</sic></choice> ἐν πεδίῳ τῆς <expan>ἡμετέρ<ex cert="high">ας</ex></expan>

    <lb n="15"/>κώμης ἐν τόπῳ κλήρου <expan>καλουμ<ex cert="high">ένου</ex></expan> Πλαια<unclear reason="undefined" cert="high">τυ</unclear>

    <lb n="16"/>ἐκ βορρᾶ τοῦ κλήρου τοῦ υἱοῦ Θ<unclear reason="undefined" cert="high">αι</unclear>σ<unclear reason="undefined" cert="high">α</unclear>ρίου <expan>ἀρούρ<ex cert="high">ας</ex></expan>

    <lb n="17"/>ὀκτὼ <expan>πλήρ<ex cert="high">ης</ex></expan>, <expan><ex cert="high">ἀρούρας</ex></expan> <num value="8">η</num> <expan>πλήρ<ex cert="high">ης</ex></expan>, πρὸς <choice><corr>τὸ</corr><sic>τ<unclear reason="undefined" cert="high">ω</unclear></sic></choice> <supplied reason="lost" cert="high">ὑμ</supplied><unclear reason="undefined" cert="high">ᾶ</unclear>ς ταύτας

    <lb n="18"/><unclear reason="undefined" cert="high">σ</unclear>π<unclear reason="undefined" cert="high">ε</unclear><supplied reason="lost" cert="high">ί</supplied>ρειν καὶ καρ<supplied reason="lost" cert="high">ποῦ</supplied>σθαι <supplied reason="lost" cert="high">κα</supplied>ὶ προσοδε<unclear reason="undefined" cert="high">ύε</unclear><supplied reason="lost" cert="high">σθ</supplied><unclear reason="undefined" cert="high">αι</unclear>

    <lb n="19"/><unclear reason="undefined" cert="high">ἐ</unclear>νι<unclear reason="undefined" cert="high">α</unclear>υσ<supplied reason="lost" cert="high">ίως ἀ</supplied><unclear reason="undefined" cert="high">π</unclear>ὸ καρ<unclear reason="undefined" cert="high">π</unclear>ῶν τῆς <expan>εἰσιούσ<ex cert="high">ης</ex></expan>

    <lb n="20"/><supplied reason="lost" cert="high">τεσσαρεσκαι</supplied>δεκάτης <expan><supplied reason="lost" cert="high">ἰ</supplied>ν<ex cert="high">δικτίωνος</ex></expan> <unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">α</supplied>ὶ <choice><corr>παρασχεῖν</corr><sic>παρασχομεν</sic></choice>

    <lb n="21"/><supplied reason="lost" cert="high">ἡμῖν ὑπὲρ τοῦ</supplied> φόρ<supplied reason="lost" cert="high">ου</supplied> α<unclear reason="undefined" cert="high">ὐ</unclear>τῶ<supplied reason="lost" cert="high">ν</supplied> ἐνιαυσίως

    <lb n="22"/><supplied reason="lost" cert="high">χρυσ</supplied>οῦ νομ<unclear reason="undefined" cert="high">ισμά</unclear>τια τέσ<unclear reason="undefined" cert="high">σαρα</unclear> <num value="4"/> <supplied reason="lost" cert="high">π</supplied>α-

    <lb n="23"/><supplied reason="lost" cert="high">ρὰ</supplied> <unclear reason="undefined" cert="high">κε</unclear>ρατίων εἴκοσι τριῶν <num value="23"/> <choice><corr>ἐπὶ</corr><sic>επει</sic></choice> <expan>μη<ex cert="high">νὶ</ex></expan>

    <lb n="24"/>Ἐπεὶφ νεομηνίᾳ καὶ μὴ δύνασθαι <choice><corr>ὑμᾶς</corr><sic>ημας</sic></choice>

    <lb n="25"/>ἀγνωμονῆσαι περὶ τὴν δόσι<supplied reason="lost" cert="high">ν</supplied> τοῦ αὐτοῦ φόρου

    <lb n="26"/>ἐνιαυσίως καὶ ἐὰν συμβῇ ἐπιθεῖναι

    <lb n="27"/>προσθήκην ἐπάνω τῶν ἀρ<supplied reason="lost" cert="high">ο</supplied>υρῶν Μηνᾶ υἱοῦ

    <lb n="28"/>Βενιαμῖν ὥστε καὶ <choice><corr>ὑμᾶς</corr><sic>ημας</sic></choice> παρασχεῖν τὴν <expan>προσθήκ<ex cert="high">ην</ex></expan>

    <lb n="29"/>τῶν αὐτῶν ἀρουρῶν ἀναλόγως καὶ ἐὰν συμβῇ

    <lb n="30"/>τὰς αὐτὰς ἀρούρας ἀναλωθῆναι ὑπὸ τοῦ ὕδατος

    <lb n="31"/>ὥστε <choice><corr>ὑμᾶς</corr><sic>ημας</sic></choice> ὑποδεῖξαι <choice><corr>ἡμῖν</corr><sic>υμιν</sic></choice> αὐτὰς καὶ εἶναι

    <lb n="32"/>αὐτὰς χωρὶς δημοσίου καὶ πάλιν ἐὰν σωθῶσι

    <lb n="33"/>ἀπὸ τοῦ ὕδατος καὶ συμβῇ τὰς ἐκ λιβὸς καὶ <expan>ἀνατολ<ex cert="high">ῶν</ex></expan>

    <lb n="34"/>καὶ βορρᾶ αὐτῶν ἀρούρας σπαρῆναι ὥστε

    <lb n="35"/><choice><corr>ὑμᾶς</corr><sic>ημας</sic></choice> πάλιν παρασχεῖν <choice><corr>ἡμῖν</corr><sic>υ<unclear reason="undefined" cert="high">μ</unclear>ιν</sic></choice> τὰ αὐτὰ τέσσαρ<supplied reason="lost" cert="high">α</supplied> <num value="4"/> <expan>νομ<ex cert="high">ισμάτια</ex></expan>

    <lb n="36"/>τοῦ φόρου ἐτησίως. <choice><corr>κατεβάλομεν</corr><sic>καταβαλομεν</sic></choice> δὲ ἐφ᾽ ὑμᾶς

    <lb n="37"/>καὶ τὰ τιμήματα κατὰ μίμησιν τοῦ αὐτοῦ

    <lb n="38"/>Μηνᾶ καὶ ἐὰν ἐθελήσητε <choice><corr>ἰδιοσπορήσειν</corr><sic>ιδιοσπορησι</sic></choice>

    <lb n="39"/>αὐτὰς ἄδειαν ὑμᾶς ἔχειν εἰς το<supplied reason="lost" cert="high">ῦ</supplied>το. ἡ μίσθωσις

    <lb n="40"/>κυρία <expan><ex cert="high">καὶ</ex></expan> <expan>ἐπερ<ex cert="high">ωτηθέντες</ex></expan> <expan>ὡμ<ex cert="high">ολογήσαμεν</ex></expan>. <g type="stauros"/> Γεώργιος <expan><ex cert="high">καὶ</ex></expan> Σαμβᾶς στοι-

    <lb n="41"/>χεῖ ἡμῖν ὡς <expan>πρόκ<ex cert="high">ειται</ex></expan>.

    <lb n="42"/><handShift new="m2"/><g type="stauros"/> <foreign lang="la">Di emu Georgiu etelioth</foreign>.
