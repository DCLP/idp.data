    <lb n="1"/>[<app type="alternative"><lem>ἔτους <num value="1">α</num></lem><rdg><expan><ex cert="high">ἔτους</ex></expan> πρώτου</rdg></app> Αὐτοκράτορος Καίσαρος Μάρκου] Κλαυδίου Τακίτου Εὐσε<unclear reason="undefined" cert="high">β</unclear><supplied reason="lost" cert="high">οῦς Εὐτυχοῦς Σεβαστοῦ Παῦ</supplied>νι <num value="29">κθ</num>. διαγραφὴ <expan>δι<ex cert="high">ὰ</ex></expan> τῆς ἐν Ἑρ[μοῦ πόλει <gap reason="illegible" extent="unknown" unit="character"/> τραπέζης. <space extent="unknown" unit="character" dim="horizontal"/>]

    <lb n="2"/>[<expan>Αὐρ<ex cert="high">ηλία</ex></expan> <gap reason="illegible" extent="6" unit="character"/>α ἡ καὶ Ἀρμενία Ἑρμίνου <note lang="en" anchored="yes" place="not specified">?</note> τοῦ] <unclear reason="undefined" cert="high">κ</unclear>αὶ Διδυμάμμωνος <expan>ἄρ[ξ<ex cert="high">αντος</ex></expan> <gap reason="illegible" extent="18" unit="character"/> <expan>Αὐρ<ex cert="high">ηλίᾳ</ex></expan> Κορ]νηλι´ᾳ Ἑρμίνου τοῦ καὶ Μέλανος <unclear reason="undefined" cert="high">γε</unclear><supplied reason="lost" cert="high">νομένου </supplied><gap reason="lost" extent="38" unit="character"/>

    <lb n="3"/>[<gap reason="illegible" extent="5" unit="character"/> ἀναγραφομένῃ ἐπὶ τοῦ αὐτοῦ <note lang="en" anchored="yes" place="not specified">?</note> ἀμ]φόδου πεπρακυίῃ αὐτῇ <supplied reason="lost" cert="high">τὰ ὑπάρχοντα αὐτῇ περὶ Μαγδῶλα</supplied> <unclear reason="undefined" cert="high">Β</unclear>ουκόλων ἐκ τοῦ Διοκλέο<unclear reason="undefined" cert="high">υ</unclear><supplied reason="lost" cert="high">ς κλήρου </supplied><gap reason="lost" extent="39" unit="character"/>

    <lb n="4"/><gap reason="lost" extent="29" unit="character"/><supplied reason="lost" cert="high"> ὑ</supplied>δροδοχείῳ καὶ λάκκο<supplied reason="lost" cert="high">ις </supplied><gap reason="lost" extent="10" unit="character"/><supplied reason="lost" cert="high"> καὶ ὀργάνῳ ὑ</supplied>δραντλητικῷ καὶ πωμαρίοις καὶ τῇ <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="5"/>[<gap reason="illegible" extent="5" unit="character"/> τιμὴν ἀργυρίου δραχμῶν <gap reason="illegible" extent="4" unit="character"/>α]<unclear reason="undefined" cert="high">κ</unclear>ισχειλίων <gap reason="illegible" extent="unknown" unit="character"/> ἃ <unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">αὶ αὐτόθι ἀπέσχεν παρὰ τ</supplied>ῆς <expan>ὠνουμ<ex cert="high">ένης</ex></expan> κατὰ τήνδε τὴν διαγρ[αφὴν καὶ εἶναι περὶ αὐτὴν τὴν <expan>ὠνουμ<ex cert="high">ένην</ex></expan> καὶ τοὺς παρ᾽ αὐτῆς]

    <lb n="6"/>[τὴν τούτων κυριείαν καὶ κράτησιν καθὼς <expan>γ]έγρ<ex cert="high">απται</ex></expan>. <handShift new="m2"/> <expan>Αὐρ<unclear reason="undefined" cert="high">ήλ</unclear><ex cert="high">ιος</ex></expan> Ἰσί<unclear reason="undefined" cert="high">δ</unclear>[ωρος τραπεζίτης <expan>σεση]<unclear reason="undefined" cert="high">μ</unclear><ex cert="high">είωμαι</ex></expan>.

    <lb n="7"/><handShift new="m3"/><supplied reason="lost" cert="high">Αὐρηλία Κορνηλία Ἑρμίνου τοῦ καὶ Μ</supplied>έλανος γενομέν<supplied reason="lost" cert="high">ου </supplied><gap reason="lost" extent="13" unit="character"/><supplied reason="lost" cert="high"> Ἑρμ</supplied>οῦ πόλεως τῆς μεγάλης <supplied reason="lost" cert="high">ἀρχαίας καὶ λαμπρᾶς καὶ σεμνοτάτης χρηματίζουσα</supplied>

    <lb n="8"/><supplied reason="lost" cert="high">χωρὶς κυρίου τέκνων δικαίῳ κατὰ</supplied> <unclear reason="undefined" cert="high">τ</unclear>οὺς νόμους συνεστ<supplied reason="lost" cert="high">ῶτός μοι τοῦ ἀδελφο</supplied>ῦ μου Αὐρηλίου Ἑρμοφίλ<supplied reason="lost" cert="high">ου ἐπηκολούθηκα τῇδε τῇ διαγραφῇ καὶ πέπρακα ἀπὸ τοῦ</supplied>

    <lb n="9"/>[νῦν ἐπὶ τὸν ἀεὶ χρόνον <expan>Αὐρ<ex cert="high">ηλίᾳ</ex></expan> <gap reason="illegible" extent="6" unit="character"/>]<gap reason="illegible" extent="1" unit="character"/>ᾳ τῇ καὶ Ἀρμενίᾳ <supplied reason="lost" cert="low">ἙρμίνουἙρμοπολίτο</supplied>υ εὐθηνιαρχήσαντος [τῆς λαμπροτάτης πόλεως τῶν <expan>Ἀλεξ<ex cert="high">ανδρέων</ex></expan> συμπαρόντος αὐτῇ]

    <lb n="10"/><gap reason="lost" extent="27" unit="character"/> ἄρξαντος Ἀλεξ<supplied reason="lost" cert="high">ανδρείας τὸ ὑπάρχο</supplied>ν μοι περὶ Μαγδῶλα Βου<supplied reason="lost" cert="high">κόλων </supplied><gap reason="lost" extent="40" unit="character"/>

    <lb n="11"/><gap reason="lost" extent="27" unit="character"/>μενον σὺν τοῖς ἐνο<supplied reason="lost" cert="high">ῦσι </supplied><gap reason="lost" extent="13" unit="character"/><supplied reason="lost" cert="high"> κα</supplied>ὶ τῇ ἐν αὐτῷ <choice><corr>καλαμείᾳ</corr><sic>καλαμια</sic></choice> οὐ<gap reason="lost" extent="45" unit="character"/>

    <lb n="12"/><gap reason="lost" extent="22" unit="character"/><supplied reason="lost" cert="high"> περισ</supplied>τερεῶνι καὶ β<unclear reason="undefined" cert="high">οσ</unclear>κ<supplied reason="lost" cert="high">ήματι καὶ τοῖς συγκυ</supplied><unclear reason="undefined" cert="high">ρο</unclear><supplied reason="lost" cert="high">ῦσ</supplied>ι πᾶσι, ὃ π<unclear reason="undefined" cert="high">α</unclear>ραδ<supplied reason="lost" cert="high">ώσω </supplied><gap reason="lost" extent="42" unit="character"/>

    <lb n="13"/>[<gap reason="illegible" extent="24" unit="character"/> <expan>οἰκ]οπ<ex cert="high">εδ- </ex></expan> Δημητρίας Φλα<gap reason="lost" extent="15" unit="character"/><supplied reason="lost" cert="high"> τὴν δὲ πρὸς</supplied> ἀλλήλας συνπεφωνη<supplied reason="lost" cert="high">μένην τιμὴν ἀργυρίου δραχμὰς </supplied><gap reason="lost" extent="4" unit="character"/><supplied reason="lost" cert="high">ακισχιλίας αὐτόθι ἀπέσχον</supplied>

    <lb n="14"/>[παρὰ τῆς <expan>ὠνουμ<ex cert="high">ένης</ex></expan> καὶ εἶναι τὴν τ]<unclear reason="undefined" cert="high">ῶ</unclear>ν πεπραμένω<supplied reason="lost" cert="high">ν </supplied><gap reason="lost" extent="14" unit="character"/><supplied reason="lost" cert="high"> κυριείαν κα</supplied>ὶ κράτ<supplied reason="lost" cert="high">ησι</supplied>ν περὶ τὴν <unclear reason="undefined" cert="high">ὠ</unclear><supplied reason="lost" cert="high">νουμένην, χρωμένους καὶ οἰκονομοῦντας περὶ αὐτῶν καθ᾽</supplied>

    <lb n="15"/><supplied reason="lost" cert="high">ὃν ἐὰν αἱρῶνται τρόπον, καρπιζομ</supplied>ένους καὶ ἀποφε<supplied reason="lost" cert="high">ρομένους τὰ ἐξ αὐτῶν κατ᾽ ἔτος γε</supplied><unclear reason="undefined" cert="high">ν</unclear>ήματα καὶ περιεσ<supplied reason="lost" cert="high">όμενα, πάντων δὲ τῶν δημοσίων καὶ ἐπιμερισμῶν τῶν μὲν</supplied>

    <lb n="16"/>[ἔμπροσθεν χρόνων μέχρι τοῦ <expan>διελ<ex cert="high">θόντος</ex></expan> <num value="6">ϛ</num> <num value="1/2">𐅵</num> <expan><ex cert="high">ἔτους</ex></expan> ὄ]ντων πρὸς ἐμὲ τ<supplied reason="lost" cert="high">ὴν πεπρακυῖαν, τῶν δὲ ἀπὸ τοῦ ἐνεσ</supplied>τῶτος <num value="1">α</num> <num value="1/2">𐅵</num> ὄντω<supplied reason="lost" cert="high">ν πρὸς σὲ τὴν ὠνουμένην, τῆς βεβαιώσεως διὰ παντὸς πρὸς πᾶσαν βεβαίωσιν ἐξακολουθούσης μοι τῇ πωλούσῃ</supplied>

    <lb n="17"/><supplied reason="lost" cert="high">καὶ μὴ ἐπελεύσεσθαι ἐπὶ σὲ τὴν ὠ</supplied>νουμένην μηδ᾽ <supplied reason="lost" cert="high">ἐπὶ τοὺς παρὰ σοῦ περὶ μηδενὸς τῆσ</supplied>δε τῆς πράσεως τρ<supplied reason="lost" cert="high">όπῳ μηδενὶ, ἐὰν δὲ ἐπέλθω ἢ μὴ βεβαιῶ, ἥ τ᾽ ἔφοδος ἄκυρος ἔστω καὶ προσαποτίσω τῇ ὠνουμένῃ τά τε βλάβη καὶ</supplied>

    <lb n="18"/><supplied reason="lost" cert="high">δαπανήματα καὶ ἐπιτίμου ὡς ἴδιον</supplied> χρέος διπλῆν τὴν <supplied reason="lost" cert="high">τιμὴν καὶ εἰς τὸ δημόσιον τὴν ἴσην κ</supplied>αὶ μη<unclear reason="undefined" cert="high">δὲ</unclear>[ν ἧσσον ἡ πρᾶσις κυρία, καὶ <expan>ἐπερω<ex cert="high">τηθεῖσα</ex></expan> <expan>ὡμολ<ex cert="high">όγησα</ex></expan>. <expan><ex cert="high">ἔτους</ex></expan> <num value="1">α</num> <expan>Αὐτοκρά<ex cert="high">τορος</ex></expan> <expan>Καίσ<ex cert="high">αρος</ex></expan> Μάρκου Κλαυδίου]

    <lb n="19"/>[Τακίτου <expan>Εὐσεβ<ex cert="high">οῦς</ex></expan> <expan>Εὐτυχ<ex cert="high">οῦς</ex></expan> <expan>Σεβ<ex cert="high">αστοῦ</ex></expan> <gap reason="illegible" extent="unknown" unit="character"/> ] <num value="2">β</num>. <handShift new="m4"/> <expan>Α<ex cert="high">ὐρηλία</ex></expan> Κορν<supplied reason="lost" cert="high">ηλία Ἑρμίνου τοῦ καὶ Μέλαν</supplied>ος πέπ<supplied reason="lost" cert="high">ρακα καὶ ἀπέσχον τὴν τιμὴν καὶ βεβαιώσω ὡς πρόκειται.</supplied>

    <lb n="20"/>[<expan>Αὐρ<ex cert="high">ήλιος</ex></expan> Ἑρμόφιλος <gap reason="illegible" extent="15" unit="character"/>]υ <choice><corr>συμπάρειμ<supplied reason="lost" cert="high">ι</supplied></corr><sic>συμπαριμι<supplied reason="lost" cert="high">ι</supplied></sic></choice>[ καὶ ἔγραψα ὑπὲρ αὐτῆς γράμματα μὴ εἰδυίης <note lang="en" anchored="yes" place="not specified">?</note>]

