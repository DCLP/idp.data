    <lb n="11"/><supplied cert="high" reason="lost">μὴ ἀποδ</supplied>ῷ <supplied cert="high" reason="lost">μοι</supplied> <unclear cert="high" reason="undefined">τὰ</unclear> <unclear cert="high" reason="undefined">ὡ</unclear>ρισμένα αὐτῇ ὑπάρ<unclear cert="high" reason="undefined">χ</unclear>οντα, ἃ ἐστιν περὶ μὲν κώμην Ἰβιῶνα 

    <lb n="12"/><supplied cert="low" reason="lost">Ἀρσάμου</supplied><gap extent="3" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/> <expan><ex cert="high">ἄρουραι</ex></expan> <num value="25">κε</num>, καὶ ἐν τῇ μητροπόλει ἐντὸς τοῦ καινοῦ Ἀκώριος περιβόλου <expan><ex cert="high">τέταρτον</ex></expan> 

    <lb n="13"/><supplied cert="high" reason="lost">μέρος οἰκίας καὶ</supplied> <unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ἰθρίο</supplied><unclear cert="high" reason="undefined">υ</unclear> καὶ αὐλῆς καὶ ἄλλων τῶν συνκυρόντων πάντων <unclear cert="high" reason="undefined">πα</unclear><supplied cert="high" reason="lost">ν</supplied><unclear cert="high" reason="undefined">τοί</unclear>ων 

