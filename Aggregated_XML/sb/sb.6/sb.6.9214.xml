    <lb n="1"/><choice><corr>ὑπατείας</corr><sic>υπατιας</sic></choice> τῶν δεσπ<supplied reason="lost" cert="high">οτ</supplied>ῶν ἡμῶν 

    <lb n="2"/>Γαλερίου Οὐαλερίου Μα<supplied reason="lost" cert="high">ξι</supplied><unclear reason="undefined" cert="high">μ</unclear>ιανοῦ Σε<supplied reason="lost" cert="high">β</supplied>α<unclear reason="undefined" cert="high">σ</unclear>τοῦ 

    <lb n="3"/>τὸ <num value="8">η</num> καὶ Γαλερίου Οὐαλ<supplied reason="lost" cert="high">ερίο</supplied>υ Μαξιμίνου 

    <lb n="4"/>Σεβαστοῦ τὸ <num value="2">β</num>. 

    <lb n="5"/>Αὐρήλιος Σακαῶνος Πόσ<unclear reason="undefined" cert="high">ιτ</unclear>ος 

    <lb n="6"/><expan>μη<ex cert="high">τρὸς</ex></expan> Θαήσιος ἀπὸ κώμης <unclear reason="undefined" cert="high">Τ</unclear><supplied reason="lost" cert="high">ερ</supplied>ύθ<unclear reason="undefined" cert="high">ε</unclear>ω<unclear reason="undefined" cert="high">ς</unclear> 

