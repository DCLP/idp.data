    <lb n="1"/><choice><corr>ὑπατείας</corr><sic>υπατιας</sic></choice> τῶν δεσπ<supplied cert="high" reason="lost">οτ</supplied>ῶν ἡμῶν 

    <lb n="2"/>Γαλερίου Οὐαλερίου Μα<supplied cert="high" reason="lost">ξι</supplied><unclear cert="high" reason="undefined">μ</unclear>ιανοῦ Σε<supplied cert="high" reason="lost">β</supplied>α<unclear cert="high" reason="undefined">σ</unclear>τοῦ 

    <lb n="3"/>τὸ <num value="8">η</num> καὶ Γαλερίου Οὐαλ<supplied cert="high" reason="lost">ερίο</supplied>υ Μαξιμίνου 

    <lb n="4"/>Σεβαστοῦ τὸ <num value="2">β</num>. 

    <lb n="5"/>Αὐρήλιος Σακαῶνος Πόσ<unclear cert="high" reason="undefined">ιτ</unclear>ος 

    <lb n="6"/><expan>μη<ex cert="high">τρὸς</ex></expan> Θαήσιος ἀπὸ κώμης <unclear cert="high" reason="undefined">Τ</unclear><supplied cert="high" reason="lost">ερ</supplied>ύθ<unclear cert="high" reason="undefined">ε</unclear>ω<unclear cert="high" reason="undefined">ς</unclear> 

    <lb n="7"/>τοῦ Ὀξυρ<unclear cert="high" reason="undefined">υ</unclear>γχείτου νο<supplied cert="high" reason="lost">μοῦ</supplied> Αὐ<supplied cert="high" reason="lost">ρ</supplied><unclear cert="high" reason="undefined">η</unclear>λ<unclear cert="high" reason="undefined">ίῳ</unclear> 

    <lb n="8"/>Ἡρακλᾷ Ὡ<unclear cert="high" reason="undefined">ρί</unclear>ωνος δι<gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>μιω<gap extent="1" reason="lost" unit="character"/>ος 


    <lb n="9"/>ἀπὸ τῆ<unclear cert="high" reason="undefined">ς</unclear> <expan>λαμ<ex cert="high">πρᾶ</ex></expan>ς καὶ <expan>λαμ<ex cert="high">προτάτη</ex></expan>ς <expan>Ὀξυρυγχ<ex cert="high">ειτῶν</ex></expan> 

    <lb n="10"/>πόλεως <space dim="horizontal" extent="unknown" unit="character"/> <expan>χαίρ<ex cert="high">ειν</ex></expan>. ὁμολογῶ 

    <lb n="11"/>πεπρακέναι σοι ἐπ᾽ ἀγορᾶς τοῦ <expan>Ὀξυ<ex cert="high">ρυγχείτου</ex></expan> 

    <lb n="12"/>ὄνον <choice><corr>ἄρρενον</corr><sic>αρενον</sic></choice> μελανόχρωτα 

    <lb n="13"/>ἄβωλον τιμῆς τῆς συνπεφω-

    <lb n="14"/>νημένης πρὸς ἀλλήλους ἀργ<supplied cert="high" reason="lost">υρίο</supplied>υ 

    <lb n="15"/>Σεβαστῶν νομίσματος <choice><corr>τάλα<supplied cert="high" reason="lost">ν</supplied>-

    <lb n="16"/>των</corr><sic>ταλα<supplied cert="high" reason="lost">ν</supplied>τα</sic></choice> ἓξ καὶ <choice><corr>δραχμῶν</corr><sic>δραχμας</sic></choice> <choice><corr>τρισχιλίων</corr><sic>τρισχιλιας</sic></choice>, 

    <lb n="17"/><expan>γί<ex cert="high">νονται</ex></expan> <expan><ex cert="high">τάλαντα</ex></expan> <num value="6">ϛ</num> <expan><ex cert="high">δραχμαὶ</ex></expan> <num value="3000">Γ</num>, ἅπερ <choice><corr>αὐτόθι</corr><sic>αυ<unclear cert="high" reason="undefined">τ</unclear>ο<unclear cert="high" reason="undefined">τ</unclear>ι</sic></choice> ἀ<unclear cert="high" reason="undefined">π</unclear>έσχον 

    <lb n="18"/>παρὰ σοῦ ἐκ πλήρους διὰ χειρός, 

    <lb n="19"/>περὶ ἧς ἀριθμήσεως <choice><corr>ἐπερωτη-

    <lb n="20"/>θεὶς</corr><sic>επερωτηθις</sic></choice> ὡμολόγησα. τὸ<supplied cert="high" reason="lost">ν</supplied> δ᾽ αὐτὸν ὄνον 

    <lb n="21"/>ἐντεῦθεν <choice><corr>παρείληφας</corr><sic>παριληφας</sic></choice> παρ᾽ ἐμοῦ <supplied cert="high" reason="lost">τοῦ</supplied>-

    <lb n="22"/>τον τοιοῦτον <choice><corr>ἀναπόρριφον</corr><sic>αναποριφον</sic></choice> καὶ ἀν-

    <lb n="23"/>επίλημπτον, ὃν καὶ <choice><corr>βεβαιώσω</corr><sic>βαιβαιωσ<unclear cert="high" reason="undefined">ω</unclear></sic></choice> 

    <lb n="24"/><choice><corr>πάσῃ</corr><sic>πασι</sic></choice> <choice><corr>βεβαιώσει</corr><sic>βεβαιωσι</sic></choice>, καὶ τὸν ἐπελευ-

    <lb n="25"/>σόμενον τούτο<supplied cert="high" reason="lost">υ χά</supplied>ριν ἐπάναγκες 

    <lb n="26"/>ἀποστήσω παραχρῆ<unclear cert="high" reason="undefined">μα</unclear> <unclear cert="high" reason="undefined">τ</unclear>α<unclear cert="high" reason="undefined">ῖς</unclear> <unclear cert="high" reason="undefined">ἐ</unclear>μαυτοῦ 

    <lb n="27"/>δαπάναις καθάπερ ἐκ <supplied cert="high" reason="lost">δίκ</supplied>ης, κυρία 

    <lb n="28"/>ἡ πρᾶσις <supplied cert="high" reason="lost">ἁ</supplied>πλῆ γρα[φεῖσα, καὶ <choice><corr>ἐπερω]-

    <lb n="29"/>τηθεὶς</corr><sic><supplied cert="high" reason="lost">επερω</supplied>τηθις</sic></choice> ὡμολόγησα. <supplied cert="high" reason="lost">τῆς προκειμένης</supplied> 

    <lb n="30"/><choice><corr>ὑπατείας</corr><sic>υπατιας</sic></choice> μηνὶ <hi rend="diaeresis">Ἰ</hi>ουλί<supplied cert="high" reason="lost">ῳ </supplied><gap extent="2" reason="lost" unit="character"/> 

    <lb n="31"/><handShift new="m2"/> Αὐρήλιος Ζακαῶ<supplied cert="high" reason="lost">ν πέπρακά σοι</supplied> 

    <lb n="32"/>τὸν προκίμενον ὄν<supplied cert="high" reason="lost">ον καὶ ἀπέχω</supplied> 

    <lb n="33"/>τὴν τιμὴν πλήρη<supplied cert="high" reason="lost">ς, ὡς πρόκειται.</supplied> 

    <lb n="34"/>Αὐρήλιος Ὡρίων ἔγρα<supplied cert="high" reason="lost">ψα ὑπὲρ αὐτοῦ</supplied> 

    <lb n="35"/>μὴ εἰδότος γράμμα<supplied cert="high" reason="lost">τα</supplied>. </ab></div><div n="meta.17871" part="N" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.17871" part="N" subtype="textDate" type="commentary"><p><date notAfter="0311-07-31" notBefore="0311-07-01" type="textDate">1. - 31. Juli 311</date></p></div><div n="meta.17871" part="N" subtype="general" type="commentary"><p>Zur Datierung vgl. BL VIII, S. 341.</p></div><div n="meta.17871" part="N" subtype="principalEdition" type="bibliography"><listBibl><bibl subtype="principal" type="publication"><title level="s" type="abbreviated">SB</title><biblScope type="volume">6</biblScope><biblScope type="numbers">9214</biblScope></bibl><bibl type="Trismegistos"><biblScope type="numbers">17871</biblScope></bibl><bibl type="DDbDP"><series>0239</series>; <biblScope type="volume">6</biblScope>; <biblScope type="numbers">9214</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0239;6;9214" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.17871" part="N" subtype="corrections" type="bibliography"><head>BL-Einträge nach BL-Konkordanz</head><bibl type="BL"><biblScope type="volume">VIII</biblScope>,S. <biblScope type="pages"/></bibl></div><div n="meta.17871" part="N" subtype="illustrations" type="bibliography"><p><bibl type="illustration">CE 33, 1958, Fig. 18, S. 270</bibl><bibl type="illustration">Youtie, Scriptiunculae II, Plate I</bibl></p></div><div n="meta.17871" part="N" subtype="locations" type="history"><p><placeName cert="high" type="ancientFindspot">Oxyrhynchos</placeName></p></div><div n="meta.17871" part="N" type="figure"><p><figure href="http://www.papyrology.uw.edu.pl/papyrology/papyri/PVrat22.html"><figDesc>Abbildung im Internet</figDesc></figure></p></div></body></text></TEI.2>