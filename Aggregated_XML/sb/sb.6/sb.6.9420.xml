    <lb n="1"/>παση<gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="3" unit="character"/> 

    <lb n="2"/>τοῦ <supplied reason="lost" cert="high">Ὀ</supplied>ννώφριος τῶν ἀπὸ τοῦ αὐτοῦ 

    <lb n="3"/>τόπ<supplied reason="lost" cert="high">ο</supplied>υ στρατιωτῶν <supplied reason="lost" cert="high">ἐν τῷ τοῦ</supplied> <num value="40">μ</num> <expan><ex cert="high">ἔτους</ex></expan> 

    <lb n="4"/>Φαῶφι, συγγραψαμένων μοι αὐτῶν 

    <lb n="5"/>κατὰ συγγραφὴν Αἰ<supplied reason="lost" cert="high">γυπτί</supplied>αν δανείου 

    <lb n="6"/><supplied reason="lost" cert="high">χαλ</supplied>κοῦ <expan>τάλ<ex cert="high">αντα</ex></expan> <num value="6">ϛ</num> <expan><ex cert="high">δραχμὰς</ex></expan> <num value="4000">Δ</num> τιμὴν πυροῦ <num value="100">ρ</num> 

    <lb n="7"/>ὧν ἤμην δι᾽ αὐτῶν <supplied reason="lost" cert="high">π</supplied>αραμε<supplied reason="lost" cert="high">μετ</supplied>ρηκ<supplied reason="lost" cert="high">υῖα</supplied> 

    <lb n="8"/>τοῖς ἐν τῷ σημείῳ αὐ<supplied reason="lost" cert="high">τ</supplied>ῶν στρατιώ<supplied reason="lost" cert="high">ταις</supplied>, 

    <lb n="9"/>ἐφ᾽ ᾧ διαγράφουσί μοι αὐτὰ ἐν τῷ 

    <lb n="10"/>Φαρμοῦθι μηνὶ τοῦ αὐτοῦ <expan><ex cert="high">ἔτους</ex></expan> ἢ ὅτι ἐ<supplied reason="lost" cert="high">ν</supplied> 

    <lb n="11"/><supplied reason="lost" cert="high">τ</supplied>ῷ Παχῶνι μηνὶ <choice><corr>τὰ αὐτά</corr><sic>ταυτα</sic></choice> τε καὶ τὸ 

    <lb n="12"/>ἡμ<supplied reason="lost" cert="high">ιό</supplied>λιον. καίπερ ἄλλων τῶν ὁμοίων μ<supplied reason="lost" cert="high">οι</supplied> 

    <lb n="13"/>παραχρῆμα εἰληφότων τὴν τιμὴν 

    <lb n="14"/>τοῦ αὐτῶν πυροῦ, συμπεριενενηγμέν<supplied reason="lost" cert="high">ης</supplied> 

    <lb n="15"/>δέ μ<supplied reason="lost" cert="high">ου</supplied> τοῖς ἐνκαλουμένοις ἄλλον ἐνιαυτὸ<supplied reason="lost" cert="high">ν</supplied> 

    <lb n="16"/>ἕνα <num value="1"/>, νυνὶ πλεονάκις <supplied reason="lost" cert="high">ἀπ</supplied>αιτούμενοι 

    <lb n="17"/>οὐκ ἀ<supplied reason="lost" cert="high">π</supplied>οδίδωσι. διὸ ἀξιῶ, <choice><corr>ἐὰν</corr><sic>εαμ</sic></choice> φαίνηται, 

    <lb n="18"/>συντάξαι ἀνακαλεσαμένους αὐτοὺς 

    <lb n="19"/>ἐπισκέψασθαι περὶ ὧν γράφω, <choice><corr>καὶ ἂν</corr><sic>καν</sic></choice> ᾖ 

    <lb n="20"/>ἀληθ<supplied reason="lost" cert="high">ῆ</supplied> συναναγκάσαι αὐτοὺς ἀποδοῦναί 

    <lb n="21"/>μοι τὰ τοῦ χαλκοῦ <expan>τάλ<ex cert="high">αντα</ex></expan> <num value="6">ϛ</num> <expan><ex cert="high">δραχμὰς</ex></expan> <num value="4000">Δ</num> καὶ τοὺς <supplied reason="lost" cert="high">τού</supplied>-

    <lb n="22"/>των συναχθησομένους τόκους. τούτου 

    <lb n="23"/>δὲ γενομένου ἔσομαι τετευχυῖα τῆς 

    <lb n="24"/>παρὰ σοῦ ἀντιλήψεως. 

