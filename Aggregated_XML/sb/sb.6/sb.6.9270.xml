    <lb n="1"/><gap extent="unknown" reason="lost" unit="character"/> Αὐρή<supplied cert="high" reason="lost">λιος Ῥοῦφος</supplied> 

    <lb n="2"/><supplied cert="high" reason="lost">Λογγίνου ἀπὸ</supplied> κώμης <supplied cert="high" reason="lost">Ἄ</supplied>ρ<unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">ως</supplied> 

    <lb n="3"/><gap extent="unknown" reason="lost" unit="character"/> χαίρειν. ὁμ<supplied cert="high" reason="lost">ολογῶ</supplied> 

    <lb n="4"/><supplied cert="high" reason="lost">ἐσχηκέναι καὶ δεδανεῖ</supplied>σθαι παρὰ σοῦ εἰς ἰδίαν μου 

    <lb n="5"/><supplied cert="high" reason="lost">καὶ ἀναγκαίαν χρείαν</supplied> διὰ χειρὸς ἐξ οἴκου <unclear cert="high" reason="undefined">παρα</unclear>-

    <lb n="6"/><supplied cert="high" reason="lost">χρῆμα ἀργυρίου</supplied> σεβαστῶν νομίσματος 

