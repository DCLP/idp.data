    <lb n="1"/>ἔτους <choice><corr>τρεισκαιδεκάτου</corr><sic>τρισκαιδεκατου</sic></choice> [Τιβερίου Καίσαρος Σεβαστοῦ μηνὸς <unclear cert="high" reason="undefined">Σεβαστοῦ</unclear>] 

    <lb n="2"/>ὀκτωκαιδεκάτηι ἐν Τ<unclear reason="undefined" cert="high">εβ</unclear><supplied reason="lost" cert="high">τύνει τῆς Πολέμονος μερίδος τοῦ Ἀρσινοείτου</supplied> 

    <lb n="3"/>νομοῦ. ἐμίσθωσεν Ἁρυώτης Ἁρυώτου <unclear reason="undefined" cert="high">ὡς</unclear> <supplied reason="lost" cert="high">ἐτῶν </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> οὐλὴ</supplied> 

    <lb n="4"/>μετόπωι <choice><corr>ἐξ</corr><sic>εκς</sic></choice> ἀριστερῶν Παώπι Ὀρσεῦτος ὡς [ἐτῶν <gap reason="illegible" extent="unknown" unit="character"/> οὐλὴ <choice><corr>δα]-

    <lb n="5"/>κτύλῳ</corr><sic><supplied reason="lost" cert="high">δα</supplied>κτυλο</sic></choice> <choice><corr>πρῳτῳ</corr><sic>προτωι</sic></choice> <choice><corr>χειρὸς</corr><sic>χιρος</sic></choice> ἀριστερᾶς καὶ <choice><corr>τῷ</corr><sic>των</sic></choice> <choice><corr>υἱ<supplied reason="lost" cert="high">ῷ</supplied></corr><sic>υει<supplied reason="lost" cert="high">ω</supplied></sic></choice> <supplied reason="lost" cert="high">αὐτοῦ Ὀρσεῦτι ὡς ἐτῶν τριάκον</supplied>-

    <lb n="6"/><unclear cert="high" reason="undefined">τα</unclear> οὐλὴ μετόπωι <choice><corr>ἐξ</corr><sic>εκς</sic></choice> ἀριστερῶν καὶ <unclear reason="undefined" cert="high">Σε</unclear>μέν<supplied reason="lost" cert="high">ι Ἁρυώτου ὡς ἐτῶν </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> οὐλὴ</supplied> 

    <lb n="7"/>μετόπωι <choice><corr>ἐξ</corr><sic>εκς</sic></choice> ἀριστερῶν τοῖς τρισὶ <num value="3"/> <choice><corr>Πέρσαις</corr><sic>περσες</sic></choice> τ<supplied reason="lost" cert="high">ῆς ἐπιγονῆς ἀλλήλων ἐγγύοις</supplied> 

    <lb n="8"/>εἰς ἔκτισιν μεμισθωκέναι αὐτοῖς τὸν ὑπ<supplied reason="lost" cert="high">άρχοντα αὐτῷ περὶ Τεβτύνιν κλῆρον</supplied> 

    <lb n="9"/><choice><corr>κατοικικὸν</corr><sic>κατυκικον</sic></choice> ἀρουρῶν εἴκοσι ἓξ <num value="26"/> ἢ ὅσων ἐ<supplied reason="lost" cert="high">ὰν ὦσιν εἰς τὸ ἐνεστὸς τρεισ</supplied>-

    <lb n="10"/>καιδέκατον <choice><corr>ἔτος</corr><sic>ετους</sic></choice> Τιβερίου Καίσαρος Σε<supplied reason="lost" cert="high">βαστοῦ εἰς πυροῦ καὶ κριθῆς σπο</supplied>-

    <lb n="11"/>ράν, πυροῦ μὲν ἀρταβῶν ἑκατὸν πέντε <num value="105"/> <supplied reason="lost" cert="high">καὶ κριθῆς ἀρταβῶν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>καθαρὸν ἄδωλον, ἅπαντα δὲ μέτρωι τ<supplied reason="lost" cert="high">ετραχοινίκῳ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>ἃς καὶ ἀποδώτωσαν <choice><corr>ἐν</corr><sic>εμ</sic></choice> μηνὶ Παῦνι τοῦ <supplied reason="lost" cert="high">τρεισκαιδεκάτου ἔτους ἐν Τεβτύ</supplied>-

    <lb n="14"/>νι, τῶν μεμισθωμένων τοὺς μισθοὺς <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/>γήσουσι. οἱ αὐτοὶ καθ᾽ ἑκάστους κακου με<unclear reason="undefined" cert="high">τ</unclear>ρ<gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> χωρηγήσει ὁ μισθ</supplied>-

    <lb n="16"/>ωτὴς τοῖς μεμισθωμένοις σπέρματ[α πυροῦ μὲν ἀρταβῶν δέκα ἓξ <num value="16"/>] 

    <lb n="17"/>καὶ κριθῆς ἀρταβῶν δέκα <num value="10"/> καὶ ταυρικ<supplied reason="lost" cert="high">ῶν ἐργασίαν. καὶ μετὰ τὸν χρόνον παρα</supplied>-

    <lb n="18"/>δώσουσι οἱ μεμισθωμένοι τὸν κλῆρ<supplied reason="lost" cert="high">ον καθαρὸν ἀπὸ θρύου κα</supplied>-

    <lb n="19"/>λαμοῦ <choice><corr>ἀγρώστεως</corr><sic>αγρωστεος</sic></choice> πάσης <choice><corr>δείσης</corr><sic>δισης</sic></choice> τὸν <supplied reason="lost" cert="high">δὲ ἐπὶ αὐτῷ καλαμὸν ἐξ</supplied> 

    <lb n="20"/>ἐπικοπῆς, οὔσης τῆς <sic>τῆς</sic> πράξεως τῷ <supplied reason="lost" cert="high">Ἁρυώτῃ καὶ τοῖς παρ᾽ αὐτοῦ</supplied> 

    <lb n="21"/>ἔκ τε τῶν μεμισθωμένων καὶ τῶν <supplied reason="lost" cert="high">ὑπαρχόντων αὐτοῖς πάντων</supplied> 

    <lb n="22"/>καὶ θαλλοῦ ἄρτων πεπτ<supplied reason="lost" cert="high">ῶν</supplied> <unclear reason="undefined" cert="high">ἀ</unclear>ρτ[άβην μίαν <num value="1"/> <gap reason="illegible" extent="unknown" unit="character"/> ] 

    <lb n="23"/>καθάπερ ἐ<unclear reason="undefined" cert="high">κ</unclear> <unclear reason="undefined" cert="high">δίκ</unclear><supplied reason="lost" cert="high">ης</supplied> <unclear reason="undefined" cert="high">βε</unclear>βαιουμέ<supplied reason="lost" cert="high">νης τῆς μισθώσεως ὑπὸ τοῦ</supplied> 

    <lb n="24"/>Ἁρυώτου ἐπὶ τοῖς προκιμένοις καὶ <supplied reason="lost" cert="high">ἀπὸ δημοσίων καὶ παντὸς</supplied> 

    <lb n="25"/>εἴδους ἀμεταμίσθωτα καὶ ἀναυ<unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ούργητα ἐπὶ τὸν προκείμενον χρόνον.</supplied> 

    <lb n="26"/>ὑπογραφεῖς τῶν μὲν μεμισθωμένων [ <gap reason="illegible" extent="unknown" unit="character"/> Λυσιμάχου ὡς ἐτῶν <gap reason="illegible" extent="unknown" unit="character"/> ] 

    <lb n="27"/>οὐλὴ <choice><corr>μετώπωι</corr><sic>μετοπωι</sic></choice> <choice><corr>ἐξ</corr><sic>εκς</sic></choice> ἀριστερῶν καὶ τοῦ Ἁρυ[ώτου <gap reason="illegible" extent="unknown" unit="character"/> ὡς ἐτῶν <gap reason="illegible" extent="unknown" unit="character"/> ] 

    <lb n="28"/>οὐλὴ <choice><corr>μετώπωι</corr><sic>μετοπωι</sic></choice> μέσωι. <handShift new="m2"/> Παῶπις Ὀρσεῦ<supplied reason="lost" cert="high">τος καὶ ὁ υἱός μου</supplied> 

    <lb n="29"/>Ὀρσεῦς καὶ Σέ<unclear reason="undefined" cert="high">μεν</unclear>ις Ἁρυώτου <supplied reason="lost" cert="high">Πέρσαι τῆς ἐπι</supplied>-

    <lb n="30"/>γονῆς μεμισθώμεθα παρὰ Ἁρ<supplied reason="lost" cert="high">υώτου τοῦ Ἁρυώτου</supplied> 

    <lb n="31"/>καθὼς πρόκειται. ἔγραψεν ὑ<supplied reason="lost" cert="high">πὲρ αὐτῶν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="32"/>ς Λυσιμάχου διὰ τὸ τὸν Ὀρ<supplied reason="lost" cert="high">σεῦν βραδέα γρά</supplied>-

    <lb n="33"/>φειν, τοὺς δὲ ἄλλους μὴ εἰδέ<supplied reason="lost" cert="high">ναι γράμματα.</supplied> 

    <lb n="34"/><handShift new="m3"/> Ὀρσεῦς Παῶπις <choice><corr>ἐπιτέταχα</corr><sic>επειτεταχα</sic></choice> 

γρ[άφειν. <handShift new="m4"/> Ἁρυώτης Ἁρυώτου μεμίσ]-

    <lb n="35"/>θωκα καθὼς πρόκειται. ἔγραψεν ὑπὲρ αὐτοῦ <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> διὰ τὸ μὴ εἰδέναι</supplied> 

    <lb n="36"/>αὐτὸν <choice><corr>γράφειν</corr><sic>γραφιν</sic></choice>. <handShift new="m5"/> ἔτους <num value="13">ιγ</num> Τιβερίου Κα<supplied reason="lost" cert="high">ίσαρος *σεβαστοῦ μηνὸς</supplied> 

    <lb n="37"/>Σεβαστοῦ <num value="18">ιη</num>. ἀναγέγραπται διὰ Κρ<supplied reason="lost" cert="high">ονίωνος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

