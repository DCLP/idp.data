    <lb n="2"/><unclear reason="undefined" cert="high">ι</unclear>αν εὑρὼν τοῦ κοινοῦ ἡμῶν ἀδελφοῦ Φαμού<unclear reason="undefined" cert="high">τ</unclear>ωνος <gap reason="illegible" extent="3" unit="character"/><unclear reason="undefined" cert="high">βε</unclear><gap reason="illegible" extent="3" unit="character"/> ἡμῶν <choice><corr>δὲ</corr><sic>δαι</sic></choice> <gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/>δ<gap reason="lost" extent="5" unit="character"/><unclear reason="undefined" cert="high">ν</unclear><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="1" unit="character"/> 

