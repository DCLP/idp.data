    <lb n="20"/><gap extent="10" reason="lost" unit="character"/> διακειμ<supplied cert="high" reason="lost">ένου ἐ</supplied>πὶ τῆς <supplied cert="high" reason="lost">Ἑρμο</supplied>πολιτῶν 

    <lb n="21"/><supplied cert="high" reason="lost">πόλεως ἐν ἀμφόδῳ </supplied><gap extent="2" reason="lost" unit="character"/>λειας ἐν ῥύμῃ καλουμέ<supplied cert="high" reason="lost">ν</supplied>ῃ τῶν <choice><corr>ἀγοραίων</corr><sic>αγορεων</sic></choice> 

    <lb n="22"/><supplied cert="high" reason="lost">γειτόνων ὄντων </supplied><choice><corr><supplied cert="high" reason="lost">κ</supplied>αμί<supplied cert="high" reason="omitted">ν</supplied>ου</corr><sic><supplied cert="high" reason="lost">κ</supplied>αμιου</sic></choice> τοῦ <choice><corr>οἰκ<supplied cert="high" reason="omitted">ε</supplied>ίου</corr><sic>οικιου</sic></choice> τῶν υἱῶν Διδύμου νευούσης 

    <lb n="23"/><supplied cert="high" reason="lost">εἰς </supplied><gap extent="5" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἢ οἵας ἐὰν οὔσης πάντῃ </supplied><choice><corr><supplied cert="high" reason="lost">π</supplied>άντοθεν</corr><sic><supplied cert="high" reason="lost">π</supplied>αντοθην</sic></choice> ταύτης <choice><corr>γειτνίας</corr><sic>γε<supplied cert="high" reason="lost">ι</supplied>τνειας</sic></choice> ἐκ <choice><corr>τετραγώνου</corr><sic>τετραγωνον</sic></choice>, 

