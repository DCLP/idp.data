    <lb n="6"/>τῶν ἑξῆς <choice><corr>ἐντεταγμένων</corr><sic>εντεταγμενου</sic></choice> τόπων <choice><corr>οἰκίας</corr><sic>οικειας</sic></choice> 

    <lb n="7"/>καὶ <choice><corr>Φοιβάμμωνι</corr><sic>φοιβαμμ<supplied cert="high" reason="lost">ων</supplied>ος</sic></choice> <choice><corr>υἱῷ</corr><sic>υιου</sic></choice> Ἀφοῦτος <sic>τος</sic> <unclear cert="high" reason="undefined">τ</unclear>ῷ <unclear cert="high" reason="undefined">ἐπισ</unclear>η<unclear cert="high" reason="undefined">μο</unclear>-

    <lb n="8"/>τάτῳ ὑπὲρ ἄλλου <choice><corr>ἐνάτου</corr><sic>εννατου</sic></choice> μέρους τῶν αὐτῶν 

    <lb n="9"/>τόπων <choice><corr>οἰκίας</corr><sic>οικειας</sic></choice> ἀπὸ τῆς <expan>Ἑρμ<ex cert="high">ου</ex>πόλ<ex cert="high">εως</ex></expan> Αὐρηλίου 

    <lb n="10"/>Σινουθίου υἱοῦ Ἠλία ἑξῆς <supplied cert="high" reason="lost">ὑπο</supplied>γράφοντος 

    <lb n="11"/>ὑπὸ τῆσ<supplied cert="high" reason="lost">δε ὁ</supplied>μολ<unclear cert="high" reason="undefined">ο</unclear>γίας ἐγ<supplied cert="high" reason="lost">γ</supplied>υωμένου αὐτοῦ 

    <lb n="12"/>καὶ ἀναδεχ<supplied cert="high" reason="lost">ομένου τ</supplied>ὴν ἀπό<unclear cert="high" reason="undefined">δ</unclear>οσιν τοῦ ἑξῆς 

    <lb n="13"/>ἐντετα<supplied cert="high" reason="lost">γμένου</supplied> <choice><corr>ἐνοικίου</corr><sic>ενοι<supplied cert="high" reason="lost">κ</supplied>ειου</sic></choice> <supplied cert="high" reason="omitted">ὑπὲρ</supplied> ἐμοῦ Φλαυίου 

    <lb n="14"/>Σερήν<supplied cert="high" reason="lost">ου </supplied><gap extent="6" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ρ</unclear>ίου ἀπὸ <supplied cert="high" reason="lost">τῆς</supplied> <unclear cert="high" reason="undefined">αὐ</unclear>τῆς <expan>Ἑρμ<ex cert="high">ου</ex>πό<ex cert="high">λεως</ex></expan>. 

    <lb n="15"/><supplied cert="high" reason="lost">ὁμολογῶ ἑκ</supplied>ουσίως καὶ αὐ<supplied cert="high" reason="lost">θαιρέτ</supplied>ως <supplied cert="high" reason="lost">μ</supplied>εμισ-

    <lb n="16"/><supplied cert="high" reason="lost">θῶσθαι ἐμαυτ</supplied>ὸν παρὰ σοῦ <supplied cert="high" reason="lost">πρὸς</supplied> <choice><corr>ἑξαέτη</corr><sic>εξ<supplied cert="high" reason="lost">α</supplied>ετους</sic></choice> 

    <lb n="17"/>χρόνον λογιζόμενον ἀπὸ <supplied cert="high" reason="lost">τῆς σή</supplied>μερον 

    <lb n="18"/>ἡμέρας, ἥ ἐστιν <supplied cert="high" reason="lost">Φ</supplied>αῶφι τρεισκαιδεκάτη 

