    <lb n="6"/>ἡμῶν <expan>δεσπότ<ex cert="high">ου</ex></expan> ἔτους <supplied cert="high" reason="lost"><num value="7">ζ</num></supplied> Τῦβι <num value="7">ζ</num> <expan>ἰνδικ<ex cert="high">τίωνος</ex></expan> <supplied cert="high" reason="lost"><num value="8">η</num></supplied>. 

    <lb n="7"/>Φλαουίᾳ Ἀναστασίᾳ τῇ ἐνδο<supplied cert="high" reason="lost">ξο</supplied>τά<supplied cert="high" reason="lost">τῃ</supplied> 

    <lb n="8"/>ἰλλουσ<supplied cert="high" reason="lost">τρίᾳ</supplied> θυγατρὶ τοῦ τῆς ἐν<supplied cert="high" reason="lost">δό</supplied>ξου 

    <lb n="9"/>μν<unclear cert="high" reason="undefined">ή</unclear><supplied cert="high" reason="lost">μ</supplied>ης Μ<gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">α</unclear> Εὐδ<unclear cert="high" reason="undefined">αί</unclear>μονος γεου<supplied cert="high" reason="lost">χ</supplied><unclear cert="high" reason="undefined">ο</unclear>ύσῃ 

    <lb n="10"/>ἐνταῦθα τῇ λαμπρᾷ Ὀξυρυγχιτῶν 

    <lb n="11"/>πόλε<supplied cert="high" reason="lost">ι διὰ</supplied> σοῦ Φλαουίου Φοιβάμμ<supplied cert="high" reason="lost">ωνος</supplied> 

