    <lb n="1"/>επιτη<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/><gap extent="1" reason="illegible" unit="character"/>απ<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>Αὐρηλ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>οἱ ἀπὸ το<unclear cert="high" reason="undefined">ῦ</unclear> <supplied cert="high" reason="lost">κοινοῦ τῶν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/><unclear cert="high" reason="undefined">ω</unclear>ν τ<unclear cert="high" reason="undefined">ῆ</unclear><supplied cert="high" reason="lost">ς αὐτῆς πόλεως   ὑπογρα</supplied>-

    <lb n="6"/>φόντ<supplied cert="high" reason="lost">ων Αὐρηλίων Σαραπάμμωνος</supplied> 

    <lb n="7"/>Φιλαν<supplied cert="high" reason="lost">τινόου βουλευτοῦ καὶ Ἀμμω</supplied>-

    <lb n="8"/>νίου Β<supplied cert="high" reason="lost">ησαρίωνος </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>καὶ <hi rend="diaeresis">Ἰ</hi>σιδ<supplied cert="high" reason="lost">ώρου </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/>ρου <hi rend="diaeresis"><unclear cert="high" reason="undefined">ι</unclear></hi><unclear cert="high" reason="undefined">ε</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ὁμολογοῦμεν ὀμνύοντες</supplied> 

    <lb n="11"/>τ<unclear cert="high" reason="undefined">ὴ</unclear><supplied cert="high" reason="lost">ν τῶν κυρίων ἡμῶν Διοκλητιανοῦ καὶ</supplied> 

    <lb n="12"/>Μαξ<supplied cert="high" reason="lost">ιμια</supplied>νοῦ <supplied cert="high" reason="lost">Σε</supplied>β<supplied cert="high" reason="lost">αστῶν καὶ Κωνσταντίου</supplied> 

    <lb n="13"/>καὶ Μ<supplied cert="high" reason="lost">αξιμ</supplied>ιανοῦ τῶν <supplied cert="high" reason="lost">ἐ</supplied>πιφαν<supplied cert="high" reason="lost">εστάτων</supplied> 

    <lb n="14"/>Καισάρων τύχην προσφωνεῖ<supplied cert="high" reason="lost">ν</supplied> <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">ατὰ</supplied> 

    <lb n="15"/>κέλε<unclear cert="high" reason="undefined">υ</unclear><supplied cert="high" reason="lost">σι</supplied>ν ἡγεμονικ<supplied cert="high" reason="lost">ὴν</supplied> τὴν μὲν 

    <lb n="16"/>λίτραν εἶναι τοῦ κατερ<supplied cert="high" reason="lost">γα</supplied>ζομένου 

    <lb n="17"/><supplied cert="high" reason="lost">χαλ</supplied><unclear cert="high" reason="undefined">κ</unclear>ο<unclear cert="high" reason="undefined">ῦ</unclear> δηναρίων ἑξήκ<supplied cert="high" reason="lost">ο</supplied>ντα δύο <num value="62"/> 

    <lb n="18"/>τὴν δὲ τοῦ ἀργοῦ χυτοῦ ὁμοίως λί-

    <lb n="19"/>τραν ἀργυρίου δηναρίων τριάκον-

    <lb n="20"/>τα ἑνὸς <num value="31"/> ἵν᾽ εἰδῇς, καὶ μηδὲν διεψεῦ-

    <lb n="21"/>σθαι ἢ ἔνοχοι εἴημεν τῷ θείῳ ὅρ-

    <lb n="22"/>κῳ· καὶ <expan>ἐπερωτη<ex cert="high">θέντες</ex></expan> <expan>ὡμολ<ex cert="high">ογήσαμεν</ex></expan>. <expan><ex cert="high">ἔτους</ex></expan> <num value="16">ιϛ</num> καὶ <expan><ex cert="high">ἔτους</ex></expan> <num value="15">ιε</num> 

    <lb n="23"/>τῶν κυρίων ἡμῶν Διοκλητιανοῦ καὶ 

    <lb n="24"/>Μαξιμιανοῦ Σεβαστῶν καὶ ἔτους <num value="8">η</num> τῶν 

    <lb n="25"/>κυρίων ἡμῶν Κωνσταντίου καὶ Μαξιμιανοῦ 

    <lb n="26"/>τῶν ἐπιφανεστάτων Καισάρων 

    <lb n="27"/>Φαρμοῦθι <num value="17">ιζ</num>. <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> Σαραπάμμων 

    <lb n="28"/><supplied cert="high" reason="lost">Φι</supplied>λαντινόου <expan>βουλ<ex cert="high">ευτὴς</ex></expan> ὥμοσα τὸν ὅρκον 

    <lb n="29"/><supplied cert="high" reason="lost">ὡς</supplied> <expan>πρόκ<ex cert="high">ειται</ex></expan>. <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> Ἀμμῶνις Βησαρίων 

    <lb n="30"/><supplied cert="high" reason="lost">ὤμ</supplied>οσα τὸν ὅρκον ὡς <expan>πρ<supplied cert="high" reason="lost">ό</supplied>κ<ex cert="high">ειται</ex></expan>. <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> Ἰσίδωρος 

    <lb n="31"/><supplied cert="high" reason="lost">ὤμ</supplied>οσα τὸν ὅρκον ὡς πρόκειται </ab></div><div n="meta.16689" org="uniform" part="N" sample="complete" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.16689" org="uniform" part="N" sample="complete" subtype="textDate" type="commentary"><p><date notAfter="300" notBefore="300" type="textDate" value="0300-04-12">12. Apr. 300</date></p></div><div n="meta.16689" org="uniform" part="N" sample="complete" subtype="principalEdition" type="bibliography"><listBibl default="NO"><bibl default="NO" subtype="principal" type="publication"><title level="s" type="abbreviated">SB</title><biblScope type="volume">10</biblScope><biblScope type="numbers">10257</biblScope></bibl><bibl default="NO" type="Trismegistos"><biblScope type="numbers">16689</biblScope></bibl><bibl default="NO" type="DDbDP"><series>0239</series>; <biblScope type="volume">10</biblScope>; <biblScope type="numbers">10257</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0239;10;10257" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.16689" org="uniform" part="N" sample="complete" subtype="corrections" type="bibliography"><head>BL-Einträge nach BL-Konkordanz</head><bibl default="NO" type="BL"><biblScope type="volume">VIII</biblScope>, S. <biblScope type="pages">358</biblScope></bibl></div><div n="meta.16689" org="uniform" part="N" sample="complete" subtype="illustrations" type="bibliography"><p>keine</p></div><div n="meta.16689" org="uniform" part="N" sample="complete" subtype="otherPublications" type="bibliography"><head>Andere Publikation</head><bibl default="NO" subtype="other" type="publication">P.Ant. I 38</bibl></div><div n="meta.16689" org="uniform" part="N" sample="complete" subtype="locations" type="history"><p><placeName cert="high" full="yes" type="ancientFindspot">Antinoopolis</placeName></p></div></body></text></TEI.2>