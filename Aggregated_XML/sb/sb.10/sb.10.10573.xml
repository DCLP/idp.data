    <lb n="9"/>τοῦ ἑσταμένου πρὸς ἀλλ<unclear cert="high" reason="undefined">ή</unclear><supplied cert="high" reason="lost">λο</supplied><unclear cert="high" reason="undefined">υς</unclear> <unclear cert="high" reason="undefined">π</unclear>υρο<unclear cert="high" reason="undefined">ῦ</unclear> ἀ<unclear cert="high" reason="undefined">ρ</unclear>τ<unclear cert="high" reason="undefined">αβῶν</unclear> 

    <lb n="10"/>εἴκοσι ἑπτὰ <num value="27"/> ἐφ᾽ ᾧ χωρὶς τούτω<unclear cert="high" reason="undefined">ν</unclear> <supplied cert="high" reason="lost">ἐ</supplied><unclear cert="high" reason="undefined">κ</unclear>θρέ<unclear cert="high" reason="undefined">ψαι</unclear> τὰ <unclear cert="high" reason="undefined">κτή</unclear>-

    <lb n="11"/>νη ἐκ τοῦ ἰδίου ὄντα ἀκίνδυ<unclear cert="high" reason="undefined">να</unclear> <unclear cert="high" reason="undefined">σὺ</unclear><supplied cert="high" reason="lost">ν τ</supplied>ῷ ἀρό<unclear cert="high" reason="undefined">τρῳ</unclear> 

    <lb n="12"/>καὶ τῷ φόρῳ παντὸς κινδύν<unclear cert="high" reason="undefined">ου</unclear>, βεβαιούτω δὲ 

    <lb n="13"/><choice><corr>κ<supplied cert="high" reason="omitted">αὶ</supplied> αὐτὸς</corr><sic>καυτος</sic></choice> ὁ Ἀρίγνωτος αὐτοῖς <unclear cert="high" reason="undefined">τὴ</unclear>ν μίσθωσιν ἀπὸ 

    <lb n="14"/>πά<unclear cert="high" reason="undefined">ν</unclear>των <choice><corr>πάσῃ</corr><sic>πασει</sic></choice> βεβαιώσει. βε<unclear cert="high" reason="undefined">β</unclear>αιουμένης δ᾽ α<unclear cert="high" reason="undefined">ὐ</unclear>-

    <lb n="15"/>τῆς ἀποδότωσαν οἱ <choice><corr>μεμισθωμένοι</corr><sic>μεμεμισθωμενοι</sic></choice> τῷ 

    <lb n="16"/>Ἀριγνώτω<unclear cert="high" reason="undefined">ι</unclear> τὸν φόρον ἐν τῷ Πα<unclear cert="high" reason="undefined">ῦν</unclear>ι μηνὶ τοῦ 

    <lb n="17"/>αὐτοῦ ἔτους ἐπὶ τῶν περὶ <choice><corr>Ἰβιῶνα</corr><sic><unclear cert="high" reason="undefined">ειβιωνα</unclear></sic></choice> <unclear cert="high" reason="undefined">Ἀ</unclear>ντιλόχου <expan>ἅ<unclear cert="high" reason="undefined">λω</unclear><ex cert="high">ν</ex></expan> 

    <lb n="18"/>πυρὸν νέον καθα<unclear cert="high" reason="undefined">ρ</unclear>ὸν ἄδολον μέ<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ρῳ</supplied> <choice><corr>τετραχοι-

    <lb n="19"/>νίκῳ</corr><sic><unclear cert="high" reason="undefined">τε</unclear><supplied cert="high" reason="lost">τρ</supplied><unclear cert="high" reason="undefined">αχοι</unclear>νιν<unclear cert="high" reason="undefined">κ</unclear>ω</sic></choice> ἀγ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ρ</supplied><unclear cert="high" reason="undefined">αν</unclear>ο<unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ικῷ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

