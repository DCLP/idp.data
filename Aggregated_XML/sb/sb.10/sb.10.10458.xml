    <lb n="2"/>δεχόμενος οὖν τ<supplied reason="lost" cert="high">ὰ παρ</supplied><unclear reason="undefined" cert="high">όντα</unclear> <unclear reason="undefined" cert="high">γράμ</unclear>ματ<supplied reason="lost" cert="high">α, πρὸς τὴν δύναμιν τῶν</supplied> 

    <lb n="3"/>ἡμετέρων ἐνταγίων, συνάγων τ<supplied reason="lost" cert="high">οὺς</supplied> τ<supplied reason="lost" cert="high">ο</supplied>ι<supplied reason="lost" cert="high">ού</supplied>του<supplied reason="lost" cert="high">ς ἀ</supplied><unclear reason="undefined" cert="high">γγ</unclear><supplied reason="lost" cert="high">αρευ</supplied>τ<supplied reason="lost" cert="high">ὰς</supplied> 

    <lb n="4"/>μετὰ καὶ τῶν εἰδῶν καὶ σιδηρικῶν <choice><corr>ἐργαλείων</corr><sic>εργαλαιων</sic></choice>, παρασκεύα<supplied reason="lost" cert="high">σον</supplied> 

