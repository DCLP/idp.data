    <lb n="1"/><gap extent="4" reason="illegible" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>παρ᾽ α<unclear cert="high" reason="undefined">ὐ</unclear>τῶ<supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>νο<unclear cert="high" reason="undefined">ς</unclear> <add place="supralinear"><unclear cert="high" reason="undefined">το</unclear>ῦ</add> γραμματέως αὐτοῦ <unclear cert="high" reason="undefined">συνχωρ</unclear><supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">ῖ</unclear> <unclear cert="high" reason="undefined">αὐ</unclear>-

    <lb n="4"/>το<unclear cert="high" reason="undefined">ῖ</unclear>ς τὸν φόρον τοῦ <num value="15">ιε</num> <expan><ex cert="high">ἔτους</ex></expan> ὁλ<unclear cert="high" reason="undefined">ό</unclear>κ<unclear cert="high" reason="undefined">ληρο</unclear>ν μ<unclear cert="high" reason="undefined">ὴ</unclear> εἰ-

    <lb n="5"/>σενενγκεῖ<unclear cert="high" reason="undefined">ν</unclear> ἀλλὰ παρὰ <num value="100"><unclear cert="high" reason="undefined">ρ</unclear></num> <expan><ex cert="high">τάλαντα</ex></expan>· καὶ <unclear cert="high" reason="undefined">τού</unclear><supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ων</unclear> 

    <lb n="6"/>οὕ<unclear cert="high" reason="undefined">τως</unclear> ἐχόντων, σὺ ὁ κύριος ἔ<unclear cert="high" reason="undefined">γ</unclear>ραψ<unclear cert="high" reason="undefined">α</unclear>ς τῷ 

    <lb n="7"/><choice><corr>νυνὶ</corr><sic>νυ<unclear cert="high" reason="undefined">ν</unclear>ει</sic></choice> στρατηγῷ Ἀπολλωνίῳ ὥστε <unclear cert="high" reason="undefined">π</unclear>έμ-

    <lb n="8"/>ψαι σοι τῶν τελωνικῶν ὠ<unclear cert="high" reason="undefined">νίω</unclear>ν <unclear cert="high" reason="undefined">τ</unclear>ὸ<unclear cert="high" reason="undefined">ν</unclear> 

    <lb n="9"/>λόγον τῆς εἰσπράξεως <unclear cert="high" reason="undefined">τῆς</unclear> <unclear cert="high" reason="undefined">ἀνὰ</unclear> <unclear cert="high" reason="undefined">χ</unclear>εῖ<unclear cert="high" reason="undefined">ρ</unclear>α 

    <lb n="10"/>πενταετίας, τοῦτ᾽ ἐστιν <unclear cert="high" reason="undefined">τ</unclear>ῶ<unclear cert="high" reason="undefined">ν</unclear> <unclear cert="high" reason="undefined">ἀπὸ</unclear> <num value="15">ι<unclear cert="high" reason="undefined">ε</unclear></num> <expan><ex cert="high"><unclear cert="high" reason="undefined">ἔτους</unclear></ex></expan> 

    <lb n="11"/>ἵνα κυρῶν τὰ ὤνια εἰδῇς πόσ<unclear cert="high" reason="undefined">ον</unclear> <unclear cert="high" reason="undefined">εἰ</unclear>σηνέ-

    <lb n="12"/><unclear cert="high" reason="undefined">χθ</unclear>η ὑπὲρ ἑκάστης ὠνῆς καὶ οὕτως <unclear cert="high" reason="undefined">τὸν</unclear> λο-

    <lb n="13"/>γισμὸν μ<unclear cert="high" reason="undefined">ά</unclear>θῃς. καὶ ὁ στρατηγὸς ἀκό<unclear cert="high" reason="undefined">λ</unclear>ουθ<unclear cert="high" reason="undefined">α</unclear> 

    <lb n="14"/>ποιῶν τῇ <choice><corr>ἐνκελεύσει</corr><sic>ενκελευσι</sic></choice> σου ἐ<unclear cert="high" reason="undefined">πέσ</unclear>τει<unclear cert="high" reason="undefined">λεν</unclear> 

    <lb n="15"/>Ποτάμωνι τῷ πρὸ αὐτοῦ, <unclear cert="high" reason="undefined">διὰ</unclear> <unclear cert="high" reason="undefined">τ</unclear>ὸ αὑ<unclear cert="high" reason="undefined">τὸν</unclear> 

    <lb n="16"/>τοὺς λόγους μὴ ἔχειν, <unclear cert="high" reason="undefined">μ</unclear>ε<unclear cert="high" reason="undefined">τα</unclear>δοῦναι αὐ-

    <lb n="17"/>τῷ ὡς ἠθέλησας παρα<unclear cert="high" reason="undefined">κει</unclear>μένη<supplied cert="high" reason="lost">ς τ</supplied><unclear cert="high" reason="undefined">ῆ</unclear>ς 

    <lb n="18"/>εἰσπράξεως. ὁ γενναιότατος Ποτ<unclear cert="high" reason="undefined">άμ</unclear>ων 

    <lb n="19"/>τί ποιεῖ; μεταδιδοὺς γὰρ ὅλον τὸ β<unclear cert="high" reason="undefined">ιβ</unclear><supplied cert="high" reason="lost">λίο</supplied><unclear cert="high" reason="undefined">ν</unclear>, 

    <lb n="20"/>ἐπὶ ταύτης τῆς ὠνῆς <unclear cert="high" reason="undefined">οὐ</unclear> <unclear cert="high" reason="undefined">πρ</unclear>οσ<unclear cert="high" reason="undefined">γράφει</unclear> <unclear cert="high" reason="undefined">τί</unclear> 

    <lb n="21"/>εἰσεπράχθη τῇ ἀνὰ χεῖ<unclear cert="high" reason="undefined">ρ</unclear>α πενταε<unclear cert="high" reason="undefined">τί</unclear>ᾳ, ἵ<unclear cert="high" reason="undefined">ν</unclear>α 

    <lb n="22"/>μὴ εὔδηλον γένη<unclear cert="high" reason="undefined">τ</unclear>α<unclear cert="high" reason="undefined">ι</unclear> τ<unclear cert="high" reason="undefined">ῷ</unclear> <unclear cert="high" reason="undefined">σ</unclear>τρατηγῷ <unclear cert="high" reason="undefined">ὅτι</unclear> δὴ 

    <lb n="23"/>οὖτοι οἱ μισθωταὶ χρ<unclear cert="high" reason="undefined">εῶσ</unclear>τ<unclear cert="high" reason="undefined">αί</unclear> εἰσιν <unclear cert="high" reason="undefined">κ</unclear>αὶ 

    <lb n="24"/>παρατηρηθῇ ἡ ὠνὴ <unclear cert="high" reason="undefined">ὑπὸ</unclear> <unclear cert="high" reason="undefined">ἐπιτηρη</unclear><supplied cert="high" reason="lost">τῶ</supplied>ν </ab></div><div n="meta.14306" part="N" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.14306" part="N" subtype="textDate" type="commentary"><p><date type="textDate">176 (?)</date></p></div><div n="meta.14306" part="N" subtype="general" type="commentary"><p>Zu Z. 19-24 vgl. Reiter, Nomarchen, S. 282, Anm. 71.</p></div><div n="meta.14306" part="N" subtype="principalEdition" type="bibliography"><listBibl><bibl subtype="principal" type="publication"><title level="s" type="abbreviated">SB</title><biblScope type="volume">10</biblScope><biblScope type="numbers">10292</biblScope></bibl><bibl type="Trismegistos"><biblScope type="numbers">14306</biblScope></bibl><bibl type="DDbDP"><series>0239</series>; <biblScope type="volume">10</biblScope>; <biblScope type="numbers">10292</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0239;10;10292" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.14306" part="N" subtype="illustrations" type="bibliography"><p>keine</p></div><div n="meta.14306" part="N" subtype="translations" type="bibliography"><head>Übersetzungen</head><listBibl lang="en"><head lang="de">Englisch:</head><bibl type="translations">R.A. Coles, JEA 52, 1966, S. 131</bibl></listBibl></div><div n="meta.14306" part="N" subtype="locations" type="history"><p><geogName cert="high" type="nome">Arsinoites</geogName>, <geogName cert="high" key="Aegyptus" type="ancientRegion">Egypt</geogName></p></div></body></text></TEI.2>