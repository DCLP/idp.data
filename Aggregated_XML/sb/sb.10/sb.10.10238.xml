    <lb n="1"/><supplied cert="high" reason="lost">Θαμοῦνις τῆς Ὀν</supplied><unclear cert="high" reason="undefined">ν</unclear>ώφριο<supplied cert="high" reason="lost">ς Περ</supplied>σείνη μέτ<supplied cert="high" reason="lost">ὰ κ</supplied>υρίου 

    <lb n="2"/><supplied cert="high" reason="lost">τοῦ </supplied><gap extent="13" reason="lost" unit="character"/>ερῶτος τῷ ἐ<unclear cert="high" reason="undefined">μα</unclear>υτῆς υἱῷ 

    <lb n="3"/><supplied cert="high" reason="lost">Τρύφωνι Διονυσίου χαίρειν. Ὁ</supplied>μολογῶ ἔχειν παρὰ <supplied cert="high" reason="lost">σ</supplied>οῦ ἐπὶ τοῦ 

    <lb n="4"/><supplied cert="high" reason="lost">πρὸς Ὀξυρύγχ</supplied>ων πόλει Σ<supplied cert="high" reason="lost">α</supplied>ραπείου διὰ τῆς Πλο<supplied cert="high" reason="lost">υ</supplied>-

    <lb n="5"/><supplied cert="high" reason="lost">τάρχου Σαραπί</supplied><unclear cert="high" reason="undefined">ω</unclear>νος τραπέζης ἀργυρίου Σεβαστοῦ 

    <lb n="6"/><supplied cert="high" reason="lost">καὶ Πτολεμαικοῦ</supplied> νομίσματος δραχμὰς δέκα ἕξ <num value="16"/>, 

