    <lb n="12"/><gap extent="8" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐ</supplied>μοῦ μὲν υ<supplied cert="high" reason="lost">ἱοῦ</supplied> Θοώνιος τοῦ <expan>Διον<ex cert="high">υσίου</ex></expan>. 

    <lb n="13"/><supplied cert="high" reason="lost">ἐὰν δὲ</supplied> μὴ ἀποδῶ <supplied cert="high" reason="lost">σοι κ</supplied>αθὰ γέγραπται, ἐκτεί-

    <lb n="14"/><supplied cert="high" reason="lost">σω σοι τὸ προ</supplied>κείμεν<supplied cert="high" reason="lost">ον</supplied> κεφάλαιον μεθ᾽ ἡ-

    <lb n="15"/><supplied cert="high" reason="lost">μιολίας καὶ</supplied> τοῦ ὑπερπ<supplied cert="high" reason="lost">ε</supplied>σόντος χρόνου τὸν κα-

    <lb n="16"/><supplied cert="high" reason="lost">θήκοντα, τῆς π</supplied>ράξεώς <choice><corr>σοι</corr><sic>σου</sic></choice> <supplied cert="high" reason="lost">ο</supplied>ὔσης ἔκ τε ἐμοῦ 

    <lb n="17"/><supplied cert="high" reason="lost">καὶ ἐκ τῶν ὑπαρχόντων μοι</supplied> πάντων καθάπερ 

    <lb n="18"/><choice><corr>ἐκ</corr><sic><supplied cert="high" reason="lost">εγ</supplied></sic></choice> <supplied cert="high" reason="lost">δίκῆς. κυρία ἡ χεὶρ παντα</supplied>χῇ ἐπιφερομένη 

    <lb n="19"/>[καὶ παντὶ τῷ ἐπιφέροντι. <expan><ex cert="high">ἔτους</ex></expan> <num value="2">β</num>] Γαίου Καίσαρος 

    <lb n="20"/><supplied cert="high" reason="lost">Σεβαστοῦ Γερμανικοῦ</supplied> Χοιὰχ <num value="24">κδ</num>. 

    <lb n="21"/><handShift new="m2"/> <supplied cert="high" reason="lost">Θαμοῦνις ἔχω τὰς τοῦ ἀργ</supplied>υρίου δραχμὰς δ<unclear cert="high" reason="undefined">έκ</unclear>α ἓξ <num value="16"/> κεφαλαίου 

    <lb n="22"/><gap extent="21" reason="lost" unit="character"/>ερῶτος ἐπιγέγραμμαι 

    <lb n="23"/><supplied cert="high" reason="lost">καὶ γέγρ</supplied>αφα ὑπὲρ αὐ<supplied cert="high" reason="lost">τὴς μ</supplied>ὴ εἰδυίης γράμματα. 

    <lb n="24"/>[<expan><ex cert="high">ἔτους</ex></expan> <num value="2">β</num> Γαίου Καίσαρ]ος Σεβασροῦ Γερ<supplied cert="high" reason="lost">μ</supplied>ανικοῦ Χοιὰχ [<num value="2">β</num>]<num value="4">δ</num>. 

