    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="3" unit="character"/> δωρεῶν <unclear reason="undefined" cert="high">κα</unclear><supplied reason="lost" cert="high">ὶ</supplied>

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" extent="2" unit="character"/> ἀγαθὴν βούλησιν καὶ ἀσάλευτον

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> εἰ</supplied>ς ἐμὲ ἀπὸ γονικῆς μου διαδοχῆς

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/>ν μετὰ τῆς εἰσόδου καὶ ἐξόδου καὶ

    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/><unclear reason="undefined" cert="high">κ</unclear>ου ἐν τῷ ἕ<unclear reason="undefined" cert="high">λει</unclear> κώμης Θύνεως

    <lb n="6"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἀ</supplied>πὸ ἀρουρῶν τεσσάρων <num value="4"/>

    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/> μάρτυρος ἀββᾶ Θεοδώρου

    <lb n="8"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> εἰς</supplied> πλήρωσιν τῶν αὐτῶν ἀρουρῶν

    <lb n="9"/><gap reason="lost" extent="unknown" unit="character"/> βορρᾶ

    <lb n="10"/><gap reason="lost" extent="unknown" unit="character"/> λιβὸς

    <lb n="11"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> πά</supplied>ντῃ πάντοθεν ὡμολογήσας

    <lb n="12"/><choice><corr><gap reason="lost" extent="unknown" unit="character"/>υσιαν</corr><sic><gap reason="lost" extent="unknown" unit="character"/>υσια</sic></choice> οἱανδήποτε ἐκ τῶν <expan>προειρημέ<ex cert="high">νων</ex></expan>

    <lb n="13"/><gap reason="lost" extent="unknown" unit="character"/>μετὰ παντὸς αὐτῶν τοῦ δικαίου

    <lb n="14"/><gap reason="lost" extent="unknown" unit="character"/><unclear reason="undefined" cert="high">α</unclear>δος ἔτι τοῦ λοιποῦ τὸ παράπαν

    <lb n="15"/>ἀλλὰ πᾶσαν αὐθεντίαν <unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">αὶ </supplied><gap reason="lost" extent="17" unit="character"/><unclear reason="undefined" cert="high">α</unclear>ν δεσποτείαν καὶ αὐτοτελῆ ἐξουσίαν

    <lb n="16"/>δίδωμι τῷ αὐτῷ εὐαγεῖ φ<supplied reason="lost" cert="high">ι</supplied>λ<supplied reason="lost" cert="high">οπονί</supplied>ῳ <supplied reason="lost" cert="high">διοικουμένῳ διὰ τῶν νυ</supplied>νὶ λαμπροτάτων φιλοπόνων

    <lb n="17"/>καὶ διὰ τῶν μετ᾽ αὐτοὺς ἐσομένω<supplied reason="lost" cert="high">ν πάντων ἐντεῦθεν ἤδη ἤ</supplied> τοι ἀπὸ τῆς σήμερον ἡμέρας ἥτις ἐστὶν

    <lb n="18"/>Ἐπεὶφ εἰκὰς δευτέρα τῆς πα<unclear reason="undefined" cert="high">ρ</unclear>ούση<unclear reason="undefined" cert="high">ς</unclear> <gap reason="lost" extent="17" unit="character"/><supplied reason="lost" cert="high">η</supplied>ς <expan>ἰνδ<ex cert="high">ικτίωνος</ex></expan> καὶ ἀπὸ καρπῶν τῆς σὺν <expan>θ<ex cert="high">εῷ</ex></expan>

    <lb n="19"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἀπὸ τοῦ νῦν</supplied> <unclear reason="undefined" cert="high">ἐπ</unclear>ὶ τὸ διηνεκὲς καὶ τούτων ἐπικρατεῖν

    <lb n="20"/><gap reason="lost" extent="unknown" unit="character"/><unclear reason="undefined" cert="high">ε</unclear><supplied reason="lost" cert="high">ῖ</supplied>ν καὶ διοικεῖν καὶ κτᾶσθαι καὶ νέμεσθαι

    <lb n="21"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" extent="1" unit="character"/>ελον ἀνενεγκεῖν ἐν αὐταῖς

    <lb n="22"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> κα</supplied><unclear reason="undefined" cert="high">ὶ</unclear> αὐτὸν τὴν ἐνιαυσιαῖαν πρόσοδον

    <lb n="23"/><gap reason="lost" extent="unknown" unit="character"/>ρειας τοῦ αὐτοῦ ἁγίου φιλοπονίου

    <lb n="24"/><gap reason="lost" extent="unknown" unit="character"/> <unclear reason="undefined" cert="high">δ</unclear>ωρεᾶς καὶ δεσποτείας δίκαιον

    <lb n="25"/><gap reason="lost" extent="unknown" unit="character"/> περὶ τὰ ἴδια αὐτῶν διαπράττεσθαι
