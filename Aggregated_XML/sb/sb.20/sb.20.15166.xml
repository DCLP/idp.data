    <lb n="1"/><expan><unclear cert="high" reason="undefined">Φλ</unclear><ex cert="high">άουιος</ex></expan> <unclear cert="high" reason="undefined">Παιώνιος</unclear> <gap desc="vestiges" extent="unknown" reason="illegible" unit="character"/> <unclear cert="high" reason="undefined">καὶ</unclear> <choice><corr>ἀπαιτητὴς ἁλὸς</corr><sic>απετητης αλλος</sic></choice> <gap extent="10" reason="illegible" unit="character"/> Ἑρ<supplied cert="high" reason="lost">μου</supplied><unclear cert="high" reason="undefined">πολιτ</unclear><supplied cert="high" reason="lost">ῶν πόλεως</supplied>

