    <lb n="10"/>[<expan>χειρογρά<ex cert="high">φου</ex></expan> ἕν <num value="1"/>, γίνονται τοὺς προκειμένους ὁλοκοττίνους <num value="8">η</num> <gap reason="illegible" extent="8" unit="character"/>]<gap reason="illegible" extent="2" unit="character"/>ν χρυσοῦ <num value="1">α</num> καὶ <unclear reason="undefined" cert="high">ς</unclear>[<gap reason="illegible" extent="15" unit="character"/> <num value="1">α</num> κοβαθρον ἡμουλίτρινον <num value="1">α</num> ἀργυρίου]

