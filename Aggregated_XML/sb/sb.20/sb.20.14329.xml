    <lb n="4"/>μητ<supplied cert="high" reason="lost">ρ</supplied>ὸς Θαυμαστῆτος ἀ<supplied cert="high" reason="lost">πὸ κώμης </supplied><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="5"/>μετὰ κυρίου <supplied cert="high" reason="lost">το</supplied><unclear cert="high" reason="undefined">ῦ</unclear> <supplied cert="high" reason="lost">σ</supplied><unclear cert="high" reason="undefined">υ</unclear>γ<supplied cert="high" reason="lost">γε</supplied><unclear cert="high" reason="undefined">νο</unclear><supplied cert="high" reason="lost">ῦς </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> -φεως τοῦ</supplied>

    <lb n="6"/>Αὐρηλίο<unclear cert="high" reason="undefined">υ</unclear>· <unclear cert="high" reason="undefined">ὑπ</unclear>άρ<unclear cert="high" reason="undefined">χ</unclear>ει τ<supplied cert="high" reason="lost">ῷ</supplied> υἱῷ <supplied cert="high" reason="lost">μου Νουμᾶτι ἐν τῇ κώ-</supplied>

