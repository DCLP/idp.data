    <lb n="1"/><supplied cert="high" reason="lost">τοῖς ἐσομένοις ὑ</supplied>πάτοις τὸ τέταρτο<supplied cert="high" reason="lost">ν</supplied> Παχὼν <num value="13">ιγ</num>.

    <lb n="2"/><gap extent="15" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ῳ ἴλης τρίτης Ἀ<supplied cert="high" reason="lost">στο</supplied>υρίων διακ<unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">ιμ</supplied><unclear cert="high" reason="undefined">έ</unclear>-

