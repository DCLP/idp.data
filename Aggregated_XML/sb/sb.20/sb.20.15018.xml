    <lb n="3"/>[<gap reason="illegible" extent="15" unit="character"/> <expan>πρ]<unclear reason="undefined" cert="high">ω</unclear>τοκωμή<unclear reason="undefined" cert="high">τ</unclear><ex cert="high">αις</ex></expan> τῆς αὐτῆς <expan><unclear reason="undefined" cert="high">κ</unclear>ώμ<ex cert="high">ης</ex></expan> <expan>χαίρ<ex cert="high">ειν</ex></expan>· ἐδεξάμην καὶ ἐπληρώ<unclear reason="undefined" cert="high">θη</unclear><supplied reason="lost" cert="high">ν</supplied>

    <lb n="4"/><supplied reason="lost" cert="high">παρ᾽ ὑμῶν τοῦ γρ</supplied>αμματίου τῶν πεν<unclear reason="undefined" cert="high">τ</unclear>ήκοντα ἓν <num value="51"/> <supplied reason="omitted" cert="high">νομισματίων</supplied> <expan>εὐστάθμ<ex cert="high">ων</ex></expan> ζυγῷ καὶ οὐδὲν <unclear reason="undefined" cert="high">ὑ</unclear><supplied reason="lost" cert="high">μῖν</supplied>

    <lb n="5"/><supplied reason="lost" cert="high">ἐγκαλῶ περὶ τοῦ εἰρη</supplied><unclear reason="undefined" cert="high">μέν</unclear>ου χρυσίου μηδὲ περὶ <supplied reason="lost" cert="high">οἱου</supplied><unclear reason="undefined" cert="high">δή</unclear><supplied reason="lost" cert="high">ποτε ἄλλου πράγματος</supplied>

    <lb n="6"/><supplied reason="lost" cert="high">ἐγγράφου ἢ ἀγράφου</supplied> παρὲξ χρυσοῦ <expan>κερατί<ex cert="high">ων</ex></expan> πεντήκ<unclear reason="undefined" cert="high">οντ</unclear><supplied reason="lost" cert="high">α</supplied> τεσ<unclear reason="undefined" cert="high">σά</unclear>ρων <sic><expan>κερατί<ex cert="high">ων</ex></expan></sic>

