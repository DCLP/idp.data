    <lb n="1"/><supplied reason="lost" cert="high"><g type="chirho"/> μετὰ τὴν ὑπατείαν Φλαουίου Πέ</supplied><unclear reason="undefined" cert="high">τ</unclear>ρ<unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">υ</supplied> τοῦ λαμπροτάτου Φαμενὼθ <num value="29"><unclear reason="undefined" cert="high">κθ</unclear></num>

    <lb n="2"/><expan>ἰνδικ<ex cert="high">τίωνος</ex></expan> <num value="10">ι</num>.

    <lb n="3"/><supplied reason="lost" cert="high">μερίδι τοῦ οἴκου τοῦ τῆς</supplied> <unclear reason="undefined" cert="high">π</unclear>εριβλ<unclear reason="undefined" cert="high">έπ</unclear>του μνήμης Τιμαγένους

    <lb n="4"/><supplied reason="lost" cert="high">δι ὑμῶν τῶν μεγαλοπρε</supplied><unclear reason="undefined" cert="high">π</unclear>εστάτων κομίτων Φοιβάμμωνος καὶ Σαμουηλίου

    <lb n="5"/><supplied reason="lost" cert="high">λαχόντων τὴν λο</supplied>γιστίαν <unclear reason="undefined" cert="high">κα</unclear>ὶ π<supplied reason="lost" cert="high">ατ</supplied>ερίαν καὶ προεδρίαν ταύτης

    <lb n="6"/>[τῆς <expan>λαμ<ex cert="high">πρᾶς</ex></expan> <expan>Ὀξυρυγχ<ex cert="high">ιτῶν</ex></expan> π]όλεως διὰ Σερήνου <unclear reason="undefined" cert="high">δι</unclear>αδόχου τὸ κοινὸν τῆς

    <lb n="7"/>[ἐργασίας τῶν ἐνθάδε <expan>ἰ]<unclear reason="undefined" cert="high">σ</unclear>ικιομαγίρ<ex cert="high">ων</ex></expan> δι᾽ ἡμῶν τῶν παρόντων καὶ ἑξῆς

    <lb n="8"/>[<expan>ἐγγεγρ<ex cert="high">αμμένων</ex></expan> κεφαλαιωτῶν ἀ]ναδεχομένων καὶ τὴν γνώμην τῶν ἄλλων

    <lb n="9"/><supplied reason="lost" cert="high">συντεχνιτῶν ἡμῶν καὶ ἑ</supplied><unclear reason="undefined" cert="high">μ</unclear>μενόντων πᾶσιν τοῖς ἑξῆς ἐγγεγραμμένοις

    <lb n="10"/><gap reason="lost" extent="18" unit="character"/><unclear reason="undefined" cert="high">ς</unclear> καὶ τῆς χρεί<unclear reason="undefined" cert="high">ας</unclear> ἀπαιτούσης πρὸς τὰ ἡμ<gap reason="illegible" extent="2" unit="character"/>

    <lb n="11"/><gap reason="lost" extent="25" unit="character"/><supplied reason="lost" cert="high"> πρὸ</supplied><unclear reason="undefined" cert="high">ς</unclear> <unclear reason="undefined" cert="high">τ</unclear>ὸ ἡμᾶς ποιήσασθαι τοῦ

    <lb n="12"/><gap reason="lost" extent="28" unit="character"/> ἐπὶ τῆς εὐτυχεστάτης ὑμῶν

    <lb n="13"/>[<expan>μεγαλοπρ<ex cert="high">επείας</ex></expan> ἀπὶ <gap reason="illegible" extent="5" unit="character"/> τῆς παρούσης ἐ]<unclear reason="undefined" cert="high">π</unclear>ινεμήσεως ὀνομ<add place="supralinear">ά</add>ζομεν

    <lb n="14"/><gap reason="lost" extent="28" unit="character"/><unclear reason="undefined" cert="high">ο</unclear>ρούαν υἱὸν Ἰουλιανοῦ

    <lb n="15"/>[<expan>μη<ex cert="high">τρὸς</ex></expan> <gap reason="illegible" extent="5" unit="character"/> ἀπὸ τῆς αὐτῆς πόλεως εὔπο]<unclear reason="undefined" cert="high">ρ</unclear>ον τὸ<supplied reason="lost" cert="high">ν</supplied> καὶ ἐπιτήδιον δυνάμενον

    <lb n="16"/>[τὴν <expan>ἰσικιομαγ<ex cert="high">είρων</ex></expan> χρείαν ἐκτελεῖν κ]<unclear reason="undefined" cert="high">αὶ</unclear> <unclear reason="undefined" cert="high">ἀ</unclear>νύσ<unclear reason="undefined" cert="high">ει</unclear>ν καὶ ἀναδεχόμεθα τοῦτον

    <lb n="17"/>[παρὰ τῇ ὑμῶν <expan>μεγαλοπρ<ex cert="high">επεία</ex></expan> <expan>ἐμφαν<ex cert="high">είας</ex></expan>] καὶ παρ<unclear reason="undefined" cert="high">α</unclear>νομῆ<supplied reason="omitted" cert="high">ς</supplied> καὶ τοῦτον ἐπιζητούμενον

    <lb n="18"/><supplied reason="lost" cert="high">πρὸς ἡμᾶς παραδώσομεν ἐν τῷ δη</supplied>μοσίᾳ ἐπὶ τῆσδε τῆς πόλεως. κυρία ἡ

    <lb n="19"/>[<expan>προγεγρ<ex cert="high">αμμένη</ex></expan> ὀνομασία ἁπλῆ <expan>γ]ρ<supplied reason="lost" cert="high">α</supplied>φ<ex cert="high">εῖσα</ex></expan> κ<unclear reason="undefined" cert="high">αὶ</unclear> <expan>ἐπερ<ex cert="high">ωτηθέντες</ex></expan> <expan>ὠμολ<ex cert="high">ογήσαμεν</ex></expan>. <handShift new="m2"/><expan>Αὐρ<ex cert="high">ήλιοι</ex></expan> Βίκτωρ

    <lb n="20"/><supplied reason="lost" cert="high">τοῦ δεῖνος καὶ </supplied><gap reason="lost" extent="unknown" unit="character"/><unclear reason="undefined" cert="high">μ</unclear>μων Ἰω<unclear reason="undefined" cert="high">άν</unclear>νου καὶ Βίκτωρ Μαρτυρίτου

    <lb n="21"/><supplied reason="lost" cert="high">ἀναδεχόμεθα καὶ ποιού</supplied>μεθα τὴν ὀνομασίαν καὶ <expan>συμφ<ex cert="high">ωνεῖ</ex></expan> ἡμῖν

    <lb n="22"/>[τὰ <expan>προγεγρ<ex cert="high">αμμένα</ex></expan> ὡς <expan>πρόκ<ex cert="high">ειται</ex></expan>. <gap reason="illegible" extent="5" unit="character"/>]<unclear reason="undefined" cert="high">ο</unclear>νίου <expan>ἔγρ<ex cert="high">αψα</ex></expan> ὑπὲρ αὐτῶν ἀγραμμάτων ὄντων.

