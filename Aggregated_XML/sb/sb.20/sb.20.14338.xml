    <lb n="1"/>γ<unclear cert="high" reason="undefined">ῆ</unclear><supplied cert="high" reason="lost">ς δημοσίων ὄντω</supplied><unclear cert="high" reason="undefined">ν</unclear> π<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ὸς τὸν μεμισθω-</supplied>

