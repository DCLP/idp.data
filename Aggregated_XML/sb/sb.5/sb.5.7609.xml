    <lb n="1"/>δ<unclear cert="high" reason="undefined">ι</unclear>[ὰ <expan>π]<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ογ</supplied><unclear cert="high" reason="undefined">ρ</unclear><ex cert="high">άμματος</ex></expan> 

    <lb n="2"/>Φιβίωνα καὶ τοὺς 

    <lb n="3"/>τούτου ἀδελφοὺς 

    <lb n="4"/>Ἀπόλλωνα καὶ Θεόφ<unclear cert="high" reason="undefined">ιλον</unclear> 

    <lb n="5"/>τῶν ἀπὸ Πώεως τῆς 

    <lb n="6"/>μεγάλης μὴ ἀπη<supplied cert="high" reason="lost">ντη</supplied>-

    <lb n="7"/>κότας κατὰ τὴ<supplied cert="high" reason="lost">ν</supplied> 

    <lb n="8"/>γεγονυῖαν αὐτο<unclear cert="high" reason="undefined">ῖς</unclear> <supplied cert="high" reason="lost">παραγ</supplied>-

    <lb n="9"/>γελίαν περιιστα<gap extent="6" reason="lost" unit="character"/> 

    <lb n="10"/>τηνε<gap extent="2" reason="illegible" unit="character"/><gap extent="12" reason="lost" unit="character"/> 

    <lb n="11"/>ὁ ὑποστ<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">άτηγος </supplied><gap extent="6" reason="lost" unit="character"/> 

    <lb n="12"/>περὶ πόλ<unclear cert="high" reason="undefined">ιν</unclear> <gap extent="7" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀνε</supplied>-

    <lb n="13"/>νήνοχεν ἔτι καὶ νῦν 

    <lb n="14"/>παραγίνεσθαι ἐπ᾽ Εὐρύλοχον 

    <lb n="15"/>τὸν συγγενῆ καὶ στρατηγὸν 

    <lb n="16"/>καὶ ἐπὶ τῶν προσόδων 

    <lb n="17"/>ἐν ἡμέραις τρισ<unclear cert="high" reason="undefined">ὶν</unclear> <num value="3"/> 

    <lb n="18"/>πρὸς τὴν ἐσομένην διά-

    <lb n="19"/>κρισιν ἢ <supplied cert="high" reason="lost">ὅτ</supplied>ι οὐδεὶς αὐτοῖς 

    <lb n="20"/>ἐξ ὑστέρ<supplied cert="high" reason="lost">ου καταλειφθή</supplied>-

    <lb n="21"/>σεται λόγος. 
<milestone rend="paragraphos" unit="undefined"/>
