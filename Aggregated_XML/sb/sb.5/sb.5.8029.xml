    <lb n="1"/><g type="stauros"/> <supplied reason="lost" cert="high">μετὰ τὴν</supplied> <hi rend="diaeresis">ὑ</hi>πα<supplied reason="lost" cert="high">τείαν <expan>Φλ<ex cert="high">αυΐου</ex></expan></supplied> Βελισαρίου τοῦ ἐνδοξο<unclear reason="undefined" cert="high">τά</unclear>του, Μεσορὴ <choice><corr>τρεισκαιδεκάτῃ</corr><sic>τρισ<supplied reason="lost" cert="high">καιδεκατη</supplied></sic></choice> 

    <lb n="2"/>πρώτης <expan>ἰνδ<ex cert="high">ικτίονος</ex></expan> ἐν Ἀντινόου πόλει τῇ <expan>λαμπρο<ex cert="high">τάτῃ</ex></expan>. 

    <lb n="3"/>Αὐρήλιος Ἀνδρόνικος Φιβίου, μητρὸς Νόννας, ἀπὸ τῆς Ἑρμουπολιτῶν, <supplied reason="lost" cert="high">τὰ ν</supplied>ῦν 

    <lb n="4"/>διάγων ἐνταῦθα ἐπὶ τῆς Ἀντινοέων βραδέως ὑπογράφων προ<supplied reason="lost" cert="high">νοητὴ</supplied>ς 

    <lb n="5"/>Αὐρήλίῳ Φοιβάμμωνι υ<hi rend="diaeresis">ἱ</hi>ῷ Μηνᾶ τῷ θαυμασιωτάτῳ βοηθῷ ἀπὸ τῆς αὐτῆς 

    <lb n="6"/>Ἑρμ<supplied reason="lost" cert="high">ου</supplied>πολιτῶν, τὰ νῦν καὶ αὐτῷ διάγοντι ἐνταῦθα <supplied reason="lost" cert="high">ἐπ</supplied>ὶ τῆς αὐτῆς 

Ἀντινοέων. 

    <lb n="7"/>ἐπειδ<supplied reason="lost" cert="high">ὴ</supplied> κατὰ παράκλησιν ἐμὴν καὶ ἀξίω<supplied reason="lost" cert="high">σι</supplied>ν κ<supplied reason="lost" cert="high">αὶ αἴ</supplied>τησι<supplied reason="lost" cert="high">ν</supplied> ἀ<supplied reason="lost" cert="high">νεδέξατό με</supplied> 

    <lb n="8"/>ἡ σὴ θαυμασιότης ἐγγράφως παρὰ τῷ λαμπροτάτῳ Διο<unclear reason="undefined" cert="high">σ</unclear><supplied reason="lost" cert="high">κορί</supplied><unclear reason="undefined" cert="high">δῃ</unclear> 

    <lb n="9"/>σιγγου<unclear reason="undefined" cert="high">λ</unclear><supplied reason="lost" cert="high">αρ</supplied>ίῳ τῆς κατὰ Θηβα<hi rend="diaeresis">ί</hi>δα δ<supplied reason="lost" cert="high">ουκι</supplied>κῆς <supplied reason="lost" cert="high">τ</supplied><unclear reason="undefined" cert="high">ά</unclear>ξεως ὥστ᾽ ἐμὲ τὴ<unclear reason="undefined" cert="high">ν</unclear> <supplied reason="lost" cert="high">τῶν</supplied> 

    <lb n="10"/>πρ<unclear reason="undefined" cert="high">ο</unclear>ν<unclear reason="undefined" cert="high">ο</unclear>ητῶν χρείαν ἐκτελέσαι ἐπὶ <unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">ῆνας </supplied><gap reason="lost" extent="1" unit="character"/><supplied reason="lost" cert="high"> ἀπὸ </supplied><choice><corr><supplied reason="lost" cert="high">κα</supplied>ρπῶν</corr><sic><supplied reason="lost" cert="high">κα</supplied>ρπον</sic></choice> τῆς σὺν θεῷ ἐσομ<unclear reason="undefined" cert="high">έ</unclear>ν<unclear reason="undefined" cert="high">ης</unclear> 

    <lb n="11"/>δευτέρας <expan>ἰνδ<ex cert="high">ικτίονος</ex></expan> καὶ πάντα τὰ ἐμπιστευ<supplied reason="lost" cert="high">όμενά μο</supplied>ι παρά τε τῆς αὐτοῦ λαμπρότητος 

    <lb n="12"/>καὶ παρὰ τῶν <hi rend="diaeresis">ὑ</hi>μετέρων αὐτ<supplied reason="lost" cert="high">όθι γεωργῶν καὶ</supplied> ἀμπελουργῶν ἀποκαταστῆ<supplied reason="lost" cert="high">σα</supplied>ι 

    <lb n="13"/>τῇ αὐτοῦ λαμπρότητι μεθ᾽ <hi rend="diaeresis">ὑ</hi>γιοῦς τῆς πί<supplied reason="lost" cert="high">στεως ἀκατα</supplied>φρονήτως, <choice><corr>εἰκότως</corr><sic>εικοδ<unclear reason="undefined" cert="high">ως</unclear></sic></choice> 

    <lb n="14"/>καὶ ἡ σὴ θαυμασιότης ἐπεζήτησεν ἀσ<supplied reason="lost" cert="high">φάλεια</supplied>ν <unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">α</supplied>ρ᾽ <supplied reason="lost" cert="high">ἐ</supplied>μοῦ περὶ τ<unclear reason="undefined" cert="high">ο</unclear>ύτου, 

    <lb n="15"/>κατὰ τοῦτο ταύτην σο<unclear reason="undefined" cert="high">ι</unclear> <choice><corr>τίθημι</corr><sic><supplied reason="lost" cert="high">τιθ</supplied>ημει</sic></choice> τὴν παρακλ<unclear reason="undefined" cert="high">η</unclear><supplied reason="lost" cert="high">τικὴν ὁμολογίαν</supplied>, δι᾽ ἧς 

    <lb n="16"/>ὁμολ<supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">γῶ</unclear> ἑτοίμως ἔχειν πάντα τὰ <hi rend="diaeresis">ὑ</hi>ποδεχόμενά μοι ὑ<unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ό τε τῆς αὐτοῦ</supplied> 

    <lb n="17"/><choice><corr>λαμπρότητος</corr><sic>λαμ<supplied reason="lost" cert="high">πρ</supplied>οτης</sic></choice> κ<unclear reason="undefined" cert="high">αὶ</unclear> παρὰ τῶν γεωρ<supplied reason="lost" cert="high">γῶν</supplied> καὶ α᾿̣<unclear reason="undefined" cert="high">μ</unclear>πελουργῶν <supplied reason="lost" cert="high">αὐτοῦ</supplied> 

    <lb n="18"/>ἔν τε σ<unclear reason="undefined" cert="high">ίτ</unclear>ῳ καὶ χρυσίῳ καὶ <choice><corr>κριθίοις</corr><sic>κριθοις</sic></choice> καὶ οἴνῳ καὶ παντοίοις <unclear reason="undefined" cert="high">εἴδ</unclear><supplied reason="lost" cert="high">εσιν ἀπο</supplied>-

    <lb n="19"/><unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">α</supplied>τ<supplied reason="lost" cert="high">α</supplied><unclear reason="undefined" cert="high">στ</unclear>ῆσ<supplied reason="lost" cert="high">αι</supplied> τῇ αὐτοῦ λαμπρότητι ἀκαταγνώστως· εἰ δ<supplied reason="lost" cert="low">ὲ ὀχληθείης</supplied> 

    <lb n="20"/>ἢ καὶ ἀ<unclear reason="undefined" cert="high">παιτηθείης</unclear> <unclear reason="undefined" cert="high">τι</unclear> <supplied reason="lost" cert="high">ὑπ</supplied>ὲρ ἐμοῦ, ὁμολογῶ παρασχεῖ<supplied reason="lost" cert="high">ν οἴκοθεν κ</supplied>αὶ 

    <lb n="21"/>ἐκ τῆς ἐμῆς <hi rend="diaeresis">ὑ</hi>ποστάσ<unclear reason="undefined" cert="high">ε</unclear><supplied reason="lost" cert="high">ως</supplied> καὶ ἁπλῶς εἰ<supplied reason="lost" cert="high">πεῖν τὸ ἀ</supplied>ζήμιον καὶ τὸ ἀβλαβὲς 

    <lb n="22"/>καὶ τὸ ἀν<hi rend="diaeresis">υ</hi>πόμνηστον παραφ<supplied reason="lost" cert="high">υλ</supplied>ά<supplied reason="lost" cert="high">ξαι</supplied> τῇ <unclear reason="undefined" cert="high">σ</unclear>ῇ <supplied reason="lost" cert="high">θαυ</supplied>μασιότητι κατὰ πάντα 

    <lb n="23"/>τρόπον περὶ τούτου καὶ ἐπὶ τούτοις <unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ᾶ</supplied>σι ἐπωμοσάμην τὴν ἁγίαν καὶ 

    <lb n="24"/>ὁμοούσιον τριάδα καὶ τὴν νί<supplied reason="lost" cert="high">κην καὶ δι</supplied><unclear reason="undefined" cert="high">α</unclear>μονὴν τοῦ καλλινίκου 

    <lb n="25"/>δεσπότου ἡμῶν Φλαυ<hi rend="diaeresis">ί</hi>ου <hi rend="diaeresis">Ἰ</hi>ουστινι<supplied reason="lost" cert="high">ανοῦ τοῦ αἰω</supplied>νίου αὐγο<unclear reason="undefined" cert="high">ύ</unclear>στου 

    <lb n="26"/>αὐτοκράτορος ἐμμεῖναι πᾶσι τοῖς προγεγραμμένοις καὶ κατὰ 

    <lb n="27"/>μηδένα τρόπον παραβῆναι καὶ εἰς σὴν ἀσφάλειαν ταύτην σοι ἐθέμην 

    <lb n="28"/>τὴν παρακλητικὴν ὁμολογίαν, <supplied reason="lost" cert="high">ἥτις κυρία</supplied> καὶ βεβαί<supplied reason="lost" cert="high">α ἐστὶ</supplied><unclear reason="undefined" cert="high">ν</unclear> καὶ 

    <lb n="29"/>εἰς τὰ προγεγραμμένα πάντα <expan>ἐπ<supplied reason="lost" cert="high">ε</supplied><unclear reason="undefined" cert="high">ρ</unclear><ex cert="high">ωτηθεὶς</ex></expan> <expan><supplied reason="lost" cert="high">ὡμο</supplied><unclear reason="undefined" cert="high">λ</unclear><ex cert="high">όγησα</ex></expan> <hi rend="diaeresis">ὑ</hi>ποθέμενός σοι εἰς τ<unclear reason="undefined" cert="high">ο</unclear>ῦτο 

    <lb n="30"/>πάντα τὰ νῦν ὄντα καὶ ἐσ<supplied reason="lost" cert="high">ό</supplied>μενά <supplied reason="lost" cert="high">μ</supplied>οι πράγματα. <g type="chirho"/> <handShift new="m2"/> <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> Ἀνδρόνικος Φιβίο<supplied reason="lost" cert="high">υ</supplied> 

    <lb n="31"/>ὁ <expan>προκ<ex cert="high">είμενος</ex></expan> <choice><corr>στοιχεῖ</corr><sic>στοιχι</sic></choice> μοι ἡ παρακλητικὴ <supplied reason="lost" cert="high">ὁμολο</supplied>γία, ὡς <expan>πρόκ<ex cert="high">ειται</ex></expan>. <handShift new="m3"/> Α<hi rend="diaeresis">ὐ</hi>ρήλιος 

    <lb n="32"/>Κολλοῦθος Φωσφορίου ἀπὸ <expan>Ἀντι<ex cert="high">νόου</ex></expan> μαρ<supplied reason="lost" cert="high">τυρῶ τῇ πα</supplied>ρακλητικῇ ὁμολογίᾳ 

    <lb n="33"/>ἀκούσας παρὰ τοῦ θεμένου. <g type="stauros"/> <handShift new="m4"/> <g type="stauros"/> <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> Θε<unclear reason="undefined" cert="high">ι</unclear><gap reason="lost" extent="7" unit="character"/>ω<gap reason="illegible" extent="3" unit="character"/>ο<hi rend="diaeresis">υ</hi> ἀπὸ <expan>Ἀντι<ex cert="high">νόου</ex></expan> μαρτυρῶ 

    <lb n="34"/><supplied reason="lost" cert="high">τῇ</supplied> παρακλητικῇ ὁμολογίᾳ ἀκούσας <supplied reason="lost" cert="high">παρὰ τοῦ</supplied> θεμένου. <g type="stauros"/> <handShift new="m5"/> <g type="stauros"/> <expan>Φλ<ex cert="high">αύιος</ex></expan> Χριστόδωρος 

    <lb n="35"/><expan>Ἀμμωνίο<ex cert="high">υ</ex></expan> ἀπὸ <expan>Ἀντι<ex cert="high">νόου</ex></expan> μαρτυρῶ τῇ παρ<supplied reason="lost" cert="high">ακλητικῇ</supplied> ὁμολογίᾳ ἀκούσας παρὰ <expan>το<ex cert="high">ῦ</ex></expan> θεμέν<unclear reason="undefined" cert="high">ο</unclear>υ. <g type="stauros"/> 

    <lb n="36"/>δι᾽ ἐμοῦ Κολλούθου Φοιβάμμων<unclear reason="undefined" cert="high">ος</unclear> <supplied reason="lost" cert="high"><expan>συμβ<ex cert="high">ολαιογράφου</ex></expan> </supplied><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/> </ab></div><div n="meta.18008" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.18008" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" value="0537-08-06" notBefore="537" notAfter="537">6. Aug. 537</date></p></div><div n="meta.18008" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Zur Datierung vgl. BL VII, S. 197.</p></div><div n="meta.18008" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">SB</title><biblScope type="volume">5</biblScope><biblScope type="numbers">8029</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">18008</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0239</series>; <biblScope type="volume">5</biblScope>; <biblScope type="numbers">8029</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0239;5;8029" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.18008" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p>keine</p></div><div n="meta.18008" type="history" subtype="locations" org="uniform" part="N" sample="complete"><p><placeName type="ancientFindspot" cert="high" full="yes">Antinoopolis</placeName></p></div></body></text></TEI.2>