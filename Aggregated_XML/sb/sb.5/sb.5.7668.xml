    <lb n="2"/><choice><corr>ἐλαιοπρατῶν</corr><sic>ελεοπρατων</sic></choice> τ<unclear cert="high" reason="undefined">ῆς</unclear> <unclear cert="high" reason="undefined">αὐτ</unclear>ῆς πόλεως <unclear cert="high" reason="undefined">δι</unclear><gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ο<unclear cert="high" reason="undefined">π</unclear>α <expan>κεφ<ex cert="low">αλαιωτοῦ</ex></expan> <expan>χ<ex cert="high">αίρειν</ex></expan>. 

