    <lb n="1"/><supplied reason="lost" cert="high">ἔτου</supplied>ς δ<supplied reason="lost" cert="high">ωδ</supplied>εκ<supplied reason="lost" cert="high">άτου Τιβ</supplied>ερίου Κλαυδίου Κ<supplied reason="lost" cert="high">αίσαρ</supplied>ος 

    <lb n="2"/><supplied reason="lost" cert="high">Σεβ</supplied>αστοῦ Γερμαν<supplied reason="lost" cert="high">ικοῦ</supplied> αὐτοκράτορος, Παῦνι <num value="20"><unclear reason="undefined" cert="high">κ</unclear></num> <expan>Σεβα<ex cert="high">στῇ</ex></expan> 

    <lb n="3"/><supplied reason="lost" cert="high">ἐν Ὀ</supplied>ξυρύγχων π<supplied reason="lost" cert="high">όλει</supplied> τῆς Θηβαΐδος. ὁμολ<supplied reason="lost" cert="high">ογ</supplied>οῦσιν 

    <lb n="4"/><supplied reason="lost" cert="high">Ἀμ</supplied>μ<supplied reason="lost" cert="high">ώ</supplied>νιος καὶ Ἑρμόδοτος ἀμφότεροι <expan>Σαραπίων<ex cert="high">ος</ex></expan> 

    <lb n="5"/><supplied reason="lost" cert="high">Κεν</supplied>δέᾳ Κενδέου καὶ τῇ τούτου μὲν γυναικί, <supplied reason="lost" cert="high">τ</supplied>οῦ δε` 

    <lb n="6"/>Ἀμμων<supplied reason="lost" cert="high">ί</supplied>ου καὶ Ἑρμοδότου ἀδελφῇ Ἀπολλωνίᾳ τοῦ 

    <lb n="7"/>αὐτ<supplied reason="lost" cert="high">οῦ</supplied> Σαραπίωνος, ἐν ἀγυιᾷ, ἀπέχε<supplied reason="lost" cert="high">ι</supplied>ν παρ᾽ αὐτῶν 

    <lb n="8"/>ὁ μὲ<supplied reason="lost" cert="high">ν</supplied> Ἀμμώνιος τὸ ἐπιβάλλον αὐτ<supplied reason="lost" cert="high">ῷ</supplied> τε καὶ <supplied reason="lost" cert="high">τ</supplied>ῷ ἑτέρῳ 

    <lb n="9"/>αὐτ<supplied reason="lost" cert="high">ο</supplied>ῦ καὶ τοῦ Ἑρμοδότου καὶ Ἀπολλωνίας ἀδελφῷ Πε-

    <lb n="10"/>τουσ<supplied reason="lost" cert="high">ί</supplied>ῳ τοῦ αὐτοῦ <supplied reason="lost" cert="high">Σ</supplied>αραπίωνος, <choice><corr>νυνὶ</corr><sic>νυνει</sic></choice> ὄντι ἀποδήμῳ, 

    <lb n="11"/>δίμοιρον μέρος· ὁ δὲ Ἑρμόδοτος τὸ καὶ αὐτῷ <expan>ἐπιβάλλ<ex cert="high">ον</ex></expan> 

    <lb n="12"/>τρίτ<supplied reason="lost" cert="high">ο</supplied>ν μέρος ἀργυρ<supplied reason="lost" cert="high">ί</supplied>ου Σεβαστοῦ καὶ Πτολεμαικοῦ νο-

    <lb n="13"/>μίσματος δραχμῶν ἑβδομήκοντα δύο <num value="72"/> <expan>κεφαλαίο<ex cert="high">υ</ex></expan> 

    <lb n="14"/><unclear reason="undefined" cert="high">δανε</unclear><supplied reason="lost" cert="low">ίου, ὃ Ἀ</supplied>μμώνιος καὶ Ἑρμόδοτο<supplied reason="lost" cert="high">ς</supplied> καὶ Πετούσιος δεδανεί-

    <lb n="15"/>κασι τ<supplied reason="lost" cert="high">ῷ</supplied> τε Κενδέᾳ καὶ τῇ γυνα<supplied reason="lost" cert="high">ι</supplied>κὶ Ἀπολλωνίᾳ κατὰ 

    <lb n="16"/>συνγ<supplied reason="lost" cert="high">ρ</supplied>αφὴν τὴν τελειωθεῖσαν διὰ τοῦ ἐν Ὀξυρύγχων 

    <lb n="17"/>πόλε<supplied reason="lost" cert="high">ι</supplied> μνημονείου τῷ Παῦνι μηνὶ τοῦ ὀγδόου ἔτους 

    <lb n="18"/>Τιβε<supplied reason="lost" cert="high">ρί</supplied>ου Κλαυδίου Καίσαρος Σεβαστοῦ Γερμανικοῦ <expan>αὐτοκράτ<ex cert="high">ορος</ex></expan> 

    <lb n="19"/>ἐπ᾽ ἐ<supplied reason="lost" cert="high">ν</supplied>οικισμῷ τῶν σημανθέντων διὰ ταύτης <choice><corr>τ<supplied reason="omitted" cert="high">ῆς συνγραφῆς</supplied></corr><sic>τ</sic></choice> 

    <lb n="20"/>καὶ ἐπ<supplied reason="lost" cert="high">ὶ</supplied> τὸν δηλωθέντα χρόνον· καὶ μηδὲν ἐνκαλεῖν 

    <lb n="21"/>μηδ᾽ ἐνκαλέσειν μηδ᾽ <choice><corr>ἐπελεύσεσθαι</corr><sic>επελευσασθαι</sic></choice> τοὺς <expan>ὁμολογοῦντ<ex cert="high">ας</ex></expan> 

    <lb n="22"/>μηδ᾽ ἄλλον ὑπὲρ αὐ<supplied reason="lost" cert="high">τ</supplied>ῶν τῷ Κενδέᾳ μηδὲ τῇ γυναικὶ 

    <lb n="23"/>Ἀπ<supplied reason="lost" cert="high">ολ</supplied>λωνίᾳ μηδ<supplied reason="lost" cert="high">ὲ</supplied> τοῖς παρ᾽ αὐτῶν μήτε περὶ τού<supplied reason="lost" cert="high">το</supplied>υ 

    <lb n="24"/>μηδὲ πε<supplied reason="lost" cert="high">ρ</supplied>ὶ ἄλλου μηδενὸς ἁπλῶς μέχρι τῆς <expan>ἐνεστώσ<ex cert="high">ης</ex></expan> 

    <lb n="25"/>ἡμέρας, ὁ δὲ Ἀμμ<supplied reason="lost" cert="high">ώ</supplied>νιος καὶ πάντα τὸν <expan>ἐπελευσόμε<ex cert="high">νον</ex></expan> 

    <lb n="26"/>αὐτο<supplied reason="lost" cert="high">ῖ</supplied>ς ἢ τοῖς παρ᾽ <supplied reason="lost" cert="high">αὐτ</supplied>ῶν ἐκ τοῦ τοῦ ἀδελφοῦ <expan>Πετουσίο<ex cert="high">υ</ex></expan> 

    <lb n="27"/><gap reason="lost" extent="8" unit="character"/> τούτων <supplied reason="lost" cert="high">χ</supplied>άρ<supplied reason="lost" cert="high">ι</supplied>ν ἀποστήσειν αὐτὸν τοῖς ἰδίοις 

    <lb n="28"/>αὐ<supplied reason="lost" cert="high">τοῦ δ</supplied>α<supplied reason="lost" cert="high">π</supplied>ανή<supplied reason="lost" cert="high">μα</supplied>σι, ἢ χωρ<supplied reason="lost" cert="high">ὶς</supplied> τοῦ τὴ<supplied reason="lost" cert="high">ν</supplied> ἐσομένην 

    <lb n="29"/>ἔφοδ<supplied reason="lost" cert="high">ον ἄ</supplied>κυρον <supplied reason="lost" cert="high">εἶν</supplied>αι ἔτι καὶ ἐκτίνειν τ<supplied reason="lost" cert="high">οὺς</supplied> <expan>ὁμολογοῦντ<ex cert="high">ας</ex></expan> 

    <lb n="30"/>ἢ τὸν <supplied reason="lost" cert="high">ὑπὲρ</supplied> αὐτῶν ἐπελευσόμενον τοῖς <expan>προγεγραμμέ<ex cert="high">νοις</ex></expan> 

    <lb n="31"/>ἢ <supplied reason="lost" cert="high">τοῖς παρ᾽ α</supplied>ὐτῶν <supplied reason="lost" cert="high">κα</supplied>θ᾽ ἑκάστην ἔφοδον τό τε βλάβος 

    <lb n="32"/><supplied reason="lost" cert="high">καὶ </supplied><choice><corr><supplied reason="lost" cert="high">ἐπίτ</supplied>ιμον</corr><sic><supplied reason="lost" cert="high">επιτ</supplied>ειμον</sic></choice> <expan>ἀργ<ex cert="high">υρίου</ex></expan> <supplied reason="lost" cert="high"><expan><ex cert="high">δραχμὰς</ex></expan></supplied> τεσσαράκοντα <num value="40"/> καὶ εἰς τὸ <expan>δημόσιο<ex cert="high">ν</ex></expan> 

    <lb n="33"/><supplied reason="lost" cert="high">τὰ</supplied>ς <supplied reason="lost" cert="high">ἴσ</supplied>ας, καὶ μηθὲν ἧσσον κυρία ἡ συγγραφή. 

