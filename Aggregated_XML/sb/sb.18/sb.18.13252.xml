    <lb n="1"/>Αὐρηλίῳ Λε<unclear cert="high" reason="undefined">ων</unclear>ίδῃ στρατηγῷ ἤτοι ἐξά-

    <lb n="2"/>κτορι Ὀάσεως Μεγάλης Αὐρήλιος 

    <lb n="3"/>Χρῆστος Οὐαλερ<supplied cert="high" reason="lost">ί</supplied>ου φυλάρχης τρεισκαιδεκάτης 

    <lb n="4"/>ἰνδικ<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ί</supplied>ονος <unclear cert="high" reason="undefined">τῷ</unclear> φιλτάτῳ χαίρειν. ἐπισταλεὶς 

    <lb n="5"/>ὑπὸ τ<supplied cert="high" reason="lost">ῆς</supplied> σῆς <choice><corr>ἐπιεικείας</corr><sic>επι<unclear cert="high" reason="undefined">ει</unclear>κιας</sic></choice> ἀποδεῖξαι <choice><corr>ὑποδέκτας</corr><sic>υποδεκτης</sic></choice> 

    <lb n="6"/>ἤτοι <unclear cert="high" reason="undefined">κα</unclear>ταπ<unclear cert="high" reason="undefined">ο</unclear>μ<unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ο</supplied>ὺς τῶν κελευσθέντων ἀπαι-

    <lb n="7"/>τηθῆναι χρ<unclear cert="high" reason="undefined">ημ</unclear><supplied cert="high" reason="lost">ά</supplied>των ὑπὸ τῆς ἐξουσίας τοῦ 

    <lb n="8"/>κυρίο<unclear cert="high" reason="undefined">υ</unclear> μου λα<unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">πρ</supplied>οτάτου ἡγεμόνος Φλαυίου Δομιτ<supplied cert="high" reason="lost">τ</supplied>ιανοῦ <unclear cert="high" reason="undefined">ἀν</unclear>αγκαίως αἱροῦμαι Αὐρήλιον 

    <lb n="9"/><gap extent="2" reason="illegible" unit="character"/>λ<gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ι</unclear><gap extent="1" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">α</unclear>γει<unclear cert="high" reason="undefined">ν</unclear> <gap extent="1" reason="illegible" unit="character"/> τοῦ τε ναύλου καὶ χρυ-

    <lb n="10"/>σοῦ <choice><corr>βουρδώνων</corr><sic>βο<supplied cert="high" reason="lost">υ</supplied><unclear cert="high" reason="undefined">ρ</unclear>δονω<unclear cert="high" reason="undefined">ν</unclear></sic></choice> καὶ <choice><corr>τιρώνων</corr><sic>δειρονων</sic></choice> και τῶν 

    <lb n="11"/>ἀδηλ<unclear cert="high" reason="undefined">ηγ</unclear>ατεύτων ἑνδεκάτης καὶ δωδεκάτης 

    <lb n="12"/>καὶ τρε<supplied cert="high" reason="lost">ισ</supplied>καιδεκ<unclear cert="high" reason="undefined">άτ</unclear>ης ἰνδικτιόνων, Αὐρήλι-

    <lb n="13"/>ον δὲ <gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/> <unclear cert="high" reason="undefined">γ</unclear>ενόμε<unclear cert="high" reason="undefined">νον</unclear> ὑποδέκτην 

    <lb n="14"/>τῆς <choice><corr>ὀβρύζης</corr><sic><unclear cert="high" reason="undefined">ομ</unclear><supplied cert="high" reason="lost">β</supplied><unclear cert="high" reason="undefined">ρ</unclear>υζης</sic></choice> τοῦ το<unclear cert="low" reason="undefined">ῦ</unclear> <unclear cert="high" reason="undefined">ἀπ</unclear>αιτηθέντος 

    <lb n="15"/>πραγμ<supplied cert="high" reason="lost">ατευ</supplied>τικ<supplied cert="high" reason="lost">ο</supplied>ῦ χρυσαργύρου ἱκανοὺς 

    <lb n="16"/>ὄντας <supplied cert="high" reason="lost">κα</supplied>ὶ <choice><corr>ἐπιτηδείους</corr><sic>επει<supplied cert="high" reason="lost">τ</supplied>ηδιους</sic></choice>. 

