    <lb n="1"/><gap extent="unknown" reason="lost" unit="line"/> 

    <lb n="1"/><gap extent="25" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τ</supplied>οῦ λαμπροτ<supplied cert="high" reason="lost">ά</supplied>του καὶ το<supplied cert="high" reason="lost">ῦ</supplied> δηλωθησομένου, <unclear cert="high" reason="undefined">Θὼθ</unclear> <num value="17">ιζ</num>, <num value="12">ιβ</num> <expan>ἰνδικ<ex cert="high">τίονος</ex></expan>. 

    <lb n="2"/><gap extent="23" reason="lost" unit="character"/><supplied cert="high" reason="lost"> βι</supplied><unclear cert="high" reason="undefined">ά</unclear>ρχῳ ἀριθμοῦ τῶν γενναιοτάτων καὶ καθωσι-

    <lb n="3"/><supplied cert="high" reason="lost">ωμένων Μαύρων τῶν ἐπὶ τῆς</supplied> <unclear cert="high" reason="undefined">Ἑ</unclear>ρμουπολιτῶν καθιδρυμένων, <expan>παρ<ex cert="high">ὰ</ex></expan> Σαμουὴλ Ἰα-

    <lb n="4"/><supplied cert="high" reason="lost">κώβου μητρὸς </supplied><gap extent="10" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Ἰου</supplied>δαίου τῇ <choice><corr>θρησκείᾳ</corr><sic>θρησκια</sic></choice> ἀπὸ τῆς αὐτῆς Ἑρμουπολιτῶν. 

    <lb n="5"/><supplied cert="high" reason="lost">βούλομαι ἑκουσίως καὶ αὐθαι</supplied>ρέτως μισθώσασθαι παρὰ σοῦ <supplied cert="high" reason="lost">ἐ</supplied>φ᾽ ὃν β<supplied cert="high" reason="lost">ούλ</supplied>εται 

    <lb n="6"/><supplied cert="high" reason="lost">χρόνον λογιζόμενον ἀπὸ τοῦ ὄν</supplied><unclear cert="high" reason="undefined">τ</unclear>ος μηνὸς Θὼθ τῆς παρούσης δ<supplied cert="high" reason="lost">ωδ</supplied>εκάτης <expan>ἰνδικ<ex cert="high">τίονος</ex></expan> 

    <lb n="7"/><supplied cert="high" reason="lost">τὸ ὑπάρχον σοι δίμοιρον μέρ</supplied><unclear cert="high" reason="undefined">ο</unclear>ς ὁλοκλήρου οἰκίας οἷάς ἐστιν διαθέσεως 

    <lb n="8"/><gap extent="22" reason="lost" unit="character"/><supplied cert="high" reason="lost"> αὐλ</supplied>ῆς καὶ φρέατος καὶ παντὸς δικ<supplied cert="high" reason="lost">αίου</supplied> καὶ χρηστη-

    <lb n="9"/><supplied cert="high" reason="lost">ρίου διακειμένης ἐν τῇ αὐτῇ Ἑρμ</supplied>οῦ πόλει ἐπ᾽ ἀμφόδου Φρουρίου Ἀπη<unclear cert="high" reason="undefined">λ</unclear>ιώτου πρὸς 

    <lb n="10"/><supplied cert="high" reason="lost">ἡμετέραν χρῆσιν καὶ οἴκησι</supplied>ν, ἐνοικείου τοῦ αὐτοῦ διμοίρου μέρους τοῦ ὅλου 

    <lb n="11"/><supplied cert="high" reason="lost">ἐνιαυτοῦ ἀργυρίου τα</supplied><unclear cert="high" reason="undefined">λ</unclear>άν<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ων </supplied><choice><corr><supplied cert="high" reason="lost">ὀ</supplied>κτακισχιλίων</corr><sic><supplied cert="high" reason="lost">ο</supplied>κτακισχειλιων</sic></choice> <num value="8000"/>, ὅπερ ἐνοίκει<supplied cert="high" reason="lost">ο</supplied>ν ἀποδώσω 

    <lb n="12"/><supplied cert="high" reason="lost">σοι πρὸς λῆ</supplied>ξιν ἑκάστου ἔτους ἀνυπερθέτως· καὶ ὁπόταν βουληθῇς ἔχειν, παρα-

    <lb n="13"/><supplied cert="high" reason="lost">δώσω σοι τὸ</supplied> αὐτὸ δίμοιρ<unclear cert="high" reason="undefined">ον</unclear> μέρος σὺν θύραις καὶ <choice><corr>κλεισὶ</corr><sic>κλισι</sic></choice> ὡς παρείληφα. ἡ μίσθω-

    <lb n="14"/><supplied cert="high" reason="lost">σις κυρία κ</supplied>αὶ βεβαία καὶ <expan>ἐπερ<ex cert="high">ωτηθεὶς</ex></expan> <expan>ὡ<unclear cert="high" reason="undefined">μ</unclear>ολ<ex cert="high">όγησα</ex></expan>. <handShift new="m2"/> Αὐρήλιος Σαμουὴλ Ἰακώβου ὁ <choice><corr>προκείμενος</corr><sic>προγιμενος</sic></choice> 

    <lb n="15"/><supplied cert="high" reason="lost">μεμίσθω</supplied>μαι ὡς <expan>π<supplied cert="high" reason="lost">ρόκ<ex cert="high">ειται</ex></supplied></expan>. <handShift new="m3"/> <unclear cert="high" reason="undefined">Αὐρήλ</unclear>ι<supplied cert="high" reason="lost">ος</supplied> Ἀμμώνιος Σαραπίωνος <expan>ἐπερ<ex cert="high">ωτηθεὶς</ex></expan> μαρτυρῶ τῇ μισθώσει ἀκούσας 

    <lb n="16"/><supplied cert="high" reason="lost">παρὰ το</supplied>ῦ <unclear cert="high" reason="undefined">θεμένου</unclear>. <handShift new="m4"/> <unclear cert="high" reason="undefined">Αὐρήλιος</unclear> <unclear cert="high" reason="undefined">Φοιβά</unclear>μμων Πινοτίωνος <expan>ἐπερ<ex cert="high">ωτηθεὶς</ex></expan> μαρτυρῶ τῇ μισθώσει 

    <lb n="17"/><supplied cert="high" reason="lost">ἀκούσας π</supplied>αρ<supplied cert="high" reason="lost">ὰ</supplied> τοῦ θεμέ<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">ου</supplied>. 

    <lb n="18"/><handShift new="m1"/> δι᾽ ἐμοῦ Ἀφοῦτος <expan>ἐγράφ<ex cert="high">η</ex></expan> διὰ Φοιβάμμωνος <expan>βοηθ<ex cert="high">οῦ</ex></expan>. 

