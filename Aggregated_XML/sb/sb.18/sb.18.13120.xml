    <lb n="10"/>γυναικὶ Τερ<unclear cert="high" reason="undefined">ε</unclear>ῦτι Πτολεμ<supplied cert="high" reason="lost">αίου τοῦ </supplied><gap extent="18" reason="lost" unit="character"/><supplied cert="high" reason="lost"> μητρὸς</supplied> 

