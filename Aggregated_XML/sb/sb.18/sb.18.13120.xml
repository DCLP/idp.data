    <lb n="16"/><supplied cert="high" reason="lost">Θ</supplied>εανοῦς ὥστε τὴν <choice><corr>ὁμ<supplied cert="high" reason="lost">ολογοῦσαν</supplied></corr><sic><unclear cert="high" reason="undefined">ωμ</unclear><supplied cert="high" reason="lost">ολογουσαν</supplied></sic></choice> <supplied cert="high" reason="lost">τοῦτο θρέψαι καὶ θη</supplied>-

