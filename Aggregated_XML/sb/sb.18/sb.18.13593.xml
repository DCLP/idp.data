    <lb n="19"/>ς ἐμ<unclear reason="undefined" cert="high">ῆ</unclear>ς τιμῆς πόκον <unclear reason="undefined" cert="high">ἐ</unclear><supplied reason="lost" cert="high">ρι</supplied><unclear reason="undefined" cert="high">δί</unclear><add place="supralinear">ων</add> 

