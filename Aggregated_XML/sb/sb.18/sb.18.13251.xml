    <lb n="1"/><handShift new="m1"/> Ἰου<unclear cert="high" reason="undefined">λί</unclear><supplied cert="high" reason="lost">ῳ</supplied> Εὐβ<supplied cert="high" reason="lost">ου</supplied><unclear cert="high" reason="undefined">λ</unclear>ίῳ Ἰ<supplied cert="high" reason="lost">ο</supplied>υλιανῷ τῷ λαμπροτ<unclear cert="high" reason="undefined">ά</unclear>τῳ <unclear cert="high" reason="undefined">ἡγε</unclear>μ<unclear cert="high" reason="undefined">ό</unclear>νι 

    <lb n="2"/>Αὐρήλ<supplied cert="high" reason="lost">ι</supplied>οι <unclear cert="high" reason="undefined">Ὑ</unclear>π<unclear cert="high" reason="undefined">ερέχ</unclear>ιο<supplied cert="high" reason="lost">ς</supplied> <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">α</supplied>ὶ Ἀνουβί<unclear cert="high" reason="undefined">ω</unclear>ν 

    <lb n="3"/>ἐξάκτορες Ἀπ<supplied cert="high" reason="lost">ό</supplied>λλωνος Μικρᾶς καὶ <supplied cert="high" reason="lost">Φ</supplied>λάϋιος 

    <lb n="4"/>Ἀμμώνιος <unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">τα</supplied><unclear cert="high" reason="undefined">τ</unclear>ι<unclear cert="high" reason="undefined">ων</unclear>άριος τῆς α<unclear cert="high" reason="undefined">ὐ</unclear>τῆς 

    <lb n="5"/>καὶ Μακάριος ῥιπάρι<unclear cert="high" reason="undefined">ος</unclear> <supplied cert="high" reason="lost">τῆ</supplied><unclear cert="high" reason="undefined">ς</unclear> π<unclear cert="high" reason="undefined">όλε</unclear>ως χαίρειν. 

    <lb n="6"/>ἐπιστολὴν γ<supplied cert="high" reason="lost">ρ</supplied>αφεῖσ<unclear cert="high" reason="undefined">α</unclear>ν ἡμῖν ὑπὸ τοῦ σοῦ 

    <lb n="7"/>μεγαλείου, λαμ<supplied cert="high" reason="lost">πρό</supplied>τατε ἡγεμὼν κύριε, 

    <lb n="8"/>περὶ τ<unclear cert="high" reason="undefined">ο</unclear>ῦ πάντας τοὺς ἀπὸ κεφ<unclear cert="high" reason="undefined">αλ</unclear>αιωτῶν 

    <lb n="9"/>ἀπὸ ἑνδεκάτης ἰνδικτίονος <unclear cert="high" reason="undefined">ἕ</unclear>ως <num value="14">ιδ</num> καὶ 

    <lb n="10"/><unclear cert="high" reason="undefined">αὐτ</unclear>ῆς εἰς τὴν <unclear cert="high" reason="undefined">τ</unclear>άξιν ἐκ<unclear cert="high" reason="undefined">πέμψαι</unclear> ἐ<unclear cert="high" reason="undefined">κομι</unclear>-

    <lb n="11"/>σάμεθα σήμερ<unclear cert="high" reason="undefined">ον</unclear> ἥτις ἐστὶν Μεσορὴ 

    <lb n="12"/><unclear cert="high" reason="undefined">π</unclear>έμπτη διὰ Ἰσιδώρου καὶ Θεοδώρου <expan>ὀφ<ex cert="high">φικιαλίων</ex></expan> 

    <lb n="13"/>τῆς σῆς τοῦ ἐμ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ῦ</supplied> <unclear cert="high" reason="undefined">κυρ</unclear>ίου τάξεως καὶ, ἵ<unclear cert="high" reason="undefined">ν</unclear>᾽ <unclear cert="high" reason="undefined">εἰ</unclear>δέν<supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">ι</unclear> 

    <lb n="14"/>ἔχοι σο<supplied cert="high" reason="lost">υ</supplied> τὸ μεγαλεῖ<unclear cert="high" reason="undefined">ο</unclear>ν, <unclear cert="high" reason="undefined">ἀ</unclear>ναφέρομεν, 


    <lb n="15"/>λαμπρότατε ἡγεμὼν κύριε. <handShift new="m2"/> ἐρρῶ<unclear cert="high" reason="undefined">θ</unclear>αί σε 

    <lb n="16"/>εὔχομαι, λαμπρότατε 

    <lb n="17"/>ἡγεμὼν κύριε. 

    <lb n="18"/><handShift new="m3"/> ἐρρῶσθαί σε εὔχομαι, 

    <lb n="19"/><choice><corr>λαμπρότατε</corr><sic>λαμπροταται</sic></choice> κύριε. 

    <lb n="20"/><handShift new="m1"/> ὑπ<unclear cert="high" reason="undefined">α</unclear>τεία<unclear cert="high" reason="undefined">ς</unclear> Δομιτίου <unclear cert="high" reason="undefined">Μο</unclear>δέστου τοῦ λαμπροτάτου 

    <lb n="21"/>ἐπάρχου τοῦ ἱεροῦ πραιτωρίου καὶ <expan>Φλ<ex cert="high">αυίου</ex></expan> Ἀρι<supplied cert="high" reason="lost">ν</supplied>θίο<unclear cert="high" reason="undefined">υ</unclear> 

