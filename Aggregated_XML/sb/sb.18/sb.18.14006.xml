    <lb n="9"/>τῇ <expan>ἁγί<ex cert="high">ᾳ</ex></expan> <expan>ἐκκλ<ex cert="high">ησίᾳ</ex></expan> τῶν ἁ<unclear reason="undefined" cert="high">γίων</unclear> <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> καὶ</supplied> 

