    <lb n="27"/>τος Βιέγχ<unclear reason="undefined" cert="high">εως</unclear>, <unclear reason="undefined" cert="high">ἀπη</unclear><supplied reason="lost" cert="high">λιώτου Π</supplied>ινούτιος ψ<supplied reason="lost" cert="high">ιλὸς τόπ</supplied>ος, λι<supplied reason="lost" cert="high">βὸς τ</supplied>οῦ <supplied reason="lost" cert="high">Αὐρηλ</supplied>ίου 

    <lb n="28"/>Σαραπίωνος, κ<supplied reason="lost" cert="high">αὶ περὶ</supplied> <gap reason="illegible" extent="3" unit="character"/>ροδην κλήρου κατοικ<supplied reason="lost" cert="high">ικοῦ</supplied> ἄρο<supplied reason="lost" cert="high">υρ</supplied>αν μίαν <num value="1"/>, 

