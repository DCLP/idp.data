    <lb n="11"/>τεσσα<supplied reason="lost" cert="high">ράκο</supplied>ντα <app type="BL"><lem resp="6.131">ἐννέα</lem><rdg>ἑπτὰ</rdg></app> <num value="49"/> ἀ<supplied reason="lost" cert="high">ρ</supplied>γυρίου Σεβαστοῦ νομίσματος κεφ<supplied reason="lost" cert="high">α</supplied>-

