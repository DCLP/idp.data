    <lb n="1"/><gap reason="lost" extent="41" unit="character"/><supplied reason="lost" cert="high"> παρέθετο εἰς ἀπόδειξιν</supplied> 

    <lb n="2"/><supplied reason="lost" cert="high">τοῦ εἶναι ἱερατικοῦ γένους ἀντίγραφον τῆς κατ᾽ οἰκίαν <expan>ἀπογρ<ex cert="high">αφῆς</ex></expan> τῆς γενομένης <num value="9">θ</num> <expan><ex cert="high">ἔτει</ex></expan></supplied> 

    <lb n="3"/><supplied reason="lost" cert="high">Ἀν</supplied>τωνεί<supplied reason="lost" cert="high">νου τοῦ <app type="BL"><lem resp="8.303">κυρίου</lem><rdg>κυρίου ἡμῶν</rdg></app> </supplied><gap reason="lost" extent="22" unit="character"/><supplied reason="lost" cert="high"> <app type="BL"><lem>ἐπὶ τῆς γραφῆς</lem><rdg>ἐπεσκεμμένον <gap reason="illegible" extent="6" unit="character"/> ἐκ τῆς βιβλιοθήκης</rdg></app> τῶν τοῦ</supplied> 

