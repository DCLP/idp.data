    <lb n="7"/><gap extent="12" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τιμῆς τῆς πρὸ</supplied>ς ἀλλ<supplied cert="high" reason="lost">ή</supplied>λου<supplied cert="high" reason="lost">ς συμπεφωνημένης </supplied><gap extent="4" reason="lost" unit="character"/> 

