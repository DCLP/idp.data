    <lb n="24"/><gap extent="13" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀ</supplied>ξίαν <gap extent="10" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀναφ</supplied>όρι<supplied cert="high" reason="lost">ον</supplied> 

