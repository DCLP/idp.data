    <lb n="31"/><gap extent="2" reason="lost" unit="character"/>τος δὲ καὶ <gap extent="unknown" reason="lost" unit="character"/>λλ<gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> ὡ<supplied cert="high" reason="lost">ς ἐτ</supplied>ῶν 

