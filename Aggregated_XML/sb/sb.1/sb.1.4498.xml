    <lb n="1"/><expan>αἰων<ex cert="high">ίου</ex></expan> Αὐγούστ<supplied cert="high" reason="lost">ου</supplied> 

    <lb n="1"/>Ἁθὺρ <num value="9">θ</num>, <num value="13">ιγ</num> <expan>[ἰνδ<ex cert="high">ικτίονος</ex>]</expan>, ἐν <expan>Ἀρ<ex cert="high">σινοιτῶν πόλει</ex></expan>. 

    <lb n="2"/>Αὐρήλιος Ἀλ<supplied cert="high" reason="lost">όν</supplied>τιος υἱὸς 

    <lb n="3"/>Παπνουθίου <supplied cert="high" reason="lost">ἀπ</supplied>ὸ ἐποικίου 

    <lb n="4"/>Κιαράτου τοῦ Ἀρσινοίτου <expan>νομ<ex cert="high">οῦ</ex></expan> 

    <lb n="5"/>Αὐρηλίῳ Θεοδώρῳ <expan>γραμμ<ex cert="high">ατεῖ</ex></expan> τῷ <space dim="horizontal" extent="unknown" unit="character"/> ἀπὸ τῆς Ἀρσινοι-

    <lb n="6"/>τῶν π[όλεως <expan>χ<ex cert="high">αίρειν</ex></expan>]. ὁμολογ<supplied cert="high" reason="lost">ῶ</supplied> 

    <lb n="7"/>καὶ νῦν ἐσχηκέναι με παρὰ σ<supplied cert="high" reason="lost">οῦ</supplied> 

    <lb n="8"/>διὰ χειρὸς εἰς <hi rend="diaeresis">ἰ</hi>δίαν μου χρείαν 

    <lb n="9"/>χρυσίου νομισμάτιον ἓν παρὰ <expan>κ<ex cert="high">εράτια</ex></expan> 

    <lb n="10"/>ἑπτὰ ἥμισυ τέταρτον, <expan>χρ<ex cert="high">υσίου</ex></expan> <expan>νο<ex cert="high">μισμάτιον</ex></expan> <num value="1">α</num> 

    <lb n="11"/><expan>π<ex cert="high">αρὰ</ex></expan> <expan>κερ<ex cert="high">άτια</ex></expan> <num value="7">ζ</num> <num value="1/2">𐅵</num> <num value="1/4">δ</num> κεφαλαίου, ἐπὶ τῷ 

    <lb n="12"/>με χορηγῆσαί σοι λόγῳ <expan>τόκ<ex cert="high">ου</ex></expan> 

    <lb n="13"/>αὐτοῦ ἐνιαυσίως ἀπὸ τοῦ παρόν-

    <lb n="14"/>τος μηνὸς Ἁθὺρ τῆς <expan>παρούσ<ex cert="high">ης</ex></expan> 

    <lb n="15"/><choice><corr>τρεισκαιδεκάτης</corr><sic>τρισκαιδεκατης</sic></choice> <expan>ἰν<ex cert="high">δικτίονος</ex></expan> <expan>κέρμ<ex cert="high">ατος</ex></expan> 

    <lb n="16"/>μυριάδας ἑπτακοσίας 

    <lb n="17"/>πεντήκοντα, <expan>κέρμ<ex cert="high">ατος</ex></expan> <expan><ex cert="high">δηναρίων</ex></expan> <expan>μ<ex cert="high">υριάδας</ex></expan> <num value="750">ψν</num> <num value="7500000"/>. 

    <lb n="18"/><supplied cert="high" reason="lost">ἄ</supplied>χρι ἀποδόσεως τοῦ χρέους, 

    <lb n="19"/>τὴν δὲ τούτου ἀπόδοσίν σοι 

    <lb n="20"/>ποιήσομαι, ὁπόταν βουληθείης, 

    <lb n="21"/>ἀνυπερθέτως, καὶ <expan>ἐπερ<ex cert="high">ωτηθεὶς</ex></expan> 

    <lb n="22"/><expan>ὡμολ<ex cert="high">όγησα</ex></expan>. <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> Ἀλόντιος υἱὸς 

    <lb n="23"/>Παπνουθίου ὁ <expan>πρ<ex cert="high">οκείμενος</ex></expan> <expan>συμφ<ex cert="high">ωνῶ</ex></expan> 

    <lb n="24"/>τὸ γράμμα ὡς <expan>πρ<ex cert="high">όκειται</ex></expan>. <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> 

    <lb n="25"/>Ἠλίας Παύλου <app type="BL"><lem resp="7.184"><expan>ἔγ[ρ<ex cert="high">αψα</ex></expan> <expan>ὑ<ex cert="high">πὲρ</ex></expan> <expan>αὐτο<ex cert="high">ῦ</ex></expan> παρόντος]</lem><rdg>ἔγ<supplied cert="high" reason="lost">ραψα ὑπὲρ αὐτοῦ</supplied></rdg></app> 

    <lb n="26"/>ἀγραμμ<supplied cert="high" reason="lost">άτου ὄντος</supplied>. 

    <lb n="27"/><g type="stauros"/> <foreign lang="la">di emu Ioannu [ </foreign><gap extent="unknown" reason="illegible" unit="character"/> ]  </ab></div><div n="meta.41319" org="uniform" part="N" sample="complete" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.41319" org="uniform" part="N" sample="complete" subtype="textDate" type="commentary"><p><date notAfter="600" notBefore="551" type="textDate">2. Hälfte VI</date></p></div><div n="meta.41319" org="uniform" part="N" sample="complete" subtype="general" type="commentary"><p>Datierung: 5. (6.) Nov., 13. Indiktion. Zur Datierung vgl. BL VII, S. 184.</p></div><div n="meta.41319" org="uniform" part="N" sample="complete" subtype="principalEdition" type="bibliography"><listBibl default="NO"><bibl default="NO" subtype="principal" type="publication"><title level="s" type="abbreviated">SB</title><biblScope type="volume">1</biblScope><biblScope type="numbers">4498</biblScope></bibl><bibl default="NO" type="Trismegistos"><biblScope type="numbers">41319</biblScope></bibl><bibl default="NO" type="DDbDP"><series>0239</series>; <biblScope type="volume">1</biblScope>; <biblScope type="numbers">4498</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0239;1;4498" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.41319" org="uniform" part="N" sample="complete" subtype="illustrations" type="bibliography"><p>keine</p></div><div n="meta.41319" org="uniform" part="N" sample="complete" subtype="locations" type="history"><p><placeName cert="high" full="yes" type="ancientFindspot">Arsinoiton Polis</placeName></p></div></body></text></TEI.2>