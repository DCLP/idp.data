    <lb n="50"/>[ἡμῖν ἐν μιᾶι <num value="1"/> σφραγῖδι] <expan>ἀρου<ex cert="high">ρῶν</ex></expan> <num value="35">λε</num>, <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">αὶ</supplied> <unclear cert="high" reason="undefined">ἐν</unclear> ἄλληι <unclear cert="high" reason="undefined">σ</unclear>φραγῖδι ἄ<unclear cert="high" reason="undefined">λ</unclear><supplied cert="high" reason="lost">λ</supplied>ων 

    <lb n="51"/>[<expan>ἀρου<ex cert="high">ρῶν</ex></expan> <num value="10">ι</num>, πασῶν περὶ Παθ]ῦριν, ἐπέδω<supplied cert="high" reason="lost">κ</supplied><unclear cert="high" reason="undefined">α</unclear>ν ὑπό<supplied cert="high" reason="lost">μ</supplied>νημα 

    <lb n="52"/><app type="BL"><lem resp="7.184"><seg cert="low" part="N">[ <gap extent="11" reason="illegible" unit="character"/> τῷ τοτε σ]τρατηγῶι</seg></lem><rdg><gap extent="17" reason="lost" unit="character"/><supplied cert="high" reason="lost"> σ</supplied>τρατηγῶι</rdg></app> <supplied cert="high" reason="lost">κα</supplied>τά τε ἡμῶν καὶ τοῦ <handShift new="m2"/> <supplied cert="high" reason="lost">τῆ</supplied>ς Σενενούπιος 

