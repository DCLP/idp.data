    <lb n="15"/><gap extent="7" reason="lost" unit="character"/><unclear cert="high" reason="undefined">νη</unclear>κέναι δ<unclear cert="high" reason="undefined">ίκ</unclear>ην <unclear cert="high" reason="undefined">η</unclear>σα<gap extent="4" reason="illegible" unit="character"/>, <supplied cert="high" reason="lost">τ</supplied>ῶι γὰρ <num value="3">γ</num> <expan><ex cert="high">ἔτει</ex></expan> θεοῦ Τίτ<supplied cert="high" reason="lost">ου κ</supplied>αθαρὸν 

    <lb n="16"/><gap extent="7" reason="lost" unit="character"/><unclear cert="high" reason="undefined">α</unclear>ν<gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ου</unclear>, <unclear cert="high" reason="undefined">ἀλλὰ</unclear> παρ<gap extent="4" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ωπ</unclear>ων θείων ὄντων τ<unclear cert="high" reason="undefined">ῆ</unclear>ς ἀντι<supplied cert="high" reason="lost">δίκο</supplied><unclear cert="high" reason="undefined">υ</unclear>, μὴ ε<unclear cert="high" reason="undefined">ἶν</unclear>αι δὲ <unclear cert="high" reason="undefined">αὐ</unclear>τὴν <gap extent="3" reason="lost" unit="character"/>ον, εἰ ἐκε<unclear cert="high" reason="undefined">ῖ</unclear>νοι οἱ λα<unclear cert="high" reason="undefined">β</unclear>ό<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">τ</supplied>ες 

