    <lb n="1"/>τοῖς ἐπὶ τῶν θυσιῶν ᾑρημένοις 

    <lb n="2"/>κώμης Φι<unclear cert="high" reason="undefined">λ</unclear>αδελφίας 

    <lb n="3"/>παρὰ Αὐρηλίων <unclear cert="high" reason="undefined">Σ</unclear>ύρου κ<unclear cert="high" reason="undefined">α</unclear>ὶ Πασβεί<unclear cert="high" reason="undefined">ο</unclear>υ <unclear cert="high" reason="undefined">τ</unclear>οῦ 

    <lb n="4"/>ἀδελφοῦ καὶ Δημητρίας καὶ Σαραπιάδος 

    <lb n="5"/>γυναικῶν <unclear cert="high" reason="undefined">ἡ</unclear>μῶν ἐξωπυλειτῶν. 

    <lb n="6"/>ἀεὶ <unclear cert="high" reason="undefined">θ</unclear>ύον<unclear cert="high" reason="undefined">τε</unclear>ς τοῖς θεοῖς διετελέ-

    <lb n="7"/>σαμεν καὶ νῦν ἐπὶ παρόντων ὑμῶν 

    <lb n="8"/>κατὰ τὰ προσταχθέντα καὶ <choice><corr>ἐσπείσαμεν</corr><sic>εσ<unclear cert="high" reason="undefined">π</unclear>ισαμεν</sic></choice> 

    <lb n="9"/><unclear cert="high" reason="undefined">κ</unclear>α<unclear cert="high" reason="undefined">ὶ</unclear> <supplied cert="high" reason="lost">τῶ</supplied>ν <unclear cert="high" reason="undefined">ἱ</unclear><supplied cert="high" reason="lost">ερείων</supplied> ἐ<unclear cert="high" reason="undefined">γευσάμεθα</unclear> <unclear cert="high" reason="undefined">καὶ</unclear> 

    <lb n="10"/><supplied cert="high" reason="lost">ἀξιοῦμεν ὑμᾶς ὑποσημειώ</supplied>-

    <lb n="11"/>σασ<unclear cert="high" reason="undefined">θ</unclear>αι ἡμῖν. διευτ<unclear cert="high" reason="undefined">υ</unclear><supplied cert="high" reason="lost">χεῖτε</supplied>. 

    <lb n="12"/><handShift new="m2"/> <expan>Αὐρήλ<ex cert="high">ιος</ex></expan> Σῦρος καὶ Πασβῆς <expan>ἐπιδεδώκ<ex cert="high">αμεν</ex></expan>. Ἰσίδωρος <expan>ἔγρα<ex cert="high">ψα</ex></expan> <expan>ὑπ<ex cert="high">ὲρ</ex></expan> <expan>αὐτ<ex cert="high">ῶν</ex></expan> <expan>ἀγρα<ex cert="high">μμάτων</ex></expan>. 

