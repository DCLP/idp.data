    <lb n="14"/><gap extent="18" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀπὸ κώμ</supplied>η<supplied cert="high" reason="lost">ς</supplied> Ἰσί<supplied cert="high" reason="lost">ου Τρύ</supplied><unclear cert="high" reason="undefined">φω</unclear><supplied cert="high" reason="lost">ν</supplied><unclear cert="high" reason="undefined">ος</unclear> τ<unclear cert="high" reason="undefined">ῆ</unclear><supplied cert="high" reason="lost">ς κάτω τοπαρχίας</supplied> 

    <lb n="15"/><supplied cert="high" reason="lost">τοῦ Ὀξυρυγχείτου νομοῦ </supplied><gap extent="6" reason="lost" unit="character"/>νη<gap extent="unknown" reason="lost" unit="character"/> 

