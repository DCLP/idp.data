<placeName cert="high" full="yes">Oxyrhynchus</placeName>
<xref evaluate="all" from="ROOT" n="p.yale;1;68" targOrder="U" to="DITTO" type="reprint from">PYale1</xref></head><div lang="grc" org="uniform" part="N" sample="complete" type="edition"><ab part="N">
    <lb n="1"/>Ἀπίων Ἀπολλωνίου τοῦ Σαραπίωνος μητρὸς <gap extent="6" reason="lost" unit="character"/><supplied cert="high" reason="lost"> καὶ ὁ ὁμοπάτριος αὐτοῦ ἀδελφὸς</supplied> 

    <lb n="2"/>Διογένης ὁ καὶ Ἄρειος μητρὸς Δημητροῦτος ἀπ᾽ <supplied cert="high" reason="lost">Ὀξυρύγχων πόλεως Ζωίλῳ τῷ καὶ </supplied><gap extent="7" reason="lost" unit="character"/> 

    <lb n="3"/>τι Γερμανοῦ μητρὸς Θαήσιος ἀπὸ τῆς αὐτῆς πόλ<supplied cert="high" reason="lost">εως χαίρειν. ὁμολογοῦμεν ἐσχηκέναι καὶ πα</supplied>-


    <lb n="4"/>ραμεμετρῆσθαι παρὰ σοῦ διὰ χειρὸς ἐξ οἴκου σου <supplied cert="high" reason="lost">ἀργυρίου Σεβαστῶν νομίσματος δραχμὰς χι</supplied>-

    <lb n="5"/>λίας ἑπτακοσίας <num value="1700"/> καὶ πυροῦ γενήματος τοῦ διελ<unclear cert="high" reason="undefined">θ</unclear><supplied cert="high" reason="lost">όντος δωδεκάτου ἔτους μέτρῳ </supplied><gap extent="9" reason="lost" unit="character"/> 

    <lb n="6"/>κω<gap extent="1" reason="illegible" unit="character"/> Ἀντᾶτος Μεγίστου ἀπὸ Ἐντείεως ἀρτάβ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ς εἴκοσι τέσσαρας <num value="24"/> κεφαλαίου, αἷς οὐδὲν</supplied> 

    <lb n="7"/>προσῆκται, καὶ <choice><corr>ταξόμεθά</corr><sic>δαξομεθα</sic></choice> σοι μόνου τοῦ ἀργ<supplied cert="high" reason="lost">υρίου κατὰ μῆνα ἕκαστον ἀπὸ τοῦ ὄντος</supplied> 

    <lb n="8"/>μηνὸς Ἁθὺρ τοῦ ἐνεστῶτος <choice><corr>τρεισκαιδεκ<supplied cert="high" reason="lost">άτου</supplied></corr><sic>τρισκαιδεκ<supplied cert="high" reason="lost">ατου</supplied></sic></choice> <supplied cert="high" reason="lost">ἔτους τόκου δραχμὰς δεκατρεῖς <num value="13"/>, εἰς δὲ</supplied> 

    <lb n="9"/>τὴν τούτου καὶ τόκων αὐτοῦ καὶ τοῦ πυροῦ <supplied cert="high" reason="lost">διάλυσιν ἐξεῖναί σοι σπείρειν καὶ καρ</supplied>-

    <lb n="10"/>πίζεσθαι ἔτι δὲ καὶ <choice><corr>ἐκμισθοῦν</corr><sic>εκμισθοιν</sic></choice> οἷς ἐὰν αἱρῇ ἐ<unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ὶ χρόνον ἔτη τέσσαρα <num value="4"/> ἀπὸ τοῦ ἐνεστῶτος</supplied> 

    <lb n="11"/><choice><corr>τρεισκαιδεκάτου</corr><sic>τρισκαιδεκατου</sic></choice> <supplied cert="high" reason="omitted">ἔτους</supplied> τὰς ὑπαρχούσας τῷ ἑνὶ ἡμῶν <supplied cert="high" reason="lost">Διογένει τῷ καὶ Ἀρείῳ περὶ τὴν αὐτὴν Ἐν</supplied>-

    <lb n="12"/>τεῖ<hi rend="diaeresis">ι</hi>ν ἐκ τοῦ Σποκέως κλήρου <unclear cert="high" reason="undefined">ἀ</unclear>ρούρας δ<supplied cert="high" reason="lost">ε</supplied>κ<supplied cert="high" reason="lost">απέντε <num value="15"/> καὶ τῷ Ἀπίωνι περὶ τὴν αὐτὴν κώμην</supplied> 

    <lb n="13"/>ἐκ τοῦ Φιλητίωνος κλήρου ἀρούρας πέντε <supplied cert="high" reason="lost">ἥμισυ <num value="5  1/2 "/> σπείροντι τὰς ἐπὶ τὸ αὐτὸ ἀρούρας</supplied> 

    <lb n="14"/>εἴκοσι ἥ<unclear cert="high" reason="undefined">μι</unclear><supplied cert="high" reason="lost">σ</supplied><unclear cert="high" reason="undefined">υ</unclear> <num value="20  1/2 "/> <supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ἷ</unclear><supplied cert="high" reason="lost">ς ἐὰ</supplied>ν αἱρῇ χωρὶς ἰσάτεως καὶ <supplied cert="high" reason="lost">ὀχομενίου, ἐκφορίου τῶν ὅλων ἀρουρῶν</supplied> 

    <lb n="15"/>ἐπὶ τὴν τετραετίαν κατ᾽ ἔτος πυ<unclear cert="high" reason="undefined">ρο</unclear>ῦ μέτρῳ τ<unclear cert="high" reason="undefined">ετ</unclear><supplied cert="high" reason="lost">ραχοινίκῳ ἀρταβῶν </supplied><gap extent="16" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἑ</supplied>-

    <lb n="16"/>πτά <num value="7"/>, ἀφ᾽ ὧν <choice><corr>ἐκκρούσεις</corr><sic>εκκρουσις</sic></choice> σεαυτ<unclear cert="high" reason="undefined">ῷ</unclear> τῷ μὲν <num value="13">ι<unclear cert="high" reason="undefined">γ</unclear></num> <supplied cert="high" reason="lost"><expan><ex cert="high">ἔτει</ex></expan> </supplied><gap extent="38" reason="lost" unit="character"/> 

    <lb n="17"/>μέν<supplied cert="high" reason="lost">α</supplied>ς ἀρτά<unclear cert="high" reason="undefined">β</unclear><supplied cert="high" reason="lost">ας εἴ</supplied><unclear cert="high" reason="undefined">κ</unclear>οσι τέσσαρ<supplied cert="high" reason="lost">ας <num value="24"/> </supplied><gap extent="3" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="33" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀρτά</supplied>-

    <lb n="18"/>βα<supplied cert="high" reason="lost">ς τ</supplied>ριάκοντα ἓξ <num value="36"/> <supplied cert="high" reason="lost">τῆς</supplied> <unclear cert="high" reason="undefined">ἐ</unclear>σομένης <gap extent="44" reason="lost" unit="character"/> 

    <lb n="19"/>τιμῆς, ἀφ᾽ ἧς εἰς μὲν <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ό</supplied>κου <gap extent="3" reason="illegible" unit="character"/><gap extent="45" reason="lost" unit="character"/> 

    <lb n="20"/>Ἁθὺρ <gap extent="3" reason="illegible" unit="character"/> Παῦνι <gap extent="3" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="43" reason="lost" unit="character"/><supplied cert="high" reason="lost"> κεφα</supplied>-

    <lb n="21"/>λαίου, ἀπὸ δὲ τοῦ ἑ<supplied cert="high" reason="lost">ξῆς <num value="1">α</num></supplied><num value="4">δ</num> <expan><ex cert="high">ἔτους</ex></expan> ἐπὶ τὰ λ<supplied cert="high" reason="lost">ο</supplied>ιπ<unclear cert="high" reason="undefined">ὰ</unclear> <supplied cert="high" reason="lost">ἔτη τρία <num value="3"/> </supplied><gap extent="18" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐκκρούσεις σεαυ</supplied>-

    <lb n="22"/>τῷ <supplied cert="high" reason="lost">π</supplied>υροῦ ἀρτάβ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ς</supplied> <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ε</supplied>ντήκοντ<supplied cert="high" reason="lost">α</supplied> <num value="50"/>, τῆ<unclear cert="high" reason="undefined">ς</unclear> ἐσ<supplied cert="high" reason="lost">ομένης τιμῆς </supplied><gap extent="26" reason="lost" unit="character"/> 

    <lb n="23"/>κατ᾽ ἔτος εἰς λόγον τόκου καὶ κεφαλαίου. τὸ δ<supplied cert="high" reason="lost">ὲ περιλειπόμενον ἐκφόριον ἀποδώσεις</supplied> 

    <lb n="24"/>κατ᾽ ἔτος μηνὶ Παῦνι ἐφ᾽ ἅλω τῆς Ἐντείεως μέτρ<supplied cert="high" reason="lost">ῳ τετραχοινίκῳ τῆς μετρήσεως γινομέ</supplied>-

    <lb n="25"/>νης <hi rend="diaeresis">ὑ</hi>πὸ τῶν παρ᾽ ἡμῶν. ἐὰν δὲ ἐν τῷ τετραετί<supplied cert="high" reason="lost">ας χρόνῳ </supplied><gap extent="26" reason="lost" unit="character"/> 

    <lb n="26"/><gap extent="4" reason="illegible" unit="character"/><gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ι</unclear> τόκοι, ἕξεις τ<unclear cert="high" reason="undefined">ὴν</unclear> τῶν αὐτῶν ἀρουρ<supplied cert="high" reason="lost">ῶν </supplied><gap extent="28" reason="lost" unit="character"/><supplied cert="high" reason="lost"> συγ</supplied>-

    <lb n="27"/>κομιδὴν ἐπὶ τοῖς προκειμένοις πᾶσι ἕως οὗ πληρω<supplied cert="high" reason="lost">θῇς. καὶ τελέσομεν ἡμεῖς τὰ ὑπὲρ τοῦ τῶν</supplied> 

    <lb n="28"/>αὐτῶν ἀρουρῶν ἡμίσους <num value=" 1/2 "/> κατ᾽ ἔτος δημόσια πάντ<supplied cert="high" reason="lost">α, περὶ ω῟ν κατὰ πάντα τρόπον ἀπαρενόχλη</supplied>-

    <lb n="29"/>τόν σε καὶ ἀνείσπρακτον παρεξόμεθα, ἐπὶ δὲ π<unclear cert="high" reason="undefined">α</unclear><gap extent="32" reason="lost" unit="character"/> 

    <lb n="30"/>λογίας καὶ βεβαιώσομέν σοι τὴν δηλουμένη<supplied cert="high" reason="lost">ν γῆν πάσῃ βεβαιώσει, τῆς πράξεώς σοι γι</supplied>-

    <lb n="31"/>νομένης παρά τε ἡμῶν ἀλληλεγγύων ὄντω<supplied cert="high" reason="lost">ν εἰς ἔκτισιν καὶ ἐξ οὗ ἐὰν ἡμῶν αἱρῇ καὶ</supplied> 

    <lb n="32"/>ἐκ τῶν προκειμένων ἀρουρῶν καὶ ἐκ τῶν ἄλ<supplied cert="high" reason="lost">λων τῶν ὑπαρχόντων ἡμῖν πάντων καθάπερ</supplied> 

    <lb n="33"/>ἐκ δίκης. κύριον τὸ χειρόγραφον δισσὸν γραφὲν π<supplied cert="high" reason="lost">ανταχῇ ἐπιφερόμενον καὶ παντὶ τῷ ἐπι</supplied>-

    <lb n="34"/>φέροντι. <expan><ex cert="high">ἔτους</ex></expan> <num value="13">ιγ</num> Αὐτ<supplied cert="high" reason="lost">ο</supplied>κρατόρων <supplied cert="high" reason="lost">Καισά</supplied>ρων Λουκίου Σε<supplied cert="high" reason="lost">πτιμίου Σεουήρου Εὐσεβοῦς Περτίνακος Ἀραβικοῦ</supplied> 

    <lb n="35"/>Ἀδιαβηνικοῦ Παρθικοῦ με<unclear cert="high" reason="undefined">γίστου</unclear> καὶ Μάρκου Αὐρηλί<supplied cert="high" reason="lost">ου Ἀντωνίνου Εὐσεβοῦς Σεβαστῶν καὶ Πουβλίου</supplied> 

    <lb n="36"/>Σεπτιμίου Γέτα <choice><corr>Καίσαρο<supplied cert="high" reason="omitted">ς</supplied></corr><sic>καισαρο</sic></choice> Σεβαστοῦ, Ἁθὺρ <num value="20">κ</num>. <handShift new="m2"/> Ἀπίων <supplied cert="high" reason="lost">Ἀπολλωνίου καὶ ὁ ἀδελφὸς Διογένης</supplied> 

    <lb n="37"/>ὁ καὶ Ἄριος <choice><corr>ὀφείλομεν</corr><sic>οφιλομεν</sic></choice> τῷ Ζωίλῳ τὰς <supplied cert="high" reason="lost">δραχμὰς χιλίας ἑπτακοσίας <num value="1700"/> καὶ πυροῦ</supplied> 

    <lb n="38"/>ἀρτάβας εἴκοσι τέσσαρας <num value="24"/> καὶ ταξόμε<supplied cert="high" reason="lost">θα μόνου τοῦ ἀργυρίου τόκου κατὰ μῆ</supplied>-

    <lb n="39"/>να ἕκαστον δραχμὰς <choice><corr>δεκατρεῖς</corr><sic>δεκατρις</sic></choice> <num value="13"/>, εἰς δ<unclear cert="high" reason="undefined">ι</unclear><supplied cert="high" reason="lost">άλυσιν δὲ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="40"/>την τῶν <gap extent="12" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">σι</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="41"/>προδ<gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

