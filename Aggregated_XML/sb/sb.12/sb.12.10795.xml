    <lb n="9"/>ἐπακολουθοῦντας ἄρ-

    <lb n="10"/>νας, ἐρίφους, ἃ νεμήσον-

    <lb n="11"/>ται περὶ Π<unclear cert="high" reason="undefined">έλα</unclear> <unclear cert="high" reason="undefined">τῆς</unclear> <unclear cert="high" reason="undefined">πρὸς</unclear> <unclear cert="high" reason="undefined">λίβα</unclear> 

    <lb n="12"/><unclear cert="high" reason="undefined">τ</unclear>οπαρχίας καὶ δι᾽ ὅλου τ<unclear cert="high" reason="undefined">οῦ</unclear> 

    <lb n="13"/>νομοῦ διᾶ νομέως Νι-

    <lb n="14"/>κοστ<unclear cert="high" reason="undefined">ράτο</unclear>υ τοῦ <gap extent="3" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">εύ</unclear>-

    <lb n="15"/><unclear cert="high" reason="undefined">ρ</unclear>ιος, ἀφηλίκου, ἀνα-

    <lb n="16"/><unclear cert="high" reason="undefined">γρα</unclear>φομένου πε<unclear cert="high" reason="undefined">ρ</unclear>ὶ τὴν 

    <lb n="17"/><unclear cert="high" reason="undefined">αὐτὴν</unclear> κώμ<unclear cert="high" reason="undefined">η</unclear>ν· ὧν καὶ τα-

    <lb n="18"/><unclear cert="high" reason="undefined">ξόμεθα</unclear> <unclear cert="high" reason="undefined">τὸ</unclear> <choice><corr>καθῆκον</corr><sic>καθηκων</sic></choice> <expan><unclear cert="high" reason="undefined">τέ</unclear><ex cert="high">λος</ex></expan>. <expan>εὐ<ex cert="high">τύ</ex>χ<ex cert="high">ει</ex></expan>. 

    <lb n="19"/><expan>Σαρ<ex cert="high">απίων</ex></expan> <expan><unclear cert="high" reason="undefined">τοπ</unclear><ex cert="high">άρχης</ex></expan> <expan>σεσ<ex cert="high">ημείωμαι</ex></expan> 

    <lb n="20"/><expan>πρό<ex cert="high">βατα</ex></expan> εἴκοσι δύο, 

    <lb n="21"/><expan><ex cert="high">αἶ</ex>γ<ex cert="high">α</ex></expan> μίαν, <expan><ex cert="high">γίνονται</ex></expan> <expan>πρό<ex cert="high">βατα</ex></expan> <num value="22">κ<unclear cert="high" reason="undefined">β</unclear></num>, 

