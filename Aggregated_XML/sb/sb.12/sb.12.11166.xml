    <lb n="8"/><expan><ex cert="high">πυροῦ</ex></expan> γενήματος <num value="11">ια</num> <expan><ex cert="high">ἔτους</ex></expan> <expan>Ἀντωνείν<ex cert="high">ου</ex></expan> 

    <lb n="9"/>Καίσαρος τοῦ κυρίου Ἡραΐδι 

    <lb n="10"/>Διονυσίου ἀρτάβας ἕνδε-

    <lb n="11"/>κα, χοίνικας ὀκτώ, 

    <lb n="12"/><expan><ex cert="high">γίνονται</ex></expan> <expan><ex cert="high">ἀρτάβαι</ex></expan> <num value="11">ια</num>, <expan>χ<ex cert="high">οίνικες</ex></expan> <num value="8">η</num>. <expan><ex cert="high">ἔτους</ex></expan> <num value="11">ια</num> <expan>Ἀντωνείν<ex cert="high">ου</ex></expan> 

