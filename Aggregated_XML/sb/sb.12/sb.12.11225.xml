    <lb n="1"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ρου καὶ ἐπὶ τούτοις <choice><corr>καὶ ἐγὼ</corr><sic>καγω</sic></choice> ὁ Παῦλος ἐπερωτηθεὶς εἰς τὸ κατ᾽ ἐμὲ τρίτον μέρος ὡμολόγ<unclear cert="high" reason="undefined">ησα</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ι</unclear>κῷ πύργῳ συμπόσιον τὸ ἐπάνω τῆς ἐξέδρας καὶ τὰς δύο <num value="2"/> καμάρας τὰς <hi rend="diaeresis">ὑ</hi>ποκάτω τῆς αὐτῆ<unclear cert="high" reason="undefined">ς</unclear> <supplied cert="high" reason="lost">ἐξέδρας </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/><gap extent="unknown" reason="lost" unit="character"/> <unclear cert="high" reason="undefined">ὑ</unclear>μῖν μεμενηκότω<unclear cert="high" reason="undefined">ν</unclear> τοῦ τε αἰθρίου καὶ φρέατος καὶ τοῦ ξυστοῦ τοῦ καταγαίου καὶ εἰσόδων <supplied cert="high" reason="lost">καὶ ἐξόδων </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> οἰκίας αὐλῆς τῆς ῥ<supplied cert="high" reason="lost">ύμ</supplied>ης διειργούσης· καὶ εἰ <choice><corr><hi rend="diaeresis">ὑ</hi>πορραφή</corr><sic>υποραφη</sic></choice> <choice><corr>τι<supplied cert="high" reason="omitted">ς</supplied></corr><sic>τι</sic></choice> γένηται ἐν τῇ οἰκίᾳ καὶ τῇ αὐλῇ ἐπὶ τῷ ἡ<supplied cert="high" reason="lost">μᾶς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> κε</supplied><unclear cert="high" reason="undefined">κλ</unclear>ήρωται τό<unclear cert="high" reason="undefined">π</unclear>ων σὺν <supplied cert="high" reason="lost">τέ</supplied>κνοις καὶ ἐκγόνοις καὶ τοῖς μεταλημψομένοις καὶ ἐξουσίαν ἔχειν χρᾶσθα<unclear cert="high" reason="undefined">ι</unclear> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> δεσπο</supplied>τῶν ἡμῶν Θεοδοσί<supplied cert="high" reason="lost">ο</supplied>υ Οὐαλεντινιανοῦ τῶν αἰωνίων Αὐγούστων ἐμμένειν <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ταύ</supplied>τῃ τῇ <choice><corr>διαιρέσει</corr><sic>διαιρεσι</sic></choice> δοξά<supplied cert="high" reason="lost">ν</supplied>των ἐπὶ τῷ <choice><corr>τὸν</corr><sic>τομ</sic></choice> παραβαίνοντα παρασχεῖν <hi rend="diaeresis">ὑ</hi>πὲρ λόγου π<supplied cert="high" reason="lost">ροστίμου </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐγ</supplied>ράφη τρισσὴ πρὸς τὸ ἑ<unclear cert="high" reason="undefined">κ</unclear>άτερον μέρος ἔχε<unclear cert="high" reason="undefined">ι</unclear>ν ἀνὰ μοναχὸν πρὸς <choice><corr>ἀσφάλειαν</corr><sic>ασφαλιαν</sic></choice> καὶ ἐπερω<supplied cert="high" reason="lost">τηθέντες ὡμολόγησαν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/><handShift new="m2"/> <gap extent="unknown" reason="lost" unit="character"/><choice><corr><supplied cert="high" reason="lost">διαί</supplied>ρεσις</corr><sic><supplied cert="high" reason="lost">δι</supplied>ερισις</sic></choice> καὶ <choice><corr>ἐκλη<supplied cert="high" reason="omitted">ρώ</supplied>θην</corr><sic>εκληθην</sic></choice> τοὺς ἐπ᾽ ἐμοὶ προδηλωθέντας <supplied cert="high" reason="lost">τόπους </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐπερ</supplied>ωτηθεὶς <choice><corr>ὡμολόγησ<supplied cert="high" reason="omitted">α</supplied></corr><sic>ωμολογης</sic></choice>. <choice><corr>Θε<supplied cert="high" reason="omitted">ό</supplied>δωρος</corr><sic>θεδ<unclear cert="high" reason="undefined">ωρ</unclear>ος</sic></choice> διάκονος καθολικῆς ἐκκλησίας <hi rend="diaeresis">υ</hi>ἱὸς Βη<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/><gap extent="unknown" reason="lost" unit="character"/> καὶ σ<unclear cert="high" reason="undefined">υ</unclear>φ<gap extent="4" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> <choice><corr>πά<supplied cert="high" reason="omitted">ν</supplied>τα</corr><sic>πατα</sic></choice> τὰ προ<supplied cert="high" reason="lost">γεγρ</supplied>αμμένα ὡς π<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">όκ</supplied>ιται καὶ ἐπερωτηθεὶς <supplied cert="high" reason="lost">ὡμολόγησα </supplied><gap extent="unknown" reason="lost" unit="character"/> 

