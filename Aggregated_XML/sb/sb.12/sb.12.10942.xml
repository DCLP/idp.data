    <lb n="2"/>ἐκφο<unclear reason="undefined" cert="high">ρ</unclear>ί<unclear reason="undefined" cert="high">ου</unclear> <supplied reason="lost" cert="high">τ</supplied>ῆς κριθῇ καὶ <choice><corr>ἀράκῳ</corr><sic>αρακου</sic></choice> ἀνὰ πυροῦ ἀρτάβας δύο <num value="2"/>, 

    <lb n="3"/>πάσας δ<unclear reason="undefined" cert="high">ὲ</unclear> <choice><corr>ἐκ</corr><sic>εγ</sic></choice> <choice><corr>γεωμετρίας</corr><sic>γεομετριας</sic></choice> ἐξ ὀρθογωνίου· τὴν δὲ διώ-

    <lb n="4"/>ρυγα ξυλαμήσει <choice><corr>χόρτῳ</corr><sic>χορτου</sic></choice> ἐφ᾽ <choice><corr>ἡμισείᾳ</corr><sic>ημισεα</sic></choice> <num value=" 1/2 "/> ὥστ᾽ ἔχειν ἑκάτερον 

    <lb n="5"/>ἐξ ἴσου τὸ ἥμισυ <num value=" 1/2 "/>· ὁ δὲ Δημήτριος ἕξει τὸ αὐτοῦ ἥμισυ <num value=" 1/2 "/> 

    <lb n="6"/>κ<unclear reason="undefined" cert="high">ε</unclear>κ<unclear reason="undefined" cert="high">ομμ</unclear>ένον ὑπὸ τοῦ Θοώνιος τοῖς ἑαυτοῦ <expan>ἀνηλώμασι<ex cert="high">ν</ex></expan>· 

    <lb n="7"/>τὰς δὲ πρότερον Νεοπτολέμου ἀρούρας ἓξ <num value="6"/> χερσοκο-

    <lb n="8"/>πήσει ὁ <unclear reason="undefined" cert="high">με</unclear>μισθωμένος ἐφ᾽ ᾧ ὁ Δημήτριος παρέξεται 

    <lb n="9"/>αὐτῷ μετὰ τὴν κατασπορὰν ταυρικὸν ζεῦγος μέχρι 

    <lb n="10"/>τοῦ χε<supplied reason="lost" cert="high">ρσ</supplied><unclear reason="undefined" cert="high">ο</unclear>κοπηθῆναι ταύτας χωρὶς μισθοῦ.  ἀκίνδυ-

    <lb n="11"/>να τὰ <unclear reason="undefined" cert="high">ἐ</unclear><supplied reason="lost" cert="high">κφ</supplied>όρια παντὸς κινδύνου. <choice><corr>ὁμολογε<supplied reason="omitted" cert="high">ῖ</supplied></corr><sic>ομολογε</sic></choice> δὲ ὁ Δημή-

    <lb n="12"/>τρι<unclear reason="undefined" cert="high">ο</unclear>ς ἔχ<supplied reason="lost" cert="high">ε</supplied>ιν παρὰ τοῦ <subst><add place="inline">Θοώνιος</add><del rend="corrected" status="unremarkable">θεωνος</del></subst> <choice><corr>τιμὴν</corr><sic>τειμην</sic></choice> πυροῦ ἀρταβῶν 

    <lb n="13"/>ὀκτὼ <num value="8"/> ε<supplied reason="lost" cert="high">ἰ</supplied>ς λόγον κυπηροτομίας, ἃς παραδέξεται αὐ-

    <lb n="14"/>τῷ ἀπὸ τῶν ἐκφορίων. δώσει δὲ ὁ Θοώνιος τῶι 

    <lb n="15"/>Δημη<supplied reason="lost" cert="high">τ</supplied><unclear reason="undefined" cert="high">ρ</unclear>ίωι ὁπόταν αἱρῆται ὁ Δημήτριος ἐν τῷ <expan>Παχὼ<ex cert="high">ν</ex></expan> 

    <lb n="16"/><choice><corr>ἑστιατ<supplied reason="lost" cert="high">ορ</supplied>ίαν</corr><sic>ιστιατ<supplied reason="lost" cert="high">ορ</supplied><unclear reason="undefined" cert="high">ια</unclear>ν</sic></choice> εἰς Παγκῦ<unclear reason="undefined" cert="high">λ</unclear>ιν τὴν μικράν. ἐὰν δὲ πρα-

    <lb n="17"/>χθῆι ὁ <unclear reason="undefined" cert="high">μ</unclear>εμισθωμέ<supplied reason="lost" cert="high">νο</supplied><unclear reason="undefined" cert="high">ς</unclear> εἰς τὸ δημόσιον ἢ <supplied reason="lost" cert="high">ε</supplied><unclear reason="undefined" cert="high">ἰ</unclear>ς ἄλλο τι 

    <lb n="18"/>κ<unclear reason="undefined" cert="high">αθ</unclear>᾽ ὁ<unclear reason="undefined" cert="high">ντιν</unclear>οῦν τ<unclear reason="undefined" cert="high">ρόπον</unclear> <supplied reason="lost" cert="high">ὑπ</supplied>ὲρ τοῦ Δημητρίο<unclear reason="undefined" cert="high">υ</unclear> <supplied reason="lost" cert="high">ἢ τῆ</supplied>ς γῆ<unclear reason="undefined" cert="high">ς</unclear> 

    <lb n="19"/>ὑπολο<unclear reason="undefined" cert="high">γ</unclear>είτω ἀπὸ τ<supplied reason="lost" cert="high">ῶν</supplied> <unclear reason="undefined" cert="high">ἐκ</unclear>φορίων. τῶν δὲ <unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">α</supplied><unclear reason="undefined" cert="high">ρ</unclear>πῶν 

    <lb n="20"/>πάν<unclear reason="undefined" cert="high">τω</unclear><supplied reason="lost" cert="high">ν κ</supplied><unclear reason="undefined" cert="high">υρι</unclear>ευέτω ὁ Δημήτριος καὶ οἱ παρ᾽ αὐτοῦ 

    <lb n="21"/>ἕως τὰ <unclear reason="undefined" cert="high">αὐ</unclear>τοῦ κομίσηται καὶ βεβαιούτω αὐτῷ τὴν 

    <lb n="22"/>μίσ<unclear reason="undefined" cert="high">θ</unclear><supplied reason="lost" cert="high">ωσι</supplied><unclear reason="undefined" cert="high">ν</unclear> πάσῃ βεβαιώσει· βεβαιουμένης δ᾽ αὐτῆς 

    <lb n="23"/>ἀποδ<supplied reason="lost" cert="high">ότω</supplied> τῶι Δημητρίῳ τὰ ἐκφόρια ἐν τῷ Παῦνι <unclear reason="undefined" cert="high">εἰ</unclear>ς 

    <lb n="24"/>τὸ <choice><corr>εἰσ<supplied reason="lost" cert="high">ιὸν</supplied></corr><sic>ισ<supplied reason="lost" cert="high">ιον</supplied></sic></choice> ἔτος ἐπὶ τῶν περὶ τὸ Δείλου ἐποίκιον 

    <lb n="25"/>ἅλ<unclear reason="undefined" cert="high">ων</unclear> ὸν νέον καθαρὸν <choice><corr>ἄδολον</corr><sic>αδαλον</sic></choice> ἄκριθον μέτρῳ 

    <lb n="26"/>τετρ<unclear reason="undefined" cert="high">αχο</unclear>ινίκῳ τῷ τοῦ Θοώνιος ἢ ἀποτεισάτω 

    <lb n="27"/><choice><corr>τιμὴν</corr><sic>τειμ<supplied reason="lost" cert="high">ην</supplied></sic></choice> <choice><corr>ἑκάστ<supplied reason="omitted" cert="high">ης ἀρτ</supplied>άβης</corr><sic>εκασταβης</sic></choice> ἧς ἐὰν <choice><corr>μὴ</corr><sic>πη</sic></choice> <unclear reason="undefined" cert="high">ἀ</unclear>ποδῷ χαλκοῦ <expan>δραχμὰ<ex cert="high">ς</ex></expan> 

    <lb n="28"/><choice><corr>τετρακισχιλίας</corr><sic>τετρ<supplied reason="lost" cert="high">α</supplied><unclear reason="undefined" cert="high">κ</unclear>ισχειλιας</sic></choice> <num value="4000"/>· ἧς δ᾽ ἐὰν καταλίπῃ εἰς ἄσπορον 

    <lb n="29"/>τὸ ἐκ<unclear reason="undefined" cert="high">φό</unclear>ριον διπλοῦν· τοῦ δ᾽ ἐνκαταλιπεῖν τὴν <expan>γεωργία<ex cert="high">ν</ex></expan> 

    <lb n="30"/>χωρὶ<supplied reason="lost" cert="high">ς τ</supplied>ῶν προκειμένων <choice><corr>ἐπίτιμον</corr><sic>επιτειμον</sic></choice> ἀργυρίου <expan>δραχ<ex cert="high">μὰς</ex></expan> 

    <lb n="31"/>διακοσίας <num value="200"/> καὶ εἰς τὸ δημόσιον τὰς ἴσας. ἡ δὲ πρᾶξις <expan>ἔστ<ex cert="high">ω</ex></expan> 

    <lb n="32"/>τῷ Δη<unclear reason="undefined" cert="high">μη</unclear>τρίῳ ἐκ τε τοῦ μεμισθωμένου καὶ ἐκ τῶν 

    <lb n="33"/>ὑπαρχ<supplied reason="lost" cert="high">ό</supplied>ντων αὐτοῦ πάντων καθάπερ <choice><corr>ἐκ</corr><sic>εγ</sic></choice> δίκης. τὰς 

