    <lb n="1"/><supplied cert="low" reason="lost">Δεῖος</supplied><supplied cert="high" reason="lost"> ὁ συν</supplied>εσταμένος ὑπὸ Κλ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">υδί</supplied>-

