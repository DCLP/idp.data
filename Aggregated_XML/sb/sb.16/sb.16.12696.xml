    <lb n="1"/><expan>ἀντίγρα<ex cert="high">φον</ex></expan> ἐπιστάλματος· <gap extent="11" reason="lost" unit="character"/> <space dim="horizontal" extent="unknown" unit="character"/> 

    <lb n="2"/>Ἀπολινάριος <expan>στ<unclear cert="high" reason="undefined">ρ</unclear>[α<ex cert="high">τηγὸς</ex></expan> Ἀρσινοίτου] <unclear cert="high" reason="undefined">Ἡρα</unclear>κλείδου μερίδος Σαραπίωνι βασιλικῷ <expan>γρα<ex cert="high">μματεῖ</ex></expan> 

    <lb n="3"/>τῷ <unclear cert="high" reason="undefined">φι</unclear><supplied cert="high" reason="lost">λτάτῳ πλεῖστα</supplied> χαίρειν. 

    <lb n="4"/>τοῦ ἐπισταλέντος <unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">οι βιβλιδίου ὑπ</supplied><unclear cert="high" reason="undefined">ὸ</unclear> Αἰλίου Νουμισσιανοῦ <expan>στρα<ex cert="high">τηγοῦ</ex></expan> Θεμίστου καὶ <expan>Πολέ<ex cert="high">μωνος</ex></expan> 

