    <lb n="5"/><supplied cert="high" reason="lost">μ</supplied><unclear cert="high" reason="undefined">νήμη</unclear>ς <unclear cert="high" reason="undefined">Ε</unclear><supplied cert="high" reason="lost">ὐ</supplied><unclear cert="high" reason="undefined">λ</unclear>ο<supplied cert="high" reason="lost">γ</supplied>ίου γεν<unclear cert="high" reason="undefined">α</unclear>μένου παλατίνου γε<supplied cert="high" reason="lost">ου</supplied><unclear cert="high" reason="undefined">χ</unclear>οῦσιν 

    <lb n="6"/><supplied cert="high" reason="lost">ἐντα</supplied><unclear cert="high" reason="undefined">ῦ</unclear>θα τῇ λαμπρᾷ καὶ λαμπροτάτῃ Ὀξυρ<unclear cert="high" reason="undefined">υ</unclear>γ<unclear cert="high" reason="undefined">χ</unclear>ιτῶ<unclear cert="high" reason="undefined">ν</unclear> 

    <lb n="7"/><supplied cert="high" reason="lost">πόλ</supplied>ει <unclear cert="high" reason="undefined">Α</unclear><supplied cert="high" reason="lost">ὐ</supplied><unclear cert="high" reason="undefined">ρ</unclear>ήλιος Ἄπα Νάκιος υἱὸς Φιλοξέν<unclear cert="high" reason="undefined">ου</unclear> ἀπὸ 

    <lb n="8"/><supplied cert="high" reason="lost">τῆ</supplied><unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">λ</unclear><supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">μπρ</unclear>ᾶς <supplied cert="high" reason="lost">καὶ</supplied> λαμπροτάτης Ὀξυρυγχιτῶ<supplied cert="high" reason="lost">ν</supplied> 

    <lb n="9"/><supplied cert="high" reason="lost">πόλ</supplied><unclear cert="high" reason="undefined">εως</unclear> <unclear cert="high" reason="undefined">χ</unclear><supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">ί</unclear>ρε<supplied cert="high" reason="lost">ι</supplied>ν. ἑκουσίως ἐπιδέχομαι μισθώ-

    <lb n="10"/><supplied cert="high" reason="lost">σασ</supplied>θα<supplied cert="high" reason="lost">ι ἀ</supplied>πὸ νεομηνίας τοῦ Θὼθ μηνὸς τοῦ ἐνεσ-

    <lb n="11"/><supplied cert="high" reason="lost">τῶ</supplied><unclear cert="high" reason="undefined">τ</unclear>ος <supplied cert="high" reason="lost">ἔ</supplied>τ<supplied cert="high" reason="lost">ου</supplied><unclear cert="high" reason="undefined">ς</unclear> <num value="177">ροζ</num>  <num value="146"><unclear cert="high" reason="undefined">ρμϛ</unclear></num> τῆς παρούσης ἐνάτης ἰνδι-

    <lb n="12"/><supplied cert="high" reason="lost">κτίο</supplied>νο<supplied cert="high" reason="lost">ς ἀ</supplied>πὸ τῶν ὑπαρχόντ<unclear cert="high" reason="undefined">ω</unclear>ν τῇ ὑμῶν λαμ-

    <lb n="13"/><supplied cert="high" reason="lost">πρό</supplied>τη<unclear cert="high" reason="undefined">τ</unclear>ι διακειμένων ἐπὶ τῆσδε τῆς πόλεως 

    <lb n="14"/><supplied cert="high" reason="lost">ἐπ᾽ ἀμφό</supplied><unclear cert="high" reason="undefined">δ</unclear><supplied cert="high" reason="lost">ο</supplied>υ <unclear cert="high" reason="undefined">Πα</unclear>μμένους Παραδίσου ὁλόκληρον 

    <lb n="15"/><supplied cert="high" reason="lost">μικρὰν</supplied> οἰ<supplied cert="high" reason="lost">κί</supplied>αν νεύουσαν ἐπὶ νότον μετὰ τῶν 

    <lb n="16"/><supplied cert="low" reason="lost">οἰκοπέδ</supplied><unclear cert="low" reason="undefined">ω</unclear>ν σὺν χρηστηρίοις καὶ δικαίοις πᾶσι καὶ 

    <lb n="17"/><supplied cert="high" reason="lost">τελέσω</supplied> ὑπὲρ ἐνοικίου ἐνιαυσίως χρυσοῦ κερά-

    <lb n="18"/><supplied cert="high" reason="lost">τια ὀκτ</supplied><unclear cert="high" reason="undefined">ώ</unclear> <num value="8"/>, ὅπερ ἐνοίκιον ἀποδώσω κατ᾽ ἔτος 

    <lb n="19"/><supplied cert="high" reason="lost">δι᾽ ἑξα</supplied><unclear cert="high" reason="undefined">μ</unclear>ήνου τ<supplied cert="high" reason="lost">ὸ</supplied> ἥμισυ, καὶ ὁπόταν <choice><corr>βουληθείη</corr><sic>βουλειθειη</sic></choice> 

    <lb n="20"/><supplied cert="high" reason="lost">ἡ ὑμ</supplied><unclear cert="high" reason="undefined">ῶν</unclear> <supplied cert="high" reason="lost">λαμπρ</supplied>ότης παραδώσω τὴν αὐτὴν 

    <lb n="21"/><supplied cert="high" reason="lost">μικρὰν οἰκία</supplied>ν μετὰ καὶ τῆς ἀτρώτου αὐτῆς 

    <lb n="22"/><supplied cert="high" reason="lost">νομῆς ἀν</supplied>υπερθέτως. κυρία ἡ μίσθωσις 

    <lb n="23"/><supplied cert="low" reason="lost">ἁπλῆ</supplied><supplied cert="high" reason="lost"> γρα</supplied>φεῖσα καὶ ἐπερωτηθεὶς ὡμολόγησα. 

    <lb n="24"/><handShift new="m2"/> <supplied cert="high" reason="lost">Αὐρήλιος</supplied> Ἄ<supplied cert="high" reason="lost">πα</supplied> Νάκιος υἱὸς Φιλοξένου <supplied cert="high" reason="lost">ὁ</supplied> προκείμενος πεποίη-

    <lb n="25"/><supplied cert="high" reason="lost">μαι τὴν μίσ</supplied>θωσιν τῆς αὐτῆς μι<supplied cert="high" reason="lost">κ</supplied><unclear cert="high" reason="undefined">ρ</unclear>ᾶς <choice><corr>οἰκίας</corr><sic>οικειας</sic></choice> μετὰ παντὸς 

    <lb n="26"/><supplied cert="high" reason="lost">αὐτῆς δ</supplied>ικαίου καὶ ἀποδώσω τὸ ἐνοίκιον καὶ συμφω-

    <lb n="27"/><supplied cert="high" reason="lost">νεῖ μοι πάν</supplied>τα τὰ ἐνγεγραμμένα ὡς πρόκιται. Αὐρήλιος Ἰωάν-

    <lb n="28"/><supplied cert="high" reason="lost">νης </supplied><gap extent="6" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ρ</unclear>ος ἀξιωθεὶς ἔγραψα ὑπὲρ αὐτοῦ παρόντος 

    <lb n="29"/><supplied cert="high" reason="lost">γράμμα</supplied><unclear cert="high" reason="undefined">τ</unclear>α <supplied cert="high" reason="lost">μ</supplied><unclear cert="high" reason="undefined">ὴ</unclear> εἰδότος. 

