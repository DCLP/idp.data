    <lb n="1"/><gap extent="unknown" reason="lost" unit="line"/> 

    <lb n="1"/><gap extent="unknown" reason="lost" unit="character"/> καὶ τῶν <supplied cert="high" reason="lost">συγκυρόντων κοινῶν καὶ ἀδιαιρέτων</supplied> 

    <lb n="2"/><supplied cert="high" reason="lost">ἐπὶ τοῦ πρὸς Ὀξυρύγχων πόλει Σ</supplied>αραπειήου <supplied cert="high" reason="lost">ἐν </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐπ᾽ ἔτη </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> καὶ μῆνας </supplied><gap extent="unknown" reason="lost" unit="character"/> 


    <lb n="3"/><supplied cert="high" reason="lost">χρωμένης ἀπὸ τῆς ἐνεστώσης</supplied> ἡμέρας τῆ<supplied cert="high" reason="lost">ς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/><gap extent="unknown" reason="lost" unit="character"/> καὶ <choice><corr>τῷ</corr><sic>το</sic></choice> μελάθ<supplied cert="high" reason="lost">ρῳ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> μὴ οὔ</supplied>-

    <lb n="5"/><supplied cert="high" reason="lost">σης τῇ δεδανεισμένῃ μηδ᾽ ἄλλω</supplied>ι μηδενὶ ἐξ<supplied cert="high" reason="lost">ουσίας ἐκβάλλειν σε οὐδὲ τοὺς παρὰ</supplied> 

    <lb n="6"/><supplied cert="high" reason="lost">σοῦ ἐκ τοῦ ἐνοικισμοῦ μέχρ</supplied>ι τοῦ τὸν χρόν<supplied cert="high" reason="lost">ον πληρωθῆναι, ἐφ᾽ οἷς καὶ βεβαιῶσαί σοι καὶ τοῖς πα</supplied>-

    <lb n="7"/><supplied cert="high" reason="lost">ρὰ σοῦ τὸν ἐνοικισμὸν πάσῃ </supplied><choice><corr><supplied cert="high" reason="lost">β</supplied>εβαίωσει</corr><sic><supplied cert="high" reason="lost">β</supplied>εβαιωσι</sic></choice> τοῦ <choice><corr>δ<supplied cert="high" reason="lost">ὲ</supplied></corr><sic>τ<supplied cert="high" reason="lost">ε</supplied></sic></choice> <supplied cert="high" reason="lost">χρόνου πληρωθέντος ἀποδώσω σοὶ τὰς τοῦ</supplied> 

    <lb n="8"/><supplied cert="high" reason="lost"><expan>ἀργ<ex cert="high">υρίου</ex></expan> <expan><ex cert="high">δραχμὰς</ex></expan> </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"><foreign lang="la">.</foreign> ἐὰν δὲ παρασυγγραφῶ ἢ</supplied> μὴ ποιῶ κα<supplied cert="high" reason="lost">θὰ γέγραπται ἐκτείσω σοι ἢ τοῖς παρὰ σοῦ</supplied> 

    <lb n="9"/><supplied cert="high" reason="lost">τοῦ μὴ μὲν βεβαιῶσαι τὸν ἐνο</supplied>ικισμὸν ὡ<supplied cert="high" reason="lost">ς πρόκειται <expan>ἀργ<ex cert="high">υρίου</ex></expan> <expan><ex cert="high">δραχμὰς</ex></expan> </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> καὶ εἰς τὸ δημόσι</supplied>-

    <lb n="10"/><supplied cert="high" reason="lost">ον τὰς ἴσας, τὸ δ᾽ ἀργύριον πα</supplied>ραχρῆμα <supplied cert="high" reason="lost">μεθ᾽ ἡμιολίας </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/><gap extent="unknown" reason="lost" unit="line"/> </ab></div><div n="meta.25058" part="N" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.25058" part="N" subtype="textDate" type="commentary"><p><date notAfter="200" notBefore="1" type="textDate">I - II</date></p></div><div n="meta.25058" part="N" subtype="principalEdition" type="bibliography"><listBibl><bibl subtype="principal" type="publication"><title level="s" type="abbreviated">SB</title><biblScope type="volume">16</biblScope><biblScope type="numbers">13041</biblScope></bibl><bibl type="Trismegistos"><biblScope type="numbers">25058</biblScope></bibl><bibl type="DDbDP"><series>0239</series>; <biblScope type="volume">16</biblScope>; <biblScope type="numbers">13041</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0239;16;13041" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.25058" part="N" subtype="corrections" type="bibliography"><head>BL-Einträge nach BL-Konkordanz</head><bibl type="BL"><biblScope type="volume">VIII</biblScope>, S. <biblScope type="pages">387/388</biblScope></bibl></div><div n="meta.25058" part="N" subtype="illustrations" type="bibliography"><p>keine</p></div><div n="meta.25058" part="N" subtype="otherPublications" type="bibliography"><head>Andere Publikation</head><bibl subtype="other" type="publication">SB VIII 9828</bibl></div><div n="meta.25058" part="N" subtype="locations" type="history"><p><placeName cert="high" type="ancientFindspot">Oxyrhynchos</placeName></p></div><div n="meta.25058" part="N" type="figure"><p><figure href="http://ipap.csad.ox.ac.uk/4DLink4/4DACTION/IPAPwebquery?vPub=SB&amp;vVol=16&amp;vNum=13041"><figDesc>Abbildung im Internet</figDesc></figure></p></div></body></text></TEI.2>