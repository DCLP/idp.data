    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/> καὶ τῶν <supplied reason="lost" cert="high">συγκυρόντων κοινῶν καὶ ἀδιαιρέτων</supplied> 

    <lb n="2"/><supplied reason="lost" cert="high">ἐπὶ τοῦ πρὸς Ὀξυρύγχων πόλει Σ</supplied>αραπειήου <supplied reason="lost" cert="high">ἐν </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἐπ᾽ ἔτη </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> καὶ μῆνας </supplied><gap reason="lost" extent="unknown" unit="character"/> 


    <lb n="3"/><supplied reason="lost" cert="high">χρωμένης ἀπὸ τῆς ἐνεστώσης</supplied> ἡμέρας τῆ<supplied reason="lost" cert="high">ς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/> καὶ <choice><corr>τῷ</corr><sic>το</sic></choice> μελάθ<supplied reason="lost" cert="high">ρῳ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> μὴ οὔ</supplied>-

    <lb n="5"/><supplied reason="lost" cert="high">σης τῇ δεδανεισμένῃ μηδ᾽ ἄλλω</supplied>ι μηδενὶ ἐξ<supplied reason="lost" cert="high">ουσίας ἐκβάλλειν σε οὐδὲ τοὺς παρὰ</supplied> 

    <lb n="6"/><supplied reason="lost" cert="high">σοῦ ἐκ τοῦ ἐνοικισμοῦ μέχρ</supplied>ι τοῦ τὸν χρόν<supplied reason="lost" cert="high">ον πληρωθῆναι, ἐφ᾽ οἷς καὶ βεβαιῶσαί σοι καὶ τοῖς πα</supplied>-

    <lb n="7"/><supplied reason="lost" cert="high">ρὰ σοῦ τὸν ἐνοικισμὸν πάσῃ </supplied><choice><corr><supplied reason="lost" cert="high">β</supplied>εβαίωσει</corr><sic><supplied reason="lost" cert="high">β</supplied>εβαιωσι</sic></choice> τοῦ <choice><corr>δ<supplied reason="lost" cert="high">ὲ</supplied></corr><sic>τ<supplied reason="lost" cert="high">ε</supplied></sic></choice> <supplied reason="lost" cert="high">χρόνου πληρωθέντος ἀποδώσω σοὶ τὰς τοῦ</supplied> 

    <lb n="8"/><supplied reason="lost" cert="high"><expan>ἀργ<ex cert="high">υρίου</ex></expan> <expan><ex cert="high">δραχμὰς</ex></expan> </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"><foreign lang="la">.</foreign> ἐὰν δὲ παρασυγγραφῶ ἢ</supplied> μὴ ποιῶ κα<supplied reason="lost" cert="high">θὰ γέγραπται ἐκτείσω σοι ἢ τοῖς παρὰ σοῦ</supplied> 

    <lb n="9"/><supplied reason="lost" cert="high">τοῦ μὴ μὲν βεβαιῶσαι τὸν ἐνο</supplied>ικισμὸν ὡ<supplied reason="lost" cert="high">ς πρόκειται <expan>ἀργ<ex cert="high">υρίου</ex></expan> <expan><ex cert="high">δραχμὰς</ex></expan> </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> καὶ εἰς τὸ δημόσι</supplied>-

    <lb n="10"/><supplied reason="lost" cert="high">ον τὰς ἴσας, τὸ δ᾽ ἀργύριον πα</supplied>ραχρῆμα <supplied reason="lost" cert="high">μεθ᾽ ἡμιολίας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

