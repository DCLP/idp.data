    <lb n="1"/><gap extent="unknown" reason="lost" unit="line"/> 

    <lb n="2"/>μη<unclear cert="high" reason="undefined">ν</unclear><supplied cert="low" reason="lost">ὶ </supplied><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>Ἰοῦστ<unclear cert="high" reason="undefined">ος</unclear> <supplied cert="high" reason="lost">στιπ</supplied><unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ου</supplied><unclear cert="high" reason="undefined">ργ</unclear><supplied cert="high" reason="lost">ὸ</supplied>ς υἱὸς Ἰωάννου 

    <lb n="4"/>Ἀβρ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">αμίου</supplied> ἀφ᾽ <expan>Ἡρ<ex cert="high">ακλέους</ex></expan> <expan>πόλε<ex cert="high">ως</ex></expan> σοὶ 

    <lb n="5"/>ἄπα Σίω<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">ι</supplied> κεφαλαιωτῇ τῶν 

    <lb n="6"/>βαφέων <supplied cert="high" reason="lost">υἱ</supplied>ῷ Καλοῦ ἀπὸ <expan>τ<ex cert="high">ῆς</ex></expan> <expan>α<unclear cert="high" reason="undefined">ὐτ</unclear><ex cert="high">ῆς</ex></expan> <expan>πόλ<ex cert="high">εως</ex></expan>. 

    <lb n="7"/>ὁμολογῶ ἑκο<supplied cert="high" reason="lost">υ</supplied>σίᾳ <unclear cert="high" reason="undefined">γ</unclear>νώμῃ καὶ 

    <lb n="8"/>οὐκ ἔκ τινος ἀνάγκης ἐ<unclear cert="high" reason="undefined">γ</unclear>γυᾶσθ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ι</supplied> <unclear cert="high" reason="undefined">κ</unclear>αὶ 

    <lb n="9"/>ἀναδέχεσθαι παρὰ σοὶ Ἰωάννη<supplied cert="high" reason="lost">ν</supplied> 

    <lb n="10"/>καὶ αὐτὸν βαφέα υἱ<supplied cert="high" reason="lost">ὸ</supplied>ν Ἡρακλείδου 

    <lb n="11"/>ἀπὸ τῆς <expan>α<unclear cert="high" reason="undefined">ὐτ</unclear><ex cert="high">ῆς</ex></expan> πόλεως ἱστάμενόν 

    <lb n="12"/><supplied cert="high" reason="lost">τε καὶ</supplied> ποιοῦντα τὴν ἀναλογίαν 

    <lb n="13"/><supplied cert="high" reason="lost">τοῦ ἔ</supplied><unclear cert="high" reason="undefined">ρ</unclear>γου ἔκ τε τοῦ κέρδους 

    <lb n="14"/><unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">αὶ</supplied> <unclear cert="high" reason="undefined">ἐ</unclear>κ τῆς <supplied cert="high" reason="lost">ὅ</supplied>περ ἀπείη ζημί<supplied cert="high" reason="lost">α</supplied>ς 

    <lb n="15"/><unclear cert="high" reason="undefined">γ</unclear>ι<supplied cert="high" reason="lost">νο</supplied>μέ<supplied cert="high" reason="lost">νης</supplied> ἐκ πάσης τῆς <expan>αὐτ<ex cert="high">οῦ</ex></expan> <expan>τ<unclear cert="high" reason="undefined">έχ</unclear>ν<ex cert="high">ης</ex></expan> 

    <lb n="16"/>κατὰ μίμησιν τῶν <expan>αὐτ<ex cert="high">οῦ</ex></expan> <choice><corr>ἑταίρων</corr><sic>ετερων</sic></choice> 

    <lb n="17"/><expan>ἀνεμποδ<ex cert="high">ίσ</ex>τ<ex cert="high">ως</ex></expan>· εἰ δὲ μὴ τοῦτο πράξῃ 

    <lb n="18"/>ὥστε <unclear cert="high" reason="undefined">μ</unclear>ε τοῦτον παρα<unclear cert="high" reason="undefined">γάγ</unclear>αι καὶ 

    <lb n="19"/>π<unclear cert="high" reason="undefined">αρ</unclear><supplied cert="high" reason="lost">α</supplied>δοῦναί <supplied cert="high" reason="lost">σ</supplied><unclear cert="high" reason="undefined">ο</unclear>ι χωρὶς λόγου 

    <lb n="20"/>ἐν <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ῇ</supplied> φυλα<unclear cert="high" reason="undefined">κ</unclear>ῇ ἔνθα αὐτὸν 

    <lb n="21"/>καὶ <unclear cert="high" reason="undefined">π</unclear>αρείληφον ἤγουν τὰς 

    <lb n="22"/>ἀπολογίας ποιήσασθαί σοι 

    <lb n="23"/>πάντων τῶν <expan>ἐπιζητουμέ<ex cert="high">νων</ex></expan> παρὰ σοὶ 

    <lb n="24"/>ὑπ<unclear cert="high" reason="undefined">ὲρ</unclear> τῆς τούτου παραγωγῆς. 

    <lb n="25"/><expan>κυρ<ex cert="high">ία</ex></expan> ἡ <expan>παρ<ex cert="high">ο</ex>ῦ<ex cert="high">σα</ex></expan> ἐγγύη καὶ <expan>ἐπερ<ex cert="high">ωτη</ex>θ<ex cert="high">εὶς</ex></expan> <expan>ὡμ<ex cert="high">ο</ex>λ<ex cert="high">όγησα</ex></expan> 

    <lb n="26"/>καὶ <unclear cert="high" reason="undefined">ἀπ</unclear>έλυσα. <g type="stauros"/> Ἰοῦστος ὁ <expan>πρ<ex cert="high">ογεγραμμένος</ex></expan> <expan>στοιχ<ex cert="high">εῖ</ex></expan> μοι 

    <lb n="27"/>ὡς <expan>πρό<ex cert="high">κειται</ex></expan>. <g type="stauros"/> Ἀνο<supplied cert="high" reason="lost">ῦπ Π</supplied><unclear cert="high" reason="undefined">έ</unclear><supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ρ</unclear>ου ἔγραψα 

    <lb n="28"/>ὑπὲρ <expan>αὐτ<ex cert="high">οῦ</ex></expan> <expan>ἀγραμμ<ex cert="high">ά</ex>τ<ex cert="high">ου</ex></expan> ὄντος <g type="stauros"/>. 

