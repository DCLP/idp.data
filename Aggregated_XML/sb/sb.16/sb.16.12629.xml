    <lb n="1"/><gap extent="9" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="37" reason="lost" unit="character"/> 

    <lb n="2"/>πρὸς τῷ Ἁδριανίῳ. συνπαρ<unclear cert="high" reason="undefined">όντων</unclear> πρ<gap extent="3" reason="illegible" unit="character"/><gap extent="20" reason="lost" unit="character"/> 

    <lb n="3"/><subst><add place="inline">Τιθο<add place="supralinear">οῦς</add></add><del rend="corrected">τιθο<del rend="erasure">η</del></del></subst> καὶ Σαραπᾶ ὑπηρετῶν καὶ Γερ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="low" reason="lost">ντίου </supplied><gap extent="15" reason="lost" unit="character"/> 

    <lb n="4"/>ὑπὲρ Ἡρακλείδου ἀπὸ τῶν αὐτόθι ο<unclear cert="high" reason="undefined">π</unclear><gap extent="20" reason="lost" unit="character"/> 

    <lb n="5"/><unclear cert="high" reason="undefined">ω</unclear>ν διὰ τῶν μηνιάρχων <unclear cert="high" reason="undefined">Νε</unclear><supplied cert="high" reason="lost">μ</supplied><unclear cert="high" reason="undefined">ε</unclear><supplied cert="low" reason="lost">σι </supplied><gap extent="20" reason="lost" unit="character"/> 

    <lb n="6"/><choice><corr>μειζόνων</corr><sic>μιζονων</sic></choice> καὶ <unclear cert="high" reason="undefined">Γ</unclear>ερ<unclear cert="high" reason="undefined">μ</unclear><gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="23" reason="lost" unit="character"/> 

    <lb n="7"/><gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="illegible" unit="character"/> συνειδοῦσα <unclear cert="high" reason="undefined">τ</unclear><gap extent="24" reason="lost" unit="character"/> 

    <lb n="8"/>μενου οἱ ἀντίδικοι. ἡ ἀρετὴ τοῦ κυρίου μ<supplied cert="low" reason="lost">ου καθολικοῦ ἐκώλυσεν</supplied> 

    <lb n="9"/>τὴν κατ᾽ αὐτοῦ καινοτομίαν. ἀνα<unclear cert="high" reason="undefined">γν</unclear><supplied cert="high" reason="lost">ώσομαι τὴν ἀπόφασιν. </supplied><gap extent="4" reason="lost" unit="character"/> 

    <lb n="10"/><expan>ῥ<ex cert="high">ήτωρ</ex></expan> <expan>εἶ<ex cert="high">πεν</ex></expan>· παραγράφομαι. <g type="slanting-stroke"/> ὁ λογιστὴς <expan>εἶ<ex cert="high">πεν</ex></expan>· ἀ<supplied cert="low" reason="lost">νάγνωθι ὃ ἀπεφήνατο ὁ</supplied> 

    <lb n="11"/>διασημότατος καθολικός. καὶ ἀνεγνώσθη ἐπὶ ἀποφάσ<unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">ως οὕτως</supplied> 

    <lb n="12"/>ἐχούσης· Ἀντωνῖνος ὁ διασημότατος καθολικὸς <expan>εἶ<ex cert="high">πεν</ex></expan>· ὁ λ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">γιστὴς</supplied> 

    <lb n="13"/>προνοήσεται μηδὲν καινοτομηθῆναι κατὰ τοῦ ὑπ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">τεταγ</supplied>-

    <lb n="14"/><supplied cert="high" reason="lost">μ</supplied>ένου <foreign lang="la">?.</foreign> μετὰ τὴν ἀνάγνωσιν Μακρόβιος <expan>ῥ<ex cert="high">ήτωρ</ex></expan> <expan>εἶ<ex cert="high">πεν</ex></expan>· συν<gap extent="1" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τῇ</supplied> 

    <lb n="15"/>παραγραφῇ· τὸν <add place="supralinear"><gap extent="4" reason="illegible" unit="character"/></add> <unclear cert="high" reason="undefined">τρ</unclear>όπον τῆς παραγραφῆς προτίθημι. θ<supplied cert="low" reason="lost">έλω</supplied> 

    <lb n="16"/>ὡς οὐδὲ λόγου μεταδοίης τῷ ἀντιδίκῳ. ἐγὼ δὲ προς<gap extent="4" reason="lost" unit="character"/> 

    <lb n="17"/><supplied cert="low" reason="lost">των</supplied> τῆς φιλοπραγμοσύνης αὐτοῦ τοῖς ἤδη κεκριμέ<supplied cert="high" reason="lost">νοις</supplied> 

    <lb n="18"/><gap extent="3" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ιου</unclear><gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="8" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ου</unclear><gap extent="2" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">λους</unclear><gap extent="2" reason="illegible" unit="character"/><gap extent="4" reason="lost" unit="character"/> 

