    <lb n="4"/><gap reason="lost" extent="22" unit="character"/><unclear reason="undefined" cert="high">σ</unclear>οντος <choice><corr>ὡμολόγησεν</corr><sic>ο<unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">ο</supplied>λογησεν</sic></choice> <unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">αὶ</supplied> οἶδα τοῦτο ἀσφα<unclear reason="undefined" cert="high">λ</unclear><supplied reason="lost" cert="high">ῶ</supplied><unclear reason="undefined" cert="high">ς</unclear> ἐγὼ τω 

