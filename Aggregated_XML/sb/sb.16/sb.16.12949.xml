    <lb n="21"/><gap reason="lost" extent="6" unit="character"/>σοι προση<gap reason="lost" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="2" unit="character"/>; ἀπε<supplied reason="lost" cert="high">κ</supplied>ρ<unclear reason="undefined" cert="high">ί</unclear>ν<unclear reason="undefined" cert="high">ατ</unclear>ο, Ἰσάριον εξ<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">μ</unclear>ενο<unclear reason="undefined" cert="high">ν</unclear> ἐδω-

