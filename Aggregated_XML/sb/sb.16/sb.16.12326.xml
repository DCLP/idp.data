    <lb n="1"/>Ἡλιοδώρᾳ <add place="supralinear">τῇ</add> μητρί μου πολλὰ χαί<unclear reason="undefined" cert="high">ρ</unclear><supplied reason="lost" cert="high">ειν. εὐ</supplied>-

    <lb n="2"/>τόνω<supplied reason="lost" cert="high">ς</supplied> <choice><corr>πικραίνομαί</corr><sic>πικρενο<unclear reason="undefined" cert="high">με</unclear></sic></choice> σοι ὅτι <choice><corr>οὐδὲ</corr><sic>ουτε</sic></choice> <choice><corr>φάσεις</corr><sic>φασι<unclear reason="undefined" cert="high">ς</unclear></sic></choice> <supplied reason="lost" cert="high">λαβεῖν</supplied> 

    <lb n="3"/>διὰ γραμμάτων <unclear reason="undefined" cert="high">σ</unclear>ου κατηξίωσάς με <supplied reason="lost" cert="high">ἀφ᾽ ὅτε</supplied> 

    <lb n="4"/>ἐξῆλθα ἀπὸ σοῦ. πολλὰ ἐντέτριπτ<supplied reason="lost" cert="high">αί μοι ὑπὸ</supplied> 

    <lb n="5"/>τῆς θυγατρός μου. εἰδὲ δὲ πόσον <supplied reason="lost" cert="high">ὀργίσασα</supplied> 

    <lb n="6"/>τὸν σταθμοῦχον καὶ τοὺς γείτονας <unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">ὐτοῦ ἠγανά</supplied>-


    <lb n="7"/>κτησε κατ᾽ αὐτοῦ. ἐξέδυσέν με σύ<unclear reason="undefined" cert="high">μπ</unclear><supplied reason="lost" cert="high">α</supplied>ν καὶ 

    <lb n="8"/>ἦρκεν μου τὰ χρυσάφια καὶ τὰ ε᾿<supplied reason="lost" cert="high">νώ</supplied>τια 

    <lb n="9"/>καὶ ἔδ<unclear reason="undefined" cert="high">ω</unclear>κέν <unclear reason="undefined" cert="high">μοι</unclear> <gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/>κον <unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">ι</supplied><unclear reason="undefined" cert="high">θ</unclear>ῶ<unclear reason="undefined" cert="high">ν</unclear>ιν 

    <lb n="10"/>ἵνα <unclear reason="undefined" cert="high">οὕτ</unclear>ως ἐ<gap reason="lost" extent="8" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/>ια δὲ <unclear reason="undefined" cert="high">διῆ</unclear>ρα. 

    <lb n="11"/>πα<unclear reason="undefined" cert="high">ρ</unclear><supplied reason="lost" cert="high">ακ</supplied><unclear reason="undefined" cert="high">ά</unclear>λεσο<unclear reason="undefined" cert="high">ν</unclear> <supplied reason="lost" cert="high">ὑπὲρ ἐμοῦ τὸ</supplied>ν <unclear reason="undefined" cert="high">θ</unclear>εὸν ἵνα 

    <lb n="12"/>με <unclear reason="undefined" cert="high">ἐλε</unclear>ήσῃ. πᾶν δὲ <unclear reason="undefined" cert="high">πο</unclear><supplied reason="lost" cert="high">ίησ</supplied><unclear reason="undefined" cert="high">ον</unclear> <supplied reason="lost" cert="high">τ</supplied>ὸν ἀδελφόν 

    <lb n="13"/>μου πρός με πέμψαι· <choice><corr>ἐπὶ</corr><sic>επ<unclear reason="undefined" cert="high">ε</unclear><supplied reason="lost" cert="high">ι</supplied></sic></choice> <supplied reason="lost" cert="high">Σ</supplied><unclear reason="undefined" cert="high">εν</unclear>έπτα <unclear reason="undefined" cert="high">σὺ</unclear>ν Ἐ-

    <lb n="14"/>μο<supplied reason="lost" cert="high">ῦτι</supplied> ὑπάγω. <unclear reason="undefined" cert="high">μ</unclear>ὴ δὲ σα<supplied reason="lost" cert="high">κκ</supplied><unclear reason="undefined" cert="high">ί</unclear><supplied reason="lost" cert="high">α</supplied> πέμψ<unclear reason="undefined" cert="high">ῃ</unclear>ς· <choice><corr>ἀρκεῖ</corr><sic>αρκι</sic></choice> 

    <lb n="15"/>μοι ἃ ἔχω <supplied reason="lost" cert="high">ὧ</supplied>δε. ἀσπάζου πάντας τοὺς 

    <lb n="16"/>ἀδελφούς μο<supplied reason="lost" cert="high">υ</supplied> καὶ τοὺς φιλοῦντάς σε. 

    <lb n="17"/>ἐρρῶσθαί σε <choice><corr>εὔχομαι</corr><sic>ευχομε</sic></choice>. 
