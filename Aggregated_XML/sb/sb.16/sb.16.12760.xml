    <lb n="1"/><handShift new="m3"/> <num value="1">α</num> 

    <lb n="2"/><handShift new="m1"/> Χαιρέα<supplied cert="high" reason="lost">ι</supplied> στρατηγῶι <supplied cert="high" reason="lost"> <space dim="horizontal" extent="unknown" unit="character"/> </supplied> 

    <lb n="3"/>παρὰ Θαΐδος τῆς Ἀττ<gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀπογρά</supplied>-

    <lb n="4"/>φομαι εἰς τὸ <choice><corr>ἐνεστὸς</corr><sic>ενοστος</sic></choice> ἑν<supplied cert="high" reason="lost">δέκατον <expan><ex cert="high">ἔτος</ex></expan></supplied> 

    <lb n="5"/>Τιβερίου Καίσαρος Σεβ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">στοῦ τὰ ὑπάρ</supplied>-

    <lb n="6"/><supplied cert="high" reason="lost">χ</supplied><unclear cert="high" reason="undefined">ο</unclear>ντά μοι πρόβατα <unclear cert="high" reason="undefined">ἑ</unclear><supplied cert="high" reason="lost">κατὸν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/>αἶγας ἑπτά, <expan><ex cert="high">γίνονται</ex></expan> <expan>πρ<ex cert="high">όβατα</ex></expan> <num value="100"><unclear cert="high" reason="undefined">ρ</unclear></num> <supplied cert="high" reason="lost"><expan>αἶγ<ex cert="high">ες</ex></expan> <num value="7">ζ</num>, καὶ τοὺς</supplied> 

