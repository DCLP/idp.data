    <lb n="2"/>ἡ<supplied cert="high" reason="lost">μῶ</supplied>ν δεσ<supplied cert="high" reason="lost">πότ</supplied>ου Φλα<supplied cert="high" reason="lost">ουίου Ὁν</supplied>ωρίου 

    <lb n="3"/>τοῦ αἰωνίου Αὐγούστου <supplied cert="high" reason="lost">καὶ Α</supplied>ὐτοκράτορο<supplied cert="high" reason="lost">ς</supplied> 

    <lb n="4"/>ἔτου[ς <num value="1">α</num>]<num value="1">α</num> ὑ<supplied cert="high" reason="lost">πατείας</supplied> Φ<supplied cert="high" reason="lost">λαου</supplied>ίου Στι<supplied cert="high" reason="lost">λίχωνος</supplied> 

