    <lb n="14"/><gap reason="lost" extent="28" unit="character"/><gap reason="illegible" extent="1" unit="character"/>της<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="17" unit="character"/> <note lang="en" anchored="yes" place="not specified">vestig</note> </ab></div><div n="meta.22905" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.22905" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" value="0031-08-29" notBefore="31" notAfter="31">29. Aug. 31</date></p></div><div n="meta.22905" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Zur Datierung vgl. BL V, S. 119.</p></div><div n="meta.22905" type="commentary" subtype="mentionedDates" org="uniform" part="N" sample="complete"><head>Erwähnte Daten</head></div><div n="meta.22905" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">SB</title><biblScope type="volume">8</biblScope><biblScope type="numbers">9824</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">22905</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0239</series>; <biblScope type="volume">8</biblScope>; <biblScope type="numbers">9824</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0239;8;9824" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.22905" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p>keine</p></div><div n="meta.22905" type="history" subtype="locations" org="uniform" part="N" sample="complete"><p><placeName type="ancientFindspot" cert="high" full="yes">Oxyrhynchos</placeName></p></div><div n="meta.22905" type="figure" org="uniform" part="N" sample