    <lb n="1"/><g type="stauros"/> βασιλείας τοῦ θειοτάτου ἡ<supplied reason="lost" cert="high">μῶν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>ἔτους ἑπτακαιδεκάτου, <unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="low">ηνὸς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><g type="stauros"/> Φλαουίῳ Γερμανῷ τῷ λ<supplied reason="lost" cert="high">αμπροτάτῳ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> τοῦ τῆς</supplied> 

    <lb n="4"/>λαμπρᾶς μνήμης Φ<gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> Ὑπερέχιος Δωροθέου πρεσβύτερος</supplied> 

    <lb n="5"/>ἀπὸ τῆς Ἑρμου<unclear reason="undefined" cert="high">πο</unclear>λιτῶν <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ὁμολογῶ μεμισθῶσθαι παρὰ</supplied> 

    <lb n="6"/>τῆς σῆς λαμπρό<supplied reason="lost" cert="high">τητος, ἐφ᾽ ὃν βούλει χρόνον, λογιζόμενον ἀπὸ τῆς σήμερον,</supplied> 

    <lb n="7"/>ἥτις ἐστὶν Τῦβι ὀγ<supplied reason="lost" cert="high">δόη τῆς παρούσης δευτέρας ἰνδικτίωνος τὸ ὑπάρχον σοι</supplied> 

    <lb n="8"/>στάβλον ὁλόκληρον <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> σὺν</supplied> 

    <lb n="9"/>καμάρᾳ καὶ λοι<unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">οῖς χρηστηρίοις πᾶσιν καὶ δικαίοις ἐξ ὅλου τοῦ στάβλου δια</supplied>-

    <lb n="10"/>κείμενον ἐπὶ τὴς αὐ<supplied reason="lost" cert="high">τῆς Ἑρμοῦ πόλεως </supplied><gap reason="lost" extent="25" unit="character"/><supplied reason="lost" cert="high"> πρὸς ἐμὴν</supplied> 

    <lb n="11"/>χρῆσιν, <choice><corr>ἐνοικίου</corr><sic>ενοικειου</sic></choice> <unclear reason="undefined" cert="high">κατὰ</unclear> <supplied reason="lost" cert="high">μῆνα χρυσίου νομισματίου </supplied><gap reason="lost" extent="9" unit="character"/><supplied reason="lost" cert="high">, ὅπερ ἐνοίκιον ἀπο</supplied>-

    <lb n="12"/>δώσω τῷ ὑμετέρῳ εὐ<supplied reason="lost" cert="low">δοκίμῳ</supplied><gap reason="lost" extent="7" unit="character"/><supplied reason="lost" cert="high"> πρὸς λῆξιν ἑκάστου μηνός, καὶ ὁπόταν βου</supplied>-

    <lb n="13"/>ληθείη ἡ σὴ λαμπρ<unclear reason="undefined" cert="high">ότ</unclear><supplied reason="lost" cert="high">ης ἔχειν, παραδώσω σοι τὸ αὐτὸ στάβλον σὺν ἐφεστώσαις</supplied> 

    <lb n="14"/>αὐτῷ θύραις δυσι <num value="2"/> <del rend="erasure" status="unremarkable">η</del> ἐν <supplied reason="lost" cert="high">μηδενὶ καταβλάψας. ἡ μίσθωσις κυρία καὶ <expan>ἐπερωτ<ex cert="high">ηθεὶς</ex></expan> <expan>ὡμολόγ<ex cert="high">ησα</ex></expan>.</supplied> 

    <lb n="15"/><handShift new="m2"/> <g type="stauros"/> Ὑπερέχιος Δωροθ<supplied reason="lost" cert="high">έου ἀπὸ <expan>Ἑρ<ex cert="high">μοῦ πόλεως</ex></expan> μαρτυρῶ τῇ μισθώσει. <handShift new="m3"/> <g type="stauros"/></supplied><gap reason="lost" extent="11" unit="character"/> 

