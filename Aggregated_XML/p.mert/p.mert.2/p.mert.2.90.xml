    <lb n="19"/><gap reason="lost" extent="14" unit="character"/> <unclear reason="undefined" cert="high">ὅπ</unclear>ως τῷ <expan>αὐ<ex cert="high">τῷ</ex></expan> <unclear reason="undefined" cert="high">Σα</unclear>ρα-

    <lb n="20"/><unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ίωνι </supplied><gap reason="lost" extent="8" unit="character"/><gap reason="illegible" extent="2" unit="character"/>ι πρό<unclear reason="undefined" cert="high">σεσ</unclear>τι ἐπιτη-

    <lb n="21"/><choice><corr>δείως</corr><sic>επιτηδιω<unclear reason="undefined" cert="high">ς</unclear></sic></choice> <gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="3" unit="character"/><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="3" unit="character"/><gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">η</unclear> μηδ<unclear reason="undefined" cert="high">αμ</unclear>ῶς <unclear reason="undefined" cert="high">ἀ</unclear>πα<unclear reason="undefined" cert="high">σ</unclear>χο-

    <lb n="22"/>λή<unclear reason="undefined" cert="high">σῃς</unclear> ἀπὸ τοῦ ἐ<unclear reason="undefined" cert="high">πιτ</unclear>άγματος τῆς ἐμβολαρ-

    <lb n="23"/>χίας ἀναγ<g type="apostrophe"/>καίου <unclear reason="undefined" cert="high">ὄν</unclear>τος ἄχρις τῆς ἐπιδη-

    <lb n="24"/><supplied reason="lost" cert="high">μί</supplied>ας τοῦ κυρίου <unclear reason="undefined" cert="high">μ</unclear>ο<unclear reason="undefined" cert="high">υ</unclear> ἐπιτρόπου, <hi rend="diaeresis">ἵ</hi>να τῶν 

