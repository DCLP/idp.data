    <lb n="1"/><supplied cert="high" reason="lost">Ἑ</supplied><unclear cert="high" reason="undefined">λ</unclear>ενία <choice><corr>Πομπηίωι</corr><sic><subst><add place="inline">πονπηγιωι</add><del rend="corrected" status="unremarkable">πονπη<gap extent="1" reason="illegible" unit="character"/>ιωι</del></subst></sic></choice> τῶι πατρὶ 

    <lb n="2"/><supplied cert="high" reason="lost">π</supplied><unclear cert="high" reason="undefined">λεῖ</unclear>στα <choice><corr>χαίρειν</corr><sic>χαιριν</sic></choice> καὶ διὰ παντὸς 

    <lb n="3"/><choice><corr>ὑγιαίνειν</corr><sic><supplied cert="high" reason="lost">υ</supplied><unclear cert="high" reason="undefined">γι</unclear>αινιν</sic></choice>.  ἠγόρακά <choice><corr>σοι</corr><sic>συ</sic></choice> 

    <lb n="4"/><supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ὰς</unclear> <choice><corr>ἐλαίας</corr><sic>ελεας</sic></choice>.  αἰτοῦσι παν-

    <lb n="5"/><choice><corr>ταχόθεν</corr><sic>παν<supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">αχο</unclear>θην</sic></choice> εἰς τὴν εὐσέ-

    <lb n="6"/><choice><corr>βειαν</corr><sic>ευσε<unclear cert="high" reason="undefined">βι</unclear>αν</sic></choice> <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ο</supplied>ῦ ναοῦ Σούχου, 

    <lb n="7"/><choice><corr>πάντας</corr><sic><unclear cert="high" reason="undefined">πα</unclear>ντο<unclear cert="high" reason="undefined">ς</unclear></sic></choice> ἀνθρώπους, 

    <lb n="8"/><unclear cert="high" reason="undefined">κ</unclear>αὶ Ῥωμαίους καὶ Ἀλεξαν-

    <lb n="9"/><unclear cert="high" reason="undefined">δρεῖς</unclear> <unclear cert="high" reason="undefined">κα</unclear>ὶ <choice><corr>κατοικοῦντας</corr><sic>κα<unclear cert="high" reason="undefined">τ</unclear>υκουτος</sic></choice> ἐν 

    <lb n="10"/><unclear cert="high" reason="undefined">τ</unclear>ῶι <choice><corr>Ἀρσινοΐτηι</corr><sic>αρ<supplied cert="high" reason="lost">σ</supplied>ενοειτηι</sic></choice>.  αἰτ<unclear cert="high" reason="undefined">οῦ</unclear>σι 

    <lb n="11"/><choice><corr>Πομπήιον</corr><sic><unclear cert="high" reason="undefined">π</unclear>ονπ<supplied cert="high" reason="lost">η</supplied>γιν</sic></choice>.  ο<unclear cert="high" reason="undefined">ὐ</unclear> δέδωκα 

    <lb n="12"/><unclear cert="high" reason="undefined">δὴ</unclear> ὡς σήμερον προσ-

    <lb n="13"/><choice><corr>δεχομένη</corr><sic>προσ<supplied cert="high" reason="lost">δ</supplied><unclear cert="high" reason="undefined">ε</unclear>χω<unclear cert="high" reason="undefined">μ</unclear>ενυ</sic></choice> <choice><corr cert="low">ὅτι</corr><sic>ωτι</sic></choice> <choice><corr>ἐλεύσει</corr><sic>ελευση</sic></choice> 

    <lb n="14"/>ἢ σὺ φροντίσας <choice><corr>ποίησον</corr><sic>πειησον</sic></choice>, ἂν 

    <lb n="15"/><unclear cert="high" reason="undefined">δὲ</unclear> <unclear cert="high" reason="undefined">μή</unclear>, <supplied cert="high" reason="lost">αὐτὴ</supplied>ν δ<supplied cert="high" reason="lost">ί</supplied>δομεν· καὶ 

    <lb n="16"/><choice><corr cert="low">ὑγιῆ</corr><sic><unclear cert="high" reason="undefined">υγεης</unclear></sic></choice> <unclear cert="high" reason="undefined">κε</unclear>κόμισμαι τὴν 

    <lb n="17"/>ἐπιστ<unclear cert="high" reason="undefined">ολ</unclear>ὴν παρὰ τοῦ Ὀνο-

    <lb n="18"/><choice><corr>μαστοῦ</corr><sic>ων<unclear cert="high" reason="undefined">ο</unclear>μα<supplied cert="high" reason="lost">σ</supplied>του</sic></choice>.  <choice><corr>εὐχόμεθα</corr><sic>ευχωμετα</sic></choice> σε 

    <lb n="19"/>ἵνα καλῶς <choice><corr>ἔχειν</corr><sic>εχην</sic></choice>.  ἵνα <supplied cert="high" reason="omitted">μὴ</supplied> 

    <lb n="20"/>ἐπιλάθῃς τῶν <choice><corr>τέκνων</corr><sic>τεκνω</sic></choice> 

    <lb n="21"/>σου, κόμισαι ἑτέραν ἐπισ-

    <lb n="22"/><choice><corr>τολὴν</corr><sic>επισστολην</sic></choice> Συρίωνος τοῦ υἱοῦ 

    <lb n="23"/><unclear cert="high" reason="undefined">σ</unclear>ου. 

    <lb n="24"/><space dim="horizontal" extent="unknown" unit="character"/> 

    <lb n="24"/><choice><corr>ἐπισκοπούμεθά</corr><sic>επισσκοπουμεθα</sic></choice> σε καὶ 

    <lb n="25"/>Χαριτοῦν καὶ τὰ τέκνα 

    <lb n="26"/><choice><corr>αὐτῆς</corr><sic>αυτην</sic></choice>.  καὶ <choice><corr>Πομπήιος</corr><sic>πονπηγις</sic></choice> <choice><corr>ὁ</corr><sic>ω</sic></choice> μι-

    <lb n="27"/>κρὸς <choice><corr>ἐπισκοπεῖται</corr><sic>επισσκοπιται</sic></choice> ἡμᾶς. 

    <lb n="28"/><expan><ex cert="high">ἔτους</ex></expan> <num value="4">δ</num> Νέρωνος Κλαυδίου Καίσαρος 

    <lb n="29"/>Σεβ<unclear cert="high" reason="undefined">ασ</unclear>το<unclear cert="high" reason="undefined">ῦ</unclear> <choice><corr>Γερμανικοῦ</corr><sic>γερμανκου</sic></choice> Ἀυτοκράτορος 

    <lb n="30"/>Τῦ<unclear cert="high" reason="undefined">β</unclear>ι <num value="23">κγ</num>. 
