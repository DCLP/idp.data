    <lb n="1"/>Αὐρήλιοι Σαραπάμμων καὶ Ποτάμμων <choice><corr>ἐπιμεληταὶ</corr><sic>επιμελητε</sic></choice> ἀσήμου 

    <lb n="2"/>Αὐρηλίῳ Ἡρᾷ <choice><corr>Πτολεμαίου</corr><sic>πτωλεμεου</sic></choice> χαίρειν. παρελάβαμεν 

    <lb n="3"/>παρὰ σοῦ κατὰ θείαν πρόσταξιν ὑπ<supplied reason="lost" cert="high">ὲ</supplied><unclear reason="undefined" cert="high">ρ</unclear> τῆς κτήσ<unclear reason="undefined" cert="high">ε</unclear>ω<unclear reason="undefined" cert="high">ς</unclear> 

    <lb n="4"/>κώμης Καιρανίδος <choice><corr>ὁριοδεικτίας</corr><sic>ορωδικτιας</sic></choice> ἀσήμου καθαροῦ 


    <lb n="5"/>οὐγκίας δύο <choice><corr>γί</corr><sic>γει</sic></choice><expan><ex cert="high">νονται</ex></expan> <expan><ex cert="high">οὐγκίαι</ex></expan> <num value="2">β</num>. Ποτάμμων <expan>σεσημ<ex cert="high">είωμαι</ex></expan>. 

    <lb n="6"/>πρὸ <num value="17">ιζ</num> <choice><corr>καλανδῶν</corr><sic>καλαντων</sic></choice> Ἰανουαρίων. 

    <lb n="7"/><handShift new="m2"/> Αὐρήλιοι Ποτάμμων καὶ Σαραπάμων ἐπι<unclear reason="undefined" cert="high">μ</unclear>εληταὶ ἀσήμου 

    <lb n="8"/>Αὐρηλίῳ Ἡρᾷ Πτολεμαίου <expan>χ<ex cert="high">αίρειν</ex></expan>. 

    <lb n="9"/>παρελάβαμεν <supplied reason="lost" cert="high">π</supplied>α<unclear reason="undefined" cert="high">ρ</unclear><supplied reason="lost" cert="high">ὰ σ</supplied>οῦ κατὰ θείαν π<unclear reason="undefined" cert="high">ρ</unclear>όσταξιν ὑπὲρ τῆς 

    <lb n="10"/>κτήσεώς σου <expan><supplied reason="lost" cert="high">κ</supplied><unclear reason="undefined" cert="high">ώ</unclear><ex cert="high">μης</ex></expan> Καρ<supplied reason="lost" cert="high">α</supplied><unclear reason="undefined" cert="high">ν</unclear>ίδος <choice><corr>ὁριοδεικτίας</corr><sic>οροδικ<supplied reason="lost" cert="high">τ</supplied><unclear reason="undefined" cert="high">ιας</unclear></sic></choice> <expan>ἀσήμο<ex cert="high">υ</ex></expan> <expan>καθαρο<ex cert="high">ῦ</ex></expan> 

    <lb n="11"/>γράμματα ἓξ <expan><supplied reason="lost" cert="high">γί<ex cert="high">νεται</ex></supplied></expan> <expan>γρά<ex cert="high">μματα</ex></expan> <num value="6">ϛ</num>. Ποτάμμων <expan>σεσημ<ex cert="high">είωμαι</ex></expan>. 

    <lb n="12"/><choice><corr>ὑπατείας</corr><sic>υπατιας</sic></choice> τοῦ <expan>δεσπότο<ex cert="high">υ</ex></expan> <unclear reason="undefined" cert="high">ἡ</unclear><supplied reason="lost" cert="high">μῶ</supplied><unclear reason="undefined" cert="high">ν</unclear> <expan>Μαξιμίνο<ex cert="high">υ</ex></expan> Καίσ<unclear reason="undefined" cert="high">αρ</unclear>ος <unclear reason="undefined" cert="high">π</unclear>ρὸ <num value="9">θ</num> 

    <lb n="13"/>καλανδῶν Ἰαν<unclear reason="undefined" cert="high">ο</unclear>υαρίω<supplied reason="lost" cert="high">ν</supplied>. 

    <lb n="14"/>Αὐρήλιος Ἰσίας <expan>γυμ<ex cert="high">νασιαρχήσας</ex></expan> <expan>βο<unclear reason="undefined" cert="high">υ</unclear><supplied reason="lost" cert="high">λ<ex cert="high">ευτὴς</ex></supplied></expan> <expan>ἐπιμελ<ex cert="high">ητὴς</ex></expan> ἀσήμου 

    <lb n="15"/>Αὐρηλίῳ Ταησίῳ <expan>Κ<unclear reason="undefined" cert="high">ο</unclear>πρ<ex cert="high">έους</ex></expan> <expan><supplied reason="lost" cert="high">χ</supplied><unclear reason="undefined" cert="high">αί</unclear><ex cert="high">ρειν</ex></expan>. παρέλαβον <unclear reason="undefined" cert="high">παρ</unclear>ὰ σοῦ 

    <lb n="16"/>κατα θείαν <choice><corr>πρόσταξιν</corr><sic>προσταξειν</sic></choice> <unclear reason="undefined" cert="high">ἀ</unclear><supplied reason="lost" cert="high">σ</supplied>ήμου καθαροῦ κώ<supplied reason="lost" cert="high">μ</supplied>ης 

    <lb n="17"/>Καρανίδος γράμματα δώδεκα <expan>γ<ex cert="high">ίνεται</ex></expan> <expan>γρ<ex cert="high">άμματα</ex></expan> <num value="12">ιβ</num>. Ἰσ<supplied reason="lost" cert="high">ί</supplied>ας <expan>ἐπιμ<ex cert="high">ελητὴς</ex></expan> 

    <lb n="18"/><expan>σεσημ<ex cert="high">είωμαι</ex></expan> τὰ <choice><corr>γράμματα</corr><sic>γραματα</sic></choice> δώδεκα <num value="12"/>. 

    <lb n="19"/>ὑπατείας τοῦ δεσπότου ἡ<unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">ῶ</supplied>ν Μαξιμίνου τοῦ ἐ<unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ι</supplied>φ<supplied reason="lost" cert="high">α</supplied>νεστάτου 

