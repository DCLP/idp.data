    <lb n="3"/>παρὰ Αὐρηλίας Τα<unclear cert="high" reason="undefined">υ</unclear>σι<unclear cert="high" reason="undefined">ρ</unclear>ίης Πεκυσίου ἀπὸ κώμης 

    <lb n="4"/>Σενεκελε<unclear cert="high" reason="undefined">ὺ</unclear> <num value="3"><unclear cert="high" reason="undefined">γ</unclear></num> <unclear cert="high" reason="undefined">πάγο</unclear>υ τοῦ αὐτοῦ νομοῦ. 

    <lb n="5"/>ἀλλότρ<unclear cert="high" reason="undefined">ι</unclear><supplied cert="high" reason="lost">α τῶν ν</supplied><unclear cert="high" reason="undefined">όμων</unclear> διεπράξατο κατ᾽ ἐμοῦ 

    <lb n="6"/>ὁ μὴ καλεῖσθαι <choice><corr>ὀφείλων</corr><sic>οφιλων</sic></choice> ἀνήρ μου Παῦλος 

    <lb n="7"/><supplied cert="low" reason="lost">υἱὸς</supplied><supplied cert="high" reason="lost"> Κ</supplied><unclear cert="high" reason="undefined">άστο</unclear>ρο<unclear cert="high" reason="undefined">ς</unclear>. σύμβιον οὖν ᾧ προσηνε 

    <lb n="8"/><gap extent="16" reason="lost" unit="character"/><unclear cert="high" reason="undefined">μη</unclear>θι χρη<unclear cert="high" reason="undefined">σάμ</unclear>ε<unclear cert="high" reason="undefined">ν</unclear>ος 

