    <lb n="2"/>καὶ παρ᾽ αὐτῷ τυγχάνει <gap reason="illegible" extent="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>ἐπιδοῦναι τῇ σῇ <choice><corr>στερρότη<supplied reason="lost" cert="high">τι</supplied></corr><sic>στερρωτη<supplied reason="lost" cert="high">τι</supplied></sic></choice> <gap reason="lost" extent="unknown" unit="character"/> 

