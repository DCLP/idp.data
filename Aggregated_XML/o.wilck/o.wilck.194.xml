    <lb n="9"/>Ἀντωνείνου Καίσαρος τοῦ κυρίου 

    <lb n="10"/>Ἁθὺρ <num value="3">γ</num>, <handShift new="m2"/> <num value="9">θ</num> ὁ <expan>α<ex cert="high">ὐτὸς</ex></expan> <expan>διέ<ex cert="high">γραψεν</ex></expan> <choice><corr>χειρω</corr><sic>χειρο</sic></choice><expan><ex cert="high">ναξίου</ex></expan> <expan>δραχ<ex cert="high">μὰς</ex></expan> 

