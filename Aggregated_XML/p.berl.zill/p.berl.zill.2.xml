    <lb n="3"/>τῆ<unclear reason="undefined" cert="high">ι</unclear> <num value="15">ιε</num> το<unclear reason="undefined" cert="high">ῦ</unclear> <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="25" unit="character"/><supplied reason="lost" cert="high"> προσ</supplied>-

    <lb n="4"/>ενήνεκται Τήρης τῶν α<gap reason="lost" extent="15" unit="character"/> 

    <lb n="5"/>κωλύεσθαι τὰ κατὰ τὴν <supplied reason="lost" cert="high">οἰκοδο</supplied><unclear reason="undefined" cert="high">μί</unclear>αν τοῦ κτι-

    <lb n="6"/>ζομένου φρουρίου ἐπὶ τοῦ καθ᾽ Ἡρ<supplied reason="lost" cert="high">α</supplied>κ<unclear reason="undefined" cert="high">λέ</unclear>ους πόλιν 

    <lb n="7"/>ὅρμου <del rend="erasure" status="unremarkable">φρουρίου</del> ἕνεκα το<supplied reason="lost" cert="high">ῦ </supplied><gap reason="lost" extent="3" unit="character"/>α<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="1" unit="character"/><add place="supralinear">μενον <gap reason="illegible" extent="1" unit="character"/></add> <num value="15">ιε</num> ἀπὸ 

