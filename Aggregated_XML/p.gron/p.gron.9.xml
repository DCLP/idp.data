    <lb n="2"/>νοητὴς ἐποικ<supplied cert="high" reason="lost">ίου </supplied><gap extent="8" reason="lost" unit="character"/> 

    <lb n="3"/>τοῦ Ἀρσινοΐτου νο<unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">οῦ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>Ἀκ<unclear cert="high" reason="undefined">αμούσαι</unclear> <unclear cert="high" reason="undefined">α</unclear>ὐτ<unclear cert="high" reason="undefined">ο</unclear><gap extent="4" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τοῦ αὐτοῦ</supplied> 

    <lb n="5"/>νομο<supplied cert="high" reason="lost">ῦ</supplied> <space dim="horizontal" extent="unknown" unit="character"/> <supplied cert="high" reason="lost">χαίρειν</supplied>. 

    <lb n="6"/>μεμί<supplied cert="high" reason="lost">σθ</supplied>ωκά σοι κ<supplied cert="high" reason="lost">ατὰ τήνδε τὴν</supplied> 

    <lb n="7"/>μι<unclear cert="high" reason="undefined">σθ</unclear><supplied cert="high" reason="lost">απο</supplied><unclear cert="high" reason="undefined">χὴν</unclear> <unclear cert="high" reason="undefined">ἀ</unclear><supplied cert="high" reason="lost">πὸ τῶν ὑπαρχόν</supplied>-

