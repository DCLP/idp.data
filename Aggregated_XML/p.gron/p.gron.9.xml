    <lb n="23"/><unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">αὶ</supplied> <unclear cert="high" reason="undefined">ἀπ</unclear><supplied cert="high" reason="lost">έσχον τὸν φόρον παρὰ σοῦ</supplied> 

    <lb n="24"/>πλήρη <supplied cert="high" reason="lost">ὡς πρόκειτ</supplied>αι. Αὐ<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ήλιος </supplied><gap extent="3" reason="lost" unit="character"/> 

    <lb n="25"/>Με<unclear cert="high" reason="undefined">γ</unclear>ίων<supplied cert="high" reason="lost">ος </supplied><gap extent="3" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">λ</unclear>εους ἀναγνώ-

