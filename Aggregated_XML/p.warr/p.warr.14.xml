    <lb n="1"/><supplied reason="lost" cert="high">Ὀ</supplied>σορᾶπις <choice><corr>καὶ</corr><sic>κε</sic></choice> Φατρῆς υ<hi rend="diaeresis">ἱ</hi>ὸς 

    <lb n="2"/>Θέωνι τῷ ἀξιολο<add place="supralinear">γω</add>τάτῳ 

    <lb n="3"/>πολλὰ <choice><corr>χαίρειν</corr><sic>χερ<add place="supralinear">ε</add>ιν</sic></choice>. πρὸ μὲν 

    <lb n="4"/>πάντων <choice><corr>εὐχόμεθά</corr><sic>ευχομ<unclear reason="undefined" cert="high">α</unclear>ιθα</sic></choice> σε 

    <lb n="5"/><choice><corr>ὑγιαίνειν</corr><sic>υ<hi rend="diaeresis">ι</hi>ενειν</sic></choice>. καθὼ<unclear reason="undefined" cert="high">ς</unclear> ἐνετει-

    <lb n="6"/><choice><corr>λάμεθα</corr><sic>ε<unclear reason="undefined" cert="high">ν</unclear>εθειλαμεθα</sic></choice> τῇ ἀδελφῇ σου 

    <lb n="7"/>Ἀρ<unclear reason="undefined" cert="high">σ</unclear>οῦτι περὶ τῶν <unclear reason="undefined" cert="high">ἑκα</unclear>τὸν 

    <lb n="8"/>εἴκοσι <num value="120"/> δραχμῶ[ν, <expan>οἴδ]αμε<ex cert="high">ν</ex></expan> 

    <lb n="9"/>ὅτι οὐκ ἀμελήσ<supplied reason="lost" cert="high">ει </supplied><choice><corr><supplied reason="lost" cert="high">δ</supplied>οῦναι</corr><sic><supplied reason="lost" cert="high">δ</supplied><unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">υ</supplied>νε</sic></choice> 

    <lb n="10"/>τῇ γυναικὶ Ἀπό<supplied reason="lost" cert="high">λλων</supplied>ος 

    <lb n="11"/><choice><corr>καὶ</corr><sic>κε</sic></choice> <choice><corr>ὑπομνῆσαι</corr><sic>υπομνησε</sic></choice> <unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">ὐτὴ</supplied>ν <unclear reason="undefined" cert="high">πε</unclear>-

    <lb n="12"/>πληρωμένης <unclear reason="undefined" cert="high">τ</unclear>ο<unclear reason="undefined" cert="high">ῦ</unclear> τ<unclear reason="undefined" cert="high">ό</unclear>κο<unclear reason="undefined" cert="high">υ</unclear> 

    <lb n="13"/>τοῦ Ἁθύρ. <choice><corr>καὶ</corr><sic><unclear reason="undefined" cert="high">κε</unclear></sic></choice> περὶ <unclear reason="undefined" cert="high">τ</unclear>οῦ <unclear reason="undefined" cert="high">αὐτο</unclear><supplied reason="lost" cert="high">ῦ</supplied> 

    <lb n="14"/><choice><corr>κεφαλαίου</corr><sic>κεφαλεου</sic></choice> μαθ<unclear reason="undefined" cert="high">ε</unclear>ῖν σε δεῖ 

    <lb n="15"/>ὅτι μέλλοντες <choice><corr>πέμψαι</corr><sic>πεμψε</sic></choice> 

    <lb n="16"/>αὐτῇ διὰ φίλου, ἐμνήσθ<unclear reason="undefined" cert="high">η</unclear>-

    <lb n="17"/>μέν σου, <choice><corr>μᾶλλον</corr><sic>μαλον</sic></choice> ἐπικρίνον-

    <lb n="18"/><choice><corr>τες</corr><sic>επικρ<unclear reason="undefined" cert="high">ι</unclear>ν<unclear reason="undefined" cert="high">ον</unclear>τα</sic></choice> φίλον. <choice><corr>καὶ</corr><sic>κε</sic></choice> οὕτως μετε-

    <lb n="19"/>βαλόμεθα τὸ κέρμα Ἁρθω-

    <lb n="20"/>νίῳ τῷ ἀδελφῷ τ<unclear reason="undefined" cert="high">ῆ</unclear>ς γυ-

    <lb n="21"/><choice><corr>ναικός</corr><sic><unclear reason="undefined" cert="high">γυ</unclear>νεκος</sic></choice> σου διὰ τραπέζ<unclear reason="undefined" cert="high">ης</unclear> 

    <lb n="22"/>ὥστε σὲ μεταβαλεῖν τῇ 

    <lb n="23"/><choice><corr>γυναικὶ</corr><sic>γυνεκει</sic></choice> Ἀπόλλωνος <choice><corr>καὶ</corr><sic>κε</sic></choice> 

    <lb n="24"/><choice><corr>σκυλῆναί</corr><sic>σκυληνε</sic></choice> σε <add place="supralinear">με</add>τ᾽ αὐτῆς πα<unclear reason="undefined" cert="high">ρ</unclear>ὰ 

    <lb n="25"/>τὴν <choice><corr>γυναῖκα</corr><sic>γυνεκαν</sic></choice>, <choice><corr>καὶ</corr><sic>κε</sic></choice> ο<unclear reason="undefined" cert="high">ὕ</unclear>τω<unclear reason="undefined" cert="high">ς</unclear> 

    <lb n="26"/>ἀπολαβῆτε τὰ <unclear reason="undefined" cert="high">γ</unclear>ράμματα 

    <lb n="27"/>ἡμῶν ἐπὶ ὑπογ<unclear reason="undefined" cert="high">ρ</unclear>α<unclear reason="undefined" cert="high">φ</unclear>ῆς 

    <lb n="28"/>ἐμοῦ τοῦ Ὀσοράπιος <unclear reason="undefined" cert="high">παρ</unclear><supplied reason="lost" cert="high">ὰ</supplied> 

    <lb n="29"/><choice><corr>γυναικὸς</corr><sic>γυνεκ<unclear reason="undefined" cert="high">ος</unclear></sic></choice> Ἀπόλ<unclear reason="undefined" cert="high">λ</unclear>ω<unclear reason="undefined" cert="high">ν</unclear>ο<unclear reason="undefined" cert="high">ς</unclear> 

    <lb n="30"/><choice><corr>καὶ</corr><sic>κε</sic></choice> οὕτως κομίσητε τ<unclear reason="undefined" cert="high">ὰ</unclear> 

    <lb n="31"/>γράμματα, <choice><corr>καὶ</corr><sic>κε</sic></choice> εὐθέως 

    <lb n="32"/><choice><corr>ἀναπέμψαι</corr><sic>αναπεμψε</sic></choice> μο<unclear reason="undefined" cert="high">ι</unclear>, τῆς δὲ 

    <lb n="33"/><choice><corr>γυναικὸς</corr><sic>γυνεκος</sic></choice> <choice><corr>ἐπιδούσης</corr><sic><unclear reason="undefined" cert="high">ε</unclear>φιδουσ<unclear reason="undefined" cert="high">ης</unclear></sic></choice> αὐ-

    <lb n="34"/>τῆς τὴν ὑπογραφὴν, <choice><corr>καὶ</corr><sic>κε</sic></choice> 

    <lb n="35"/><unclear reason="undefined" cert="high">τ</unclear>ὰ ἐμὰ γράμματα ἐν <unclear reason="undefined" cert="high">τ</unclear>ά-

    <lb n="36"/><unclear reason="undefined" cert="high">χ</unclear>ει <choice><corr>πέμψαι</corr><sic>πεμψε</sic></choice>. τὸ γὰ<unclear reason="undefined" cert="high">ρ</unclear> ἀργύρι-

    <lb n="37"/>ον μετεβαλόμ<unclear reason="undefined" cert="high">ε</unclear>θα τῷ 

    <lb n="38"/><choice><corr>τραπεζίτῃ</corr><sic>τραπεδειτη</sic></choice> διὰ Ἁρθωνί-

    <lb n="39"/>ου ἄχρις ἂν τὰ γράμματα 

    <lb n="40"/>ἡμῶν κομισθῇ. διὸ οὖν 

    <lb n="41"/>μὴ ἀμελήσῃς, <choice><corr>ἄδελφε</corr><sic>αδελφαι</sic></choice>, 

    <lb n="42"/>ἵνα μὴ ὁ τόκος <choice><corr>καὶ</corr><sic>κε</sic></choice> τοῦ 

    <lb n="43"/>Χο<unclear reason="undefined" cert="high">ιὰ</unclear>κ ἡμῖν ἐπακολου-

    <lb n="44"/>θήσῃ. ἐρρῶσθαί σε εὐχό-

    <lb n="45"/>με<unclear reason="undefined" cert="high">θα</unclear> διὰ βίου. 
