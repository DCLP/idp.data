    <lb n="21"/>μοῦν τὰς ὄκτ<supplied reason="lost" cert="high">ω</supplied> <num value="8"/> <choice><corr>λίτρας</corr><sic><unclear reason="undefined" cert="high">λ</unclear>ειτρας</sic></choice> 

