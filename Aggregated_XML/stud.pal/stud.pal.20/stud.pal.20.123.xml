    <lb n="32"/><abbr>λιβ</abbr> <num value="590">φϙ</num> <num value="1/4">δ</num>   <expan><ex cert="high">ἀρούρης</ex></expan> <num value="1/4">δ</num> <num value="1/8">η</num> <gap extent="1" reason="lost" unit="character"/>εις  κρέως κρέως <expan>λ<ex cert="high">ιτρῶν</ex></expan> <expan><ex cert="high">μυριάδες</ex></expan> <num value="2">β</num>  <num value="597">φϙζ</num> <num value="20597"/>  <num value="1/2">𐅵</num> 

