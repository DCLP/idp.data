    <lb n="3"/>τῇ καὶ Δημητροῦτι Πόσειτος ἀπὸ Ὀνν<supplied cert="high" reason="lost">η με</supplied>τὰ <supplied cert="high" reason="lost"><expan>κ<ex cert="high">υρίου</ex></expan> </supplied><gap extent="4" reason="lost" unit="character"/> 

    <lb n="4"/>τοῦ καὶ Φιλοξένου ἀνδρός· ἔσχον παρὰ σοῦ <supplied cert="high" reason="lost">ἀφ᾽ ὧν</supplied> 

    <lb n="5"/>ὀφείλ<supplied cert="high" reason="lost">εις μ</supplied>οι ἀργυρίου <choice><corr>κεφαλαίου</corr><sic><unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">ε</supplied>φαλιου</sic></choice> κ<unclear cert="high" reason="undefined">αὶ</unclear> <unclear cert="high" reason="undefined">τό</unclear>κ<supplied cert="high" reason="lost">ων</supplied> 

    <lb n="6"/>ταλάντου ἑνὸς <num value="1"/> καὶ δραχμῶν πεντακ<supplied cert="high" reason="lost">οσίων</supplied> <num value="500"/> 

    <lb n="7"/>ἐπὶ κατοχῇ παντὸς τοῦ πόρου σου ἀκ<supplied cert="high" reason="lost">ολούθως</supplied> 

    <lb n="8"/>αὐτῇ τῇ ἀσφαλείᾳ ἐπὶ λόγου ἀργυρί<supplied cert="high" reason="lost">ου δρα</supplied>-

    <lb n="9"/>χμὰς <choice><corr>τρισχιλίας</corr><sic>τρισχειλιας</sic></choice> <expan><ex cert="high">γίνονται</ex></expan> ἐπὶ λό<supplied cert="high" reason="lost">γ</supplied>ου <expan><ex cert="high">δραχμαὶ</ex></expan> <num value="3000">Γ</num> 

    <lb n="10"/>μένο<unclear cert="high" reason="undefined">ν</unclear>τός <unclear cert="high" reason="undefined">μ</unclear>οι τοῦ λόγου πρὸς <unclear cert="high" reason="undefined">ὑ</unclear>μ<supplied cert="high" reason="lost">ᾶς ἐκ πλή</supplied>-

    <lb n="11"/>ρους περὶ τῶν λ<supplied cert="high" reason="lost">ο</supplied>ιπῶν δραχμῶ<supplied cert="high" reason="lost">ν</supplied> 

    <lb n="12"/><choice><corr>τρισχιλίων</corr><sic>τρισχειλιων</sic></choice> πεντακοσίων <num value="3500"/>. 

