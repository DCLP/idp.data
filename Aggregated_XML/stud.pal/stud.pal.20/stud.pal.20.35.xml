<placeName cert="high" full="yes">Herakleopolite</placeName>
<xref type="reprint from" targOrder="U" from="ROOT" to="DITTO" n="sb;1;5294" evaluate="all">SB1,5294</xref></head><div type="edition" lang="grc" org="uniform" part="N" sample="complete"><ab part="N">
    <lb n="1"/>ἔτους τεσσαρεσκαιδεκάτου Αὐτοκράτ<supplied reason="lost" cert="high">ορος Καίσαρος Μάρκου Αὐρηλίου Σεουήρου Ἀλεξάνδρου Εὐσεβοῦς</supplied> 

    <lb n="2"/>Εὐτυ<unclear reason="undefined" cert="high">χο</unclear>ῦς Σεβαστοῦ Δαισίου Φαρμοῦθι δεκάτ[ῃ <app type="BL"><lem resp="5.143">ἐν <gap reason="illegible" extent="unknown" unit="character"/></lem><rdg>δι᾽ ἐπιτηρητῶν ἀγορανομίας <gap reason="illegible" extent="unknown" unit="character"/></rdg></app> τοῦ ὑπὲρ Μέμφιν] 

    <lb n="3"/>Ἡρακλεοπολείτο<unclear reason="undefined" cert="high">υ</unclear>· τάδ<unclear reason="undefined" cert="high">ε</unclear> διέθετο νοῶν καὶ φ[ρονῶν <app type="BL"><lem resp="5.143">Αὐρήλιος</lem><rdg/></app> <gap reason="illegible" extent="unknown" unit="character"/> ] 

    <lb n="4"/>μητρὸς Ταστωοῦτος ἀφ᾽ Ἡρακλέους πόλε[ως <app type="BL"><lem resp="8.462">ἀναγραφόμενος ἐν τῇ πρότερον τοῦ δεῖνος</lem><rdg><gap reason="illegible" extent="unknown" unit="character"/></rdg></app> ἀμφοδαρ]-

    <lb n="5"/>χίᾳ ὡς ἐτῶν ἑξήκοντα τεσσάρω[ν <num value="64"/> οὐ]λὴ <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> εἴη μέν μοι ὑγιαίνοντι καὶ ζῶντι κύριον εἶναι</supplied> 

    <lb n="6"/>τῶν ἐμαυτοῦ πάντων καὶ διοικεῖν καὶ ἐσ<supplied reason="lost" cert="high">οδεύεσθαι πωλοῦντα ὑποτιθέμενον μεταδιατιθέμενον </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>ἐὰν δὲ ὃ μὴ εἴοιτο ἀνθρώπινόν τι πάθω [<app type="BL"><lem resp="2.2.159">κληρονόμον ἀπολείπω</lem><rdg>βούλομαι</rdg></app> <gap reason="illegible" extent="unknown" unit="character"/> ] 

    <lb n="8"/>ἐκ παιδόθεν <choice><corr>ὁμογνήσιον</corr><sic>ω<supplied reason="lost" cert="high">μο</supplied>γνησιον</sic></choice> υ<hi rend="diaeresis">ἱ</hi>ὸν Αὐρηλί<supplied reason="lost" cert="high">ου </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἀπὸ τῆς</supplied> 

    <lb n="9"/>αὐτῆς πόλεως ε<supplied reason="lost" cert="high">ὐ</supplied>νοί<unclear reason="undefined" cert="high">ας</unclear> καὶ φιλοστοργί[ας ἕνεκα <app type="BL"><lem resp="5.143"><gap reason="illegible" extent="unknown" unit="character"/> τῶν ἐμῶν</lem><rdg>κληρονόμον εἶναι <gap reason="illegible" extent="unknown" unit="character"/> τῶν ἐμῶν</rdg></app>] 

    <lb n="10"/>παντοίων παντ<unclear reason="undefined" cert="high">ὸς</unclear> εἴδους καθ᾽ ὃν δήπ<supplied reason="lost" cert="high">οτε τρόπον ἐμοὶ ὑπαρχόντων </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> κατ᾽ ἐξουσίαν</supplied> 

    <lb n="11"/>πᾶ<supplied reason="lost" cert="high">σαν</supplied>, ἄλλῳ δ<supplied reason="lost" cert="high">ὲ οὐ</supplied>δεν<supplied reason="lost" cert="high">ὶ οὐδ</supplied><unclear reason="undefined" cert="high">ὲν</unclear> κα<supplied reason="lost" cert="high">ταλείπω </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><unclear reason="undefined" cert="high">ὁ</unclear> <supplied reason="lost" cert="high">δὲ προ</supplied>κείμε<supplied reason="lost" cert="high">νος μου κλη</supplied>ρονόμος <app type="BL"><lem resp="5.143"><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> τὴν διαθήκην ἐποίησα γράμμασιν</supplied></lem><rdg><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="13"/>Ἑ<unclear reason="undefined" cert="high">λλη</unclear>νικοῖς ἀκο<supplied reason="lost" cert="high">λού</supplied>θως τῇ θείᾳ <app type="BL"><lem resp="5.143"><unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">ελε</supplied><unclear reason="undefined" cert="high">ύσ</unclear><supplied reason="lost" cert="high">ει</supplied></lem><rdg>ν<supplied reason="lost" cert="high">ομοθεσίᾳ</supplied></rdg></app>[ τοῦ κυρίου ἡμῶν <app type="BL"><lem resp="8.462">Αὐτοκράτορος</lem><rdg>Αὐτοκράτορος Καίσαρο?</rdg></app> Μάρκου Αὐρηλίου] 

    <lb n="14"/>Σεουήρου Ἀλεξάνδ<unclear reason="undefined" cert="high">ρ</unclear><supplied reason="lost" cert="high">ο</supplied>υ Εὐσεβοῦς Εὐ<supplied reason="lost" cert="high">τ</supplied>υχ[οῦς Σεβαστοῦ ἐφ᾽ ὑπογραφῇ μαρτύρων ἓξ <num value="6"/> <note lang="en" anchored="yes" place="not specified">?</note> ὧν τὰ ὀνόματα] 

    <lb n="15"/>καὶ οἱ εἰκονισμοὶ ἑξῆς δηλοῦνται <hi rend="diaeresis">ἰ</hi>δίοι<supplied reason="lost" cert="high">ς αὐτῶν γράμμασι καὶ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἐπερωτη</supplied>-

    <lb n="16"/>θεὶς ὡμολόγησα. ἡ διαθήκη κυρία. <handShift new="m2"/> Αὐρή[λιος <gap reason="illegible" extent="unknown" unit="character"/> <app type="BL"><lem resp="5.143">διεθέμην] 

    <lb n="17"/>ὡς</lem><rdg><supplied reason="lost" cert="high">μαρτυρῶ τῇδε τῇ διαθήκῃ</supplied> ὡ?</rdg></app> πρόκιται. <handShift new="m3"/> Α<supplied reason="lost" cert="high">ὐρ</supplied>ήλιο<supplied reason="lost" cert="high">ς</supplied> Ἡράκλιος Ἰσιδώρο<unclear reason="undefined" cert="high">υ</unclear> [ <gap reason="illegible" extent="unknown" unit="character"/> <app type="BL"><lem resp="5.143">μαρτυρῶ] 

    <lb n="18"/><unclear reason="undefined" cert="high">τῇ</unclear> <supplied reason="lost" cert="high">διαθήκ</supplied>ῃ <gap reason="lost" extent="6" unit="character"/><gap reason="illegible" extent="3" unit="character"/>φ<gap reason="illegible" extent="4" unit="character"/>[ <gap reason="illegible" extent="unknown" unit="character"/> <handShift new="m4"/> <gap reason="illegible" extent="unknown" unit="character"/> ἀπὸ] 

    <lb n="19"/><unclear reason="undefined" cert="high">τ</unclear>ῆς αὐτῆ<supplied reason="lost" cert="high">ς πόλεως</supplied></lem><rdg><supplied reason="lost" cert="high">μαρτυρῶ τῇδε</supplied> τῇ <supplied reason="lost" cert="high">δι</supplied>α<supplied reason="lost" cert="high">θηκασ</supplied>φα<unclear reason="undefined" cert="high">λ</unclear><supplied reason="lost" cert="high">είᾳ </supplied><gap reason="lost" extent="unknown" unit="character"/> Α<supplied reason="lost" cert="high">ὐρήλιος</supplied></rdg></app><gap reason="lost" extent="unknown" unit="character"/> μαρτυρῶ <supplied reason="lost" cert="high">τῇδε τῇ διαθήκῃ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/>[ <gap reason="illegible" extent="23" unit="character"/> <handShift new="m5"/> <gap reason="illegible" extent="unknown" unit="character"/> ]<unclear reason="undefined" cert="high">ην</unclear>ας μ<supplied reason="lost" cert="high">αρτυρῶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

