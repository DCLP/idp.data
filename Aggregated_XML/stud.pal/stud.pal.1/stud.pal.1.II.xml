    <lb n="1"/>Φ<supplied cert="high" reason="lost">λα</supplied>υ[ίῳ <gap extent="5" reason="illegible" unit="character"/> τῷ <expan>διαση]μ<ex cert="high">οτάτῳ</ex></expan> <gap extent="3" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/><expan>[Αὐρ<ex cert="high">ηλία</ex>]</expan> Δ<supplied cert="high" reason="lost">ημη</supplied>τρία Π<supplied cert="high" reason="lost">ολυδ</supplied>εύκους ἐντυγχάνο<supplied cert="high" reason="lost">υσα διὰ τοῦ δεῖνος</supplied> 

    <lb n="3"/>εὐτυχῆ<supplied cert="high" reason="lost">σ</supplied>αι· ὡς τὸ πρῶτον <supplied cert="high" reason="lost">ἐ</supplied>δεήθην αἱ τῆς τάξ<supplied cert="high" reason="lost">εως </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>ἐπὶ τοὺς κοινοὺς δικαστὰ<supplied cert="high" reason="lost">ς</supplied> ἀνα<supplied cert="high" reason="lost">π</supplied>έμπεσθαι εἰ κειμεν<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>ων ἐν πάσῃ μὲν γὰρ καὶ γυμνα<supplied cert="high" reason="lost">σι</supplied>αρχίᾳ καὶ τοῖς ἄλλοις ἑξῆ<supplied cert="high" reason="lost">ς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/>αὐτοῦ καὶ μήτε τοῦ πατρὸς μήτε μήτε τῆς θυγατρὸς λα<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/>ἡνίκα <supplied cert="high" reason="lost">Πολ</supplied>υδεύκης ἐτελεύτησεν τὸ τηνικαῦτα οἱ ἀ<supplied cert="high" reason="lost">ντίδικοι </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>ἀνθιστάμ<supplied cert="high" reason="lost">ενοι</supplied> κατέχουσιν τῶν ὑπαρχόντων τὰ βέλ<supplied cert="high" reason="lost">τιστα </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>τῶν ἐπ<supplied cert="high" reason="lost">ὶ τοὺς π</supplied>αραγγει<supplied cert="high" reason="lost">λα</supplied>μένο<supplied cert="high" reason="lost">υς</supplied> τὴν κρίσιν τὴν π<supplied cert="high" reason="lost">ερὶ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/><gap extent="10" reason="lost" unit="character"/>ηναι ἐφ᾽ οὗ διέξ<supplied cert="high" reason="lost">ομ</supplied>εν μήτε νομῆς δικ<supplied cert="high" reason="lost">αίας </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/><gap extent="10" reason="lost" unit="character"/>ντα νῦν δὲ τὸ <supplied cert="high" reason="lost">γ</supplied>ύναιον ἀναγκάζεται <gap extent="unknown" reason="lost" unit="character"/> 

