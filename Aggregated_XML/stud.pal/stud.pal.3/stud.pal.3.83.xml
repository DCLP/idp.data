    <lb n="4"/><gap extent="unknown" reason="lost" unit="character"/>α τρία <num value="3"/> ὀθώνια ἀνδ<unclear cert="high" reason="undefined">ρι</unclear>κὰ ἔμπλουμα τέσσαρα <num value="4"/> ὑποκαμάσια πέντ<unclear cert="high" reason="undefined">ε</unclear> <num value="5"/> <unclear cert="high" reason="undefined">σά</unclear>βανα δ<supplied cert="high" reason="lost">ύο</supplied> <num value="2"/> στιχάρια 

    <lb n="5"/><gap extent="unknown" reason="lost" unit="character"/>μαλλ<supplied cert="high" reason="lost">ω</supplied>τὰ δύο <num value="2"/> ἓν <num value="1"/> καμίσιν καὶ ἓν <num value="1"/> ὀθώνιν θυμιατήριν ἓν <num value="1"/>, εἰς <choice><corr>πλῆρες</corr><sic>πληρης</sic></choice> καὶ εἰς <expan>ὁλόκληρ<ex cert="high">ον</ex></expan> 


