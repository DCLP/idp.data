    <lb n="10"/>τῷ <num value="7">ζ</num> <expan><ex cert="high">ἔτει</ex></expan> Νέρων<supplied cert="high" reason="lost">ο</supplied>ς <expan>προσαπογράφομ<ex cert="high">αι</ex></expan> 

    <lb n="11"/>ὃ <supplied cert="high" reason="lost">ἐ</supplied>κτ<supplied cert="high" reason="lost">η</supplied>σάμην <supplied cert="high" reason="lost">ἐν τ</supplied>ῇ αὐτῇ κώμηι 

    <lb n="12"/>ἥμισυ μ<supplied cert="high" reason="lost">έ</supplied>ρος οἰ<supplied cert="high" reason="lost">κίας </supplied><choice><corr><supplied cert="high" reason="lost">τ</supplied>ιμῆς</corr><sic><supplied cert="high" reason="lost">τ</supplied>ειμης</sic></choice> <expan>ἀργυ<ex cert="high">ρίου</ex></expan> <expan><ex cert="high">δραχμῶν</ex></expan> 

    <lb n="13"/>ἑκατὸν τεσσαρ<supplied cert="high" reason="lost">άκ</supplied>οντα <num value="140"/> παρὰ Ἐριέως 

    <lb n="14"/>τοῦ <supplied cert="high" reason="lost">Ὀ</supplied>ννώφριο<supplied cert="high" reason="lost">ς το</supplied>ῦ Ἁρπαγάθου ἱε-

    <lb n="15"/>ρέως ἀπὸ τῆς α<supplied cert="high" reason="lost">ὐ</supplied>τῆς κώμης. διὸ 

    <lb n="16"/>ἐπ<supplied cert="high" reason="lost">ι</supplied>δίδωμι τὴν <supplied cert="high" reason="lost">πρ</supplied>οσαπογραφήν. 

