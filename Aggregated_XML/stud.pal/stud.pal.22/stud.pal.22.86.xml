    <lb n="3"/><handShift new="m1"/> Σωκράτῃ <expan>γεγ<ex cert="high">υμνασιαρχηκότι</ex></expan> <expan>κ<ex cert="high">αὶ</ex></expan> <expan>Ἀντιπ<ex cert="high">άτρῳ</ex></expan> 

