    <lb n="1"/><handShift new="m2"/> Ἀπολλωνίωι. 

    <lb n="2"/>ἐξαποστεῖλαι. <expan><ex cert="high">ἔτους</ex></expan> <num value="36">λϛ</num> Τῦβι <num value="4">δ</num>. 

    <lb n="3"/><handShift new="m1"/> βασιλεῖ Πτολεμαίωι καὶ βασιλίσσ<unclear reason="undefined" cert="high">ηι</unclear> 

    <lb n="4"/>Κλεοπάτραι τῆι ἀδελφῆι καὶ βασιλί<supplied reason="lost" cert="high">σσηι</supplied> 

    <lb n="5"/>Κλεοπάτραι τῆι γυναικὶ θεοῖς Εὐερ-

    <lb n="6"/>γέταις χαίρειν 

    <lb n="7"/>Πετεσοῦχος Παλοῦτος βασιλ<supplied reason="lost" cert="high">ικὸς</supplied> 

    <lb n="8"/>γεωργὸς τῶν ἀπὸ Σεβεννύτ<supplied reason="lost" cert="high">ου</supplied> 

    <lb n="9"/>τῆς Ἡρακλείδου μερίδος τοῦ 

    <lb n="10"/>Ἀρσινοίτου νομοῦ. ἀδικοῦμαι 

    <lb n="11"/>ὑπὸ Ἑρχίμιος τοῦ Ἁρυώτου <unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">αὶ</supplied> 

    <lb n="12"/><subst><add place="inline">Φανείθου</add><del rend="corrected" status="unremarkable">φανιθου</del></subst> τοῦ υἱοῦ τῶν <unclear reason="undefined" cert="high">ἐ</unclear>κ <unclear reason="undefined" cert="high">Τ</unclear><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="3" unit="character"/> 

    <lb n="13"/>τοῦ Ἡρακλεοπολίτου.  ὑπαρχ<unclear reason="undefined" cert="high">όν</unclear><supplied reason="lost" cert="high">των</supplied> 

    <lb n="14"/>γάρ μοι ἐν τῆι <unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">ὐ</supplied><unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ῆι</supplied> <unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">ώ</supplied><unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">ηι Τ</supplied><gap reason="lost" extent="5" unit="character"/> 

    <lb n="15"/>πατρικῆς οἰκί<unclear reason="undefined" cert="high">ας</unclear> <unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">αὶ τῶν προσ</supplied>-

    <lb n="16"/>όντ<unclear reason="undefined" cert="high">ω</unclear>ν τό<unclear reason="undefined" cert="high">πων</unclear> κατα<unclear reason="undefined" cert="high">φ</unclear><supplied reason="lost" cert="high">ρ</supplied><unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">ν</supplied><unclear reason="undefined" cert="high">ήσ</unclear><supplied reason="lost" cert="high">ας μου</supplied> 

    <lb n="17"/>τῶι κεχωρίσθαι με δ<unclear reason="undefined" cert="high">ια</unclear>πεπ<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/>ταῦτα καὶ τὴν καθαίρε<unclear reason="undefined" cert="high">σιν</unclear> <supplied reason="lost" cert="high">τῆς</supplied> 

    <lb n="19"/>οἰκίας ἀπενηνεγμένο<unclear reason="undefined" cert="high">ς</unclear> <gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">ον</unclear><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/><choice><corr>ὑπερῷον</corr><sic>υπερων</sic></choice>, καὶ διακωλυθεὶς <unclear reason="undefined" cert="high">ὑπ</unclear>᾽ ἐμ<supplied reason="lost" cert="high">οῦ</supplied> 

    <lb n="21"/>Σοκμῆνις Πασίωνος οἰκοδομ<unclear reason="undefined" cert="high">ῆ</unclear><supplied reason="lost" cert="high">σαι</supplied> 

    <lb n="22"/>ἐν οἷς ἐώνητο ἀπὸ τούτων το<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="23"/>οὐ προσέσχεν. διὸ τὴν ἐφ᾽ ὑμᾶς κ<supplied reason="lost" cert="high">ατα</supplied>-

    <lb n="24"/>φυγὴν <choice><corr>πεποιημένος</corr><sic>πεποημενος</sic></choice> δέομαι ὑ<unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">ῶν</supplied> 

    <lb n="25"/>εἰ δοκεῖ προστάξαι χρηματίσαι <unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">ου</supplied> 

    <lb n="26"/>τὴν ἔντευξιν ἐπὶ Πολέμαρ<unclear reason="undefined" cert="high">χ</unclear><supplied reason="lost" cert="high">ον</supplied> 

    <lb n="27"/>τὸν στρατηγόν, ὅπως μετα<unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">εμ</supplied>-

    <lb n="28"/>ψάμενος αὐτούς, ἐὰν ᾖ οἷα γράφω, 

