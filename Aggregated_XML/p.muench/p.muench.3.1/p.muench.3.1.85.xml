    <lb n="3"/>οὖσι αὐτ<unclear cert="high" reason="undefined">ῶ</unclear><supplied cert="high" reason="lost">ν ὁρ</supplied><unclear cert="high" reason="undefined">ί</unclear>οις, ε<supplied cert="high" reason="lost">ἰσό</supplied><unclear cert="high" reason="undefined">δ</unclear>ο<unclear cert="high" reason="undefined">ι</unclear>ς καὶ ἐξόδοις καὶ ποτιστρέαις <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">αὶ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>δων ὁμοί<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">ς ε</supplied>ἰσόδοι<supplied cert="high" reason="lost">ς κ</supplied><unclear cert="high" reason="undefined">αὶ</unclear> ἐξόδοις καὶ <choice><corr>φωσφορίαις</corr><sic>φοσφοριαις</sic></choice> καὶ <unclear cert="high" reason="undefined">θ</unclear><supplied cert="high" reason="lost">εμελίοις </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> γείτονες, καθὼς ἐκ συμφώνου ὑπη</supplied>-

    <lb n="5"/>γόρευσαν, τ<unclear cert="high" reason="undefined">ῶν</unclear> μὲν <gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/> Μάρωνος ὅλων ἀρουρῶν <gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Ἀπολ</supplied>-

    <lb n="6"/>λωνίου κ<unclear cert="high" reason="undefined">λ</unclear><supplied cert="high" reason="lost">ῆρ</supplied><unclear cert="high" reason="undefined">ος</unclear> ἀπηλ<supplied cert="high" reason="lost">ι</supplied><unclear cert="high" reason="undefined">ώ</unclear>του πρότερον Ἰσιδώρου καὶ τῶν <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/>πρ<gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ο<gap extent="1" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/> Μάρ<supplied cert="high" reason="lost">ω</supplied>νος κλῆρος ἀπηλιώτου χέρσος <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>ἀπηλιώτου ὑδραγω<supplied cert="high" reason="lost">γ</supplied><unclear cert="high" reason="undefined">ὸ</unclear>ς λιβὸ<supplied cert="high" reason="lost">ς</supplied> ἐλαιὼν πρότερον Τασου<gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> καὶ τῶν</supplied> 

    <lb n="9"/>μὲν ἐν τῇ <unclear cert="high" reason="undefined">κώμ</unclear>ῃ οἰκ<supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">π</unclear>έδων τῆς μὲν πρὸς τῷ ἱερῷ <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/>ἱερέων οἰ<unclear cert="high" reason="undefined">κό</unclear>πεδα <unclear cert="high" reason="undefined">ἐ</unclear>κ <unclear cert="high" reason="undefined">τ</unclear>ῆς πρότερον Ἱερανούπεως οἰκ<supplied cert="high" reason="lost">ίας </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/>οἰκόπεδα ἐκ τῆς πρότερον Κάπειτος οἰκίας καὶ <expan>ἀδελ<ex cert="high">φῶν</ex></expan> νότο<supplied cert="high" reason="lost">υ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> καὶ</supplied> 

    <lb n="12"/>ἑτέρων <choice><corr>ψιλ<supplied cert="high" reason="lost">ος</supplied></corr><sic>ψειλ<supplied cert="high" reason="lost">ος</supplied></sic></choice> τόπος, τοῦ δὲ θησαυροῦ καὶ ὅλων τόπων <unclear cert="high" reason="undefined">ν</unclear>ό<unclear cert="high" reason="undefined">τ</unclear>ου <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/>δὲ περὶ Ἡφαιστιάδαν φοινικῶνος ἀρουρῶν τριῶν <num value="3"/> νότου <unclear cert="high" reason="undefined">καὶ</unclear> βορρᾶ <unclear cert="high" reason="undefined">λ</unclear><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τὴν</supplied> 

    <lb n="14"/>δὲ ἔκστασιν ταύτην πεποιῆσθαι τὸν ὁμολογοῦντα Διογένην τῇ θυγατρὶ κο<unclear cert="high" reason="undefined">ρ</unclear><gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="15"/>ὁ ὁμολογῶν Διογένης ἀντὶ μὲν τῶν κατοικικῶν πάντων Καλ<unclear cert="high" reason="undefined">λετι</unclear> μὲν δανιστῇ δ<unclear cert="high" reason="undefined">ρ</unclear>α<supplied cert="high" reason="lost">χμ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="16"/>δ<unclear cert="high" reason="undefined">ὲ</unclear> ἑ<unclear cert="high" reason="undefined">κ</unclear>τολογουμένων Νουμηνίῳ γεγυμνασιαρχηκότι δραχμὰς τετρακοσίας <num value="400"/> <gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="17"/>δραχμὰς τετρακοσίας <num value="400"/> τῶν ἐπὶ τὸ αὐτὸ ἀργυρίου δραχμῶν <choice><corr>χιλίων</corr><sic>χειλιων</sic></choice> ἑπτακοσίων <num value="1700"/> <gap extent="2" reason="illegible" unit="character"/>τ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="18"/>ὁ ὁμολογῶν Διογένης διὰ χειρὸς ἀργυρίου δραχμῶν τριακοσίων <num value="300"/>, ἔτι δὲ καὶ ἐπ<unclear cert="high" reason="undefined">ά</unclear>ναγκον <unclear cert="high" reason="undefined">σ</unclear>χ<unclear cert="high" reason="undefined">εῖν</unclear> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="19"/>Καίσαρος τοῦ κυρίου ἐφ᾽ ὃν περίεστιν χρόνον κατ᾽ ἔτος πυροῦ ἀρτάβας ἐννέα <num value="9"/> κα<unclear cert="high" reason="undefined">ὶ</unclear> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="20"/>δεκαπέντε <num value="15"/> καὶ ἐλαιῶν <choice><corr>ἐκλεκτῆς</corr><sic>εγλεκτης</sic></choice> ἀ<supplied cert="high" reason="lost">ρτ</supplied>άβης μιᾶς <num value="1"/> καὶ φοίνικος ξηροῦ πατητοῦ ἀρτα<supplied cert="high" reason="lost">β </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="21"/>καὶ συκῶν μέτρον ἕν <num value="1"/>, πάντα μέτρῳ Β<supplied cert="high" reason="lost">α</supplied>κχιάδος, ἐπὶ μὲν τοῦ πυροῦ μέτρῳ <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="22"/>καὶ τοὺς παρ᾽ αὐτοῦ τῇ Θαισαρίῳ κα<supplied cert="high" reason="lost">ὶ τοῖ</supplied>ς παρ᾽ αὐτῆς <choice><corr>τὰ<supplied cert="high" reason="omitted">ς</supplied></corr><sic>τα</sic></choice> ἐξεσταμένας αὐτ<unclear cert="high" reason="undefined">ῇ</unclear> <gap extent="unknown" reason="lost" unit="character"/> </ab></div><div n="meta.28889" org="uniform" part="N" sample="complete" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.28889" org="uniform" part="N" sample="complete" subtype="textDate" type="commentary"><p><date notAfter="150" notBefore="101" type="textDate">1. Hälfte II</date></p></div><div n="meta.28889" org="uniform" part="N" sample="complete" subtype="general" type="commentary"><p>Rückseite: P.Münch. III  144. </p></div><div n="meta.28889" org="uniform" part="N" sample="complete" subtype="principalEdition" type="bibliography"><listBibl default="NO"><bibl default="NO" subtype="principal" type="publication"><title level="s" type="abbreviated">P.Münch.</title><biblScope type="volume">3</biblScope><biblScope type="numbers">85</biblScope></bibl><bibl default="NO" type="Trismegistos"><biblScope type="numbers">28889</biblScope></bibl><bibl default="NO" type="DDbDP"><series>0171</series>; <biblScope type="volume">3.1</biblScope>; <biblScope type="numbers">85</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0171;3.1;85" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.28889" org="uniform" part="N" sample="complete" subtype="corrections" type="bibliography"><head>BL-Einträge nach BL-Konkordanz</head><bibl default="NO" type="BL"><biblScope type="volume">IX</biblScope>, S. <biblScope type="pages">172</biblScope></bibl></div><div n="meta.28889" org="uniform" part="N" sample="complete" subtype="illustrations" type="bibliography"><p><bibl default="NO" type="illustration">Abb. 32</bibl></p></div><div n="meta.28889" org="uniform" part="N