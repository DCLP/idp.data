    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/>αθ<gap reason="lost" extent="unknown" unit="character"/> ἔγραψα οὖν σοι τὰ κατὰ τὸν ἀγρὸν 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/> ὡσαύτως δὲ καὶ ἡ <app type="editorial"><lem>ζέα</lem><rdg>ζεη</rdg></app>, ἡ δὲ κριθὴ ἐπλήγη 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/>νον κα<supplied reason="lost" cert="high">θ᾽</supplied> ἡμέραν ἀποκαταστήν με 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/>μεντα<gap reason="illegible" extent="1" unit="character"/> διατρίψας νύκτα καὶ ἡμέραν 

    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/>καμεν ἵνα ἦι πρὸς τὴν ἀνακάθαρσιν 

    <lb n="6"/><app type="editorial"><lem><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> στόμα</supplied>τι <note lang="en" anchored="yes" place="not specified">?</note></lem><rdg><gap reason="lost" extent="unknown" unit="character"/>γι</rdg></app> τῆς ἐγβατηρίας. καὶ αὐτοὶ δὲ ἐρχόμεθα 

    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/> συνεστήσαμεν δὲ καὶ <app type="editorial"><lem>Σοκέα</lem><rdg>Σοκθ<gap reason="illegible" extent="2" unit="character"/></rdg></app> αὐτῶι ἵνα <app type="editorial"><lem>ἦι μεθ᾽ αὐ<supplied reason="lost" cert="high">τοῦ</supplied></lem><rdg>ἤιμεθα</rdg></app> 

    <lb n="8"/><gap reason="lost" extent="unknown" unit="character"/> τὸ <choice><corr>ἥμισυ</corr><sic>ημυσυ</sic></choice> τοῦ <app type="editorial"><lem><unclear reason="undefined" cert="high">σ</unclear>τόματος</lem><rdg><gap reason="illegible" extent="1" unit="character"/>τοματος</rdg></app> τὸ ἐπὶ τῆς ἐγβατηρίας 

    <lb n="9"/><gap reason="lost" extent="unknown" unit="character"/>δωρειν. ἡμεῖ<supplied reason="lost" cert="high">ς</supplied> δὲ ἰδ<supplied reason="lost" cert="high">ό</supplied>ντες τὴν κακίαν τῶν 

    <lb n="10"/><gap reason="lost" extent="unknown" unit="character"/> ἡμῶν ἵνα ἀναγγ<add place="supralinear">ε</add>ιλ<gap reason="lost" extent="unknown" unit="character"/>ω<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><app type="editorial"><lem><gap reason="lost" extent="unknown" unit="character"/> ἐξε<supplied reason="lost" cert="high">λ</supplied>θεῖν ἵνα ἀποδιελώμ<supplied reason="lost" cert="high">ε</supplied>θα <gap reason="lost" extent="unknown" unit="character"/></lem><rdg/></app> 

    <lb n="12"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> μ</supplied>έχρι τοῦ ὕδωρ ἐρραγῆ<add place="supralinear">ναι</add> κα<gap reason="lost" extent="unknown" unit="character"/>τε<gap reason="lost" extent="unknown" unit="character"/> 

