    <lb n="17"/>ἐπὶ η<gap extent="unknown" reason="lost" unit="character"/>αυτ<gap extent="1" reason="illegible" unit="character"/>ος<gap extent="3" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ὑ</supplied>-

    <lb n="18"/><app type="editorial"><lem>πὲρ</lem><rdg>περὶ</rdg></app> ὧν ἀνήλωκεν Δονομάζει <app type="editorial"><lem>Θεοδοτ<unclear cert="high" reason="undefined">ω</unclear>[ <note lang="en" place="not specified">?</note><gap extent="unknown" reason="illegible" unit="character"/> ]</lem><rdg>εσ<gap extent="1" reason="illegible" unit="character"/>ατο<gap extent="unknown" reason="lost" unit="character"/></rdg></app> 

    <lb n="19"/>καὶ ἀπέχειν τὸν λόγον Ἱστιαῖος παρὰ Δ<supplied cert="high" reason="lost">ονομάζεως </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="20"/>τῶν τριακοσίων τριάκοντα <num value="330"/> δραχμῶν <app type="BL"><lem resp="1.377"><supplied cert="high" reason="lost">καὶ μὴ ἐξεῖναι</supplied></lem><rdg><gap extent="unknown" reason="lost" unit="character"/></rdg></app> 

    <lb n="21"/>Δονομάζει μήδε Ἱστιαίωι περὶ τῶν χρ[ημάτων <app type="BL"><lem resp="1.377">ἀλλήλοις</lem><rdg><supplied cert="high" reason="lost">ἔξηι </supplied><gap extent="unknown" reason="lost" unit="character"/></rdg></app>] 

    <lb n="22"/>ἐπελθεῖν, ἐπιόντι <app type="BL"><lem resp="1.377"><supplied cert="high" reason="lost">δὲ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐὰν δέ τις</supplied></lem><rdg><supplied cert="high" reason="lost">δὲ </supplied><gap extent="unknown" reason="lost" unit="character"/></rdg></app> 

    <lb n="23"/>παρασυγγραφήσηι καὶ μὴ ἐμμείνηι <app type="editorial"><lem>τ<supplied cert="high" reason="lost">ῆι ὁμολογίαι</supplied></lem><rdg>τ<gap extent="unknown" reason="lost" unit="character"/></rdg></app> 

    <lb n="24"/><app type="editorial"><lem>ἥ τε ἔφοδος</lem><rdg><gap extent="1" reason="illegible" unit="character"/> ἐπέφοδος</rdg></app> αὐτῶι ἄκυρος ἔστω καὶ <app type="editorial"><lem>προσ<supplied cert="high" reason="lost">αποτισάτω</supplied></lem><rdg>προς<gap extent="unknown" reason="lost" unit="character"/></rdg></app> 

    <lb n="25"/>ὁ ἐπιπορευ<supplied cert="high" reason="lost">όμενος </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="26"/><unclear cert="high" reason="undefined">Διόνυσος</unclear> <gap extent="5" reason="lost" unit="character"/> καὶ <app type="editorial"><lem>μηθὲν ἧττον ἡ <supplied cert="high" reason="lost">συγγραφὴ κυρία</supplied></lem><rdg><gap extent="1" reason="illegible" unit="character"/>η<gap extent="3" reason="illegible" unit="character"/>ρητη<gap extent="1" reason="illegible" unit="character"/>η</rdg></app> 

    <lb n="27"/>ἔστω. μάρτυρες <supplied cert="high" reason="lost">Στ</supplied>ρατωνίδης Χα<supplied cert="high" reason="lost">λκηδόνιος,</supplied> 

    <lb n="28"/>Φιλοκύδ<supplied cert="low" reason="lost">ης</supplied><supplied cert="high" reason="lost"> Ἀ</supplied>μφιπολίτης, Νικολάο<supplied cert="high" reason="lost">ς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

