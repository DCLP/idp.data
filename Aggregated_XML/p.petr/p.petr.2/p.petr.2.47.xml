    <lb n="10"/>εἰς τὸ τοῦ βασιλέως <supplied cert="high" reason="lost">ὄνομα</supplied> υρ<gap extent="3" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/>Ἱστιαίου ἐν τῶι τ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="12"/>ἐνεκάλεσεν Δονο<supplied cert="high" reason="lost">μάζις</supplied> Ἱστιαίωι <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/>ε<gap extent="7" reason="lost" unit="character"/>εως εἰσιν <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="14"/><gap extent="unknown" reason="lost" unit="character"/>ασωι τῶν <gap extent="unknown" reason="lost" unit="character"/> 

