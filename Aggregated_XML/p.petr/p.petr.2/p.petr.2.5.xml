    <lb n="1"/>οπ<gap extent="unknown" reason="lost" unit="character"/> τίνες α<gap extent="1" reason="lost" unit="character"/>ειοι<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>σκευοφυλάκιον καθότι καὶ <app type="editorial"><lem>προ<gap extent="unknown" reason="lost" unit="character"/></lem><rdg>οὐκ <gap extent="unknown" reason="lost" unit="character"/></rdg></app> 

    <lb n="3"/>οῦντα εἰς φυλακὴν τίθεσθαι. ἡμεῖς δὲ καθ᾽ ἡμέρ<supplied cert="high" reason="lost">αν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>ἐργάζονται ἀναδώσομεν τοῖς πιστοτάτοις ε<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>ρα ἀπὸ τῶν ἔργων ἀναχωρῶσιν πάλιν τῶι <app type="editorial"><lem>σκ<supplied cert="high" reason="lost">ευοφυλακίωι </supplied><gap extent="unknown" reason="lost" unit="character"/></lem><rdg>σι<gap extent="unknown" reason="lost" unit="character"/></rdg></app> 

    <lb n="6"/>δ<gap extent="unknown" reason="lost" unit="character"/> ἵνα μὴ ἐπεὶ ἐν τῶι νομῶι <app type="editorial"><lem>ὠμμένοι εἰσ<supplied cert="high" reason="lost">ὶν </supplied><gap extent="unknown" reason="lost" unit="character"/></lem><rdg>ωμμενθ<gap extent="1" reason="illegible" unit="character"/>εις<gap extent="unknown" reason="lost" unit="character"/></rdg></app> 
</ab></div><div n="B" part="N" type="textpart"><ab part="N">

    <lb n="1"/>ρηκότες αὐτοὺς τὸ αρι<gap extent="unknown" reason="lost" unit="character"/>ως γεν<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>γων ἡμέρας τινὰς καὶ οὐδενὸς αὐτοῖς δαν<supplied cert="high" reason="lost">ε</supplied>ίζο<supplied cert="high" reason="lost">ν</supplied>τ<supplied cert="high" reason="lost">ος </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>ἐκ τ<supplied cert="high" reason="lost">οῦ </supplied><supplied cert="low" reason="lost">ἐρή</supplied>μου εἰθ᾽ ὑπερορίοις τοῦ νομοῦ γινό<foreign lang="la">men</foreign><supplied cert="high" reason="lost">οι </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>ἐφοδίωι χρήσωνται. 

    <lb n="5"/>ἔρρωσο. <expan><ex cert="high">ἔτους</ex></expan> <num value="30">λ</num> Ἁθὺρ <gap extent="2" reason="lost" unit="character"/> 
