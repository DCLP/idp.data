    <lb n="1"/>Αὐρηλίῳ Δημ<supplied reason="lost" cert="high">έᾳ πραι</supplied>ποσίτῳ <num value="15">ιε</num> πάγ<unclear reason="undefined" cert="high">ου</unclear> νο<unclear reason="undefined" cert="high">μοῦ</unclear> 

    <lb n="2"/>Ἑρμοπολείτ<supplied reason="lost" cert="high">ου</supplied> 

    <lb n="3"/><supplied reason="lost" cert="high">παρ</supplied>ὰ Αὐρηλίων <supplied reason="lost" cert="high">Ἁτρῆτος Ἑρ</supplied>μείνου καὶ Κορνηλίου 

    <lb n="4"/><app type="editorial"><lem><supplied reason="lost" cert="high">Στε</supplied>φάνου</lem><rdg><gap reason="lost" extent="1" unit="character"/>φ<unclear reason="undefined" cert="high">αν</unclear>ου</rdg></app> ἀμφ<supplied reason="lost" cert="high">οτέρων</supplied> κωμαρχῶν καὶ Ἰσιδώρο<supplied reason="lost" cert="high">υ</supplied> 

    <lb n="5"/>Ἰβό<hi rend="diaeresis">ι</hi>τος κουαδραρ<supplied reason="lost" cert="high">ίου καὶ</supplied> Τιθοῆτος Παυσίριος 

    <lb n="6"/>εἰρηνάρχου τῶν <supplied reason="lost" cert="high">πάντ</supplied>ων ἀπὸ κώ<unclear reason="undefined" cert="high">μης</unclear> 

    <lb n="7"/>Σεσόγχων τοῦ ὑ<supplied reason="lost" cert="high">πὸ</supplied> σὲ πάγου. ἐπιζητοῦν-

    <lb n="8"/><choice><corr>τί</corr><sic>επειζητ<supplied reason="lost" cert="high">ο</supplied>υ<unclear reason="undefined" cert="high">ν</unclear>τι</sic></choice> σοι τοὺς ὄντας <supplied reason="lost" cert="high">ἐν τ</supplied>ῷ <supplied reason="lost" cert="high">π</supplied>ροειρημένῳ πά<supplied reason="lost" cert="high">γ</supplied>ῳ 

    <lb n="9"/><unclear reason="undefined" cert="high">χ</unclear>οιρεμπόρο<supplied reason="lost" cert="high">υ</supplied>ς κ<supplied reason="lost" cert="high">αὶ ὀρ</supplied>νιθοτρόπον κατὰ 

    <lb n="10"/>ταῦτα ὁμολογοῦ<supplied reason="lost" cert="high">μεν</supplied> ὀμνύντες τὴν θείαν 

    <lb n="11"/>καὶ οὐράνιον τύχ<supplied reason="lost" cert="high">ην</supplied> τῶν δεσποτῶν ἡμῶν 

    <lb n="12"/><unclear reason="undefined" cert="high">Αὐ</unclear><supplied reason="lost" cert="high">γο</supplied>ύστου τε καὶ <unclear reason="undefined" cert="high">Κ</unclear><supplied reason="lost" cert="high">αισάρ</supplied>ων τὸ καθόλον μὴ εἶναι 

    <lb n="13"/>ἐν τῇ ἡμετέρ<unclear reason="undefined" cert="high">α</unclear> κ<supplied reason="lost" cert="high">ώμῃ</supplied> χοιρεμπόρου<supplied reason="lost" cert="high">ς</supplied> καὶ <unclear reason="undefined" cert="high">ὀρνι</unclear>-

    <lb n="14"/>θοτρόπον· εἰ δὲ με<unclear reason="undefined" cert="high">τ</unclear>ὰ ταῦτα φανείημεν ψευσά-

    <lb n="15"/>μενοι ἔνοχοι ἐσ<supplied reason="lost" cert="high">ό</supplied><unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">εθα</supplied> <unclear reason="undefined" cert="high">τῷ</unclear> θείῳ ὅρκῳ καὶ τῷ 

    <lb n="16"/>περὶ <choice><corr>τούτου</corr><sic>τουτω</sic></choice> κι<unclear reason="undefined" cert="high">νδύν</unclear><supplied reason="lost" cert="high">ῳ, καὶ</supplied> <expan>ἐπερωτηθ<ex cert="high">έντες</ex></expan> <expan>ὡμολ<ex cert="high">ογήσαμεν</ex></expan> 

