    <lb n="1"/>Πρώταρχος Ἑρμίαι χ<supplied cert="high" reason="lost">α</supplied>ίρειν. εἰς τὴν γινομένην 

    <lb n="2"/>ἡμῖν σύντ<supplied cert="high" reason="lost">α</supplied>ξ<supplied cert="high" reason="lost">ιν</supplied> <unclear cert="high" reason="undefined">χρ</unclear><supplied cert="high" reason="lost">η</supplied>μάτ<supplied cert="high" reason="lost">ι</supplied>σον Δίσκωι ὥστε 

    <lb n="3"/>τοῖς ἐκ τοῦ <app type="BL"><lem resp="1.155">ἐ<unclear cert="high" reason="undefined">πι</unclear><supplied cert="high" reason="lost">σ</supplied><unclear cert="high" reason="undefined">τολο</unclear><supplied cert="high" reason="lost">γ</supplied>ραφί<supplied cert="high" reason="lost">ο</supplied>υ</lem><rdg>ε<gap extent="2" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/> <supplied cert="high" reason="lost">γ</supplied>ραφί<supplied cert="high" reason="lost">ο</supplied>υ</rdg></app> γραμματεῦσιν 

    <lb n="4"/>ὀψώνιον τοῦ Ἁθὺ<supplied cert="high" reason="lost">ρ</supplied> χ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">λκ</supplied><unclear cert="high" reason="undefined">ο</unclear>ῦ <expan><ex cert="high">δραχμὰς</ex></expan> <num value="3800">Γω</num>, καὶ Ἡρακλείδει 

    <lb n="5"/>τῶι πρὸς τοῖς <app type="BL"><lem resp="1.155"><supplied cert="high" reason="lost">ὑπομνήμ</supplied>ασιν</lem><rdg><supplied cert="high" reason="lost">σ</supplied><unclear cert="high" reason="undefined">υ</unclear><supplied cert="high" reason="lost">ντάγμ</supplied>ασιν</rdg></app>, εἰς <app type="BL"><lem resp="9.324">αὑτὸν</lem><rdg>αὐτὸν</rdg></app> καὶ ὑπογραφέα, 

    <lb n="6"/>χαλκοῦ <expan><ex cert="high">δραχμὰς</ex></expan> <num value="850">ων</num>, <supplied cert="high" reason="lost">καὶ </supplied><gap extent="6" reason="lost" unit="character"/>ωι καὶ Σαραπίωνι τοῖς 

