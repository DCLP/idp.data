    <lb n="2"/>ἐ<unclear cert="high" reason="undefined">πι</unclear>στρατήγωι <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>Ται<unclear cert="high" reason="undefined">σᾶς</unclear> <unclear cert="high" reason="undefined">Ἠ</unclear>λί<unclear cert="high" reason="undefined">ου</unclear> <gap extent="4" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

