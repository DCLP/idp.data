    <lb n="15"/><handShift new="m2"/> ὠνὴ Πετεαρσεμθεὺς 

    <lb n="16"/>Πανοβχ<unclear cert="high" reason="undefined">ούνιος</unclear> 

    <lb n="17"/>γῆ <expan>σφρα<ex cert="high">γίδων</ex></expan> <num value="4">δ</num>, <choice><corr>ἣν</corr><sic>ον</sic></choice> <expan>ἠγόρα<ex cert="high">σεν</ex></expan> 

    <lb n="18"/>παρὰ Πετεαρσεμθέως. </ab></div></div><div n="meta.106" org="uniform" part="N" sample="complete" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.106" org="uniform" part="N" sample="complete" subtype="textDate" type="commentary"><p><date notAfter="-99" notBefore="-99" type="textDate" value="-0099-08-07">7. Aug. 99 v.Chr.</date></p></div><div n="meta.106" org="uniform" part="N" sample="complete" subtype="general" type="commentary"><p>Rückseite: Demotisch.</p></div><div n="meta.106" org="uniform" part="N" sample="complete" subtype="mentionedDates" type="commentary"><head>Erwähnte Daten</head><p>Z. 5: <date type="mentioned" value="-0099-06-16">16. Juni 99 v.Chr.</date></p></div><div n="meta.106" org="uniform" part="N" sample="complete" subtype="principalEdition" type="bibliography"><listBibl default="NO"><bibl default="NO" subtype="principal" type="publication"><title level="s" type="abbreviated">P.Strasb.</title><biblScope type="volume">2</biblScope><biblScope type="numbers">89</biblScope></bibl><bibl default="NO" type="Trismegistos"><biblScope type="numbers">106</biblScope><xref evaluate="all" from="ROOT" href="106" targOrder="Y" to="DITTO" type="DAHT">Text in DAHT</xref></bibl><bibl default="NO" type="DDbDP"><series>0205</series>; <biblScope type="volume">2</biblScope>; <biblScope type="numbers">89</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0205;2;89" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.106" org="uniform" part="N" sample="complete" subtype="corrections" type="bibliography"><head>BL-Einträge nach BL-Konkordanz</head><bibl default="NO" type="BL"><biblScope type="volume">VIII</biblScope>, S. <biblScope type="pages">414</biblScope></bibl></div><div n="meta.106" org="uniform" part="N" sample="complete" subtype="illustrations" type="bibliography"><p>keine</p></div><div n="meta.106" org="uniform" part="N" sample="complete" subty