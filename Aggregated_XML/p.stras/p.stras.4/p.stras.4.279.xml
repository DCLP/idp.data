    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><unclear reason="undefined" cert="high">ε</unclear><supplied reason="lost" cert="high">ὐ</supplied>τεκνίᾳ μὴ μήνυσα<unclear reason="undefined" cert="high">ς</unclear> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἐγρ</supplied>αψάμη<unclear reason="undefined" cert="high">ν</unclear> <gap reason="illegible" extent="3" unit="character"/><gap reason="lost" extent="7" unit="character"/><gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">ο</unclear>υ ἠγαπημέ<unclear reason="undefined" cert="high">νου</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/>ειδ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/>τρ<gap reason="illegible" extent="3" unit="character"/>μενο<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><gap reason="illegible" extent="4" unit="character"/>λ<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="2" unit="character"/> <unclear reason="undefined" cert="high">ἐα</unclear>θῆναι ἄχρι καιρο<supplied reason="lost" cert="high">ῦ</supplied>. καταξιωσάτω τοίνυν ἡ <hi rend="diaeresis">ὑ</hi>μετέρα εὐδοκίμησις ἀκροάσ<unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">σθαι</supplied> 

    <lb n="6"/><unclear reason="undefined" cert="high">τῶν</unclear> <supplied reason="lost" cert="high">λ</supplied><unclear reason="undefined" cert="high">εγ</unclear>ο<unclear reason="undefined" cert="high">μέ</unclear>νων παρὰ τοῦ γραμματη<unclear reason="undefined" cert="high">φ</unclear><supplied reason="lost" cert="high">ό</supplied>ρου μισθωτοῦ τοῦ ἁγίου <gap reason="lost" extent="12" unit="character"/> παθόντος οὐ μ<gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>παρὰ τῶν ποιμένων τοῦ λαμπροτάτ<supplied reason="lost" cert="high">ου κ</supplied>αὶ ψευδοτυρ<supplied reason="lost" cert="high">άν</supplied>νου ἐ<unclear reason="undefined" cert="high">ν</unclear>ωχ<gap reason="lost" extent="10" unit="character"/> καὶ ἀκριβῶς 

    <lb n="8"/><unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">α</supplied>ῦτα καταλαβεῖν ὀφείλοντα ἐαθῆναι ἕως τοῦ καιροῦ τῆς γυμν<unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">σ</supplied>ίας <unclear reason="undefined" cert="high">αὐτ</unclear>ῶν οὐδὲν π<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><unclear reason="undefined" cert="high">κατ</unclear>έχων τὸν χρόνον αὐτοῦ τότε μεῖζον πάντων. προσκυνῶ καὶ ἀσπάζομαι τὴν <hi rend="diaeresis">ὑ</hi>μετέ<supplied reason="lost" cert="high">ραν</supplied> 

    <lb n="10"/><choice><corr>ποθεινότητα</corr><sic>π<unclear reason="undefined" cert="high">οθ</unclear>ινοτητα</sic></choice> μετὰ πάντων τῶν σὺν αὐτῇ φιλτάτων, ἐξαιρέτως δὲ τὴν εὐγενεστάτην αὐτ<unclear reason="undefined" cert="high">ῆ</unclear><supplied reason="lost" cert="high">ς</supplied> 

    <lb n="11"/><unclear reason="undefined" cert="high">ἀδε</unclear>λφὴν μετὰ τῷ θαυμασιωτάτῳ αὐτῆς ἀνδρὶ καὶ εὔχομαι <hi rend="diaeresis">ὑ</hi>μῖν τὰ β<unclear reason="undefined" cert="high">έ</unclear>λτιστα καὶ ἐξ ἐμοῦ 

    <lb n="12"/>προσκυνήσετε τὸν θεοσεβέστατον καὶ μακρόγηρον κοινὸν πατέρα τὸν ἄββα Χαρίσιον 

    <lb n="13"/>οὐ μὴν δὲ ἀλλὰ καὶ τοὺς λαμπροτάτους νυμφίους μετὰ τῶν εὐγενεστάτων αὐτῶν νυμφῶν. <g type="stauros"/> </ab></div><div n="meta.36068" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.36068" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" notBefore="501" notAfter="600">VI</date></p></div><div n="meta.36068" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Zum Ort vgl. BL XI, S. 255.</p></div><div n="meta.36068" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">P.Strasb.</title><biblScope type="volume">4</biblScope><biblScope type="numbers">279</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">36068</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0205</series>; <biblScope type="volume">4</biblScope>; <biblScope type="numbers">279</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0205;4;279" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.36068" type="bibliography" subtype="corrections" org="uniform" part="N" sample="complete"><head>BL-Einträge nach BL-Konkordanz</head><bibl type="BL" default="NO"><biblScope type="volume">XI</biblScope>, S. <biblScope type="pages">255</biblScope></bibl></div><div n="meta.36068" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p>keine</p></div><div n="meta.36068" type="history" subtype="locations" org="uniform" part="N" sample="complete"><p><placeName type="ancientFindspot" cert="low" full="yes">Aphrodites Kome (Antaiopolites)</placeName></p></div></body></text></TEI.2>