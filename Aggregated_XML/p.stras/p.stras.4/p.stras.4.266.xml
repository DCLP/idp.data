    <lb n="26"/><supplied cert="high" reason="lost">Ἀσ</supplied>κ<supplied cert="high" reason="lost">λη</supplied>π[ιάδης ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <gap extent="2" reason="illegible" unit="character"/>] <expan><unclear cert="high" reason="undefined">οὐλ</unclear><ex cert="high">ὴ</ex></expan> παρὰ π<gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/><foreign lang="la">. </foreign>

