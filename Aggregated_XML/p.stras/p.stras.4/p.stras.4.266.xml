    <lb n="1"/><supplied cert="high" reason="lost">μηδὲ κακοτε</supplied>χνήσει<supplied cert="high" reason="lost">ν τὴν ὁμολογοῦσαν πρὸς τὴν</supplied> 

    <lb n="2"/><supplied cert="high" reason="lost">παραχώρησιν</supplied> ταύτην τρόπῳ <supplied cert="high" reason="lost">μηδενὶ ἀλλὰ καὶ ἐπάναγκον</supplied> 

    <lb n="3"/><supplied cert="high" reason="lost">αὐτὴν παρ</supplied>έξασθαι τῶι Παυσαν<supplied cert="high" reason="lost">ίᾳ καὶ τοῖς παρ᾽ αὐτοῦ τὰς αὐ</supplied>-

    <lb n="4"/><supplied cert="high" reason="lost">τὰς ἀρο</supplied>ύρας βεβαίας διὰ πα<supplied cert="high" reason="lost">ντὸς ἀπὸ πάντων πάσῃ βεβαιώσει</supplied> 

    <lb n="5"/><supplied cert="high" reason="lost">καὶ καθ</supplied>αρὰς ἀπὸ μὲν δη<supplied cert="high" reason="lost">μοσίων καὶ τῶν ἄλλων τελεσμάτων</supplied> 

    <lb n="6"/><supplied cert="high" reason="lost">πάντ</supplied>ων τῶν ἕως τῶ<supplied cert="high" reason="lost">ν ἐπαγομένων τοῦ διελθόντος ἔτους καὶ</supplied> 

    <lb n="7"/><supplied cert="high" reason="lost">αὐτοῦ</supplied> τοῦ διελθόντος <gap extent="6" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἔτους ἀπὸ δὲ πάσης γεωργίας βα</supplied>-

    <lb n="8"/><supplied cert="high" reason="lost">σιλικ</supplied>ῆς καὶ οὐσιακῆς κ<supplied cert="high" reason="lost">αὶ ἱερᾶς καὶ ἑτέρου τινος εἴδους·</supplied> 

    <lb n="9"/><supplied cert="high" reason="lost">ἐὰν δέ</supplied> τι τούτων ἡ ὁμ<supplied cert="high" reason="lost">ολογοῦσα παρασυνγράφῃ ἄκυρον ἔστω</supplied> 

    <lb n="10"/><supplied cert="high" reason="lost">καὶ </supplied><choice><corr><supplied cert="high" reason="lost">πρ</supplied>οσαποτεισάτω</corr><sic><supplied cert="high" reason="lost">πρ</supplied>οσαποτισατωι</sic></choice> <supplied cert="high" reason="lost">Παυσανίᾳ ἢ τοῖς παρ᾽ αὐτοῦ καθ᾽ ὃ ἐὰν παρα</supplied>-

    <lb n="11"/><supplied cert="high" reason="lost">συγγρ</supplied>άφῃ εἶδο<supplied cert="high" reason="lost">ς τό τε βλάβος καὶ ἐπίτιμον ἀργυρίου δραχμὰς</supplied> 

    <lb n="12"/><supplied cert="high" reason="lost">πεντα</supplied>κοσίας <num value="500"/> καὶ εἰς τὸ δημ<supplied cert="high" reason="lost">όσιον τὰς ἴσας καὶ μηδὲν ἦσσον τὰ</supplied> 

    <lb n="13"/><supplied cert="high" reason="lost">διωμ</supplied>ολογήμενα κύρι<supplied cert="high" reason="lost">α ἔστω καθ᾽ ὅ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="14"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> σ</supplied>αν. <handShift new="m2"/> Σινθῶνις Ὡρ[ <gap extent="unknown" reason="illegible" unit="character"/> τοῦ <gap extent="unknown" reason="illegible" unit="character"/> παρακεχώ]-

    <lb n="15"/><supplied cert="high" reason="lost">ρηκ</supplied>α Παυσανίᾳ τὰς ὑ<supplied cert="high" reason="lost">παρχούσας μοι περὶ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="16"/><supplied cert="high" reason="lost">ἐπο</supplied>ίκιον ἐκ τοῦ Σωκ<supplied cert="high" reason="lost">ράτους τοῦ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> κλή</supplied>-

    <lb n="17"/><supplied cert="high" reason="lost">ρου</supplied> κατοικικῆς γῆς ἀρ<supplied cert="high" reason="lost">ούρας </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="18"/><gap extent="unknown" reason="lost" unit="character"/> καὶ ἀπέχω τὸ παραχω<supplied cert="high" reason="lost">ρητικὸν τὰς τοῦ ἀργυρίου δραχ</supplied>-

    <lb n="19"/><supplied cert="high" reason="lost">μὰς π</supplied>εντακοσίας <num value="500"/> καὶ βεβα<supplied cert="high" reason="lost">ιώσω </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="20"/><gap extent="unknown" reason="lost" unit="character"/>ια βασιλικῆς κα<supplied cert="high" reason="lost">ὶ οὐσιακῆς καὶ ἑτέρου τινος εἴ</supplied>-

    <lb n="21"/><supplied cert="high" reason="lost">δους</supplied> Ἀσκληπιάδης Ἀσκλ<supplied cert="high" reason="lost">ηπιάδου τοῦ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="22"/><supplied cert="high" reason="lost">ἐπι</supplied>γέγραμμαι τῆς μητρός μου κύρ<supplied cert="high" reason="lost">ιος καὶ ἔγραψα ὑπὲρ αὐ</supplied>-

    <lb n="23"/><supplied cert="high" reason="lost">τῆς μ</supplied>ὴ <choice><corr>εἰδυίης</corr><sic><hi rend="diaeresis">ι</hi>δυ<hi rend="diaeresis">ι</hi>ης</sic></choice> γράμματα. <handShift new="m3"/> Παυσαν<supplied cert="high" reason="lost">ίας </supplied><gap extent="4" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ὁ προγεγρα</supplied>-

    <lb n="24"/><supplied cert="high" reason="lost">μμέ</supplied>νος παρακεχώρημαι ὡς πρόκειτ<supplied cert="high" reason="lost">αι </supplied><gap extent="unknown" reason="lost" unit="character"/><foreign lang="la">: </foreign>

