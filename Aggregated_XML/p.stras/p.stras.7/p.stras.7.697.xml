    <lb n="5"/><gap reason="lost" extent="18" unit="character"/>ετέ<unclear reason="undefined" cert="high">ρ</unclear>ας. κα<unclear reason="undefined" cert="high">ὶ</unclear> γὰρ κα<gap reason="lost" extent="6" unit="character"/> 

    <lb n="6"/><gap reason="lost" extent="18" unit="character"/>αγι τῇ ἀγ<gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="4" unit="character"/>η<gap reason="lost" extent="6" unit="character"/> 

    <lb n="7"/><choice><corr>Παυσίρεως</corr><sic>π<unclear reason="undefined" cert="high">ο</unclear>υσιρεως</sic></choice> <unclear reason="undefined" cert="high">ὡ</unclear>μολ<supplied reason="lost" cert="high">ό</supplied>γησε <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="5" unit="character"/><gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">ι</unclear>τῳ <unclear reason="undefined" cert="high">ε</unclear><gap reason="lost" extent="8" unit="character"/> 

    <lb n="8"/>μονάζοντα Ἡρακλίδη τῷ ὀνό<supplied reason="lost" cert="high">μ</supplied>ατι καὶ τῷ <gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="9" unit="character"/> 

    <lb n="9"/>καὶ ἔλαβεν πάντα τὰ αὐτοῦ. καὶ ἔγνων κα<unclear reason="undefined" cert="high">ὶ</unclear> <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="11" unit="character"/> 

    <lb n="10"/>διὰ τοὺς <expan>ἐνδοξ<ex cert="high">οτάτους</ex></expan> ἄρχ<unclear reason="undefined" cert="high">ο</unclear>ντας <unclear reason="undefined" cert="high">τ</unclear>ὸ τηνικαῦτα ἵνα ζημι<unclear reason="undefined" cert="high">ῶ</unclear><supplied reason="lost" cert="high">νται</supplied> 

    <lb n="11"/>οἱ κωμῆται. εἰ μὲν δύν<unclear reason="undefined" cert="high">α</unclear>ται ἡ ὑμετέρα λαμπρ<supplied reason="lost" cert="high">ότης </supplied><gap reason="lost" extent="3" unit="character"/> 

    <lb n="12"/>δοῦναι τύπον αὐτοῖς ἀμφοτέροις, χαρίζεταί μοι <unclear reason="undefined" cert="high">ε</unclear>ὐχὴ<supplied reason="lost" cert="high">ν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>ἐπειδὴ ἐνταῦθα εἰμὶ ζητῶν βο<hi rend="diaeresis">ί</hi>δια ἀγοράσαι εἰς τ<unclear reason="undefined" cert="high">ὰ</unclear> 

    <lb n="14"/>κτήματα τοῦ μοναστηρίου. καὶ οὐ δεδύνημαι <unclear reason="undefined" cert="high">ἀ</unclear>ποστῆ<unclear reason="undefined" cert="high">ν</unclear><supplied reason="lost" cert="high">αι </supplied><gap reason="lost" extent="3" unit="character"/> 

