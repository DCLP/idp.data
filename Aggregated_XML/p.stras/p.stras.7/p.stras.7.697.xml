    <lb n="1"/><g type="chirho"/> καὶ πρὸ τῆς ἀποδόσεως τῶν ὑμ<unclear reason="undefined" cert="high">ετ</unclear><supplied reason="lost" cert="high">έ</supplied>ρ<supplied reason="lost" cert="high">ων γραμμάτων</supplied> 

