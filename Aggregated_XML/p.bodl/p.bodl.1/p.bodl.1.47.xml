    <lb n="1"/><gap extent="25" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/>

    <lb n="2"/><gap extent="29" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>δόσθαι Χριστο-

    <lb n="3"/><supplied cert="high" reason="lost">δώρᾳ καὶ κρατεῖσθαι καὶ κατέ</supplied>χειν καὶ διακατέχειν

    <lb n="4"/><gap extent="25" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> <unclear cert="high" reason="undefined">α</unclear>λω καὶ <choice><corr>ἀφιλονείκως</corr><sic>αφειλονικως</sic></choice>

    <lb n="5"/><gap extent="25" reason="lost" unit="character"/><unclear cert="high" reason="undefined">η</unclear>σετο <choice><corr>ἀντειπεῖν</corr><sic>αντιπειν</sic></choice> ἢ ἀντι-

    <lb n="6"/><supplied cert="high" reason="lost">ποιήσασθαι ἢ ἐναντιωθῆν</supplied><unclear cert="high" reason="undefined">α</unclear>ι ταύτης μου τῆς

    <lb n="7"/><supplied cert="high" reason="lost">διαθήκης </supplied><gap extent="12" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τῶν κα</supplied>τακειμένων νόσῳ

    <lb n="8"/><gap extent="25" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ρ</unclear>ωνος ὑπὲρ τοῦ αὐτοῦ

    <lb n="9"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἢ δι᾽</supplied> <unclear cert="high" reason="undefined">ἑ</unclear>αυτῆς ἢ δι᾽ ἐντολέως

    <lb n="10"/>ἢ δι᾽ οἱ<unclear cert="high" reason="undefined">ουδήπ</unclear>ο<unclear cert="high" reason="undefined">τε</unclear> <unclear cert="high" reason="undefined">π</unclear>ρ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">σ</supplied><unclear cert="high" reason="undefined">ώ</unclear><supplied cert="high" reason="lost">που ἢ διὰ π</supplied>ροφάσεως αὐτῆς

    <lb n="11"/>οἱανδήποτε ζημίαν <unclear cert="high" reason="undefined">πρ</unclear><supplied cert="high" reason="lost">οσπ</supplied><unclear cert="high" reason="undefined">οιή</unclear>σεσθαι κατὰ το<unclear cert="high" reason="undefined">ῦ</unclear> αὐτοῦ

    <lb n="12"/><choice><corr>νοσοκομείου</corr><sic>νοσοκομιου</sic></choice>, βούλομαι αὐτὴν ἀποστερεῖσθαι τῶν ἀφωρισ-

    <lb n="13"/>μένων <unclear cert="high" reason="undefined">αὐ</unclear>τῇ παρ᾽ ἐμοῦ ἐν ταύτῃ τῇ δ<unclear cert="high" reason="undefined">ι</unclear>αθήκῃ πραγμάτων,

    <lb n="14"/>τῆς θείας καὶ νε<unclear cert="high" reason="undefined">α</unclear>ρᾶς διατάξεως τοῦτο αὐτὸ συγχωρούσης

    <lb n="15"/>γενέσθαι διὰ τὸ τὴν αὐτὴν θείαν καὶ νεαρὰν διάταξιν

    <lb n="16"/>ἄδειαν δεδωκένα<unclear cert="high" reason="undefined">ι</unclear> τοῖς <unclear cert="high" reason="undefined">διαθ</unclear>εμένοις <choice><corr>μηδέ</corr><sic>μητε</sic></choice> τὰ ἐκ τοῦ

    <lb n="17"/>ληγάτο<unclear cert="high" reason="undefined">υ</unclear> νόμου φυλάξαι τοῖς ἐναντιουμένοις τῆς αὐτῶν

    <lb n="18"/><supplied cert="high" reason="lost">διαθήκ</supplied><unclear cert="high" reason="undefined">ης</unclear>, βούλομαι τοίνυν τὴν αὐτὴν Χριστοδώραν

    <lb n="19"/>εἰ, ὅπερ ἀπείη, τολμήσῃ τοιοῦτό τ<unclear cert="high" reason="undefined">ι</unclear> <unclear cert="high" reason="undefined">π</unclear>οιῆσαι κ<unclear cert="high" reason="undefined">α</unclear>τὰ τ<supplied cert="high" reason="lost">ο</supplied>ῦ

    <lb n="20"/>αὐτοῦ <choice><corr>νοσοκομείου</corr><sic>νοσοκομιου</sic></choice> ἢ κατὰ τῶν αὐτῶν <unclear cert="high" reason="undefined">π</unclear>ραγμάτων <unclear cert="high" reason="undefined">μηδὲν</unclear>

    <lb n="21"/>πανταπᾶσιν ἔχειν ἐκ τῆς ἐμῆς κληρονομίας, ἀλλὰ

    <lb n="22"/>καὶ τούτων <subst><add place="inline">ἐκπεσεῖν</add><del rend="corrected" status="unremarkable"><gap extent="8" reason="illegible" unit="character"/></del></subst> τῶν διατυ<unclear cert="high" reason="undefined">π</unclear>ωθέντων παρ᾽ ἐμοῦ

    <lb n="23"/><unclear cert="high" reason="undefined">ἐν</unclear> ταύτῃ τῇ διαθήκῃ, δίδο<unclear cert="high" reason="undefined">σθ</unclear>αι <unclear cert="high" reason="undefined">αὐ</unclear><supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ῇ</unclear> ὑπὲρ τῆς ἰδίας

    <lb n="24"/><gap extent="6" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="4" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="11" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τῶν</supplied>
<unclear cert="high" reason="undefined">π</unclear>ρογεγραμμένων

    <lb n="25"/>πραγμάτων προ<unclear cert="high" reason="undefined">χ</unclear><supplied cert="high" reason="lost">ρείαν τῶν ὀφ</supplied>ειλομένων τῷ αὐτῷ

    <lb n="26"/><choice><corr>νοσοκομείῳ</corr><sic>νοσοκομιω</sic></choice> κ<gap extent="3" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Ἡγησά</supplied>μην ἀναγκαῖον

    <lb n="27"/>διατυπῶσαι ἐν ταύταις μ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">υ ταῖς τελ</supplied>ευταίαις βουλήσεσιν

    <lb n="28"/><gap extent="32" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ησ<unclear cert="high" reason="undefined">α</unclear> <unclear cert="high" reason="undefined">τ</unclear>ὴν

    <lb n="29"/><gap extent="24" reason="lost" unit="character"/><supplied cert="high" reason="lost"> βού</supplied><unclear cert="high" reason="undefined">λ</unclear>[ομαι <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">α</supplied>ὶ κελεύω

    <lb n="30"/><gap extent="27" reason="lost" unit="character"/><supplied cert="high" reason="lost"> π</supplied><unclear cert="high" reason="undefined">άσ</unclear>αν ἰσχὺν καὶ

    <lb n="31"/><gap extent="25" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> τὸν ἐμφερόμενον

    <lb n="32"/><gap extent="21" reason="lost" unit="character"/> <unclear cert="high" reason="undefined">δ</unclear>ιατάττω ὡς, εἰ συμβαίη

    <lb n="33"/><gap extent="29" reason="lost" unit="character"/> <unclear cert="high" reason="undefined">Χρ</unclear>ιστοδώ<unclear cert="high" reason="undefined">ρ</unclear>αν

    <lb n="34"/><gap extent="35" reason="lost" unit="character"/>εμενα
