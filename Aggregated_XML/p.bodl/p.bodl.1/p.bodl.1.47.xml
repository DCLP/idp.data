    <lb n="2"/><gap reason="lost" extent="29" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="1" unit="character"/>δόσθαι Χριστο-

    <lb n="3"/><supplied reason="lost" cert="high">δώρᾳ καὶ κρατεῖσθαι καὶ κατέ</supplied>χειν καὶ διακατέχειν

    <lb n="4"/><gap reason="lost" extent="25" unit="character"/><gap reason="illegible" extent="1" unit="character"/> <unclear reason="undefined" cert="high">α</unclear>λω καὶ <choice><corr>ἀφιλονείκως</corr><sic>αφειλονικως</sic></choice>

    <lb n="5"/><gap reason="lost" extent="25" unit="character"/><unclear reason="undefined" cert="high">η</unclear>σετο <choice><corr>ἀντειπεῖν</corr><sic>αντιπειν</sic></choice> ἢ ἀντι-

    <lb n="6"/><supplied reason="lost" cert="high">ποιήσασθαι ἢ ἐναντιωθῆν</supplied><unclear reason="undefined" cert="high">α</unclear>ι ταύτης μου τῆς

    <lb n="7"/><supplied reason="lost" cert="high">διαθήκης </supplied><gap reason="lost" extent="12" unit="character"/><supplied reason="lost" cert="high"> τῶν κα</supplied>τακειμένων νόσῳ

    <lb n="8"/><gap reason="lost" extent="25" unit="character"/><unclear reason="undefined" cert="high">ρ</unclear>ωνος ὑπὲρ τοῦ αὐτοῦ

    <lb n="9"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἢ δι᾽</supplied> <unclear reason="undefined" cert="high">ἑ</unclear>αυτῆς ἢ δι᾽ ἐντολέως

    <lb n="10"/>ἢ δι᾽ οἱ<unclear reason="undefined" cert="high">ουδήπ</unclear>ο<unclear reason="undefined" cert="high">τε</unclear> ρ<unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">σ</supplied><unclear reason="undefined" cert="high">ώ</unclear><supplied reason="lost" cert="high">που ἢ διὰ π</supplied>ροφάσεως αὐτῆς

    <lb n="11"/>οἱανδήποτε ζημίαν <unclear reason="undefined" cert="high">πρ</unclear><supplied reason="lost" cert="high">οσπ</supplied><unclear reason="undefined" cert="high">οιή</unclear>σεσθαι κατὰ το<unclear reason="undefined" cert="high">ῦ</unclear> αὐτοῦ

    <lb n="12"/><choice><corr>νοσοκομείου</corr><sic>νοσοκομιου</sic></choice>, βούλομαι αὐτὴν ἀποστερεῖσθαι τῶν ἀφωρισ-

    <lb n="13"/>μένων <unclear reason="undefined" cert="high">αὐ</unclear>τῇ παρ᾽ ἐμοῦ ἐν ταύτῃ τῇ δ<unclear reason="undefined" cert="high">ι</unclear>αθήκῃ πραγμάτων,

    <lb n="14"/>τῆς θείας καὶ νε<unclear reason="undefined" cert="high">α</unclear>ρᾶς διατάξεως τοῦτο αὐτὸ συγχωρούσης

    <lb n="15"/>γενέσθαι διὰ τὸ τὴν αὐτὴν θείαν καὶ νεαρὰν διάταξιν

    <lb n="16"/>ἄδειαν δεδωκένα<unclear reason="undefined" cert="high">ι</unclear> τοῖς <unclear reason="undefined" cert="high">διαθ</unclear>εμένοις <choice><corr>μηδέ</corr><sic>μητε</sic></choice> τὰ ἐκ τοῦ

    <lb n="17"/>ληγάτο<unclear reason="undefined" cert="high">υ</unclear> νόμου φυλάξαι τοῖς ἐναντιουμένοις τῆς αὐτῶν

    <lb n="18"/><supplied reason="lost" cert="high">διαθήκ</supplied><unclear reason="undefined" cert="high">ης</unclear>, βούλομαι τοίνυν τὴν αὐτὴν Χριστοδώραν

    <lb n="19"/>εἰ, ὅπερ ἀπείη, τολμήσῃ τοιοῦτό τ<unclear reason="undefined" cert="high">ι</unclear> οιῆσαι κ<unclear reason="undefined" cert="high">α</unclear>τὰ τ<supplied reason="lost" cert="high">ο</supplied>ῦ

    <lb n="20"/>αὐτοῦ <choice><corr>νοσοκομείου</corr><sic>νοσοκομιου</sic></choice> ἢ κατὰ τῶν αὐτῶν <unclear reason="undefined" cert="high">π</unclear>ραγμάτων <unclear reason="undefined" cert="high">μηδὲν</unclear>

    <lb n="21"/>πανταπᾶσιν ἔχειν ἐκ τῆς ἐμῆς κληρονομίας, ἀλλὰ

    <lb n="22"/>καὶ τούτων <subst><add place="inline">ἐκπεσεῖν</add><del rend="corrected" status="unremarkable"><gap reason="illegible" extent="8" unit="character"/></del></subst> τῶν διατυ<unclear reason="undefined" cert="high">π</unclear>ωθέντων παρ᾽ ἐμοῦ

    <lb n="23"/><unclear reason="undefined" cert="high">ἐν</unclear> ταύτῃ τῇ διαθήκῃ, δίδο<unclear reason="undefined" cert="high">σθ</unclear>αι <unclear reason="undefined" cert="high">αὐ</unclear><supplied reason="lost" cert="high">τ</supplied><unclear reason="undefined" cert="high">ῇ</unclear> ὑπὲρ τῆς ἰδίας

    <lb n="24"/><gap reason="lost" extent="6" unit="character"/><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="4" unit="character"/><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="11" unit="character"/><supplied reason="lost" cert="high"> τῶν</supplied>
<unclear reason="undefined" cert="high">π</unclear>ρογεγραμμένων

    <lb n="25"/>πραγμάτων προ<unclear reason="undefined" cert="high">χ</unclear><supplied reason="lost" cert="high">ρείαν τῶν ὀφ</supplied>ειλομένων τῷ αὐτῷ

    <lb n="26"/><choice><corr>νοσοκομείῳ</corr><sic>νοσοκομιω</sic></choice> κ<gap reason="lost" extent="3" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="1" unit="character"/><supplied reason="lost" cert="high"> Ἡγησά</supplied>μην ἀναγκαῖον

    <lb n="27"/>διατυπῶσαι ἐν ταύταις μ<unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">υ ταῖς τελ</supplied>ευταίαις βουλήσεσιν

    <lb n="28"/><gap reason="lost" extent="32" unit="character"/><gap reason="illegible" extent="1" unit="character"/>ησ<unclear reason="undefined" cert="high">α</unclear> ὴν

    <lb n="29"/><gap reason="lost" extent="24" unit="character"/><supplied reason="lost" cert="high"> βού</supplied><unclear reason="undefined" cert="high">λ</unclear>[ομαι <unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">α</supplied>ὶ κελεύω

    <lb n="30"/><gap reason="lost" extent="27" unit="character"/><supplied reason="lost" cert="high"> π</supplied><unclear reason="undefined" cert="high">άσ</unclear>αν ἰσχὺν καὶ

    <lb n="31"/><gap reason="lost" extent="25" unit="character"/><gap reason="illegible" extent="1" unit="character"/> τὸν ἐμφερόμενον

    <lb n="32"/><gap reason="lost" extent="21" unit="character"/> <unclear reason="undefined" cert="high">δ</unclear>ιατάττω ὡς, εἰ συμβαίη

    <lb n="33"/><gap reason="lost" extent="29" unit="character"/> <unclear reason="undefined" cert="high">Χρ</unclear>ιστοδώ<unclear reason="undefined" cert="high">ρ</unclear>αν

