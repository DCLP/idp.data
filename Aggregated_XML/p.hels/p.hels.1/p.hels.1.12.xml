    <lb n="1"/>Διονυσίωι οἰκο<supplied cert="high" reason="lost">ν</supplied>όμωι 

    <lb n="2"/>πα<unclear cert="high" reason="undefined">ρ</unclear>ὰ Ἁρμιύσιος <unclear cert="high" reason="undefined">τ</unclear>οῦ Πααβοῦτος 

    <lb n="3"/>ἡγεμόνος. ἀπο<supplied cert="high" reason="lost">γ</supplied><unclear cert="high" reason="undefined">ρ</unclear>άφομαι 

    <lb n="4"/>εἰς τὸ <num value="19">ι<unclear cert="high" reason="undefined">θ</unclear></num> <expan><ex cert="high">ἔτος</ex></expan> τὸ ὑπάρχον 

    <lb n="5"/>μοι <unclear cert="high" reason="undefined">λο</unclear>υτρωνίδιον ἐν τῆι 

    <lb n="6"/>περι<unclear cert="high" reason="undefined">σ</unclear>τάσει μου ἐν κώμηι 

    <lb n="7"/>Βουσίρει χαλκοῦ πρὸς <expan><ex cert="high">ἀργύριον</ex></expan> 

    <lb n="8"/>ταλάντου ἑνὸς <num value="1"/> ἀφ᾽ οὗ 

    <lb n="9"/>τάξομαι τὴν εἰς τὸ <expan>βα<ex cert="high">σιλικὸν</ex></expan> 

    <lb n="10"/>τρί<unclear cert="high" reason="undefined">τ</unclear>ην χαλκοῦ <expan><ex cert="high">δραχμὰς</ex></expan> <num value="2000">Β</num> 

    <lb n="11"/>χορηγουμένου μου τὰ 

    <lb n="12"/>ἐνκαύματα. ἐὰν δὲ οἱ ἐξει-

    <lb n="13"/>ληφότες τελῶναι μὴ 

    <lb n="14"/>ὑπομένωσιν συνχωρῆ-

    <lb n="15"/>σαί <unclear cert="high" reason="undefined">μ</unclear>οι ἐπὶ τούτοις, ὑπο-

    <lb n="16"/><choice><corr>μένω</corr><sic>υπομε<unclear cert="high" reason="undefined">ν</unclear>ωι</sic></choice> λαβεῖν παρ᾽ αὐτῶν 

    <lb n="17"/>τ<unclear cert="high" reason="undefined">ῶ</unclear>ν <unclear cert="high" reason="undefined">κ</unclear>αθηκόντων μοι 

    <lb n="18"/><unclear cert="high" reason="undefined">δύο</unclear> μερῶν χαλκοῦ <expan><ex cert="high">δραχμὰς</ex></expan> <num value="4000">Δ</num> 

    <lb n="19"/>παρέχειν αὐτοὺς τὸ 

    <lb n="20"/><unclear cert="high" reason="undefined">λ</unclear>ου<unclear cert="high" reason="undefined">τρω</unclear>νίδιον ἔνεργον 

    <lb n="21"/>τοῖς ἑαυτῶν <unclear cert="high" reason="undefined">ἐ</unclear>νκαύμασιν 

    <lb n="22"/>καὶ ἀ<unclear cert="high" reason="undefined">να</unclear>λώμασιν. ἀξιῶ οὖν 

    <lb n="23"/>σὲ κ<unclear cert="high" reason="undefined">ατ</unclear>ατάξαι μου τὸ <expan>ὑπόμν<ex cert="high">ημα</ex></expan> 

    <lb n="24"/>ἐν χ<unclear cert="high" reason="undefined">ρη</unclear>ματισμῶι ἵνα μὴ 

    <lb n="25"/>ἐξ<unclear cert="high" reason="undefined">ῆι</unclear> τ<unclear cert="high" reason="undefined">οῖ</unclear>ς τελώναις συκοφαν-

    <lb n="26"/>τεῖν με ὡς μὴ <choice><corr>ἀπογεγραμμένον</corr><sic>απογεγραμμενος</sic></choice>. 

    <lb n="27"/>ἐπιδέδωκα δὲ καὶ τοῖς ἄλλοις 

    <lb n="28"/><supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ῦ ὑπομνή</supplied><unclear cert="high" reason="undefined">μα</unclear>τος ἀντί-

    <lb n="29"/><supplied cert="high" reason="lost">γραφον </supplied><gap extent="10" reason="lost" unit="character"/>ε<unclear cert="high" reason="undefined">ι</unclear><gap extent="2" reason="lost" unit="character"/> 

