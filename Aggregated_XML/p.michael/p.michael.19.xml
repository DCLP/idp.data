    <lb n="6"/>καταρτείᾳ καὶ <choice><corr>σηδηρώσει</corr><sic>σ<unclear cert="high" reason="undefined">ε</unclear>ι<unclear cert="high" reason="undefined">δη</unclear><supplied cert="high" reason="lost">ρωσε</supplied><unclear cert="high" reason="undefined">ι</unclear></sic></choice> παρέχοντ<supplied cert="high" reason="lost">ός σου τοῦ μισθώσαντος τὰ </supplied><gap extent="5" reason="lost" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/>καὶ σχοινία καὶ τ<unclear cert="high" reason="undefined">ὰ</unclear> ἄλλ<unclear cert="high" reason="undefined">α</unclear> τὰ ἐνχρῄ<unclear cert="high" reason="undefined">ζ</unclear><supplied cert="high" reason="lost">οντα </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>ης κατ᾽ ἔτος οὔσης π<unclear cert="high" reason="undefined">ρ</unclear>ὸς τὸν μεμισθ<supplied cert="high" reason="lost">ωμένον </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>πρὸς τὸν γεοῦχον καὶ <unclear cert="high" reason="undefined">ἀπ</unclear>οφερόμενον <unclear cert="high" reason="undefined">τ</unclear>ὰ <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/>τὸν γεοῦχον δ<unclear cert="high" reason="undefined">ιδ</unclear><supplied cert="high" reason="lost">όν</supplied>αι τῷ μεμισθω<supplied cert="high" reason="lost">μένῳ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/>νων καὶ δεσμῶν<gap extent="3" reason="illegible" unit="character"/>λεπτοσχοινίων <gap extent="unknown" reason="lost" unit="character"/> 

