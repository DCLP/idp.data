    <lb n="13"/>ἑ<unclear cert="high" reason="undefined">κ</unclear>άστης ἀρούρης <unclear cert="high" reason="undefined">ἀ</unclear>νὰ δραχμὰς δ<supplied cert="high" reason="lost">ι</supplied>σ<unclear cert="high" reason="undefined">χ</unclear><supplied cert="low" reason="lost">ιλίας </supplied><gap extent="8" reason="lost" unit="character"/><supplied cert="high" reason="lost"> <num value="2?1?1?1"/> παντα </supplied>-

