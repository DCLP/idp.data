    <lb n="17"/>καὶ μόσχου ἑνὸς <num value="1"/> καὶ χορτοσπέρμου μά<supplied cert="low" reason="lost">τια </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="18"/>μέτρῳ δεκ<unclear cert="high" reason="undefined">α</unclear><gap extent="2" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> ἀρτάβας <choice><corr>τρεῖς</corr><sic>τρις</sic></choice> ἥμισ<supplied cert="high" reason="lost">υ <num value="3  1/2 "/> </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="19"/>καὶ εὐαρέστῳ τῷ γεούχῳ καὶ τὰ σπέρ<unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ατα </supplied><gap extent="unknown" reason="lost" unit="character"/> 

