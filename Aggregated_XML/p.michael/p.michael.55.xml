    <lb n="1"/><supplied cert="high" reason="lost">κα</supplied>ὶ χρήσασ<unclear cert="high" reason="undefined">θαι</unclear> καὶ <gap extent="2" reason="illegible" unit="character"/>ρ<gap extent="3" reason="illegible" unit="character"/> κατ<supplied cert="high" reason="lost">ὰ</supplied> <unclear cert="high" reason="undefined">τ</unclear>ὸν δοκοῦντα αὐτῷ <supplied cert="high" reason="lost">τρόπον </supplied><gap extent="3" reason="lost" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/><supplied cert="high" reason="lost">κ</supplied><unclear cert="high" reason="undefined">α</unclear>ὶ βεβαιώσομεν ἀλλήλοις τὴν ἑκάστου <choice><corr>μερίδα</corr><sic>μερειδα</sic></choice> ἀπ<supplied cert="low" reason="lost">ὸ παντὸς καὶ διὰ παντὸς τοῦ ἐπελευσομένου, ὀμοσάμενοι </supplied><gap extent="unknown" reason="lost" unit="character"/> 

