    <lb n="6"/>πάραγον <num value="1">α</num>, θρ<supplied reason="lost" cert="high">ιδάκων</supplied>

    <lb n="7"/>δέσμας <num value="2">β</num>. ς<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="8"/>κύριε, πέμψο<supplied reason="lost" cert="high">ν </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="9"/>χάριν σου <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/></ab></div><div n="meta.24013" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Ostrakon</rs></p></div><div n="meta.24013" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" notBefore="0098" notAfter="0117">98 - 117</date></p></div><div n="meta.24013" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">O.Claud.</title><biblScope type="volume">2</biblScope><biblScope type="numbers">370</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">24013</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0020</series>; <biblScope type="volume">2</biblScope>; <biblScope type="numbers">370</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0020;2;370" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.24013" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p><bibl type="illustration" default="NO">Planche XLVIII</bibl></p></div><div n="meta.24013" type="history" subtype="locations" org="uniform" part="N" sample="complete"><p><placeName type="ancientFindspot" cert="high" full="yes">Raima</placeName></p></div></body></text></TEI.2>