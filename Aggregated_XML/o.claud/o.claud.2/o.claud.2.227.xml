    <lb n="19"/>ειδ<unclear reason="undefined" cert="high">ον</unclear>νατ<unclear reason="undefined" cert="high">ι</unclear>β<unclear reason="undefined" cert="high">ο</unclear>ν

    <lb n="20"/>καὶ πέμψον ἡμῖν ἄνωι</ab></div></div><div n="meta.29650" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Ostrakon</rs></p></div><div n="meta.29650" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" notBefore="126" notAfter="175">Mitte II</date></p></div><div n="meta.29650" type="commentary" subtype="mentionedDates" org="uniform" part="N" sample="complete"><head>Erwähnte Daten</head></div><div n="meta.29650" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">O.Claud.</title><biblScope type="volume">2</biblScope><biblScope type="numbers">227</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">29650</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0020</series>; <biblScope type="volume">2</biblScope>; <biblScope type="numbers">227</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0020;2;227" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.29650" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p><bibl type="illustration" default="NO">Planche IX</bibl></p></div><div n="meta.29650" type="history" subtype="locations" org="uniform" part="N" sample="complete"><p><placeName type="ancientFindspot" cert="high" full="yes">Mons Claudianus</placeName></p></div></body></text></TEI.2>