    <lb n="6"/><space dim="horizontal" extent="unknown" unit="character"/> 

    <lb n="6"/>ἐρρῶσθεί σε <choice><corr>ευ῎χομαι</corr><sic>ευχω<add place="supralinear">μαι</add></sic></choice> 

