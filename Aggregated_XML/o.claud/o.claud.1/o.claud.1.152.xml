    <lb n="3"/><gap extent="4" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ε</unclear>ς <choice><corr>ἀδελφοῖς</corr><sic>αδελφο<hi rend="diaeresis">ι</hi></sic></choice> τὸ σήμερον <gap extent="2" reason="illegible" unit="character"/> 

    <lb n="4"/><supplied cert="high" reason="lost">οἰκο</supplied>δομήσω μου τὸ κελλίν.  κ<supplied cert="high" reason="lost">αταπέμ</supplied>-

    <lb n="5"/><unclear cert="high" reason="undefined">ψω</unclear> <choice><corr>ὑμῖν</corr><sic>υμειν</sic></choice> αὐτὴν αὔριον, αἴσθ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">μαι δὲ</supplied> 

    <lb n="6"/>καὶ ἐγὼ ὅτι κατάβασίς ἐστιν ἐξ <gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/>ἐὰν πέμψω αὐτὴν <choice><corr>ὑμῖν</corr><sic>υμειν</sic></choice> <choice><corr>π<unclear cert="high" reason="undefined">έμψ</unclear>α<supplied cert="high" reason="lost">τε</supplied></corr><sic>π<unclear cert="high" reason="undefined">εμσ</unclear>α<supplied cert="high" reason="lost">τε</supplied></sic></choice> 

    <lb n="8"/>πάλι ταχύ.  ἐρωτῶ δὲ Ἑρμογέ<supplied cert="high" reason="lost">νην</supplied> 

    <lb n="9"/>καὶ <choice><corr>πάντας</corr><sic>παντες</sic></choice> ὑμᾶς ἵνα αὐτὸν <supplied cert="high" reason="lost">βοη</supplied><unclear cert="high" reason="undefined">θή</unclear>-

    <lb n="10"/>σητε ἵνα ὃ ἂν <subst><add place="inline">αὐτ<unclear cert="high" reason="undefined">ὸν</unclear></add><del rend="corrected">αυτ<gap extent="2" reason="illegible" unit="character"/></del></subst> ἐρ<supplied cert="high" reason="lost">ωτᾷ</supplied> 

    <lb n="11"/>ἡ ἀδελφή μου ποιή<unclear cert="high" reason="undefined">σῃ</unclear>.  ἀσπ<supplied cert="high" reason="lost">άζομαι</supplied> 

    <lb n="12"/><choice><corr>πάντας</corr><sic>παντες</sic></choice> κατ᾽ ὄνομα καὶ <unclear cert="high" reason="undefined">Ὡρ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

