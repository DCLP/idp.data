    <lb n="24"/>ττὸν</corr><sic>περιτον</sic></choice> γέ<unclear cert="high" reason="undefined">γραπτα</unclear><supplied cert="high" reason="lost">ι, κα</supplied><unclear cert="high" reason="undefined">ὶ</unclear> <choice><corr>γράφεις</corr><sic>γραφις</sic></choice> <choice><corr>μοι</corr><sic>μυ</sic></choice> <choice><corr>λί<supplied cert="high" reason="omitted">α</supplied>ν</corr><sic>λειν</sic></choice> 

    <lb n="25"/><choice><corr>ὅτι</corr><sic>ωτι</sic></choice> εὐχαρι<supplied cert="high" reason="lost">σ</supplied>τῶ τῇ <choice><corr>κώμῃ</corr><sic>κομη</sic></choice> <choice><corr>ὅτε</corr><sic>ωτε</sic></choice> 

    <lb n="26"/><choice><corr>τέσσαρας</corr><sic>τεσσαρες</sic></choice> <num value="4"/> <supplied cert="high" reason="lost">στ</supplied>α<supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ῆ</unclear>ρας καθ᾽ <choice><corr>ὑμῶν</corr><sic>υμον</sic></choice> 

    <lb n="27"/>γεγραφήκασι.  ἐρρῶσθαί <choice><corr>σε</corr><sic>σαι</sic></choice> εὔχομαι 

    <lb n="28"/>εἰς τὸν ἀεὶ χρόνον.  <expan><ex cert="high">ἔτους</ex></expan> <num value="11">ια</num> Τραιανοῦ 

    <lb n="29"/>Καίσαρος το<supplied cert="high" reason="lost">ῦ κ</supplied>υρίου, <supplied cert="high" reason="lost">Τ</supplied>ῦβι <num value="19">ι<unclear cert="high" reason="undefined">θ</unclear></num>. 
