    <lb n="19"/>διὸ <choice><corr>γράφω</corr><sic>γραφο</sic></choice> <choice><corr>σοι</corr><sic>συπ</sic></choice> <choice><corr>ἵν<supplied cert="high" reason="lost">α</supplied></corr><sic>ειν<supplied cert="high" reason="lost">α</supplied></sic></choice> <gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><unclear cert="high" reason="undefined">η</unclear>ς τοὺς ἵππους 

    <lb n="20"/>οὓς <choice><corr>λαβὼν</corr><sic>λαβον</sic></choice> ἀ<unclear cert="high" reason="undefined">λλ</unclear>άσ<unclear cert="high" reason="undefined">σ</unclear>ου <choice><corr>ἵν᾽</corr><sic>ειν</sic></choice> αὐτοὺς 

    <lb n="21"/>λαμβάνῃ.  <unclear cert="high" reason="undefined">ἐκ</unclear>τί<unclear cert="high" reason="undefined">να</unclear>ξ<unclear cert="high" reason="undefined">ον</unclear> τὸ διειρον <choice><corr>ἵνα</corr><sic>εινα</sic></choice> 

    <lb n="22"/>ἀμέριμνος <unclear cert="high" reason="undefined">ἦς</unclear>.  <choice><corr><unclear cert="high" reason="undefined">ὃ</unclear></corr><sic>ω</sic></choice> <choice><corr>ἔγραφές</corr><sic>εγραφος</sic></choice> <choice><corr>μοι</corr><sic>μυ</sic></choice> 

