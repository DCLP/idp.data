    <lb n="3"/>καὶ τῶν <choice><corr>τῶν</corr><sic><supplied cert="high" reason="lost">τ</supplied>ης</sic></choice> Θεμίστου κ<supplied cert="high" reason="lost">αὶ</supplied> Πολέμωνος μερίδων κ<supplied cert="high" reason="lost">ωμογραμματέων</supplied>. 

    <lb n="4"/>πρὸς <supplied cert="high" reason="lost">τὸ</supplied> ἐπισταλὲν <choice><corr>ἡμῖν</corr><sic>ημειν</sic></choice> ὑπὸ σοῦ ἐπίσταλμα, οὗ ἐστ<supplied cert="high" reason="lost">ιν ἀντίγραφον</supplied>· 

    <lb n="5"/>Διογένης <expan>στρ<ex cert="high">ατηγὸς</ex></expan> <expan>Ἀρσι<ex cert="high">νοίτου</ex></expan> Θεμίστου καὶ Πολέμωνος <supplied cert="high" reason="lost">μερίδων</supplied> 

    <lb n="6"/>γραμματεῦσι μη<supplied cert="high" reason="lost">τ</supplied>ροπόλεως καὶ κωμογραμματε<supplied cert="high" reason="lost">ῦσι χαίρειν</supplied>. 

    <lb n="7"/>τῷ ἐ<supplied cert="high" reason="lost">νε</supplied>στῶτι <num value="13">ιγ</num> <expan><ex cert="high">ἔτει</ex></expan> Ἀντωνίνου Καίσαρος τοῦ κυρίου <gap extent="9" reason="lost" unit="character"/> 

    <lb n="8"/>τῶν ἐν ταῖς μερίσ<supplied cert="high" reason="lost">ι</supplied> <expan>γενημ<ex cert="high">ατογραφουμένων</ex></expan> <supplied cert="high" reason="lost">ὑπ</supplied>αρχόντων καὶ οἰκοπέ<supplied cert="high" reason="lost">δων </supplied><gap extent="5" reason="lost" unit="character"/> 

