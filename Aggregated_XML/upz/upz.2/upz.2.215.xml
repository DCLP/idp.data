    <lb n="3"/>ἐπεὶ προσπέπτωκ<supplied cert="high" reason="lost">εν</supplied> ἡμῖν 

    <lb n="4"/>Κόνωνα πρόσλη<supplied cert="high" reason="lost">ψίν</supplied> τινων 

    <lb n="5"/>ποιεῖσθαι εἰ<supplied cert="high" reason="lost">ς τὸ ταγμ</supplied>ατικὸν 

    <lb n="6"/>ἄνευ τῆς ἡ<supplied cert="high" reason="lost">μετέρας</supplied> γνώμης 

    <lb n="7"/>παρὰ τὰς γε<supplied cert="high" reason="lost">νομένα</supplied>ς ὑπὸ 

    <lb n="8"/>Παῶι τοῦ στ<supplied cert="high" reason="lost">ρατηγοῦ</supplied> διαστολὰς 

    <lb n="9"/>περὶ τοῦ σὺν <supplied cert="high" reason="lost">Ἑρμίαι τ</supplied>ῶι ὑπο-

    <lb n="10"/>στρατήγωι γε<supplied cert="high" reason="lost">νόμενον</supplied> τὸ προκείμενον 

    <lb n="11"/><subst><add place="inline">ἐπιτελεῖν</add><del rend="corrected">επιστελειν</del></subst> π<supplied cert="high" reason="lost">ρᾶγμα, ὃ κ</supplied>αὶ τούτωι 

    <lb n="12"/>δι<supplied cert="high" reason="lost">ασ</supplied>εσαφήκαμ<supplied cert="high" reason="lost">εν διὰ γραμμά</supplied>των, 

    <lb n="13"/>καλῶς <choice><corr>ποιήσετε</corr><sic>ποησετε</sic></choice> κατὰ <supplied cert="high" reason="lost">τ</supplied>ὸ παρὸν 

    <lb n="14"/>ἐπισχόντες μέχρι τοῦ τὰ παρὰ τοῦ 

    <lb n="15"/>Ἑρμίου πρὸς ταῦτα ἀντιφωνηθῆναι, 

    <lb n="16"/>ὅπως μετὰ τῆς πάσ<supplied cert="high" reason="lost">η</supplied>ς προσοχῆς 

    <lb n="17"/>οἰκονομηθ<supplied cert="high" reason="lost">ῆι</supplied> ἕκαστα, <supplied cert="high" reason="lost">ἀ</supplied>κολούθως 

