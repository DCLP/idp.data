    <lb n="24"/>τῆς αὐ<unclear reason="undefined" cert="high">τῆ</unclear>ς πόλεως ἐ<unclear reason="undefined" cert="high">νεποι</unclear>-

    <lb n="25"/>ησάμην αὐτῷ τῷ <unclear reason="undefined" cert="high">Ἀπίω</unclear><supplied reason="lost" cert="high">νι</supplied> 

    <lb n="26"/>περί τε τῶν ἐμῶν <unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">α</supplied><unclear reason="undefined" cert="high">ὶ</unclear> <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">ῦ</unclear> 

    <lb n="27"/>ἀνόμου α<unclear reason="undefined" cert="high">ὐ</unclear>τοῦ ἐξόδου κ<unclear reason="undefined" cert="high">αθ</unclear>᾽ <unclear reason="undefined" cert="high">ὅτι</unclear> 

