<placeName cert="high" full="yes">Oxyrhynchus</placeName></head><div lang="grc" org="uniform" part="N" sample="complete" type="edition"><div n="r" org="uniform" part="N" sample="complete" type="textpart"><ab part="N">

    <lb n="1"/><expan>ἀντίγρ<ex cert="high">αφον</ex></expan>. ἔτους πέμπτου <supplied cert="high" reason="lost">Αὐτοκράτορος Καίσαρος Δομιτιανοῦ</supplied> 

    <lb n="2"/>Σεβαστ<supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ῦ</unclear> Γερμ<unclear cert="high" reason="undefined">α</unclear>ν<supplied cert="high" reason="lost">ι</supplied><unclear cert="high" reason="undefined">κ</unclear>οῦ Φα<unclear cert="high" reason="undefined">ρ</unclear>μ<supplied cert="high" reason="lost">οῦθι </supplied><gap extent="2" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐν Ὀξυρύγχων πόλει τῆς Θηβαίδος.</supplied> 

    <lb n="3"/>ἐδάνεισεν Πετοσῖρις ὁ καὶ Πλ[ <gap extent="20" reason="illegible" unit="character"/> μητρὸς  <gap extent="10" reason="illegible" unit="character"/>] 

    <lb n="4"/>τ<unclear cert="high" reason="undefined">ῆ</unclear>ς Λοβάιτος Ταῶτι Ὥρου [μητρὸς <gap extent="6" reason="illegible" unit="character"/> τῆς <gap extent="6" reason="illegible" unit="character"/> Πέρσῃ τῆς ἐπιγονῆς με]-

    <lb n="5"/>τὰ κυρίου τοῦ ἑαυτῆς υἱοῦ <unclear cert="high" reason="undefined">Ὀ</unclear><gap extent="34" reason="lost" unit="character"/><supplied cert="high" reason="lost"> παν</supplied>-

    <lb n="6"/>τες τῶν ἀπ᾽ Ὀξυρύγχ<unclear cert="high" reason="undefined">ω</unclear>ν πόλ<supplied cert="high" reason="lost">εως ἐν ἀγυιᾷ ἀργυρίου Σεβαστοῦ καὶ Πτολεμαικοῦ</supplied> 

    <lb n="7"/><unclear cert="high" reason="undefined">νο</unclear>μίσματος δραχμὰς π[εντακοσίας <expan><ex cert="high">δραχμὰς</ex></expan> <num value="500">φ</num> κεφαλαίου αἷς οὐδὲν τῷ καθόλου] 

    <lb n="8"/>προσῆ<unclear cert="high" reason="undefined">κ</unclear>ται, ἃ<unclear cert="high" reason="undefined">ς</unclear> κ<unclear cert="high" reason="undefined">αὶ</unclear> ἀ<unclear cert="high" reason="undefined">πο</unclear>δότω <unclear cert="high" reason="undefined">ἡ</unclear> <supplied cert="high" reason="lost">δεδανεισμένη τῷ δεδανεικότι τριακάδι μηνὸς</supplied> 

    <lb n="9"/>Φαμενὼθ τοῦ ἑβδόμου ἔτου<supplied cert="high" reason="lost">ς Αὐτοκράτορος Καίσαρος Δομιτιανοῦ Σεβαστοῦ</supplied> 

    <lb n="10"/>Γερμανι<unclear cert="high" reason="undefined">κ</unclear>οῦ χωρὶς πάσης ὑπερ<supplied cert="high" reason="lost">θέσεως. ἐὰν δὲ μὴ ἀποδῷ καθὰ γέγραπται, συγχωρεῖ</supplied> 

    <lb n="11"/>ἡ δεδα<unclear cert="high" reason="undefined">ν</unclear>εισμένη μένειν πε<supplied cert="high" reason="lost">ρὶ τὸν δεδανεικότα καὶ τοὺς παρ᾽ αὐτοῦ μεταληψο</supplied>-

    <lb n="12"/>μένους ἀντὶ τοῦ κεφαλαίου <unclear cert="high" reason="undefined">ἀ</unclear>[πὸ τοῦ τῆς ἀποδόσεως χρόνου τὴν κράτησιν καὶ <choice><corr>κυ]-

    <lb n="13"/>ρίαν</corr><sic><supplied cert="high" reason="lost">κυ</supplied>ρειαν</sic></choice> ε<unclear cert="high" reason="undefined">ἰς</unclear> <unclear cert="high" reason="undefined">τ</unclear>ὸν ἀεὶ <unclear cert="high" reason="undefined">χ</unclear>ρόνον τοῦ <unclear cert="high" reason="undefined">ὑ</unclear><supplied cert="high" reason="lost">πάρχοντος αὐτῇ μέρους οἰκίας ἐν τῇ αὐτῇ μη</supplied>-

    <lb n="14"/>τροπό<unclear cert="high" reason="undefined">λ</unclear>ει τοῦ <choice><corr>Ὀξυρυγχίτου</corr><sic>οξυρυγχειτου</sic></choice> κα<supplied cert="high" reason="lost">ὶ χρηστηρίων καὶ εἰσόδων καὶ ἐξόδων πασῶν καὶ</supplied> 

    <lb n="15"/>ἐξουσίαν ἔχειν <unclear cert="high" reason="undefined">τ</unclear>ὸν δεδανει<supplied cert="high" reason="lost">κότα τάξασθαι τὰ τέλη καὶ δεσπόζειν αὐτοῦ καὶ τὰ</supplied> 

    <lb n="16"/>περιε<unclear cert="high" reason="undefined">σ</unclear>όμενα ἀπ<supplied cert="high" reason="lost">ο</supplied>φέρεσθ<unclear cert="high" reason="undefined">α</unclear>ι <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">άντα καὶ ἑτέροις πωλεῖν καὶ χρᾶσθαι καὶ διοικεῖν</supplied> 

    <lb n="17"/>περὶ αὐτοῦ ὡς ἐὰν αἱρῆ<unclear cert="high" reason="undefined">ται</unclear> μ<unclear cert="high" reason="undefined">η</unclear><supplied cert="high" reason="lost">δεμιᾶς τῇ δεδανεισμένῃ ἢ τοῖς παρ᾽ αὐτῆς καταλειπο</supplied>-

    <lb n="18"/>μένης ἐφόδου κατὰ μ<unclear cert="high" reason="undefined">ηδ</unclear>ένα <supplied cert="high" reason="lost">τρόπον, ἀλλὰ ἐπάναγκον αὐτὴν παρέξεσθαι τῷ δεδα</supplied>-

    <lb n="19"/>νεικότι καὶ τοῖς <unclear cert="high" reason="undefined">π</unclear>αρ᾽ αὐτοῦ τὸ <unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">έρος τῆς οἰκίας διὰ παντὸς βέβαιον</supplied>

    <lb n="20"/><supplied cert="high" reason="lost">ἀπὸ πάντων</supplied> 

    <lb n="21"/>πάσῃ βεβαιώσει. ἀπὸ δὲ τοῦ <unclear cert="high" reason="undefined">νῦ</unclear><supplied cert="high" reason="lost">ν ἕως ἂν ἀποδώσει ἡ δεδανεισμένη τῷ δεδανεικότι</supplied> 

    <lb n="22"/>τὰς τοῦ <expan>ἀργ<ex cert="high">υρίου</ex></expan> <expan><ex cert="high">δραχμὰς</ex></expan> <num value="500"><unclear cert="high" reason="undefined">φ</unclear></num> <supplied cert="high" reason="lost">μὴ ἐξ</supplied><unclear cert="high" reason="undefined">εῖ</unclear>ναι αὐ<supplied cert="high" reason="lost">τῇ τῇ δεδανεισμένῃ τὸ μέρος τῆς οἰκίας πωλεῖν</supplied> 

    <lb n="23"/>μηδ᾽ ὑποτίθ<unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">σθαι μηδ᾽</supplied> <unclear cert="high" reason="undefined">ἄ</unclear>λ<supplied cert="high" reason="lost">λ</supplied><unclear cert="high" reason="undefined">ω</unclear>ς κ<supplied cert="high" reason="lost">αταχρηματίζειν μηδ᾽ ἀπογράφεσθαί τινα ἐπ᾽ αὐτοῦ,</supplied> 

    <lb n="24"/><choice><corr>ἐκλογῆς</corr><sic>εγλο<unclear cert="high" reason="undefined">γης</unclear></sic></choice> <supplied cert="high" reason="lost">ο</supplied>ὔσης <supplied cert="high" reason="lost">τῷ δεδ</supplied><unclear cert="high" reason="undefined">α</unclear>νει<unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">ό</supplied>τ<supplied cert="high" reason="lost">ι διὰ ἑαυτοῦ μετὰ τὸν χρόνον μὴ ἀπολαβόντι ἐὰν</supplied> 

    <lb n="25"/><unclear cert="high" reason="undefined">β</unclear>ούληται κυ<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ιεύειν το</supplied><unclear cert="high" reason="undefined">ῦ</unclear> μ<unclear cert="high" reason="undefined">έ</unclear>ρου<supplied cert="high" reason="lost">ς ἀντὶ τοῦ κεφαλαίου ἢ τὴν πράξιν ποιεῖσθαι ἐκ τῆς δεδα</supplied>-

    <lb n="26"/><unclear cert="high" reason="undefined">ν</unclear>ει<supplied cert="high" reason="lost">σμέ</supplied>νη<supplied cert="high" reason="lost">ς καὶ τοῦ μέρους τῆς οἰκίας καὶ τῶν ἄλλων ὑπαρχόντων αὐτῇ πάντων κα</supplied>-

    <lb n="27"/><unclear cert="high" reason="undefined">θ</unclear><supplied cert="high" reason="lost">άπερ ἐ</supplied><unclear cert="high" reason="undefined">κ</unclear> <unclear cert="high" reason="undefined">δ</unclear><supplied cert="high" reason="lost">ίκης  </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="28"/><gap extent="unknown" reason="lost" unit="line"/> 
