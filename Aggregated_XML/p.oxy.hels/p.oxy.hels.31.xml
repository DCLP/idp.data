    <lb n="1"/><expan>ἀντίγρ<ex cert="high">αφον</ex></expan>. ἔτους πέμπτου <supplied reason="lost" cert="high">Αὐτοκράτορος Καίσαρος Δομιτιανοῦ</supplied> 

    <lb n="2"/>Σεβαστ<supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">ῦ</unclear> Γερμ<unclear reason="undefined" cert="high">α</unclear>ν<supplied reason="lost" cert="high">ι</supplied><unclear reason="undefined" cert="high">κ</unclear>οῦ Φα<unclear reason="undefined" cert="high">ρ</unclear>μ<supplied reason="lost" cert="high">οῦθι </supplied><gap reason="lost" extent="2" unit="character"/><supplied reason="lost" cert="high"> ἐν Ὀξυρύγχων πόλει τῆς Θηβαίδος.</supplied> 

    <lb n="3"/>ἐδάνεισεν Πετοσῖρις ὁ καὶ Πλ[ <gap reason="illegible" extent="20" unit="character"/> μητρὸς  <gap reason="illegible" extent="10" unit="character"/>] 

    <lb n="4"/>τ<unclear reason="undefined" cert="high">ῆ</unclear>ς Λοβάιτος Ταῶτι Ὥρου [μητρὸς <gap reason="illegible" extent="6" unit="character"/> τῆς <gap reason="illegible" extent="6" unit="character"/> Πέρσῃ τῆς ἐπιγονῆς με]-

    <lb n="5"/>τὰ κυρίου τοῦ ἑαυτῆς υἱοῦ <unclear reason="undefined" cert="high">Ὀ</unclear><gap reason="lost" extent="34" unit="character"/><supplied reason="lost" cert="high"> παν</supplied>-

    <lb n="6"/>τες τῶν ἀπ᾽ Ὀξυρύγχ<unclear reason="undefined" cert="high">ω</unclear>ν πόλ<supplied reason="lost" cert="high">εως ἐν ἀγυιᾷ ἀργυρίου Σεβαστοῦ καὶ Πτολεμαικοῦ</supplied> 

    <lb n="7"/><unclear reason="undefined" cert="high">νο</unclear>μίσματος δραχμὰς π[εντακοσίας <expan><ex cert="high">δραχμὰς</ex></expan> <num value="500">φ</num> κεφαλαίου αἷς οὐδὲν τῷ καθόλου] 

    <lb n="8"/>προσῆ<unclear reason="undefined" cert="high">κ</unclear>ται, ἃ<unclear reason="undefined" cert="high">ς</unclear> κ<unclear reason="undefined" cert="high">αὶ</unclear> ἀ<unclear reason="undefined" cert="high">πο</unclear>δότω <unclear reason="undefined" cert="high">ἡ</unclear> <supplied reason="lost" cert="high">δεδανεισμένη τῷ δεδανεικότι τριακάδι μηνὸς</supplied> 

    <lb n="9"/>Φαμενὼθ τοῦ ἑβδόμου ἔτου<supplied reason="lost" cert="high">ς Αὐτοκράτορος Καίσαρος Δομιτιανοῦ Σεβαστοῦ</supplied> 

    <lb n="10"/>Γερμανι<unclear reason="undefined" cert="high">κ</unclear>οῦ χωρὶς πάσης ὑπερ<supplied reason="lost" cert="high">θέσεως. ἐὰν δὲ μὴ ἀποδῷ καθὰ γέγραπται, συγχωρεῖ</supplied> 

    <lb n="11"/>ἡ δεδα<unclear reason="undefined" cert="high">ν</unclear>εισμένη μένειν πε<supplied reason="lost" cert="high">ρὶ τὸν δεδανεικότα καὶ τοὺς παρ᾽ αὐτοῦ μεταληψο</supplied>-

    <lb n="12"/>μένους ἀντὶ τοῦ κεφαλαίου <unclear reason="undefined" cert="high">ἀ</unclear>[πὸ τοῦ τῆς ἀποδόσεως χρόνου τὴν κράτησιν καὶ <choice><corr>κυ]-

    <lb n="13"/>ρίαν</corr><sic><supplied reason="lost" cert="high">κυ</supplied>ρειαν</sic></choice> ε<unclear reason="undefined" cert="high">ἰς</unclear> ὸν ἀεὶ <unclear reason="undefined" cert="high">χ</unclear>ρόνον τοῦ <unclear reason="undefined" cert="high">ὑ</unclear><supplied reason="lost" cert="high">πάρχοντος αὐτῇ μέρους οἰκίας ἐν τῇ αὐτῇ μη</supplied>-

    <lb n="14"/>τροπό<unclear reason="undefined" cert="high">λ</unclear>ει τοῦ <choice><corr>Ὀξυρυγχίτου</corr><sic>οξυρυγχειτου</sic></choice> κα<supplied reason="lost" cert="high">ὶ χρηστηρίων καὶ εἰσόδων καὶ ἐξόδων πασῶν καὶ</supplied> 

    <lb n="15"/>ἐξουσίαν ἔχειν <unclear reason="undefined" cert="high">τ</unclear>ὸν δεδανει<supplied reason="lost" cert="high">κότα τάξασθαι τὰ τέλη καὶ δεσπόζειν αὐτοῦ καὶ τὰ</supplied> 

    <lb n="16"/>περιε<unclear reason="undefined" cert="high">σ</unclear>όμενα ἀπ<supplied reason="lost" cert="high">ο</supplied>φέρεσθ<unclear reason="undefined" cert="high">α</unclear>ι <unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">άντα καὶ ἑτέροις πωλεῖν καὶ χρᾶσθαι καὶ διοικεῖν</supplied> 

    <lb n="17"/>περὶ αὐτοῦ ὡς ἐὰν αἱρῆ<unclear reason="undefined" cert="high">ται</unclear> μ<unclear reason="undefined" cert="high">η</unclear><supplied reason="lost" cert="high">δεμιᾶς τῇ δεδανεισμένῃ ἢ τοῖς παρ᾽ αὐτῆς καταλειπο</supplied>-

    <lb n="18"/>μένης ἐφόδου κατὰ μ<unclear reason="undefined" cert="high">ηδ</unclear>ένα <supplied reason="lost" cert="high">τρόπον, ἀλλὰ ἐπάναγκον αὐτὴν παρέξεσθαι τῷ δεδα</supplied>-

    <lb n="19"/>νεικότι καὶ τοῖς <unclear reason="undefined" cert="high">π</unclear>αρ᾽ αὐτοῦ τὸ <unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">έρος τῆς οἰκίας διὰ παντὸς βέβαιον</supplied>

    <lb n="20"/><supplied reason="lost" cert="high">ἀπὸ πάντων</supplied> 

    <lb n="21"/>πάσῃ βεβαιώσει. ἀπὸ δὲ τοῦ <unclear reason="undefined" cert="high">νῦ</unclear><supplied reason="lost" cert="high">ν ἕως ἂν ἀποδώσει ἡ δεδανεισμένη τῷ δεδανεικότι</supplied> 

    <lb n="22"/>τὰς τοῦ <expan>ἀργ<ex cert="high">υρίου</ex></expan> <expan><ex cert="high">δραχμὰς</ex></expan> <num value="500"><unclear reason="undefined" cert="high">φ</unclear></num> <supplied reason="lost" cert="high">μὴ ἐξ</supplied><unclear reason="undefined" cert="high">εῖ</unclear>ναι αὐ<supplied reason="lost" cert="high">τῇ τῇ δεδανεισμένῃ τὸ μέρος τῆς οἰκίας πωλεῖν</supplied> 

    <lb n="23"/>μηδ᾽ ὑποτίθ<unclear reason="undefined" cert="high">ε</unclear><supplied reason="lost" cert="high">σθαι μηδ᾽</supplied> <unclear reason="undefined" cert="high">ἄ</unclear>λ<supplied reason="lost" cert="high">λ</supplied><unclear reason="undefined" cert="high">ω</unclear>ς κ<supplied reason="lost" cert="high">αταχρηματίζειν μηδ᾽ ἀπογράφεσθαί τινα ἐπ᾽ αὐτοῦ,</supplied> 

    <lb n="24"/><choice><corr>ἐκλογῆς</corr><sic>εγλο<unclear reason="undefined" cert="high">γης</unclear></sic></choice> <supplied reason="lost" cert="high">ο</supplied>ὔσης <supplied reason="lost" cert="high">τῷ δεδ</supplied><unclear reason="undefined" cert="high">α</unclear>νει<unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">ό</supplied>τ<supplied reason="lost" cert="high">ι διὰ ἑαυτοῦ μετὰ τὸν χρόνον μὴ ἀπολαβόντι ἐὰν</supplied> 

    <lb n="25"/><unclear reason="undefined" cert="high">β</unclear>ούληται κυ<unclear reason="undefined" cert="high">ρ</unclear><supplied reason="lost" cert="high">ιεύειν το</supplied><unclear reason="undefined" cert="high">ῦ</unclear> μ<unclear reason="undefined" cert="high">έ</unclear>ρου<supplied reason="lost" cert="high">ς ἀντὶ τοῦ κεφαλαίου ἢ τὴν πράξιν ποιεῖσθαι ἐκ τῆς δεδα</supplied>-

    <lb n="26"/><unclear reason="undefined" cert="high">ν</unclear>ει<supplied reason="lost" cert="high">σμέ</supplied>νη<supplied reason="lost" cert="high">ς καὶ τοῦ μέρους τῆς οἰκίας καὶ τῶν ἄλλων ὑπαρχόντων αὐτῇ πάντων κα</supplied>-

