    <lb n="1"/>Εὐδαίνων Χεμπολεῖτι τῆι 

    <lb n="2"/><unclear cert="high" reason="undefined">κυ</unclear>ρίαι μ<supplied cert="high" reason="lost">η</supplied>τρὶ χαίρειν. 

    <lb n="3"/><supplied cert="high" reason="lost">ἔγ</supplied>ρ<supplied cert="high" reason="lost">α</supplied>ψα <supplied cert="high" reason="lost">τ</supplied>ῆι ἀδελφ<supplied cert="high" reason="lost">ῆ</supplied><unclear cert="high" reason="undefined">ι</unclear> μου, ἵνα σοι 

    <lb n="4"/><supplied cert="high" reason="lost">π</supplied><unclear cert="high" reason="undefined">οι</unclear>ήσ<unclear cert="high" reason="undefined">ῃ</unclear> ἐν πρώτοις μετρηθῆναι 

    <lb n="5"/>τὰ μ<supplied cert="high" reason="lost">ετ</supplied><unclear cert="high" reason="undefined">ρ</unclear>ημάτια. ἐὰν δὲ ἦν <supplied cert="high" reason="lost">σ</supplied>οι 

    <lb n="6"/><supplied cert="high" reason="lost">ἀ</supplied>βα<supplied cert="high" reason="lost">ρ</supplied><unclear cert="high" reason="undefined">ές</unclear>, πρότρεψα<supplied cert="high" reason="lost">ι</supplied> Κάστορα τὸν 

    <lb n="7"/><unclear cert="high" reason="undefined">ἀδ</unclear><supplied cert="high" reason="lost">ελ</supplied><unclear cert="high" reason="undefined">φόν</unclear>, ἐὰν μέλλῃ ἐλθεῖν, 

    <lb n="8"/><supplied cert="high" reason="lost">ἐ</supplied>μ<unclear cert="high" reason="undefined">β</unclear>α<supplied cert="high" reason="lost">λ</supplied>έ<supplied cert="high" reason="lost">σ</supplied>θαι τοὺς ἡμετέρους 

    <lb n="9"/><supplied cert="high" reason="lost">ἐρρ</supplied><unclear cert="high" reason="undefined">ῶ</unclear>σθαί σε εὔχομαι, κυρία <choice><corr>μῆτερ</corr><sic>μιτερ</sic></choice> 

    <lb n="10"/><supplied cert="high" reason="lost">τιμ</supplied>ιωτάτη, μετὰ τῶν σῶν πάντων. 

    <lb n="11"/>ἀσπάζονταί σε τὰ παιδία. 
