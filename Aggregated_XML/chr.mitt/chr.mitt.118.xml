    <lb n="1"/>Λοκρη<supplied cert="high" reason="lost">τί</supplied>ωι Κ<supplied cert="high" reason="lost">ε</supplied>ριάλει στρατηγῶι 

    <lb n="2"/><expan>Ἀρσι<ex cert="high">νοίτου</ex></expan> <expan>Ἡρακ<ex cert="high">λείδου</ex></expan> <expan>μερίδο<ex cert="high">ς</ex></expan> 

    <lb n="3"/>παρὰ Τεσεάφεως <expan>νεωτ<ex cert="high">έρου</ex></expan> Τεσεάφιος 

    <lb n="4"/>τοῦ Πανεφρέμμεως <choice><corr>ἱερέως</corr><sic><hi rend="diaeresis">ι</hi>ερευς</sic></choice> ἀπὸ κώμης 

    <lb n="5"/>Σοκνοπαίου Νήσου τῆς αὐτῆς μερίδος. 

    <lb n="6"/>τῆι <num value="27">κζ</num> τοῦ <expan>διελ<supplied cert="high" reason="lost">η</supplied>λυθ<ex cert="high">ότος</ex></expan> μηνὸς Τῦβι 

    <lb n="7"/>τοῦ ἐνεστῶτος <num value="10">ι</num> <supplied cert="high" reason="lost"><expan><ex cert="high">ἔτους</ex></expan></supplied> <expan>Αὐτοκ<ex cert="high">ράτορος</ex></expan> Καίσαρος Νέρουα 

    <lb n="8"/>Τρ<supplied cert="high" reason="lost">αια</supplied>νοῦ Σ<supplied cert="high" reason="lost">εβαστοῦ Γ</supplied>ερμανικοῦ Δακικοῦ. 

    <lb n="9"/>ἐτελ<unclear cert="high" reason="undefined">ε</unclear>ύτησ<unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">ν ἡ γ</supplied>υνή μου Σεγάθης 

    <lb n="10"/><choice><corr>ἡ</corr><sic>της</sic></choice> Στοτοήτιος ἐμ<supplied cert="high" reason="lost">ο</supplied>ῦ ἔχοντος κέλλαν 

    <lb n="11"/><choice><corr>ἠσφαλισμένην</corr><sic>ησφαλεισμενην</sic></choice> ἐν οἰκίᾳ τοῦ πατρὸς 

    <lb n="12"/>αὐτῆς οὗ τὰ ἐπ<gap extent="2" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> πάντα ἅπερ ἐστὶν 

    <lb n="13"/><expan>ἀργυ<ex cert="high">ρίου</ex></expan> <expan><ex cert="high">δραχμὰς</ex></expan> <num value="1585">Αφπε</num> καὶ χα<supplied cert="high" reason="lost">λ</supplied>κίνων <expan><ex cert="high">δραχμὰς</ex></expan> <gap extent="3" reason="lost" unit="character"/> καὶ στολὴν 

    <lb n="14"/><choice><corr>λινοῦν</corr><sic>λεινουν</sic></choice> καὶ <choice><corr>δακτυλίδια</corr><sic>δακτυλιδρυα</sic></choice> ἀργυρᾶ ἱερευ-

    <lb n="15"/>τικῶν δύο <num value="2"/> καὶ ποτήρια <choice><corr>κασσιτέρινα</corr><sic>κασιδερινα</sic></choice> τρία <num value="3"/> 

    <lb n="16"/>καὶ <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">οτήριο</supplied>ν χαλκοῦν καὶ ἐ<gap extent="4" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ν</unclear> καὶ κ<unclear cert="high" reason="undefined">α</unclear>-

    <lb n="17"/><gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="5" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/> ἐν κοσμαρί<supplied cert="high" reason="lost">οις </supplied><gap extent="6" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/> καὶ 

    <lb n="18"/>στατῆρ<supplied cert="high" reason="lost">ας</supplied> <num value="14">ιδ</num> <supplied cert="high" reason="lost">καὶ σ</supplied>τολὴν <unclear cert="high" reason="undefined">χρω</unclear>ματίν<supplied cert="high" reason="lost">ην</supplied> 

    <lb n="19"/><supplied cert="high" reason="lost">καὶ τὰ ἄλλα</supplied> πάν<supplied cert="high" reason="lost">τα</supplied> ὄντα ἐν τῆι κ<supplied cert="high" reason="lost">έλλ</supplied>ηι 

    <lb n="20"/><gap extent="9" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ν</unclear><gap extent="6" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/>τι<gap extent="1" reason="illegible" unit="character"/><gap extent="8" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> <gap extent="2" reason="lost" unit="character"/> 

    <lb n="21"/><gap extent="16" reason="lost" unit="character"/><unclear cert="high" reason="undefined">η</unclear> τοῦ ἀδε<supplied cert="low" reason="lost">λφοῦ</supplied> <unclear cert="high" reason="undefined">α</unclear><gap extent="3" reason="lost" unit="character"/> 

    <lb n="22"/>ς<gap extent="7" reason="lost" unit="character"/> <unclear cert="high" reason="undefined">Ἀπ</unclear>ύγ<unclear cert="high" reason="undefined">χ</unclear>εως καὶ ἡ τούτου <supplied cert="high" reason="lost">γυ</supplied>νὴ Θενα-

    <lb n="23"/>πῦγχι<supplied cert="high" reason="lost">ς</supplied> οὖσα τῆς γυναικός μου ἀδελφὴ <choice><corr cert="low">γενά-

    <lb n="24"/>μενοι</corr><sic>γεναμενων</sic></choice> ἐπὶ τὸ αὐ<unclear cert="high" reason="undefined">τὸ</unclear> ἐμοῦ ἀπὸ πάλαι ἔχον-

    <lb n="25"/>τος <gap extent="3" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/> <choice><corr>κλεῖδα</corr><sic>κλειδαν</sic></choice> <supplied cert="high" reason="lost">τ</supplied>ῆς κέλλας ἰδίαν ἄνευ 

    <lb n="26"/>τῆς <supplied cert="high" reason="lost">ἐμῆ</supplied>ς γνώμης <gap extent="1" reason="lost" unit="character"/>πυ<gap extent="1" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/><unclear cert="high" reason="undefined">α</unclear>χαν μου 

    <lb n="27"/>τὴν <supplied cert="high" reason="lost">κέ</supplied>λλαν διὰ τοῦ <supplied cert="high" reason="lost">τ</supplied>ῆς κώμης κλειδοποιοῦ 

    <lb n="28"/>καὶ συλήσαντες ὅσα <choice><corr>ἔχω</corr><sic><supplied cert="high" reason="lost">ε</supplied>χωι</sic></choice> ἐν τῇ κέλλαι καὶ 

    <lb n="29"/>τὰ <expan>προσκεί<ex cert="high">μενα</ex></expan>. διὸ ἀξιῶ ἀχθῆναι αὐτοὺς ἐπὶ σὲ 

    <lb n="30"/>ὅπως τύχω τῆς ἀ<supplied cert="high" reason="lost">πὸ</supplied> σοῦ <choice><corr>δικαιοδοσίας</corr><sic>δικαιωδωσιας</sic></choice>. 

    <lb n="31"/>Τεσεᾶφις <choice><corr>ἐπιδέδωκα</corr><sic>επιδεδωκε</sic></choice> τὸ ἔνκλημα 

    <lb n="32"/>καθὼς <expan>πρόκ<ex cert="high">ειται</ex></expan>. <expan><ex cert="high">ἔτους</ex></expan> <num value="10">ι</num> <expan>Αὐτ<ex cert="high">οκράτορος</ex></expan> Καίσαρος Νέρουα 

    <lb n="33"/>Τραιανοῦ Σεβαστοῦ Γερμανικοῦ Δακικοῦ 

