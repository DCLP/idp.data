    <lb n="20"/><gap reason="lost" extent="9" unit="character"/><unclear reason="undefined" cert="high">ν</unclear><gap reason="lost" extent="6" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="1" unit="character"/>τι<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="8" unit="character"/><gap reason="illegible" extent="1" unit="character"/> <gap reason="lost" extent="2" unit="character"/> 

    <lb n="21"/><gap reason="lost" extent="16" unit="character"/><unclear reason="undefined" cert="high">η</unclear> τοῦ ἀδε<supplied reason="lost" cert="low">λφοῦ</supplied> <gap reason="lost" extent="3" unit="character"/> 

    <lb n="22"/>ς<gap reason="lost" extent="7" unit="character"/> <unclear reason="undefined" cert="high">Ἀπ</unclear>ύγ<unclear reason="undefined" cert="high">χ</unclear>εως καὶ ἡ τούτου <supplied reason="lost" cert="high">γυ</supplied>νὴ Θενα-

