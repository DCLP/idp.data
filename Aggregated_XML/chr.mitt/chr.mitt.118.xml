    <lb n="7"/>τοῦ ἐνεστῶτος <num value="10">ι</num> <supplied reason="lost" cert="high"><expan><ex cert="high">ἔτους</ex></expan></supplied> <expan>Αὐτοκ<ex cert="high">ράτορος</ex></expan> Καίσαρος Νέρουα 

    <lb n="8"/>Τρ<supplied reason="lost" cert="high">αια</supplied>νοῦ Σ<supplied reason="lost" cert="high">εβαστοῦ Γ</supplied>ερμανικοῦ Δακικοῦ. 

