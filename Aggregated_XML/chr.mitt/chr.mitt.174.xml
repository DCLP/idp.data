    <lb n="17"/><del rend="cross-strokes" status="unremarkable"> ἀ<supplied cert="high" reason="lost">ργ</supplied>υρίου δραχμὰς τετρακοσίας, <expan><ex cert="high">δραχμὰς</ex></expan> <num value="400">υ</num>, τόκου 
