    <lb n="5"/>κυρίο<supplied cert="high" reason="lost">υ</supplied> <unclear cert="high" reason="undefined">τ</unclear>οῦ ἐμαυτ<supplied cert="high" reason="lost">ῆ</supplied>ς ἀνδρ<supplied cert="high" reason="lost">ὸς Ἐρμείνο</supplied>υ τοῦ καὶ Ἀχ<supplied cert="high" reason="lost">ιλλέ</supplied>-

    <lb n="6"/>ως <supplied cert="high" reason="lost">ἐπηκ</supplied>ολ<supplied cert="high" reason="lost">ούθηκα τῇδε τῇ δ</supplied>ιαγ<supplied cert="high" reason="lost">ραφῇ</supplied> 

    <lb n="7"/>καὶ ἔσχ<supplied cert="high" reason="lost">ηκα π</supplied>αρὰ τοῦ ἀδελφο<supplied cert="high" reason="lost">ῦ μου Ἀπολλωνίου τοῦ καὶ</supplied> 

