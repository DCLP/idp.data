    <lb n="5"/><supplied cert="high" reason="lost">ούχιος ἀπὸ κώμης Μοιρ</supplied>ῶν τοῦ Κουσσίτου ἄνω μετὰ κυρίου <expan>Αὐρηλ<ex cert="high">ίου</ex></expan> Σαρ<supplied cert="high" reason="lost">ᾶ Ὡρε</supplied>ίτου 

    <lb n="6"/><supplied cert="high" reason="lost">τοῦ αἰτηθέν</supplied>το<supplied cert="high" reason="lost">ς ὑπ᾽ αὐτῆ</supplied>ς καὶ <expan>ἐπιτραπέντ<ex cert="high">ος</ex></expan> ὑπὸ Αὐρηλίου Κάστορος τοῦ καὶ Θ<supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">υμ</unclear><supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">στ</unclear><supplied cert="high" reason="lost">οῦ</supplied> ἐν<supplied cert="high" reason="lost">ά</supplied>ρ-

    <lb n="7"/><supplied cert="high" reason="lost">χου ἐξη</supplied>γη<supplied cert="high" reason="lost">τοῦ</supplied> <expan>βου[λ<ex cert="high">ευτοῦ</ex></expan> τ]<unclear cert="high" reason="undefined">ῆς</unclear> <expan><unclear cert="high" reason="undefined">α</unclear><ex cert="high">ὐτῆς</ex></expan> <supplied cert="high" reason="lost">Ἑρμοῦ</supplied> πόλεως δίκαιον ὧν ὑπήλλαξέν μοι κατὰ <expan>διαγρα<ex cert="high">φὴν</ex></expan> <supplied cert="high" reason="lost">ἐ</supplied><unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ὶ τ</supplied><unclear cert="high" reason="undefined">οῦ</unclear> <expan><unclear cert="high" reason="undefined">α</unclear><ex cert="high">ὐτοῦ</ex></expan> <num value="4">δ</num> <expan><ex cert="high">ἔτους</ex></expan> 

