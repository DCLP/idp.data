    <lb n="1"/>Σεμφθεὺς Περιγένου<unclear cert="high" reason="undefined">ς</unclear> 

    <lb n="2"/>ὅτι διώρυξεν οἰκίαν 

    <lb n="3"/>καὶ τὴν λείαν εὗρεν ἐν τοῖς <app type="BL"><lem resp="cf. 3.145"><unclear cert="high" reason="undefined">καν</unclear>αβί<unclear cert="high" reason="undefined">νο</unclear>ις</lem><rdg><unclear cert="high" reason="undefined">χ</unclear><gap extent="2" reason="illegible" unit="character"/>αβιν<unclear cert="high" reason="undefined">οι</unclear>σ</rdg></app>· 

    <lb n="4"/>λαβὼν <expan><ex cert="high">δραχμὰς</ex></expan> <num value="200">σ</num> ἀφῆκεν.  παρεδόθη 

    <lb n="5"/>δὲ Παῶτι δεσμοφύλακι. 

    <lb n="6"/><supplied cert="high" reason="lost">Ὧ</supplied><unclear cert="high" reason="undefined">ρ</unclear>ος Ἁρουήτιος <add place="supralinear">ὅτι</add> ἐδραγματοκλέπτει 

    <lb n="7"/>τρίτος ὢν λαβὼν παρ᾽ αὐτοῦ <expan><ex cert="high">δραχμὰς</ex></expan> <num value="100">ρ</num> ἀφῆκεν. 


    <lb n="8"/>Πεμνᾶς ὁ ἐν τοῖς Ἡρακλείδου κα<unclear cert="high" reason="undefined">ι</unclear><gap extent="3" reason="lost" unit="character"/> 

    <lb n="9"/><unclear cert="high" reason="undefined">ατο</unclear>υ <unclear cert="high" reason="undefined">ο</unclear>ἰκονό<unclear cert="high" reason="undefined">μ</unclear>ου ὅτι <gap extent="3" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">κ</unclear>ετο επ<gap extent="5" reason="lost" unit="character"/> 

    <lb n="10"/><gap extent="3" reason="illegible" unit="character"/> νιτρίας <gap extent="1" reason="illegible" unit="character"/>αι<unclear cert="high" reason="undefined">η</unclear><gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="3" reason="illegible" unit="character"/> 

    <lb n="11"/>λαβὼν <num value="600">χ</num> <expan><ex cert="high">δραχμὰς</ex></expan> ἀφῆκεν. 

    <lb n="12"/><gap extent="2" reason="lost" unit="character"/>ις γ<supplied cert="high" reason="lost">ε</supplied>ωργὸς τρίτος ὢν δια<gap extent="3" reason="lost" unit="character"/> 

    <lb n="13"/>α<gap extent="3" reason="illegible" unit="character"/>εν <unclear cert="high" reason="undefined">ἀγώ</unclear>για <num value="300">τ</num> ηκους<gap extent="6" reason="lost" unit="character"/> 

    <lb n="14"/>δ<unclear cert="high" reason="undefined">η</unclear>ς καὶ <expan>βα<ex cert="high">σιλικ  </ex></expan> <expan>γρ<ex cert="high">αμματεὺς</ex></expan> παρὰ τοῦ κω<supplied cert="high" reason="lost">μάρχου</supplied> 

    <lb n="15"/>λαβὼν παρ᾽ αὐτοῦ <expan><ex cert="high">δραχμὰς</ex></expan> <num value="300">τ</num> ἀφῆ<supplied cert="high" reason="lost">κεν</supplied>. 

    <lb n="16"/>Ἁρπκῶβι<supplied cert="high" reason="lost">ς</supplied> καὶ <unclear cert="high" reason="undefined">λογαφοσα</unclear> υἱοὶ 

    <lb n="17"/><gap extent="unknown" reason="lost" unit="character"/> λαβὼν παρ᾽ <unclear cert="high" reason="undefined">αὐ</unclear>το<unclear cert="high" reason="undefined">ῦ</unclear> 

