    <lb n="17"/><choice><corr>σεσθαι</corr><sic>επελευσασθαι</sic></choice> <unclear cert="high" reason="undefined">ἐ</unclear>π<supplied cert="high" reason="lost">ὶ</supplied> τ<unclear cert="high" reason="undefined">ὴ</unclear>ν Θαῆσιν <choice><corr>μήτε</corr><sic>μηδε</sic></choice> ἐπὶ τοὺς παρ᾽ αὐτῆς περὶ ὧν ἀπέσχ<unclear cert="high" reason="undefined">ηκ</unclear>αν 

    <lb n="18"/>καθότι πρό<unclear cert="high" reason="undefined">κ</unclear>ειται μηδὲ περὶ ἑτέρου ἁπλῶς πράγματος μέχρι τῆς <expan>ἐνεστ<ex cert="high">ώσης</ex></expan> 

    <lb n="19"/>ἡμέρα<unclear cert="high" reason="undefined">ς</unclear>, <unclear cert="high" reason="undefined">μὴ</unclear> <unclear cert="high" reason="undefined">ἐ</unclear><supplied cert="high" reason="lost">λ</supplied><unclear cert="high" reason="undefined">α</unclear>ττουμένης τῆς τῶν δύο <num value="2"/> Ἡρακλείδου καὶ Ἀπίας 

    <lb n="20"/>μητρ<unclear cert="high" reason="undefined">ὸ</unclear><supplied cert="high" reason="lost">ς Θαή</supplied><unclear cert="high" reason="undefined">σι</unclear>ος ὑπὲρ <unclear cert="high" reason="undefined">ὧ</unclear><supplied cert="high" reason="lost">ν</supplied> <choice><corr>ὀφείλει</corr><sic>ο<unclear cert="high" reason="undefined">φ</unclear>ιλει</sic></choice> αὐτῆι ἡ Θαῆσις κατὰ ἀσφάλειαν <g rend="extension" type="filler"/> 

    <lb n="21"/>ἀργυρι<supplied cert="high" reason="lost">κῶν</supplied> <unclear cert="high" reason="undefined">κ</unclear>εφαλαίων ἐφ᾽ οἷς περιέχει. <expan>ὑπογ<ex cert="high">ραφεὺς</ex></expan> τῶν <expan>γυν<supplied cert="high" reason="lost">αι</supplied>κ<ex cert="high">ῶν</ex></expan> ὁ <expan>ἑκ<unclear cert="high" reason="undefined">ατ</unclear><ex cert="high">έρας</ex></expan> κύριος. 

    <lb n="22"/><handShift new="m2"/> Ἡρακλεί<unclear cert="high" reason="undefined">δη</unclear>ς <unclear cert="high" reason="undefined">κ</unclear>αὶ Ἀπία ἡ καὶ Ἡράκλεια Διδύ<unclear cert="high" reason="undefined">μου</unclear> <unclear cert="high" reason="undefined">τ</unclear>ο<unclear cert="high" reason="undefined">ῦ</unclear> <supplied cert="high" reason="lost">Ἡρώδο</supplied>υ, ἡ Ἀ<unclear cert="high" reason="undefined">πί</unclear><supplied cert="high" reason="lost">α</supplied> 

    <lb n="23"/>μετὰ κυρ<unclear cert="high" reason="undefined">ί</unclear>ου τοῦ ἀν<supplied cert="high" reason="lost">δ</supplied>ρὸς <unclear cert="high" reason="undefined">Δ</unclear>ιδύμου τοῦ <supplied cert="high" reason="lost">Λυσιμ</supplied><unclear cert="high" reason="undefined">ά</unclear>χ<supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">υ</unclear> <supplied cert="high" reason="lost">ὁμο</supplied><unclear cert="high" reason="undefined">λ</unclear>ο<supplied cert="high" reason="lost">γ</supplied><unclear cert="high" reason="undefined">οῦμεν</unclear> 

