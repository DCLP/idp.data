    <lb n="5"/>ἄση<supplied cert="high" reason="lost">μος</supplied> ἀμφότεροι Διδύμου <choice><corr>πρεσβυτέρου</corr><sic>προσβυτερου</sic></choice> τοῦ Ἡρώδου, ἡ Ἀπία μετὰ κυρίο<unclear cert="high" reason="undefined">υ</unclear> 

    <lb n="6"/>τ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ῦ ἑ</supplied>αυτῆς ἀ<unclear cert="high" reason="undefined">ν</unclear>δρὸ<supplied cert="high" reason="lost">ς</supplied> Διδύμου τοῦ <unclear cert="high" reason="undefined">Λ</unclear><supplied cert="high" reason="lost">υ</supplied>σιμά<supplied cert="high" reason="lost">χ</supplied>ου ὡς ἐτῶν τεσσεράκοντα 

    <lb n="7"/>ἓξ <num value="46"/> <choice><corr>οὐλὴ</corr><sic><unclear cert="high" reason="undefined">ου</unclear>ληι</sic></choice> <unclear cert="high" reason="undefined">μή</unclear>λωι <unclear cert="high" reason="undefined">δ</unclear>εξ<supplied cert="high" reason="lost">ι</supplied>ῶι οἱ δύο <num value="2"/> Θαή<unclear cert="high" reason="undefined">σι</unclear> Ψενκήβκιος τοῦ καὶ Κρονίωνος 

    <lb n="8"/>τοῦ Μαρσισούχου ὡς ἐτῶν πεντήκοντα <num value="50"/> <choice><corr>οὐλὴ</corr><sic>ουληι</sic></choice> ἀντικνημίωι δεξιῶι <g rend="extension" type="filler"/> 

    <lb n="9"/>με<unclear cert="high" reason="undefined">τὰ</unclear> κυρίου τοῦ ἑαυτῆς συνγενοῦς Μαρεψήμιος τοῦ Μαρεψήμιος ὡς <expan>ἐτῶ<ex cert="high">ν</ex></expan> 

    <lb n="10"/>τριάκοντα ἓξ <num value="36"/> <choice><corr>οὐλὴ</corr><sic>ουληι</sic></choice> <supplied cert="high" reason="lost">ῥ</supplied>ινὶ μέσηι ἀπέχειν <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">α</supplied>ρ᾽ αὐτῆς Θαήσιος διὰ χειρὸς 

    <lb n="11"/>ἀργυρίο<supplied cert="high" reason="lost">υ</supplied> <unclear cert="high" reason="undefined">δ</unclear>ραχμὰ<unclear cert="high" reason="undefined">ς</unclear> <choice><corr>χιλίας</corr><sic>χειλειας</sic></choice> διακοσίας εἴκοσι <choice><corr>τέσσαρας</corr><sic>τεσσαρος</sic></choice> <num value="1224"/> καὶ τοὺς τόκους 

    <lb n="12"/>ἃ <choice><corr>ὤφειλεν</corr><sic>ωφιλεν</sic></choice> ἡ Θα<unclear cert="high" reason="undefined">ῆσ</unclear>ις τῇ τῶν δύο <num value="2"/> Ἡρακλείδου καὶ Ἀπίας ἀδελφῆι Ἡρωδιαίνῃ 

    <lb n="13"/>ἥτ<unclear cert="high" reason="undefined">ις</unclear> <choice><corr>ἀπῄτησεν</corr><sic><unclear cert="high" reason="undefined">α</unclear>πηιτηισεν</sic></choice> κατὰ ἀσφάλειαν <choice><corr>τετελειωμένην</corr><sic>τετελει<unclear cert="high" reason="undefined">ο</unclear>μενην</sic></choice> διὰ τοῦ ἐν Θεογονίδι 

    <lb n="14"/>γρα<unclear cert="high" reason="undefined">φ</unclear><supplied cert="high" reason="lost">είο</supplied><unclear cert="high" reason="undefined">υ</unclear> τῶι ὀγδόωι ἔτει Τραιανοῦ Καίσαρος τοῦ κυρίου μηνὶ Καισαρείωι <num value="16">ιϛ</num> 

    <lb n="15"/>ἣν καὶ ἀναδέδωκαν τῆι Θ<unclear cert="high" reason="undefined">α</unclear>ῆσι εἰς <choice><corr>ἀθετείαν</corr><sic><supplied cert="high" reason="lost">α</supplied>θε<unclear cert="high" reason="undefined">τ</unclear>ι<unclear cert="high" reason="undefined">α</unclear>ν</sic></choice> καὶ ἀκύρωσιν, καὶ μήτε 

