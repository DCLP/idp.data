    <lb n="1"/>Ἱέρακι <expan>στρ<ex cert="high">ατηγῷ</ex></expan> <expan>Ἀρσι<ex cert="high">νοΐτου</ex></expan> <expan>Θεμίσ<ex cert="high">του</ex></expan> καὶ <expan>Πολ<ex cert="high">έμωνος</ex></expan> μερίδων 

    <lb n="2"/>καὶ Ἡρωδιανῷ <expan>βασι<ex cert="high">λικῷ</ex></expan> <expan>γρ<ex cert="high">αμματεῖ</ex></expan> <expan>Πολέμ<ex cert="high">ωνος</ex></expan> μερίδος 

    <lb n="3"/>καὶ <expan>κωμογρ<ex cert="high">αμματεῖ</ex></expan> Τεπτύνεως καὶ <expan>Κερκήσ<ex cert="high">εως</ex></expan> 

    <lb n="4"/>παρὰ Ἰσιδώρας τῆς καὶ Τυ-

    <lb n="5"/>ραννίδος τελείας καὶ Ἰουλίου 

    <lb n="6"/>τοῦ καὶ Ἡρώδου ἀφήλικος 

    <lb n="7"/>ἀμφοτέρων Φιλαντινόου 

    <lb n="8"/>τοῦ καὶ Ἡρώδου Ἀντινοέων, 

    <lb n="9"/>τοῦ δὲ ἀφήλικος δι᾽ ἐπιτρόπου 

    <lb n="10"/>Φιλοσαράπιδος Λυσιμάχου. ἀπο-

    <lb n="11"/>γραφόμεθα τὰς ὑπαρχούσας 

    <lb n="12"/>ἡμῖν περὶ κώμην Κ<unclear reason="undefined" cert="high">ερ</unclear>κῆσιν 

    <lb n="13"/>κλήρου κατοικικοῦ ἀρούρας δύο <num value="2"/> 

    <lb n="14"/>ἐν τόπῳ Δίφρ<unclear reason="undefined" cert="high">ου</unclear> <expan><unclear reason="undefined" cert="high">λεγο</unclear><ex cert="high">μένῳ</ex></expan> καθυ-

    <lb n="15"/>δάτους γενομένας π<unclear reason="undefined" cert="high">ρ</unclear>ὸς τὸ 

    <lb n="16"/>ἐνεστὸς τεσσαρεσκαιδέκα-

    <lb n="17"/><supplied reason="lost" cert="high">το</supplied>ν ἔτος. δι᾽ ὃ <choice><corr>ἐπιδίδομεν</corr><sic>επιδιδωμι</sic></choice>. 

    <lb n="18"/><handShift new="m2"/> <expan>ἀπ<ex cert="high">εγράφη</ex></expan> <expan>π<ex cert="high">αρὰ</ex></expan> <expan>στρ<ex cert="high">ατηγῷ</ex></expan> <expan>Φαμ<ex cert="high">ενὼθ</ex></expan> <num value="8">η</num>. 

    <lb n="19"/><handShift new="m3"/> <expan>ἀπ<ex cert="high">εγράφη</ex></expan> <expan>π<ex cert="high">αρὰ</ex></expan> <expan><unclear reason="undefined" cert="high">β</unclear>ασιλ<ex cert="high">ικῷ</ex></expan> <expan>γρ<ex cert="high">αμματεῖ</ex></expan> <expan>Φα<ex cert="high">μενὼθ</ex></expan> <num value="27">κ<unclear reason="undefined" cert="high">ζ</unclear></num>. 

