    <lb n="12"/>τοῦ κ<unclear cert="high" reason="undefined">υ</unclear><supplied cert="high" reason="lost">ρίου </supplied><gap extent="3" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> <num value="30">λ</num>. 

    <lb n="13"/>ἔσχον κ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ὶ</supplied> τῷ <num value="19">ιθ</num> <expan><ex cert="high">ἔτει</ex></expan> 

    <lb n="14"/>ὑπὲρ οὗ γεωργεῖς μου 

    <lb n="15"/>κλήρου ἐπὶ λόγου ἀρτάβας 

    <lb n="16"/>ὀκτὼ <num value="8"/> σῖτον γε<supplied cert="high" reason="lost">ν</supplied>ήματος τοῦ 

    <lb n="17"/>διεληλυθότος <choice><corr>ὀκτωκαιδεκά<supplied cert="high" reason="omitted">του</supplied></corr><sic><supplied cert="high" reason="lost">ο</supplied>κτωκαιδεκα</sic></choice> 

    <lb n="18"/>ἔτο<supplied cert="high" reason="lost">υς</supplied>, <unclear cert="high" reason="undefined">μὴ</unclear> ἐλ<unclear cert="high" reason="undefined">ατ</unclear>τουμένης 

    <lb n="19"/>μου <unclear cert="high" reason="undefined">π</unclear>ερὶ ὧν λοιπῶν <expan>ὀφείλ<ex cert="high">εις</ex></expan> 

    <lb n="20"/>μοι. <expan><ex cert="high">ἔτους</ex></expan> <num value="19">ιθ</num> Ἀντωνίνου Καίσαρος 

    <lb n="21"/>τοῦ κυρίου Παῦνι <num value="26">κ<unclear cert="high" reason="undefined">ϛ</unclear></num>. καὶ φακοῦ 


