    <lb n="1"/><num value="27">κζ</num> 

    <lb n="2"/>ἔτους ἑπτακαιδεκάτο<unclear cert="high" reason="undefined">υ</unclear> <supplied cert="high" reason="lost">Α</supplied>ὐ<unclear cert="high" reason="undefined">τ</unclear>οκρ<supplied cert="high" reason="lost">άτορο</supplied>ς Καίσαρ<supplied cert="high" reason="lost">ο</supplied>ς Τραιανοῦ 

    <lb n="3"/>Ἁδριανο<unclear cert="high" reason="undefined">ῦ</unclear> Σεβαστοῦ <unclear cert="high" reason="undefined">Φα</unclear>ῶφι <num value="21"><unclear cert="high" reason="undefined">κα</unclear></num> <unclear cert="high" reason="undefined">ἐν</unclear> <choice><corr>Τεβτύνει</corr><sic>τεβτυνι</sic></choice> τῆς Πολέμο-

    <lb n="4"/><unclear cert="high" reason="undefined">ν</unclear>ος μ<unclear cert="high" reason="undefined">ε</unclear>ρίδ<unclear cert="high" reason="undefined">ο</unclear>ς τοῦ Ἀρ<unclear cert="high" reason="undefined">σι</unclear>νοείτου <unclear cert="high" reason="undefined">ν</unclear>ομοῦ. ὁμολογεῖ Ἡράκλεια 

    <lb n="5"/><choice><corr cert="low">Ἑρμοῦ</corr><sic>ερμους</sic></choice> ὡ<supplied cert="high" reason="lost">ς</supplied> ἐ<unclear cert="high" reason="undefined">τῶ</unclear><supplied cert="high" reason="lost">ν τριά</supplied>κοντα <unclear cert="high" reason="undefined">ἐνν</unclear>έα <num value="39"/> ἄσ<unclear cert="high" reason="undefined">ημος</unclear> <unclear cert="high" reason="undefined">μ</unclear>ε<unclear cert="high" reason="undefined">τ</unclear>ὰ κυ-

    <lb n="6"/>ρίου τοῦ σ<unclear cert="high" reason="undefined">υνγεν</unclear>οῦς Λυσιμάχ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">υ τ</supplied><unclear cert="high" reason="undefined">οῦ</unclear> Λυσα<unclear cert="high" reason="undefined">νεί</unclear>ο<supplied cert="high" reason="lost">υ ὡς ἐτῶν</supplied> 

    <lb n="7"/>ἑβδομήκοντα δύο <num value="72"/> <choice><corr>οὐλὴ</corr><sic>ουληι</sic></choice> τρ<unclear cert="high" reason="undefined">α</unclear>χ<unclear cert="high" reason="undefined">ή</unclear>λωι <choice><corr>ἐκ</corr><sic>εγ</sic></choice> δεξιῶν τῶι 

    <lb n="8"/>ἑαυτῆς <choice><corr>ἀνδρὶ</corr><sic>α<supplied cert="high" reason="lost">ν</supplied><unclear cert="high" reason="undefined">δρει</unclear></sic></choice> Ἡρακλείδηι <supplied cert="high" reason="lost">τῶι</supplied> <unclear cert="high" reason="undefined">κ</unclear>αὶ Οὐαλερίωι <expan>Ἡρακλείδ<ex cert="high">ου</ex></expan> 

    <lb n="9"/>ὡς ἐτῶν <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">ν</unclear>τ<unclear cert="high" reason="undefined">ή</unclear>κοντα ἑπτ<supplied cert="high" reason="lost">ὰ <num value="57"/> </supplied><choice><corr><supplied cert="high" reason="lost">ο</supplied>ὐλὴ</corr><sic><supplied cert="high" reason="lost">ο</supplied>υληι</sic></choice> γόνατι ἀριστερῶι, 

    <lb n="10"/>τὴν Ἡρ<supplied cert="high" reason="lost">άκλει</supplied>αν συν<unclear cert="high" reason="undefined">εσ</unclear>τακέν<supplied cert="high" reason="lost">αι</supplied> <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">α</supplied>τὰ τήνδε τὴν ὁμολο-

    <lb n="11"/><choice><corr>γίαν</corr><sic>ομολο<unclear cert="high" reason="undefined">γ</unclear>ειαν</sic></choice> τὸ<unclear cert="high" reason="undefined">ν</unclear> <supplied cert="high" reason="lost">ἄν</supplied><unclear cert="high" reason="undefined">δ</unclear>ρα Ἡρακλείδην <supplied cert="high" reason="lost">τὸν</supplied> καὶ Οὐαλέριον πωλή-

    <lb n="12"/><choice><corr>σοντα</corr><sic>πωλη<unclear cert="high" reason="undefined">σ</unclear>αντα</sic></choice> ἐξ ὀ<supplied cert="high" reason="lost">ν</supplied>όματος αὐτῆς <supplied cert="high" reason="lost">ταύ</supplied><unclear cert="high" reason="undefined">τ</unclear>ης δι᾽ αὐτοῦ 

    <lb n="13"/>τὴν ὑπάρ<unclear cert="high" reason="undefined">χο</unclear>υσαν αὐτῆι Ἡρακλε<unclear cert="high" reason="undefined">ίᾳ</unclear> <choice><corr>οἰκογενῆ</corr><sic>οικογενην</sic></choice> δούλην 

    <lb n="14"/>Ἀθηνάριο<unclear cert="high" reason="undefined">ν</unclear> ταύτην τοιαύτ<unclear cert="high" reason="undefined">ην</unclear> <choice><corr>ἀναπόρριφον</corr><sic><unclear cert="high" reason="undefined">α</unclear>ναποριφον</sic></choice> 

    <lb n="15"/>πλὴν ἐπ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">φ</supplied>ῆς καὶ ἱερᾶς νόσο<unclear cert="high" reason="undefined">υ</unclear> τῆι ἰδίᾳ αὐτοῦ 

    <lb n="16"/>Οὐαλερίου <choice><corr><supplied cert="high" reason="lost">π</supplied>ίστει</corr><sic><supplied cert="high" reason="lost">π</supplied><unclear cert="high" reason="undefined">ει</unclear>στει</sic></choice> <subst><add place="inline"><unclear cert="high" reason="undefined">ἧ</unclear>ς</add><del rend="corrected">ες</del></subst> ἐὰν <unclear cert="high" reason="undefined">εὕρ</unclear>ῃ τιμῆς τοῖς προσ-

    <lb n="17"/>ελευσομένοις ἀγ<unclear cert="high" reason="undefined">ορ</unclear>ασταῖς, <unclear cert="high" reason="undefined">τῆ</unclear>ς βεβαιώσεως πάσῃ 

    <lb n="18"/><choice><corr>βεβαιώσει</corr><sic>βεβαιωσι</sic></choice> ἐξακολουθούσης τῇ <unclear cert="high" reason="undefined">Ἡ</unclear>ρακλείᾳ, ἣν δὲ ἐὰν 

    <lb n="19"/>ἀπολάβηι <supplied cert="high" reason="lost">ὁ</supplied> Οὐαλέριος τῆς <unclear cert="high" reason="undefined">Ἀθ</unclear>ηναρίου τιμὴν 

    <lb n="20"/>παρακομ<unclear cert="high" reason="undefined">ιε</unclear><supplied cert="high" reason="lost">ῖ</supplied><unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">αι</supplied> τῇ Ἡρακλείᾳ. <unclear cert="high" reason="undefined">τὰ</unclear> <choice><corr>διωμολογημένα</corr><sic>διομολογημενα</sic></choice> 


    <lb n="21"/>κύρια. <handShift new="m2"/> Ἡράκλεια <choice><corr cert="low">Ἑρμοῦ</corr><sic>ερμους</sic></choice> μετὰ κυρίου τοῦ συνγενοῦς 

    <lb n="22"/>Λυσιμάχου Λυσανίου πεποίημαι <unclear cert="high" reason="undefined">τ</unclear>ῷ ἀνδρί μου Ἡρακλείδῃ 

    <lb n="23"/><choice><corr>τῷ</corr><sic>το</sic></choice> καὶ Οὐα<supplied cert="high" reason="lost">λ</supplied><unclear cert="high" reason="undefined">ε</unclear>ρίωι τὸ συστατικὸν <choice><corr>πωλήσοντι</corr><sic>πολησαντα</sic></choice> τὴν δούλην 

    <lb n="24"/>Ἀθηνάριον καθὼς πρόκειται. Λυ<supplied cert="high" reason="lost">σί</supplied><unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">α</supplied>χος ἔγραψα καὶ ὑπὲρ <expan>αὐ<ex cert="high">τῆς</ex></expan> 

    <lb n="25"/>μὴ εἰδυ<supplied cert="high" reason="lost">ίας γ</supplied>ράμματα. <handShift new="m3"/> Ἡρακλεί<unclear cert="high" reason="undefined">δης</unclear> ὁ καὶ Οὐαλέρις 

    <lb n="26"/>γέγον<supplied cert="high" reason="lost">ε εἴ</supplied><unclear cert="high" reason="undefined">ς</unclear> με τὸ συστατι<unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">όν</supplied>, καὶ ἣν ἐὰν λάβω 

    <lb n="27"/>τιμ<unclear cert="high" reason="undefined">ὴν</unclear> <supplied cert="high" reason="lost">ἀπο</supplied>καταστήσω τ<unclear cert="high" reason="undefined">ῇ</unclear> <supplied cert="high" reason="lost">Ἡ</supplied>ρακλείᾳ καθὼς 

    <lb n="28"/>πρόκει<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">αι</supplied>. <handShift new="m1"/> <expan>ἀναγέγρ<ex cert="high">απται</ex></expan> δ<supplied cert="high" reason="lost">ι</supplied><unclear cert="high" reason="undefined">ὰ</unclear> τοῦ ἐν Τεβτύνει 

    <lb n="29"/>γρ<unclear cert="high" reason="undefined">α</unclear>φείου δι<unclear cert="high" reason="undefined">ὰ</unclear> Λουρίου. 
