    <lb n="6"/>μητρὸς Δημητρίας <gap reason="lost" extent="14" unit="character"/><supplied reason="lost" cert="high"> ἐν Ὀξυρύγ</supplied>-

    <lb n="7"/>χων πόλει ὡς ἐτῶ<supplied reason="lost" cert="high">ν </supplied><gap reason="lost" extent="10" unit="character"/><supplied reason="lost" cert="high"> οὐλὴ </supplied><gap reason="lost" extent="8" unit="character"/> 

    <lb n="8"/>δεξιῶι παρακεχω<supplied reason="lost" cert="high">ρηκέναι </supplied><gap reason="lost" extent="12" unit="character"/><supplied reason="lost" cert="high"> Ἀπολ</supplied>-

    <lb n="9"/>λοδώρου τοῦ Ἀπολλ<unclear reason="undefined" cert="high">ω</unclear><gap reason="lost" extent="18" unit="character"/><supplied reason="lost" cert="high"> τῷ καὶ</supplied> 

    <lb n="10"/>Ἀλθαιεῖ ὡς ἐτῶν πεν<supplied reason="lost" cert="high">τήκοντα <num value="50"/> </supplied><gap reason="lost" extent="16" unit="character"/> 

    <lb n="11"/>τὰς ὑπαρχούσας αὐτῷ <supplied reason="lost" cert="high">περὶ κώμην </supplied><gap reason="lost" extent="9" unit="character"/><supplied reason="lost" cert="high"> ἐκ τοῦ</supplied> 

    <lb n="12"/>Στεφάνου κλήρου ἀπὸ <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">οῦ νῦν εἰς τὸν ἅπαντα χρόνον</supplied> 

    <lb n="13"/>μεθ᾽ ἃς ἀπελέξατο ὁ ὁ<supplied reason="lost" cert="high">μολογῶν </supplied><gap reason="lost" extent="16" unit="character"/> 

    <lb n="14"/>ὀγδόωι ἔτει Τραϊανοῦ <supplied reason="lost" cert="high">Καίσαρος τοῦ κυρίου ἀρούρας</supplied> 

