    <lb n="16"/>ἀρούρας πέντε <num value="5"/> ὧν γ<supplied reason="lost" cert="low">είτονες νότου </supplied><gap reason="lost" extent="4" unit="character"/><supplied reason="lost" cert="low"> βορρᾶ </supplied><gap reason="lost" extent="3" unit="character"/> 

    <lb n="17"/>λιβὸς Οὐϊσπελλίου Φαν<gap reason="lost" extent="3" unit="character"/><supplied reason="lost" cert="high"> ἀπηλιώτου </supplied><gap reason="lost" extent="5" unit="character"/><supplied reason="lost" cert="high"> καὶ αὐ</supplied>-

    <lb n="18"/>τόθι προσομολογεῖ ὁ ὁ<supplied reason="lost" cert="high">μολογῶν ἀπεσχηκέναι</supplied> 

    <lb n="19"/>παρὰ τοῦ ὠνουμένου τ<supplied reason="lost" cert="high">ὴν συμπεφωνημένην τῶν ἀρου</supplied>-

