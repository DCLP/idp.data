    <lb n="1"/><g type="stauros"/> ταῦτα μαρτυρ<unclear cert="high" reason="undefined">οῦ</unclear>μεν <choice><corr>ἡμεῖς</corr><sic>ημις</sic></choice> Σέργιος Γεωρ<unclear cert="high" reason="undefined">γίο</unclear>υ ἐλέει <expan>θ<ex cert="high">εο</ex></expan>ῦ <expan>πρεσ<unclear cert="high" reason="undefined">β</unclear><ex cert="high">ύ</ex><unclear cert="high" reason="undefined">τ</unclear><ex cert="high">ερος</ex></expan> καὶ Γεώργιος 

    <lb n="2"/>Βίκτορος ἐλέει <expan>θ<ex cert="high">εο</ex></expan><unclear cert="high" reason="undefined">ῦ</unclear> <expan>ἀρχιδιάκο<ex cert="high">νος</ex></expan> καὶ Σέρ<supplied cert="high" reason="lost">γ</supplied><unclear cert="high" reason="undefined">ι</unclear>ος Παλλαδίου <expan>διάκο<ex cert="high">νος</ex></expan> καὶ <hi rend="diaeresis">Ἰ</hi><unclear cert="high" reason="undefined">ω</unclear>άννης 

    <lb n="3"/>Στεφάνου τοῦ Αζαειαθ καὶ Ζαχαρίας Γε<unclear cert="high" reason="undefined">ω</unclear>ρ<unclear cert="high" reason="undefined">γ</unclear>ίο<unclear cert="high" reason="undefined">υ</unclear> καὶ Στέφανος Οεδου καὶ Γεώργις 

    <lb n="4"/>Ἠλίου ὅτι <choice><corr>σύνηξεν</corr><sic>εσυναξεν</sic></choice> ἡμᾶς <hi rend="diaeresis">Ἰ</hi>ωάννης Ουαλιου <expan>πρεσβ<ex cert="high">ύ</ex>τ<ex cert="high">ερος</ex></expan> καὶ <choice><corr>ἡ</corr><sic>την</sic></choice> <choice><corr>γυνὴ</corr><sic>γυνεκα</sic></choice> 

    <lb n="5"/>αὐτοῦ Νόννα <hi rend="diaeresis">Ἰ</hi>ωάννου τοῦ Κοτεμου καὶ μετ᾽ αὐτῆς <choice><corr>ἡ</corr><sic>ει</sic></choice> μήτηρ αὐτῆς Θεκεμις 

    <lb n="6"/>καὶ πο<unclear cert="high" reason="undefined">λλ</unclear>ὰ κιν<unclear cert="high" reason="undefined">η</unclear>θέ<unclear cert="high" reason="undefined">ντ</unclear>α μεταξὺ <unclear cert="high" reason="undefined">α</unclear>ὐτῶν καὶ ἀμφ<unclear cert="high" reason="undefined">ιβα</unclear>λλόμενα <unclear cert="high" reason="undefined">π</unclear>ερὶ <choice><corr>κεφάλαια</corr><sic>κεφαλεα</sic></choice> 

    <lb n="7"/>καὶ <choice><corr>ἐμείναμεν</corr><sic>εμηναμεν</sic></choice> πάντες οἱ προο<unclear cert="high" reason="undefined">νο</unclear>μασθέντες <choice><corr>ἄνδρες</corr><sic>ανδραις</sic></choice> <hi rend="diaeresis">ἵ</hi>να <choice><corr>ὦσιν</corr><sic>εισιν</sic></choice> πρὸς 

    <lb n="8"/>ἄλλους καὶ πολλὰ εἴπα<unclear cert="high" reason="undefined">μ</unclear>εν μεταξὺ αὐτῶν <hi rend="diaeresis">ἵ</hi>να <choice><corr>συναχθῶσιν</corr><sic>συναχθουσιν</sic></choice> πρὸς <choice><corr>ἀλλήλους</corr><sic>αλλιλους</sic></choice> 

    <lb n="9"/>καὶ οὐκ <choice><corr>ἀνεσχέθησαν</corr><sic>ανασχεθεισαν</sic></choice>. τότε εἶπεν ὁ αὐτὸ<unclear cert="high" reason="undefined">ς</unclear> <hi rend="diaeresis"><unclear cert="high" reason="undefined">Ἰ</unclear></hi><unclear cert="high" reason="undefined">ωά</unclear>ννης <expan>πρε<ex cert="high">σβύτερος</ex></expan> <choice><corr>τῇ</corr><sic>την</sic></choice> α<unclear cert="high" reason="undefined">ὐ</unclear>τοῦ <choice><corr>γυναικὶ</corr><sic>γυνη</sic></choice> 

    <lb n="10"/>ὅτι <choice><corr>διδῶ</corr><sic>δι<unclear cert="high" reason="undefined">δο</unclear></sic></choice> <choice><corr>σοι</corr><sic>σαι</sic></choice> ἐπιλογήν· ἐὰν <choice><corr>θέλῃς</corr><sic>θελις</sic></choice> <unclear cert="high" reason="undefined">δ</unclear>ι<unclear cert="high" reason="undefined">κα</unclear>στ<unclear cert="high" reason="undefined">ὴ</unclear>ν καὶ ἐὰν θέλῃς <choice><corr>τινά</corr><sic>τηναν</sic></choice> ἐκ τοῦ 

    <lb n="11"/>χωρίου ἡμῶν καὶ <unclear cert="high" reason="undefined">ὅν</unclear> <unclear cert="high" reason="undefined">π</unclear>ου δ᾽ ἂν κ<unclear cert="high" reason="undefined">α</unclear>ὶ <unclear cert="high" reason="undefined">θ</unclear>έλῃς <choice><corr>λαμβάνω</corr><sic>λαμβανο</sic></choice> μετ᾽ ἐσοῦ δικαστήν. 


    <lb n="12"/>τότε <choice><corr>ἀπεκρίθη</corr><sic>απεκριθει</sic></choice> ἡ <choice><corr>αὐτὴ</corr><sic>αυτι</sic></choice> Νό<supplied cert="high" reason="lost">νν</supplied>α <unclear cert="high" reason="undefined">σ</unclear>ὺν <choice><corr>τῇ</corr><sic>τι</sic></choice> μητρὶ <unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ὐ</supplied><unclear cert="high" reason="undefined">τ</unclear>ῆς καὶ εἶπα<unclear cert="high" reason="undefined">ν</unclear> <supplied cert="high" reason="lost">ὅ</supplied>τι οὐδὲν θέλ<unclear cert="high" reason="undefined">ο</unclear>μεν 

    <lb n="13"/>ἔκ σου <unclear cert="high" reason="undefined">οὔ</unclear>τε λαμβάνομεν <unclear cert="high" reason="undefined">μ</unclear>ε<unclear cert="high" reason="undefined">τὰ</unclear> <unclear cert="high" reason="undefined">σοῦ</unclear> <choice><corr>τινὰ</corr><sic>τ<unclear cert="high" reason="undefined">η</unclear>ν<unclear cert="high" reason="undefined">αν</unclear></sic></choice> <unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ὔ</supplied><unclear cert="high" reason="undefined">τε</unclear> <choice><corr>εἵλομεν</corr><sic><unclear cert="high" reason="undefined">ι</unclear>λομεν</sic></choice> ἔκ σου τί ποτε 

    <lb n="14"/>οὔτε ἀπὸ πρ<unclear cert="high" reason="undefined">οικὸς</unclear> οὔτε ἀπὸ <choice><corr>οἱουδήποτε</corr><sic>οιουδ<unclear cert="high" reason="undefined">ι</unclear>π<unclear cert="high" reason="undefined">ο</unclear>τε</sic></choice> π<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ά</supplied><unclear cert="high" reason="undefined">γ</unclear>ματος κινητ<supplied cert="high" reason="lost">οῦ</supplied> ἢ <choice><corr>αὐτοκίνη-

    <lb n="15"/>του</corr><sic>αυτ<unclear cert="high" reason="undefined">ο</unclear>κινητος</sic></choice> ἀλλὰ <hi rend="diaeresis">ἵ</hi>να ἀπολ<unclear cert="high" reason="undefined">ύ</unclear>σῃς <unclear cert="high" reason="undefined">ἐ</unclear><supplied cert="high" reason="lost">μ</supplied><unclear cert="high" reason="undefined">έ</unclear>. τό<unclear cert="high" reason="undefined">τ</unclear>ε <choice><corr>ἀπεκρίθη</corr><sic>απεκριθει</sic></choice> ὁ αὐτὸς <hi rend="diaeresis">Ἰ</hi><unclear cert="high" reason="undefined">ωά</unclear>ννης 

    <lb n="16"/>παρουσίαν ἡμῶν <subst><add place="inline">τῶ<unclear cert="high" reason="undefined">ν</unclear></add><del rend="corrected">το<unclear cert="high" reason="undefined">ν</unclear></del></subst> <unclear cert="high" reason="undefined">πρ</unclear>οο<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">ο</supplied>μα<supplied cert="high" reason="lost">σθ</supplied><unclear cert="high" reason="undefined">έ</unclear><supplied cert="high" reason="lost">ν</supplied><unclear cert="high" reason="undefined">τ</unclear>ων ἀνδρ<unclear cert="high" reason="undefined">ῶ</unclear>ν ὅτι <choice><corr>ἀπολυτικὴ</corr><sic>απολυτικιν</sic></choice> εἶ. 

    <lb n="17"/>οὐδένα λόγον ἔχομ<unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">ν</supplied> <unclear cert="high" reason="undefined">π</unclear>ρὸς <choice><corr>ἀλλήλους</corr><sic>αλλ<unclear cert="high" reason="undefined">ι</unclear>λους</sic></choice>. καὶ ταῦτα μαρτυροῦμεν 

    <lb n="18"/><choice><corr>ἡμεῖς</corr><sic>ημις</sic></choice> <choice><corr>οἱ</corr><sic>ει</sic></choice> προ<unclear cert="high" reason="undefined">ο</unclear>νομασθέντες <choice><corr>ἄνδρες</corr><sic>ανδραις</sic></choice> <choice><corr>ἐνώπιον</corr><sic>ε<unclear cert="high" reason="undefined">νο</unclear>πιον</sic></choice> το<unclear cert="high" reason="undefined">ῦ</unclear> θεο<unclear cert="high" reason="undefined">ῦ</unclear>. <unclear cert="high" reason="undefined">ο</unclear>ὔτε δὲ ἐγὼ 

    <lb n="19"/>Νόννα οὔτε <choice><corr>οἱ</corr><sic>η</sic></choice> <choice><corr>κληρονόμοι</corr><sic>κληρ<unclear cert="high" reason="undefined">ο</unclear>νομου</sic></choice> <unclear cert="high" reason="undefined">μου</unclear> οὔτε διάδοχο<unclear cert="high" reason="undefined">ι</unclear> ο<unclear cert="high" reason="undefined">ὔ</unclear>τε σοι <hi rend="diaeresis">Ἰ</hi><unclear cert="high" reason="undefined">ω</unclear>άννῃ ο<supplied cert="high" reason="lost">ὔ</supplied>τε 

