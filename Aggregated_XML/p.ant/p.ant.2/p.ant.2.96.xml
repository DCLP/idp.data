    <lb n="2"/><gap extent="12" reason="lost" unit="character"/>μεν ἤδη ἐγνώσθ<supplied cert="high" reason="lost">η </supplied><gap extent="3" reason="lost" unit="character"/> <unclear cert="high" reason="undefined">πε</unclear>ρὶ τῆς ἐπιστασίας το<supplied cert="high" reason="lost">ῦ </supplied><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/><unclear cert="low" reason="undefined">σο</unclear><supplied cert="low" reason="lost">υ</supplied> ἐπιστάντος ἀπὸ ὀγδόης τοῦ μηνός. ἐπειδὴ δὲ μέμνησα<gap extent="3" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ξα</unclear><gap extent="2" reason="illegible" unit="character"/> π<unclear cert="high" reason="undefined">ά</unclear>ν<unclear cert="high" reason="undefined">τες</unclear> 

    <lb n="4"/><unclear cert="high" reason="undefined">παρ</unclear>᾽ <unclear cert="high" reason="undefined">ἐμο</unclear><add place="supralinear">ῦ</add> ἕνεκεν Χριστοδώρο<add place="supralinear">υ</add> τοῦ γ<unclear cert="high" reason="undefined">εω</unclear>μέτρου καὶ <supplied cert="high" reason="lost">ἀ</supplied>ξιώσατε ἀπαιτῆσαι αὐτὸ<supplied cert="high" reason="lost">ν</supplied> <unclear cert="high" reason="undefined">τὸ</unclear> 

    <lb n="5"/>νόμισμα ὅπερ <unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">ἴω</supplied>θεν διδόν<supplied cert="high" reason="lost">α</supplied>ι τ<supplied cert="high" reason="lost">οῖ</supplied>ς παγάρχοις, ἀπα<supplied cert="high" reason="lost">ι</supplied>τῆσαι δὲ καὶ Μούσην τὸν ἀπὸ 

    <lb n="6"/><subst><add place="inline">κεφαλαιωτ<add place="supralinear">ῶ</add>ν</add><del rend="corrected">κεφαλαιωτ<del rend="erasure">η</del>ν</del></subst> τὰ <unclear cert="high" reason="undefined">δ</unclear><supplied cert="high" reason="lost">ὲ</supplied> <unclear cert="high" reason="undefined">κ</unclear>νίδια τοῦ τε μέλ<unclear cert="high" reason="undefined">ι</unclear>τος καὶ τοῦ ταριχίου, ἐπειδὴ χρή<unclear cert="high" reason="undefined">ζ</unclear><supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">ι</unclear> 

    <lb n="7"/>αὐτῶν. μέχρι δὲ τῆς σήμερον οὐκ <unclear cert="high" reason="undefined">ἔ</unclear>γραψεν ὁ κοινὸς γεοῦχος. προσδοκῶ δὲ διὰ 

    <lb n="8"/>τῆς σήμερον γράμματα τῆς αὐτοῦ <expan>με<supplied cert="high" reason="lost">γαλ</supplied>οπρ<ex cert="high">επείας</ex></expan> <choice><corr>τοῦτό ἐστιν</corr><sic>τουτεστιν</sic></choice> Χοιὰκ ἑνδεκάτῃ. οὐκ ἐδόθη 

    <lb n="9"/>λό<unclear cert="high" reason="undefined">γ</unclear><supplied cert="high" reason="lost">ο</supplied>ς τῷ <expan>κυρ<ex cert="high">ίῳ</ex></expan> Διονυσίῳ <unclear cert="high" reason="undefined">οὔτ</unclear>ε τῷ <expan>κυρ<ex cert="high">ίῳ</ex></expan> Φ<supplied cert="high" reason="lost">οιβάμ</supplied>μων<unclear cert="high" reason="undefined">ι</unclear> τῷ καγκελλαρίῳ καὶ ὡς λ<unclear cert="high" reason="undefined">όγος</unclear> 

    <lb n="10"/>Ἰ<unclear cert="high" reason="undefined">ω</unclear>άννο<unclear cert="high" reason="undefined">υ</unclear> <gap extent="1" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ρ</unclear><gap extent="2" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/>ως<gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/>. ἐστιν ὁ πρ<supplied cert="high" reason="lost">οσ</supplied>δοκο<supplied cert="high" reason="lost">ύ</supplied>μενος ἐλθεῖν καὶ διοικῆσαι τὴ<unclear cert="high" reason="undefined">ν</unclear> 

    <lb n="11"/>ἀρχὴν τοῦ νεοῦ δοῦκος <unclear cert="high" reason="undefined">ὁ</unclear> καὶ αὐθέν<unclear cert="high" reason="undefined">τη</unclear>ς δ<unclear cert="high" reason="undefined">ι</unclear>αταγματάριος. οὗτος γὰρ ὁ νῦν ἐ<unclear cert="high" reason="undefined">λ</unclear><supplied cert="high" reason="lost">θ</supplied><unclear cert="high" reason="undefined">ὼ</unclear>ν 

    <lb n="12"/>ἀδιοβ<unclear cert="high" reason="undefined">ά</unclear>ντης αὐτοῦ ἐστιν. ἐκτὸς δὲ τῶ<unclear cert="high" reason="undefined">ν</unclear> ἀκοῶν ὑμῶν ἀπέθανεν Νόννος ὁ σχολαστικὸ<add place="supralinear">ς</add> 

    <lb n="13"/>ὁ υ<hi rend="diaeresis">ἱ</hi>ὸς Ἐ<unclear cert="high" reason="undefined">πι</unclear>φ<unclear cert="high" reason="undefined">α</unclear>νείου· καὶ προεκομ<unclear cert="high" reason="undefined">ίσθη</unclear> ἀ<unclear cert="high" reason="undefined">π</unclear>ὸ τῆς <choice><corr>ἐξόδ<supplied cert="high" reason="lost">ου</supplied></corr><sic>εξσοδ<supplied cert="high" reason="lost">ου</supplied></sic></choice> τοῦ διαταγματαρ<unclear cert="high" reason="undefined">ί</unclear>ο<add place="supralinear">υ</add> <choice><corr>τοῦτό ἐστιν</corr><sic>τουτεστιν</sic></choice> 

    <lb n="14"/><unclear cert="high" reason="undefined">ἀπ</unclear><supplied cert="high" reason="lost">ὸ</supplied> ὀγδόης. εἴ τι δὲ γράφεται ἐντ<supplied cert="high" reason="lost">αῦ</supplied>θα μη<unclear cert="high" reason="undefined">ν</unclear>ύω <unclear cert="high" reason="undefined">ὑμῖ</unclear>ν. καὶ διὰ τὸν Θεὸν πέμψα<supplied cert="high" reason="lost">ι</supplied> 

    <lb n="15"/>μοι ὀλίγον <choice><corr>ὀψάριον</corr><sic>οψαριν</sic></choice> εἴτε ἐπὶ κτήσει <supplied cert="low" reason="omitted">ἢ</supplied> <unclear cert="high" reason="undefined">ἐπὶ</unclear> τηγάνῳ ἐ<unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">ι</unclear>δὴ ο<unclear cert="high" reason="undefined">ὐ</unclear>κ εὑρ<unclear cert="high" reason="undefined">ί</unclear>σ<unclear cert="high" reason="undefined">κ</unclear>ω ἐνταῦ<unclear cert="high" reason="undefined">θ</unclear><supplied cert="high" reason="lost">α</supplied>. 

    <lb n="16"/><supplied cert="high" reason="lost">οἱ</supplied> γὰρ <unclear cert="high" reason="undefined">ἁ</unclear>λιεῖς τῶν ἐνταῦθα ἔπεμψ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ν πάντα</supplied> πρὸς τὸν <expan>κόμ<ex cert="high">ητα</ex></expan> ἐν Ἀντιν<unclear cert="high" reason="undefined">όο</unclear><add place="supralinear"><unclear cert="high" reason="undefined">υ</unclear></add>. αἰ<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">εῖ</supplied> 

    <lb n="17"/><unclear cert="high" reason="undefined">με</unclear> Μηνᾶς γράψαι ὑμῖ<supplied cert="high" reason="lost">ν</supplied> περὶ τοῦ κ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">μ</supplied>ισί<unclear cert="high" reason="undefined">ου</unclear> αὐτοῦ καὶ <unclear cert="high" reason="undefined">γν</unclear><gap extent="1" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ε</unclear><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ε</unclear>παξο<unclear cert="high" reason="undefined">μ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

