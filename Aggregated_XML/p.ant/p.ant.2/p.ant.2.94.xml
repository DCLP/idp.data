    <lb n="1"/><g type="stauros"/> τὰ γράμματα τῆς ὑμετέρας <unclear cert="high" reason="undefined">π</unclear>εριβλέ<unclear cert="high" reason="undefined">πτου</unclear> δεσποτείας ἐδ<unclear cert="high" reason="undefined">ε</unclear>ξ<unclear cert="high" reason="undefined">άμη</unclear>ν διὰ <unclear cert="high" reason="undefined">Ἀκ</unclear>ωρίου <supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ῦ συμμά</supplied><unclear cert="high" reason="undefined">χ</unclear>ου 

