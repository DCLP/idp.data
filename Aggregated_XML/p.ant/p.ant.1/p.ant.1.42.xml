    <lb n="1"/><supplied reason="lost" cert="high">β</supplied><unclear reason="undefined" cert="high">α</unclear>σιλ<unclear reason="undefined" cert="high">εί</unclear><supplied reason="lost" cert="high">ας το</supplied><unclear reason="undefined" cert="high">ῦ</unclear> θειοτάτου ἡμῶν δεσπότου <expan>Φλ<ex cert="high">αυίου</ex></expan> Ἰουστινιανοῦ τοῦ αἰωνίου 

    <lb n="2"/>Αὐγούσ<unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ου καὶ</supplied> Αὐτοκράτορος ἔτους εἰκοστοῦ τριακὰς ἐν <choice><corr>ἔτει</corr><sic>ετι</sic></choice> μετὰ τὴν 

    <lb n="3"/>ὑπατεί[αν <expan>Φλ]<ex cert="high">αυίου</ex></expan> Βασιλείου τοῦ <expan>ἐνδοξ<ex cert="high">οτάτου</ex></expan> Θὼθ <g type="unintelligible"/> <num value="29">κθ</num> ἕκτης ἰνδικτίονος· 

    <lb n="4"/>Αὐρήλιος Πε<unclear reason="undefined" cert="high">ι</unclear>ηυοῦτος ἐκ πατρὸς Ἀπολλῶτος μητρὸς Θέκλας 

    <lb n="5"/><unclear reason="undefined" cert="high">με</unclear><supplied reason="lost" cert="high">τ᾽</supplied> <choice><corr>ἐγγυητοῦ</corr><sic>ηγγυειετου</sic></choice> τοῦ <choice><corr>ἐγγυωμένου</corr><sic>εγγεομενου</sic></choice> πρὸς πᾶσαν πίστιν καὶ ἀπόδοσιν 

    <lb n="6"/>ὑποκε<unclear reason="undefined" cert="high">ιμ</unclear><supplied reason="lost" cert="high">ένη</supplied>ν σοι εἰς τοῦτο τὸ χρέος ἐμοῦ <choice><corr>Αὐρηλίου</corr><sic>αυρηλιος</sic></choice> Πέτρου 

    <lb n="7"/>ἐκ πατρὸς Ψᾶτος <expan>μητρ<ex cert="high">ὸς</ex></expan> Θερμουθίας γεωργοὶ <choice><corr>ὁρμώμενοι</corr><sic>ορμωμενοις</sic></choice> ἀπὸ 

    <lb n="8"/>κώμης Ληναίου τοῦ Ἀντινοίτου νομοῦ τὰ νῦν <choice><corr>διάγοντες</corr><sic>διαγωντος</sic></choice> ἐνταῦ-

    <lb n="9"/>θα ἐπὶ τῆς αὐτῆς κώμης Ληναίου Αὐρηλίῳ Ἰωσηφίῳ υἱῷ Σου-

    <lb n="10"/>ροῦτος Ἰουδαίῳ τὴν θρησκείαν ἀπὸ τῆς αὐτῆς κώμης Ληναίου 

    <lb n="11"/>τοῦ αὐτοῦ Ἀντινοίτου νομοῦ χαίρειν· ὁμολογῶ ἐγὼ ὁ προγεγραμ-

    <lb n="12"/>μένος Πε<unclear reason="undefined" cert="high">ι</unclear>ηῦτος μετ᾽ <choice><corr>ἐγγυητοῦ</corr><sic>ηγγυειετου</sic></choice> Πέτρου διὰ ταύτης μου τῆς 

    <lb n="13"/><choice><corr>ἐγγράφου</corr><sic>εγραφου</sic></choice> ἀσφαλείας ἐσχηκέναι καὶ πεπληρῶσθαι παρά σου τῆς 

    <lb n="14"/>τελείας καὶ <choice><corr>ἀξίας</corr><sic>αξειας</sic></choice> τιμῆς οἴνου νέου <choice><corr>καλ<supplied reason="omitted" cert="high">λ</supplied>ίσ<supplied reason="omitted" cert="high">του</supplied></corr><sic>καλις</sic></choice> καὶ εὐαρέστου μικρὰ 

    <lb n="15"/><choice><corr>ἀγγείων</corr><sic>αγγιων</sic></choice> ἑκατὸν <choice><corr>εἴκοσι</corr><sic>εικουσι</sic></choice> ἓν ἥμισυ <num value="121  1/2 "/> ἀπὸ ξεστῶν πέντε <num value="5"/> <choice><corr>Ἰουδαϊκῶν</corr><sic>ιουδαικον</sic></choice> 

    <lb n="16"/>ἑκάστου <choice><corr>μικροῦ</corr><sic>μικρα</sic></choice> <expan>γί<ex cert="high">νεται</ex></expan> <expan>οἴ<ex cert="high">νου</ex></expan> <expan>μικρ<ex cert="high">οῦ</ex></expan> <choice><corr>ἀγγεῖα</corr><sic>αγγια</sic></choice> <num value="221">σκα</num> <num value="1/2">𐅵</num> ἀπὸ <expan>ξ<ex cert="high">εστῶν</ex></expan> <num value="5">ε</num> καὶ ταῦτα ἑτοίμω-

    <lb n="17"/>ς ἔχω παρασχεῖν σοι τῷ <expan>κ<ex cert="high">αι</ex></expan>ρῷ τῶν τρυγῶν τῷ Μεσορὴ μηνὶ τῆς 

    <lb n="18"/>παρούσης σὺν θεῷ εἰσιούσης καρπῶν ἑβδόμης ἰνδικτίονος 

    <lb n="19"/><expan>κ<ex cert="high">αὶ</ex></expan> <choice><corr>τὸν</corr><sic>των</sic></choice> ναυ<gap reason="illegible" extent="1" unit="character"/>ης εὑρεθησόμενον <choice><corr>ὄξος</corr><sic>οξον</sic></choice> ἢ <choice><corr>ἀποίητον</corr><sic>αποιητος</sic></choice> ἢ <choice><corr>ὀξ<supplied reason="omitted" cert="high">ιζ</supplied>όμενον</corr><sic>οξομενος</sic></choice> 

    <lb n="20"/>ἐκ τῶν αὐτῶν οἴνου ἕως Τῦβι καὶ αὐτοῦ <choice><corr>μηνὸς</corr><sic>μενος</sic></choice> τῆς αὐτῆς 

    <lb n="21"/><expan>ἰνδ<ex cert="high">ικτίονος</ex></expan> <supplied reason="lost" cert="high">ἀ</supplied>λλαττομένων σοι παρὰ ἐμοῦ ἐπὶ καλῷ καὶ ἐπὶ <choice><corr>βελτίονι</corr><sic>βελτιου</sic></choice> <choice><corr>οἴνῳ</corr><sic>οινου</sic></choice>· 

    <lb n="22"/><expan>κ<ex cert="high">αὶ</ex></expan> ἀποκαταστήσομεν <choice><corr>αὐτῷ</corr><sic>αυτα</sic></choice> <choice><corr>τὸν</corr><sic>των</sic></choice> <choice><corr>οἶνον</corr><sic>οινου</sic></choice> ἕως ἀποκαθειστα εἰς τὰ<supplied reason="lost" cert="high">ς</supplied> 

    <lb n="23"/>θύρας τῆς <choice><corr>οἰκίας</corr><sic>οικειας</sic></choice> τῆς κώμης <choice><corr>ἰδίοις</corr><sic>ιδιαις</sic></choice> μου ἀναλώμασι <expan>κ<ex cert="high">αὶ</ex></expan> <choice><corr>ναύλῳ</corr><sic><unclear reason="undefined" cert="high">να</unclear>υλου</sic></choice> 

    <lb n="24"/>δίχα πάσης ὑπερθέσεως καὶ ἀντιλογίας· παρελθούσης δὲ ἐν τῷ 

    <lb n="25"/>αὐτῷ <expan>κ<ex cert="high">αι</ex></expan>ρ<supplied reason="lost" cert="high">ῷ</supplied> <unclear reason="undefined" cert="high">ε</unclear>ἰ μὴ <choice><corr>ἀποδοίην</corr><sic>αποδοιηνν</sic></choice> <choice><corr>τὸν</corr><sic>των</sic></choice> <choice><corr>αὐτὸν</corr><sic>αυτων</sic></choice> <choice><corr>οἶνον</corr><sic>οινου</sic></choice> ἑτοίμως ἔχω 

    <lb n="26"/>παρασχεῖν σοι ὑπὲρ τῆς τούτων <choice><corr>τιμῆς</corr><sic>διμης</sic></choice> χρύσου νομισμάτια 

    <lb n="27"/>πέντε <num value="5"/> ἐργῷ <choice><corr>παρ᾽</corr><sic>πρ</sic></choice> αὐτὰ καὶ <expan>ὑπαρχ<ex cert="high">όντων</ex></expan> <choice><corr>ἡμῖν</corr><sic>ημω</sic></choice> ὑποκειμένων σοι εἰς <choice><corr>τοῦτο</corr><sic>τυτο</sic></choice> τὸ χρέος 

    <lb n="28"/>καθάπερ ἐκ <choice><corr>δίκης</corr><sic>τικης</sic></choice>· τὸ <choice><corr>γραμματεῖον</corr><sic>γραμματιων</sic></choice> τοῦτο <choice><corr>κύριον</corr><sic>κυρια</sic></choice> καὶ <choice><corr>βέβαιον</corr><sic>βεβαια</sic></choice> καὶ <expan>ἐπερ<ex cert="high">ωτηθεὶς</ex></expan> 

    <lb n="29"/><expan>ὡμολ<ex cert="high">όγησα</ex></expan>· <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> <expan>Πεηῦτ<ex cert="high">ος</ex></expan> μετ᾽ <choice><corr>ἐγγυητοῦ</corr><sic>ηγγυειετου</sic></choice> Πέτρου <choice><corr>οἱ</corr><sic>υι</sic></choice> <expan>προκ<ex cert="high">είμενοι</ex></expan> στοίχει μοι 

    <lb n="30"/>πάντα ὡς <expan>πρόκ<ex cert="high">ειται</ex></expan> <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> <choice><corr>Κυριακὸς</corr><sic>κυριακου</sic></choice> <choice><corr>νομικὸς</corr><sic>νομικου</sic></choice> ἀξιωθεὶς ἔγραψα 

    <lb n="31"/>ὑπὲρ αὐτοῦ γράμματα <choice><corr>μὴ</corr><sic>μιη</sic></choice> <expan>εἰδότ<ex cert="high">ος</ex></expan>. <handShift new="m2"/> <expan>Φλ<ex cert="high">αύιος</ex></expan> Στέφανος Κύρου 

    <lb n="32"/><choice><corr>μαρτυρῶ</corr><sic>μαρτηρω</sic></choice> <choice><corr>τὸ</corr><sic>τω</sic></choice> <choice><corr>γραμματεῖον</corr><sic>γραματιων</sic></choice> ἀκούσας παρὰ 

    <lb n="33"/>τοῦ θεμένου. <handShift new="m3"/> <g type="stauros"/> Φοιβάμων Πόλλων <expan>διάκ<ex cert="high">ονος</ex></expan> 

    <lb n="34"/><choice><corr>μαρτυρῶ</corr><sic>μαρτηρω</sic></choice> <choice><corr>τὸ</corr><sic>τω</sic></choice> <choice><corr>γραμματεῖον</corr><sic>κραματιον</sic></choice> ἀκούσας παρὰ 

