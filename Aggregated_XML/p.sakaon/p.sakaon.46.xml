    <lb n="1"/>Αὐρηλίῳ Ἰσίωνι <expan>π<supplied reason="lost" cert="high">ρα</supplied>ι<ex cert="high">ποσίτῳ</ex></expan> <expan>π<supplied reason="lost" cert="high">ά</supplied><unclear reason="undefined" cert="high">γ</unclear><ex cert="high">ου</ex></expan> <num value="8">η</num> <expan><unclear reason="undefined" cert="high">π</unclear>ολ<ex cert="high">ιτευομένῳ</ex></expan> 

    <lb n="2"/>Πεντακωμίας        παρὰ <supplied reason="lost" cert="high">Αὐ</supplied><unclear reason="undefined" cert="high">ρ</unclear>ηλίου Σακαῶνος υἱοῦ 

    <lb n="3"/>Σαταβο<supplied reason="lost" cert="high">ῦ</supplied>τος ἀπὸ κώμης Θεαδελφίας. κα<unclear reason="undefined" cert="high">θ</unclear>ὼς ἐκέλευ-

    <lb n="4"/>σας τοὺς <choice><corr>τρεῖς</corr><sic>τρις</sic></choice> <num value="3"/> κεφαλαιω<unclear reason="undefined" cert="high">τ</unclear>ὰς τὰς τριακοσί<supplied reason="lost" cert="high">α</supplied>ς <num value="300"/> ἀρτάβας 

    <lb n="5"/>τοῦ σίτου πληρῶσαι, οὐ<unclear reason="undefined" cert="high">κ</unclear> <choice><corr><supplied reason="lost" cert="high">οἶ</supplied>δα</corr><sic><supplied reason="lost" cert="high">υ</supplied>δα</sic></choice> τίνι <choice><corr>λόγῳ</corr><sic>λογ<supplied reason="lost" cert="high">ο</supplied>υ</sic></choice> καὶ λῃστρ<unclear reason="undefined" cert="high">ι</unclear>κῷ 

    <lb n="6"/>τρόπῳ Ἥρων <choice><corr>υἱὸς</corr><sic>υιως</sic></choice> Σακ<unclear reason="undefined" cert="high">αῶ</unclear>νος ἀπὸ τῆς αὐτῆς κώμης, 

    <lb n="7"/>ἐμοῦ ἀναπληροῦντος <unclear reason="undefined" cert="high">τὸ</unclear> ἐπιβάλλον μοι μέρος τοῦ 

    <lb n="8"/>σίτου, <choice><corr>αὐτὸς</corr><sic>αυτως</sic></choice> δὲ μηδεν<unclear reason="undefined" cert="high">ὶ</unclear> <choice><corr>λόγῳ</corr><sic>λογου</sic></choice> χρησάμενος, ἀφήρπ<unclear reason="undefined" cert="high">α</unclear>-

    <lb n="9"/>σεν ἐκ τῶν προβάτω<unclear reason="undefined" cert="high">ν</unclear> μου πρόβατα <supplied reason="lost" cert="high">σύ</supplied>μποκα τὸν 

    <lb n="10"/>ἀριθμὸν ὀγδοήκοντ<unclear reason="undefined" cert="high">α</unclear> δύο <num value="82"/> καὶ παρεδωκέναι τί<unclear reason="undefined" cert="high">νι</unclear> 

    <lb n="11"/>οὐκ <choice><corr>οἶδα</corr><sic><unclear reason="undefined" cert="high">υ</unclear>δα</sic></choice>. δι᾽ αὐτὸ τοῦτο ἀξιῶ κ<unclear reason="undefined" cert="high">α</unclear>ὶ δ<unclear reason="undefined" cert="high">έο</unclear>μαί σο<supplied reason="lost" cert="high">υ τ</supplied>ῆς φιλαν-

    <lb n="12"/>θρωπίας ὅπως καταναγκάσῃς ενα<gap reason="illegible" extent="3" unit="character"/>ο<unclear reason="undefined" cert="high">π</unclear><gap reason="illegible" extent="2" unit="character"/> 

    <lb n="13"/>τὸ χρέως <choice><corr>ἀποκατασταθῆνα<supplied reason="lost" cert="high">ι</supplied></corr><sic>απο<unclear reason="undefined" cert="high">γ</unclear>ατασταθηνα<supplied reason="lost" cert="high">ι</supplied></sic></choice> <unclear reason="undefined" cert="high">τ</unclear>οῖς το<unclear reason="undefined" cert="high">ῦ</unclear> <gap reason="illegible" extent="3" unit="character"/>ου <supplied reason="lost" cert="high">σιτο</supplied>-

    <lb n="14"/>λόγ<unclear reason="undefined" cert="high">οις</unclear> <choice><corr>ἵνα</corr><sic><unclear reason="undefined" cert="high">ε</unclear>ινα</sic></choice> δυνηθῶ τὰ <choice><corr>ἴδια</corr><sic>ει<unclear reason="undefined" cert="high">δι</unclear>α</sic></choice> ἀπολαβεῖν 

    <lb n="15"/><choice><corr>εἶ<supplied reason="omitted" cert="high">τα</supplied></corr><sic>ει</sic></choice> τὰ γραφέντα <choice><corr>ὑπὸ</corr><sic>υπε</sic></choice> ἐμοῦ ε<supplied reason="lost" cert="high">ἰ</supplied>ς γνῶσιν τοῦ κυρίου 

    <lb n="16"/>μου δουκὸς <choice><corr>ἀνενέγκῃς</corr><sic>ανα<unclear reason="undefined" cert="high">ν</unclear>ι<supplied reason="lost" cert="high">κ</supplied>η<supplied reason="lost" cert="high">ς</supplied></sic></choice>, <unclear reason="undefined" cert="high">α</unclear>ὐτοῦ γάρ ἐστιν <supplied reason="omitted" cert="high">τοὺς</supplied> τὰ τοι-

    <lb n="17"/>αῦτα <choice><corr>τολμοῦντας</corr><sic>τολμουντες</sic></choice> <choice><corr>ἐκδικεῖν</corr><sic>εκδικιν</sic></choice>, καὶ τούτου τυχὼν 

    <lb n="18"/>χάριτά σοι ὁμολογήσω, κύριε.            διευτύχει. 

    <lb n="19"/>Αὐρήλιος Σακαῶν ἐπιδέδωκα. Αὐρήλιος 

    <lb n="20"/>Δημήτρις ἔγραψα ὑπ<unclear reason="undefined" cert="high">ὲρ</unclear> αὐτοῦ ἀγραμμάτου. 

    <lb n="21"/>ὑπατείας τῶν δεσποτῶν ἡ<unclear reason="undefined" cert="high">μ</unclear>ῶν <choice><corr>Κωνσταντι<sic>ν</sic>ου</corr><sic>κων<unclear reason="undefined" cert="high">σταν</unclear>τιου</sic></choice> 

    <lb n="22"/>τὸ τρίτον καὶ Κώνσταντος τὸ <num value="2">β</num>, <choice><corr>Αὐγούστων</corr><sic>αγου<unclear reason="undefined" cert="high">στων</unclear></sic></choice>, 

    <lb n="23"/>Φαρμοῦθι <num value="3">γ</num>. </ab></div><div n="meta.13066" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.13066" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" value="0342-03-29" notBefore="342" notAfter="342">29. März 342</date></p></div><div n="meta.13066" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">P.Sakaon</title><biblScope type="numbers">46</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">13066</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0199</series>; ; <biblScope type="numbers">46</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0199;;46" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.13066" type="bibliography" subtype="corrections" org="uniform" part="N" sample="complete"><head>BL-Einträge nach BL-Konkordanz</head><bibl type="BL" default="NO"><biblScope type="volume">XI</biblScope>, S. <biblScope type="pages">192</biblScope></bibl></div><div n="meta.13066" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p>keine</p></div><div n="meta.13066" type="bibliography" subtype="otherPublications" org="uniform" part="N" sample="complete"><head>Andere Publikation</head><bibl type="publication" subtype="other" default="NO">P.Thead. 22</bibl></div><div n="meta.13066" type="history" subtype="locations" org="uniform" part="N" sample="complete"><p><placeName type="ancientFindspot" cert="high" full="yes">Theadelphia</placeName> (<geogName type="nome" cert="high" full="yes">Arsinoites</geogName>), <geogName type="ancientRegion" key="Aegyptus" cert="high" full="yes">Egypt</geogName></p></div><div n="meta.