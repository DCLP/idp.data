    <lb n="16"/>ὑ<supplied cert="high" reason="lost">πατεί</supplied>ας Ῥουφί<supplied cert="high" reason="lost">ο</supplied>υ Οὐ<unclear cert="high" reason="undefined">ο</unclear>λουσιαν<supplied cert="high" reason="lost">οῦ καὶ</supplied> Πετρωνίου Ἀνν<supplied cert="high" reason="lost">ι</supplied>αν<supplied cert="high" reason="lost">οῦ</supplied> τῶν <expan>λ<supplied cert="high" reason="lost">αμπ</supplied><unclear cert="high" reason="undefined">ροτ</unclear><ex cert="high">άτων</ex></expan>, Χ<unclear cert="high" reason="undefined">οι</unclear>ὰ<supplied cert="high" reason="lost">κ</supplied> <num value="6">ϛ</num>. <handShift new="m2"/> Αὐρή<supplied cert="high" reason="lost">λιοι Π</supplied>τολεμῖνος καὶ Σαραπί<supplied cert="high" reason="lost">ων κα</supplied>ὶ Πᾶσις 

    <lb n="17"/>κ<supplied cert="high" reason="lost">αὶ Ἠ</supplied>λίας καὶ Σελ<unclear cert="high" reason="undefined">π</unclear>οῦς καὶ οἱ λοιποὶ π<supplied cert="high" reason="lost">αρ</supplied><unclear cert="high" reason="undefined">ε</unclear>λάβαμεν τὰς τοῦ πυροῦ <expan><ex cert="high">ἀρτάβας</ex></expan> [<num value="64">ξδ</num>] καὶ κριθῆς <expan><ex cert="high">ἀρτάβας</ex></expan> <num value="12">ιβ</num> καὶ ἀ<unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">οδ</supplied><unclear cert="high" reason="undefined">ώ</unclear>σωμεν τῷ <unclear cert="high" reason="undefined">καιρ</unclear>ῷ 
τῆς συ<supplied cert="high" reason="lost">γκ</supplied><unclear cert="high" reason="undefined">ο</unclear>μιδῆς <unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ετὰ τῆς</supplied> 

    <lb n="18"/><supplied cert="high" reason="lost">ἡμι</supplied>ολίας ὡς πρ<supplied cert="high" reason="lost">ό</supplied>κι<unclear cert="high" reason="undefined">τ</unclear>αι· καὶ <choice><corr>ὑπερ<supplied cert="high" reason="lost">ωτη</supplied>θέντες</corr><sic>περ<supplied cert="high" reason="lost">ωτη</supplied><unclear cert="high" reason="undefined">θ</unclear>εντες</sic></choice> ὡμολογήσαμεν. <supplied cert="high" reason="lost">Αὐρ</supplied>ήλιος <unclear cert="high" reason="undefined">Κλημάτιος</unclear> Ἑρμίου ἀ<supplied cert="high" reason="lost">πὸ κ</supplied>ώμης Φιλαγρίδος ἔγραψ<supplied cert="high" reason="lost">α ὑπ</supplied><unclear cert="high" reason="undefined">ὲρ</unclear> <unclear cert="high" reason="undefined">αὐτῶν</unclear> 

    <lb n="19"/>ἀ<supplied cert="high" reason="lost">γρα</supplied>μμάτων. <handShift new="m3"/> <expan>Αὐρ<ex cert="high">ήλιοι</ex></expan> Ἀρίων κα<supplied cert="high" reason="lost">ὶ Σακα</supplied>ῶν ἐγγυώμεθα ὡς πρόκιτ<supplied cert="high" reason="lost">αι εἰς</supplied> <expan>ἔκτ<ex cert="high">εισιν</ex></expan> <expan>τῆ<ex cert="high">ς</ex></expan> <expan>ἀποδόσ<ex cert="high">εως</ex></expan> τοῦ <supplied cert="high" reason="lost">πυροῦ</supplied> καὶ τῆς κριθῆς μ<supplied cert="high" reason="lost">ετὰ τ</supplied>ῆς 

