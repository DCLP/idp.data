    <lb n="1"/>ὑπατείας Φλαουίου Κωνσταντίου καὶ Οὐαλερίου Μαξίμ<supplied cert="high" reason="lost">ο</supplied>υ 

    <lb n="2"/>τῶν λαμπροτάτων ἐπάρχων, Παῧνι <num value="30">λ</num>. 

    <lb n="3"/>Αὐρήλιος Σακαῶν Σαταβοῦτος μητρὸς Θερμουθίου ἀπὸ κώμης 

    <lb n="4"/>Θεαδελφίας τοῦ Ἀρσινοΐτου νομοῦ <expan><ex cert="high">ἐτῶν</ex></expan> <num value="58">ν<unclear cert="high" reason="undefined">η</unclear></num> οὐ<unclear cert="high" reason="undefined">λ</unclear>ὴ παρὰ ἀντικνημίῳ 

    <lb n="5"/>δεξιῷ Αὐρηλίῳ Οὐρανίῳ Λαμπάδι<unclear cert="high" reason="undefined">ου</unclear> καὶ ὡς χρημα<supplied cert="high" reason="lost">τίζει</supplied> χαίρειν. 

    <lb n="6"/>ὁμολογῶ ἔχειν σοι καὶ <choice><corr>ὀφείλει<supplied cert="high" reason="omitted">ν</supplied></corr><sic>οφιλει</sic></choice> ἀπὸ λοιπάδος πρό<unclear cert="high" reason="undefined">βατα</unclear> <unclear cert="high" reason="undefined">λευκὰ</unclear> 

    <lb n="7"/>θηλυκὰ ἀπὸ κουρᾶς <unclear cert="high" reason="undefined">τὸ</unclear>ν <unclear cert="high" reason="undefined">ἀρ</unclear>ιθμ<unclear cert="high" reason="undefined">ὸ</unclear>ν δώ<unclear cert="high" reason="undefined">δεκα</unclear> <num value="12"/> ὧν παρεσχόμην 

    <lb n="8"/>ἀφ᾽ <choice><corr>ὧν</corr><sic>ον</sic></choice> πρόβατα δύο <num value="2"/> <choice><corr>ἅπερ</corr><sic>οπερ</sic></choice> διεύθυν<unclear cert="high" reason="undefined">ον</unclear> ἐσχηκ<unclear cert="high" reason="undefined">ότα</unclear> <unclear cert="high" reason="undefined">παρά</unclear> <unclear cert="high" reason="undefined">μο</unclear>υ τὰ αὐτὰ πρό-

    <lb n="9"/>βατα δύο <num value="2"/>, τὰ δὲ λοιπὰ πρόβατα δέκα <num value="10"/> ἀπὸ κουρᾶς ἐπάναγκ<unclear cert="high" reason="undefined">ές</unclear> 

    <lb n="10"/>σοι ἀποδώσω <unclear cert="high" reason="undefined">μην</unclear>ὶ Θ<unclear cert="high" reason="undefined">ὼ</unclear>θ <unclear cert="high" reason="undefined">τ</unclear>ῆς ἐν<unclear cert="high" reason="undefined">ε</unclear>στ<unclear cert="high" reason="undefined">ώση</unclear>ς ὑπατείας τῇ ἐνεστώσῃ 

    <lb n="11"/><unclear cert="high" reason="undefined">π</unclear>εν<unclear cert="high" reason="undefined">τ</unclear>εκαιδεκάτῃ <unclear cert="high" reason="undefined">ἰν</unclear>δικτίονι ἀνυπερθέτω<supplied cert="high" reason="lost">ς</supplied>· καὶ ἐπὶ τῆ<supplied cert="high" reason="lost">ς</supplied> <unclear cert="high" reason="undefined">ἀπ</unclear>αιτήσεως 

    <lb n="12"/>γεί<supplied cert="high" reason="lost">ν</supplied>εσθαι <supplied cert="high" reason="lost">τῷ</supplied> <unclear cert="high" reason="undefined">Οὐρ</unclear>αν<supplied cert="high" reason="lost">ί</supplied>ῳ <unclear cert="high" reason="undefined">Λαμ</unclear>παδί<unclear cert="high" reason="undefined">ου</unclear> τὴν πρᾶξιν ἔκ <unclear cert="high" reason="undefined">τε</unclear> <supplied cert="high" reason="lost">ἐμοῦ</supplied> 

    <lb n="13"/>τοῦ ὁμολογοῦ<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">το</supplied>ς <supplied cert="high" reason="lost">Σα</supplied>καῶν<supplied cert="high" reason="lost">ο</supplied>ς κ<supplied cert="high" reason="lost">αὶ ἐκ τ</supplied>ῶν ὑπαρ<supplied cert="high" reason="lost">χό</supplied>ντων μοι π<supplied cert="high" reason="lost">ά</supplied>ν-

    <lb n="14"/>των καθ<unclear cert="high" reason="undefined">άπερ</unclear> ἐκ δίκη<supplied cert="high" reason="lost">ς</supplied>· καὶ ἐπ<supplied cert="high" reason="lost">ερ</supplied>ωτηθεὶς ὡμολ<unclear cert="high" reason="undefined">όγησα</unclear>. 

    <lb n="15"/><handShift new="m2"/> Αὐρήλιος Σακαῶν Σατα<supplied cert="high" reason="lost">β</supplied>οῦτ<supplied cert="high" reason="lost">ος</supplied> <unclear cert="high" reason="undefined">ὁ</unclear>μ<unclear cert="high" reason="undefined">ολογῶ</unclear> <unclear cert="high" reason="undefined">ὀφείλε</unclear>ι<unclear cert="high" reason="undefined">ν</unclear> <choice><corr>πρόβατα</corr><sic>βρ<unclear cert="high" reason="undefined">οβατα</unclear></sic></choice> 

    <lb n="16"/>λευκὰ <choice><corr>τὸν</corr><sic>των</sic></choice> ἀριθμὸν <choice><corr>θηλυκὰ</corr><sic>θυλικα</sic></choice> ἀ<supplied cert="high" reason="lost">πὸ κο</supplied>υρᾶς δέκα <num value="10"/> καὶ ἀποδώσω 

    <lb n="17"/>τῇ προθεσμίᾳ ὡς πρόκιται <sic>Αὐρήλι<supplied cert="high" reason="lost">ο</supplied>ς Σαλακην<supplied cert="high" reason="lost">ὸ</supplied>ς</sic> καὶ διεύθυ-

    <lb n="18"/>νον ἀφ᾽ <choice><corr>ὧν</corr><sic>ον</sic></choice> ἐσχηκότα παρά <unclear cert="high" reason="undefined">μ</unclear>ου πρόβατα δύο <num value="2"/> <choice><corr>ὡ<supplied cert="high" reason="lost">ς</supplied></corr><sic>ο<supplied cert="high" reason="lost">ς</supplied></sic></choice> πρόκιται 

    <lb n="19"/>Αὐρήλιος Σαλακηνὸς ἔγραψα ὑπὲρ αὐτοῦ ἀγραμμάτ<unclear cert="high" reason="undefined">ου</unclear>. 

