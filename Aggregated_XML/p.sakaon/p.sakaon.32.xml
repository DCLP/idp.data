    <lb n="22"/>ἐπύθετο <supplied cert="high" reason="lost">εἰ ο</supplied><unclear cert="high" reason="undefined">ἱ</unclear> <supplied cert="high" reason="lost">ἀπὸ Θεαδ</supplied><unclear cert="high" reason="undefined">ελφ</unclear><supplied cert="high" reason="lost">είας</supplied> <gap extent="4" reason="illegible" unit="character"/><gap extent="3" reason="illegible" unit="character"/>ε<gap extent="1" reason="illegible" unit="character"/><gap extent="6" reason="lost" unit="character"/> <unclear cert="high" reason="undefined">ἐ</unclear>πὶ τὸ στόμιον καὶ <choice><corr><supplied cert="high" reason="lost">ἐ</supplied>κίνησαν</corr><sic><supplied cert="high" reason="lost">ε</supplied>κεινησαν</sic></choice> 

    <lb n="23"/>τῆς <choice><corr>λιθείας</corr><sic>λιθιας</sic></choice>. <choice><corr>ἀπ<supplied cert="high" reason="lost">εκρί</supplied>ναντ<supplied cert="high" reason="lost">ο</supplied></corr><sic>α<unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">εκρει</supplied>ναντ<supplied cert="high" reason="lost">ο</supplied></sic></choice> <gap extent="3" reason="lost" unit="character"/>ρ<gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">τ</unclear><gap extent="4" reason="lost" unit="character"/>υτοις <unclear cert="high" reason="undefined">χ</unclear><gap extent="4" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τ</supplied>ὸ ἐκτὸς ἐλάξ<unclear cert="high" reason="undefined">ε</unclear>υσα<supplied cert="high" reason="lost">ν</supplied>. δι᾽ ἑρμηνέ<supplied cert="high" reason="lost">ως</supplied> 

    <lb n="24"/><choice><corr>ἀπεκρίναν<supplied cert="high" reason="lost">το</supplied></corr><sic>απεκρειναν<supplied cert="high" reason="lost">το</supplied></sic></choice> <gap extent="2" reason="lost" unit="character"/>σκ<unclear cert="high" reason="undefined">αλ</unclear>αμεν ὃ κατήλθαμεν <gap extent="1" reason="illegible" unit="character"/><gap extent="5" reason="lost" unit="character"/>αι ἔγνωμεν. — ὁ στρατηγὸς <supplied cert="high" reason="lost">α</supplied>ὐτο<supplied cert="high" reason="lost">ῖ</supplied>ς 

