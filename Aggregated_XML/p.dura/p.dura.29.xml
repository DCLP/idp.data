    <lb n="1"/><handShift new="m6"/> <gap reason="lost" extent="unknown" unit="character"/> <choice><corr><expan>Ὑπερβερ<ex cert="high">ε</ex></expan>ταίου</corr><sic>υπερβερτεου</sic></choice> <num value="2">β</num>. ἐπὶ <expan>παρ<ex cert="high">α</ex>κατ<ex cert="high">α</ex>θή<ex cert="high">κῃ</ex></expan> ὁμολογία. 
</ab></div><div type="textpart" n="ext" org="uniform" part="N" sample="complete"><ab part="N">

    <lb n="2"/><handShift new="m1"/> ἐπὶ ὑπάτων τὸ <num value="3">γ</num> καὶ τὸ <num value="1">α</num>, πρὸ <num value="6"><unclear reason="undefined" cert="high">ϛ</unclear></num> Νωνῶν Ὀκτωβ<unclear reason="undefined" cert="high">ρ</unclear><supplied reason="lost" cert="high">ί</supplied><unclear reason="undefined" cert="high">ων</unclear>, <unclear reason="undefined" cert="high">ἔτ</unclear><supplied reason="lost" cert="high">ου</supplied><unclear reason="undefined" cert="high">ς</unclear> <unclear reason="undefined" cert="high">δὲ</unclear> <supplied reason="lost" cert="high">κ</supplied><unclear reason="undefined" cert="high">ατὰ</unclear> <unclear reason="undefined" cert="high">τὸν</unclear> πρότε-

    <lb n="3"/>ρον <expan>ἀριθμ<ex cert="high">ὸν</ex></expan> δευτέρου <num value="560">φξ</num>, μηνὸς Ὑπερβερεταίου δ<unclear reason="undefined" cert="high">ε</unclear>υ<unclear reason="undefined" cert="high">τέρ</unclear>ᾳ, ἐν <gap reason="lost" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/>  Α<unclear reason="undefined" cert="high">ὐρ</unclear>ηλία 

    <lb n="4"/>Γα<hi rend="diaeresis">ί</hi>α Σατορνείλου, Δουρ<unclear reason="undefined" cert="high">η</unclear>νή, οἰκοῦσα ἐνταῦθα, δι<gap reason="lost" extent="6" unit="character"/><gap reason="illegible" extent="3" unit="character"/><gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="4" unit="character"/>α 

    <lb n="5"/>ὡμολόγησεν Ἀμαθθαβείλῃ, <space extent="unknown" unit="character" dim="horizontal"/> Δουρη<supplied reason="lost" cert="high">νῇ, οἰκο</supplied><unclear reason="undefined" cert="high">ύσ</unclear>ῃ <unclear reason="undefined" cert="high">ὁμοί</unclear>ως ἐνταῦθα, 

    <lb n="6"/>ἠριθμῆσθαι καὶ ἔχειν παρ᾽ αὐτῆς ἐν παρακαταθήκῃ <supplied reason="lost" cert="high">ἀκίν</supplied><unclear reason="undefined" cert="high">δυν</unclear><gap reason="illegible" extent="2" unit="character"/> π<unclear reason="undefined" cert="high">αν</unclear>τὸς 

    <lb n="7"/>κινδύνου τῇ ἐνεστώσῃ ἡ<unclear reason="undefined" cert="high">μέ</unclear>ρᾳ ἀργυρίου καλ<unclear reason="undefined" cert="high">ο</unclear>ῦ δο<unclear reason="undefined" cert="high">κ</unclear>ίμ<unclear reason="undefined" cert="high">ου</unclear> δ<unclear reason="undefined" cert="high">ηνάρι</unclear>α <unclear reason="undefined" cert="high">ἑκατ</unclear>ὸν <num value="100"/> 

    <lb n="8"/>ἀδιάγραφα καὶ ἀπρόσθετα, ἃ <unclear reason="undefined" cert="high">ἀ</unclear>ναφυλάξει παρ᾽ ἑα<unclear reason="undefined" cert="high">υ</unclear>τῇ κ<unclear reason="undefined" cert="high">αὶ</unclear> <unclear reason="undefined" cert="high">ἐπὶ</unclear> <unclear reason="undefined" cert="high">τοῖς</unclear> <unclear reason="undefined" cert="high">ὑπάρχουσ</unclear>ι <unclear reason="undefined" cert="high">αὐ</unclear>τῆς 

    <lb n="9"/>καὶ ἀποκαταστήσει τῇ Ἀμαθθαβείλῃ ὁπότε ἂν η<gap reason="lost" extent="4" unit="character"/>η<unclear reason="undefined" cert="high">ηα</unclear><gap reason="lost" extent="2" unit="character"/><unclear reason="undefined" cert="high">α</unclear> <unclear reason="undefined" cert="high">ἀπαιτη</unclear>-

    <lb n="10"/>θῇ ἄνευ ἀναβολῆς καὶ ὑπερθέσεως· ἐὰν δὲ ἀπαιτηθεῖσα <unclear reason="undefined" cert="high">μὴ</unclear> <unclear reason="undefined" cert="high">ἀπο</unclear>δῷ, ἐνσχεθή<unclear reason="undefined" cert="high">σ</unclear>ε-

    <lb n="11"/>ται τοῖς διηγορευμένοις ἐπιτίμοις τῶν τὰς πίστεις τῶν <unclear reason="undefined" cert="high">παρακαταθηκῶν</unclear> <unclear reason="undefined" cert="high">πα</unclear>-

    <lb n="12"/>ραβαινόντων, <space extent="unknown" unit="character" dim="horizontal"/> καὶ ἔσται ἡ πρᾶξις τῇ Ἀμαθθ<unclear reason="undefined" cert="high">α</unclear>βείλῃ <unclear reason="undefined" cert="high">καὶ</unclear> <unclear reason="undefined" cert="high">ἄλλῳ</unclear> <unclear reason="undefined" cert="high">παντὶ</unclear> <unclear reason="undefined" cert="high">τ</unclear>ῷ 

    <lb n="13"/>ὑπὲρ αὐτῆς κυρίως τὴν <choice><corr>ὁμολογίαν</corr><sic>ομολο<unclear reason="undefined" cert="high">γεια</unclear>ν</sic></choice> ταύτην π<supplied reason="lost" cert="high">ρ</supplied><unclear reason="undefined" cert="high">οφ</unclear>ερο<unclear reason="undefined" cert="high">μένῳ</unclear> <gap reason="illegible" extent="4" unit="character"/> <supplied reason="lost" cert="high">ἐ</supplied><unclear reason="undefined" cert="high">ξ</unclear> <supplied reason="lost" cert="high">αὐτῆ</supplied><unclear reason="undefined" cert="high">ς</unclear> 

    <lb n="14"/>τῆς Γα<hi rend="diaeresis">ί</hi>ας καὶ ἐξ ὑπαρχόντων αὐτῇ παντῇ γε παν<unclear reason="undefined" cert="high">τῶς</unclear>. <gap reason="lost" extent="3" unit="character"/><supplied reason="lost" cert="high"> καλῇ</supplied> <choice><corr>πίστει</corr><sic><unclear reason="undefined" cert="high">π</unclear>ιστ<unclear reason="undefined" cert="high">ι</unclear></sic></choice> 

    <lb n="15"/>ἠρίθμηται καὶ ἔχει ἐν παρακαταθήκῃ ἡ Γαί<unclear reason="undefined" cert="high">α</unclear> παρὰ τῆς <unclear reason="undefined" cert="high">Ἀμαθθαβεί</unclear>λης 

    <lb n="16"/>τὰ δηλούμενα δηνάρια ἑκατὸν <num value="100"/> καὶ ἀποδώσειν ὁπότε ἂν ἀ<unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">αι</supplied><unclear reason="undefined" cert="high">τη</unclear><supplied reason="lost" cert="high">θῇ, καὶ ὤ</supplied><unclear reason="undefined" cert="high">μο</unclear>-

    <lb n="17"/>σεν τὸν Σεβάσμιον ὅρκον ταῦτα οὕτως κ<unclear reason="undefined" cert="high">αλ</unclear>ῶς <unclear reason="undefined" cert="high">γ</unclear>ενέσθαι <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="3" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="3" unit="character"/><gap reason="illegible" extent="2" unit="character"/> <unclear reason="undefined" cert="high">ταῦτα</unclear> 

    <lb n="18"/>δοθῆναι, <choice><corr>πίστει</corr><sic>πιστι</sic></choice> ἐπηρώτησεν Ἀμαθθαβείλη <supplied reason="lost" cert="high">καὶ</supplied> <unclear reason="undefined" cert="high">πίσ</unclear><supplied reason="lost" cert="high">τει ὡμολόγησεν Γαία.</supplied> 

    <lb n="19"/><handShift new="m2"/> Αὐρήλιος Θεόδωρος Βερνικιανοῦ, Ζευγματε<unclear reason="undefined" cert="high">ύς</unclear>, <unclear reason="undefined" cert="high">ο</unclear>ἰ<supplied reason="lost" cert="high">κ</supplied><unclear reason="undefined" cert="high">ῶ</unclear>ν ἐντα<unclear reason="undefined" cert="high">ῦθ</unclear><supplied reason="lost" cert="high">α</supplied>, <unclear reason="undefined" cert="high">ἀ</unclear>ξιωθεὶς ἔγρα<unclear reason="undefined" cert="high">ψα</unclear> 

    <lb n="20"/>ὑπὲρ <choice><corr>Αὐρη<supplied reason="omitted" cert="high">λί</supplied>ας</corr><sic>αυρηας</sic></choice> Γαίας, <choice><corr>ἀγραμμάτου</corr><sic>αγραματου</sic></choice>, ὁμολογούση<supplied reason="lost" cert="high">ς ἔχει</supplied>ν ἐν <choice><corr>παρακαταθήκῃ</corr><sic>παρακ<unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">τ</supplied><unclear reason="undefined" cert="high">αθ</unclear>ηκην</sic></choice> δηνάρια 

    <lb n="21"/>ἑκατόν <num value="100"/>, ἃ <choice><corr>καὶ</corr><sic>κε</sic></choice> ἀποδ<unclear reason="undefined" cert="high">ώ</unclear><supplied reason="lost" cert="high">σει ὅτ</supplied><unclear reason="undefined" cert="high">αν</unclear> <unclear reason="undefined" cert="high">ἀπα</unclear><supplied reason="lost" cert="high">ιτ</supplied><unclear reason="undefined" cert="high">η</unclear><supplied reason="lost" cert="high">θῇ</supplied>. <space extent="unknown" unit="character" dim="horizontal"/> 

    <lb n="22"/><handShift new="m3"/> Ἀντόνιος Πολυκράτης. 

