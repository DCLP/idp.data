    <lb n="1"/><unclear cert="high" reason="undefined">Θέ</unclear>ω<unclear cert="high" reason="undefined">νι</unclear> <supplied cert="high" reason="lost">στρατηγῷ Ἀρσινοίτου Θεμίστου</supplied> 

