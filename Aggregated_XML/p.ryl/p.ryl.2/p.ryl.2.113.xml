    <lb n="1"/><unclear cert="high" reason="undefined">Π</unclear><supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">τρωνίωι</unclear> <unclear cert="high" reason="undefined">Μαμερτείνωι</unclear> 

    <lb n="2"/>ἐπάρχωι Αἰγύπτου 

    <lb n="3"/>παρὰ Ἱερακίωνος 

    <lb n="4"/>Δωρίωνος τῶν <unclear cert="high" reason="undefined">ἀπὸ</unclear> 

    <lb n="5"/>Λητοῦς πόλεως 

    <lb n="6"/>μελισσουργοῦ. <choice><corr>ἐπὶ</corr><sic>επει</sic></choice> 

    <lb n="7"/>τῷ προτέρῳ διαλο-

    <lb n="8"/>γισμῶι, ἡγεμὼν κύριε, 

    <lb n="9"/>Σαραπᾶς Ψιαθᾶτος 

    <lb n="10"/>καὶ Ἑρμᾶς Ψοσναῦτος 

    <lb n="11"/>καὶ Νίννος Κόμωνος 

    <lb n="12"/>μὴ <choice><corr>ἔχοντες</corr><sic>εχοντας</sic></choice> πᾶν 

    <lb n="13"/>πρᾶγμα πρὸς ἐμὲ 

    <lb n="14"/>διεστείλαντό μοι 

    <lb n="15"/>ὥς φασιν ἔχειν 

    <lb n="16"/>περὶ <choice><corr><supplied cert="high" reason="omitted">πρά</supplied>γματος</corr><sic>γματος</sic></choice> πρός 

    <lb n="17"/>με δεομέ<supplied cert="high" reason="lost">ν</supplied>ο<unclear cert="high" reason="undefined">υ</unclear> τῆς 

    <lb n="18"/>διαγνώσεως Φλαυ-

    <lb n="19"/>ίου Τιτιανοῦ τότε 

    <lb n="20"/>τοῦ <expan>ἡγεμονεύσαντ<ex cert="high">ος</ex></expan> 

    <lb n="21"/>ὡς συνβῆναι ἐκ 

    <lb n="22"/>τούτου τὸ ὡρισ-

    <lb n="23"/>μένον ὑπὲρ τῶν 

    <lb n="24"/>διαστολικῶν ἐν 

    <lb n="25"/><expan>δραχ<ex cert="high">μαῖς</ex></expan> <num value="500">φ</num> καὶ <app type="BL"><lem resp="1.388">πρὸς 

    <lb n="26"/>κατακριθῆναι</lem><rdg>προσβάντ<unclear cert="high" reason="undefined">ος</unclear> κριθῆναι</rdg></app>, 

    <lb n="27"/>καὶ μόλις πάντα 

    <lb n="28"/>τὰ ἐμαυτοῦ πω-

    <lb n="29"/>λήσας ἐδυνήθην 

    <lb n="30"/>πληρῶσαι. τοῦ οὖν 

    <lb n="31"/>πράγματος δεο-

    <lb n="32"/>μένου τῆς σῆς 

    <lb n="33"/><choice><corr>μισοπονηρίας</corr><sic>μειζοπονηριας</sic></choice> 

    <lb n="34"/>ἀξιῶ σε τὸν κύριον 

    <lb n="35"/>καὶ δικαι<supplied cert="high" reason="lost">ο</supplied>κρίτην 

    <lb n="36"/>ἀκοῦσαί μου πρὸς 

    <lb n="37"/><unclear cert="high" reason="undefined">αὐτο</unclear>ὺ<unclear cert="high" reason="undefined">ς</unclear> <gap extent="8" reason="lost" unit="character"/> 

    <lb n="38"/><gap extent="unknown" reason="lost" unit="line"/> 
