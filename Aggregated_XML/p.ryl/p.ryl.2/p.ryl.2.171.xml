    <lb n="1"/>Εὐσχήμονι <unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ἰ</supplied><unclear cert="high" reason="undefined">κ</unclear>ονόμωι τῆς ἐν τῶι Ἀρσινοείτη<unclear cert="high" reason="undefined">ι</unclear> <supplied cert="high" reason="lost">Τιβερίου</supplied> 

    <lb n="2"/>Κλαυδίου Δορυφόρου πρότερον Ναρκισσιανῆς οὐσία<supplied cert="high" reason="lost">ς</supplied> 

    <lb n="3"/>παρὰ Πάπου τοῦ Τρύφωνος τῶν ἀπὸ Ἡρακλείας τ<supplied cert="high" reason="lost">ῆς Θεμίστου</supplied> 

    <lb n="4"/>μερίδος καταγεινομένου ἐν ἐποικίωι Ἀντωνί<unclear cert="high" reason="undefined">α</unclear><supplied cert="low" reason="lost">ςΔρούσουδιὰ</supplied> 

    <lb n="5"/>Ψ<unclear cert="high" reason="undefined">ενε</unclear>ριεῦτος Πέρσου τῆς ἐπιγονῆς. βούλομαι μισ<supplied cert="high" reason="lost">θώσασθαι ἐπὶ</supplied> 

    <lb n="6"/><choice><corr>ἔτη</corr><sic>ετηι</sic></choice> τέσ<supplied cert="high" reason="lost">σ</supplied><unclear cert="high" reason="undefined">ε</unclear>ρα <num value="4"/> ἀπὸ τοῦ εἰσιόντος τρίτου ἔτους Ν<supplied cert="high" reason="lost">έρωνος Κλαυδίου</supplied> 

    <lb n="7"/>Καίσαρος <unclear cert="high" reason="undefined">Σ</unclear>εβαστοῦ Γερμανικοῦ Αὐτοκράτορος τὰ<supplied cert="high" reason="lost">ς ὑπαρχούσας</supplied> 

    <lb n="8"/><app type="BL"><lem resp="6.123"><supplied cert="high" reason="lost">Τιβερίωι Κλαυδίωι Δορυφόρωι περὶ τ</supplied><unclear cert="high" reason="undefined">ὴν</unclear></lem><rdg><gap extent="4" reason="illegible" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="23" reason="lost" unit="character"/><supplied cert="high" reason="lost"> περὶ τ</supplied><unclear cert="high" reason="undefined">ὴν</unclear></rdg></app> αὐτὴν <unclear cert="high" reason="undefined">κ</unclear>[ώμην <app type="BL"><lem resp="6.123">σιτικῶν</lem><rdg/></app>] 

    <lb n="9"/>[<app type="BL"><lem resp="6.123">ἐδαφῶν χέρσων</lem><rdg><gap extent="12" reason="illegible" unit="character"/></rdg></app> ἐν δυσὶ <num value="2"/> σφραγῖσι ἀρο]ύρας εἴκοσι <app type="BL"><lem resp="6.123"><unclear cert="high" reason="undefined">ἑ</unclear>[πτά <num value="27"/>, ἀνὰ μέσου]</lem><rdg><unclear cert="high" reason="undefined">ἑ</unclear><supplied cert="high" reason="lost">πτά, ὧν ἐν μιᾷ</supplied></rdg></app> 

    <lb n="10"/><app type="BL"><lem resp="6.123">[ὄντος <note anchored="yes" lang="en" place="not specified">?</note><gap extent="25" reason="illegible" unit="character"/> ]<gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ι</unclear>κοντε<gap extent="unknown" reason="lost" unit="character"/></lem><rdg><supplied cert="high" reason="lost">μὲν σφραγῖδι </supplied><gap extent="18" reason="lost" unit="character"/> <gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ι</unclear>κοντε<gap extent="10" reason="lost" unit="character"/></rdg></app> 

    <lb n="11"/><app type="BL"><lem resp="6.123"><supplied cert="high" reason="lost">ὧν ἐν τῆι π</supplied>ρ<supplied cert="high" reason="lost">ώτηι μὲν σφραγῖδι</supplied></lem><rdg><gap extent="9" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ρ</unclear><gap extent="17" reason="lost" unit="character"/></rdg></app><supplied cert="high" reason="lost"> ἄ</supplied>ρ<supplied cert="high" reason="lost">ο</supplied>υραι ἑ<unclear cert="high" reason="undefined">πτά</unclear> <num value="7"/>, <unclear cert="high" reason="undefined">ἐ</unclear>[ν δὲ τῆι <num value="2">β</num> <choice><corr>σφρα]-

    <lb n="12"/>γῖδι</corr><sic><supplied cert="high" reason="lost">σφρα</supplied>γειδι</sic></choice> ἄρουραι εἴκοσι <num value="20"/>, ὧν γείτονες νότου βασιλικὴ [γῆ ἐν <app type="BL"><lem resp="6.123">ἠπεί]-

    <lb n="13"/>ρωι</lem><rdg><gap extent="5" reason="lost" unit="character"/>ρωι</rdg></app>, βορρᾶ <choice><corr>βασιλικὴ</corr><sic>βασιλικηι</sic></choice> <choice><corr>γῆ</corr><sic>γηι</sic></choice> ἐν αἰγιαλῶι, λιβὸς <app type="BL"><lem resp="6.123">Πα<supplied cert="high" reason="lost">λλαντιανῆς οὐσίας</supplied></lem><rdg>Π<unclear cert="high" reason="undefined">α</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="12" reason="lost" unit="character"/></rdg></app> 

    <lb n="14"/><choice><corr>ἐδάφη</corr><sic>εδαφηι</sic></choice>, ἀπηλιώτου τῆς <choice><corr>Μαικηνατιανῆς</corr><sic>μαικηναιτιανης</sic></choice> ο<supplied cert="high" reason="lost">ὐσίας ἐδάφη,</supplied> 

    <lb n="15"/>ἐφ᾽ ὧι λαβὼν εἰς κατεργασίαν καὶ χωματισ<supplied cert="high" reason="lost">μοὺς τῶν ἐδα</supplied>-

    <lb n="16"/>φῶν ἑκάστης ἀρούρης ἀργυρίου δραχμ<supplied cert="high" reason="lost">ὰς </supplied><gap extent="6" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀναπο</supplied>-

    <lb n="17"/>δότους <choice><corr>τελέσω</corr><sic>τελεσωι</sic></choice> ἐκφόριον τοῦ παντὸς <supplied cert="high" reason="lost">κατ᾽ ἔτος τῆς φα</supplied>-

    <lb n="18"/>νησομένης ἐκ σχοινουργίας σὺν ᾗ λήμ<supplied cert="high" reason="lost">ψομαι σπερμάτων</supplied> 

    <lb n="19"/>κατ᾽ ἄρουραν πυροῦ ἀρτάβῃ μιᾷ <num value="1"/> ἑκάστης ἀ<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ούρης </supplied><gap extent="7" reason="lost" unit="character"/> 

