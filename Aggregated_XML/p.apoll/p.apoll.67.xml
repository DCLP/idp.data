    <lb n="1"/><g type="stauros"/> α<gap extent="unknown" reason="lost" unit="character"/>ουτο<unclear cert="high" reason="undefined">σαι</unclear>φαν<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/><gap extent="unknown" reason="lost" unit="character"/> τῆς μακαρίας <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> πρ</supplied>ὸ γὰρ πάντων ὁδ<supplied cert="high" reason="lost">ο</supplied>ιπορευόμεθα <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/><gap extent="unknown" reason="lost" unit="character"/> καὶ τὸν φιλάνθρωπον ἡμῶν Θεὸν <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>καὶ ποιησαντ<gap extent="unknown" reason="lost" unit="character"/>του μεθ᾽ ἡμῶν καὶ ἐφελ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/>δεσποτικὴν ἀδελφότητα ἐνταῦθα τὴν <unclear cert="high" reason="undefined">α</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/>ὄντως ἐν μεγάλῃ περιστάσει ἐστὶ <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>ἐκ τῶν πολλῶν <choice><corr>συγχύσεων</corr><sic>συγχησεων</sic></choice> τῶν <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>ἐν ἑκάστῳ αὐτῶν <choice><corr>ἐπιτηδείῳ</corr><sic>επιτηδιω</sic></choice> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/>θεοφύλακτον ἀδελφότη<supplied cert="high" reason="lost">τα </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/>ἡμᾶς ὁ Θεῖος Ἀπόστολος <add place="supralinear">ὅτι</add> ὀφείλο<supplied cert="high" reason="lost">μεν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="12"/>παραλείψητε καὶ καταλάβητε τὰ πρὸς αὐτὸ<supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/>τὰ παράνω αὐτοῦ ἵνα καὶ αὐτὸς <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="14"/>ἐν περιστάσει ἐστὶ ἄτρι ἐὰν κατ<supplied cert="low" reason="lost">αλάβητε </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="15"/>απαιτ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="16"/><gap extent="unknown" reason="lost" unit="line"/> 
