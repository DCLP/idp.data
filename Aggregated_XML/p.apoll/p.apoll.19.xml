    <lb n="1"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost">  ἡ ὑμετέρα θεοφύλακτος</supplied> 

    <lb n="2"/>ἀδελφότης εὐτρεπεί<supplied cert="high" reason="lost">σῃ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τῶ</supplied>ν γενημάτων καὶ ἀποκλείσατε. 

    <lb n="3"/>ἀλλὰ πάντως ἔργον ὑμῖ<supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">η</unclear>τε σιαινομένῳ χάριν <expan>αὐτ<ex cert="high">οῦ</ex></expan> <foreign lang="la">?. </foreign>

    <lb n="4"/>λοιπὸν ἀντιγράψατέ μοι <unclear cert="high" reason="undefined">ε</unclear><gap extent="unknown" reason="lost" unit="character"/><supplied cert="low" reason="lost">προσ</supplied>ῆλθαν χθὲς οἱ ἀντίδικοι 

    <lb n="5"/>Σαβίνου, τοῦτ᾽ ἐστὶν Μαρτ<gap extent="unknown" reason="lost" unit="character"/><supplied cert="low" reason="lost">περιβ</supplied><unclear cert="low" reason="undefined">λ</unclear>έψητε τοὺς ἀνθρώπους 

    <lb n="6"/>αὐτῶν ἄχρις οὗ γράψω ὑμῖν π<supplied cert="high" reason="lost">ερὶ τούτου </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> καὶ ἀσπάζομαι τοὺ</supplied>ς <expan>θεοφυλ<ex cert="high">άκτους</ex></expan> καὶ <expan>μεγαλο<ex cert="high">πρεπεστάτους</ex></expan> ὑμᾶς 

    <lb n="7"/>διὰ τοῦ γράμματος <g type="stauros"/> <expan>Π<ex cert="high">α</ex>χ<ex cert="high">ὼν</ex></expan> <gap extent="unknown" reason="lost" unit="character"/> </ab></div><div n="meta.20251" part="N" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.20251" part="N" subtype="textDate" type="commentary"><p><date notAfter="0676-05-25" notBefore="0676-04-26" type="textDate">26. Apr. - 25. Mai 676 (Jahr unsicher)</date></p><p><date notAfter="0661-05-25" notBefore="0661-04-26" type="textDate">26. Apr. - 25. Mai 661 (Jahr unsicher)</date></p></div><div n="meta.20251" part="N" subtype="general" type="commentary"><p>Zur Datierung vgl. ZPE 49, 1982, S. 88 und J. Cowey . Alternativdatierung: 26. Apr. - 25. Mai 661. Vgl. P.Apoll. 18.</p></div><div n="meta.20251" part="N" subtype="principalEdition" type="bibliography"><listBibl><bibl subtype="principal" type="publication"><title level="s" type="abbreviated">P.Apoll.</title><biblScope type="numbers">19</biblScope></bibl><bibl type="Trismegistos"><biblScope type="numbers">20251</biblScope></bibl><bibl type="DDbDP"><series>0061</series>; ; <biblScope type="numbers">19</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0061;;19" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.20251" part="N" subtype="illustrations" type="bibliography"><p>keine</p></div><div n="meta.20251" part="N" subtype="locations" type="history"><p>unbekannt</p></div></body></text></TEI.2>