    <lb n="3"/><gap extent="10" reason="lost" unit="character"/>του. λοιπὸν σπουδά<supplied cert="high" reason="lost">σῃ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/><gap extent="10" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">αι</unclear><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ραν</unclear> σιδήρου<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/><supplied cert="high" reason="lost">τ</supplied>ὴν κεφαλαίωσιν τῶν ὄντ<supplied cert="high" reason="lost">ων εἰς τὴν παγαρχίαν αὐτῆς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

