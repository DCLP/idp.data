    <lb n="6"/>Μάρκου Αὐρηλίου Ἀντωνίνο<supplied reason="lost" cert="high">υ Παρθικοῦ Μεγίστου</supplied> 

    <lb n="7"/>καὶ Π<unclear reason="undefined" cert="high">ουβλ</unclear>ίου Σεπ<unclear reason="undefined" cert="high">τι</unclear><supplied reason="lost" cert="high">μί</supplied><unclear reason="undefined" cert="high">ο</unclear>υ Γέτ<supplied reason="lost" cert="high">α Βρεταννικῶν</supplied> 

    <lb n="8"/>Μεγίστων Εὐτ<supplied reason="lost" cert="high">υχ</supplied><unclear reason="undefined" cert="high">ῶ</unclear>ν Σεβα<supplied reason="lost" cert="high">στῶν τύχην </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>κέναι τὰ προκ<supplied reason="lost" cert="high">εί</supplied>μενα πλοῖα <supplied reason="lost" cert="high">εἰς διέρασιν τοῦ</supplied> 

    <lb n="10"/>δημοσίου πυροῦ καταγομ<supplied reason="lost" cert="high">ένου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>εἰς τὸν ἐν τῇ Νέᾳ Πόλει <unclear reason="undefined" cert="high">χ</unclear>ε<unclear reason="undefined" cert="high">ι</unclear><supplied reason="lost" cert="high">ρισμὸν τῇ ὀγδόῃ</supplied> 

    <lb n="12"/>τοῦ ὄντος μηνὸς Τῦβι. ἔ<supplied reason="lost" cert="high">τους εἰκοστοῦ</supplied> 

