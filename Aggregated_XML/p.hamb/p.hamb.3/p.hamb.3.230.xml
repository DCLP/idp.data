    <lb n="1"/><gap reason="illegible" extent="unknown" unit="character" desc="vestiges"/> 

    <lb n="2"/>τῆς τάξεως τῆς ὑμετέρας ἐξου<supplied reason="lost" cert="high">σίας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>τελοῦμεν ἀκολούθως, καθὼς πρ<supplied reason="lost" cert="high">οεῖ</supplied><unclear reason="undefined" cert="high">π</unclear>ον, τοῖς περὶ το<unclear reason="undefined" cert="high">ύ</unclear><supplied reason="lost" cert="high">του </supplied><gap reason="lost" extent="3" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="2" unit="character"/><unclear reason="undefined" cert="high">οις</unclear> τ<gap reason="lost" extent="6" unit="character"/> 

    <lb n="4"/>μεγίστοις τύποις. ἐνταῦθα δὲ τυγχάνω πρὸ δώδεκα <num value="12"/> ἡμερῶ<unclear reason="undefined" cert="high">ν</unclear> <supplied reason="lost" cert="high">π</supplied><unclear reason="undefined" cert="high">λ</unclear>έω ἔλαττο<unclear reason="undefined" cert="high">ν</unclear> 

    <lb n="5"/>διωχθεὶς παρὰ τοῦ αὐτοῦ αἰδεσίμου ἐξπελλευτοῦ τῆς Ἀνταιοπολιτῶ<unclear reason="undefined" cert="high">ν</unclear> καὶ μηδαμοῦ συγχ<unclear reason="undefined" cert="high">ω</unclear>ρ<supplied reason="lost" cert="high">η</supplied>-

    <lb n="6"/><unclear reason="undefined" cert="high">θεὶς</unclear> <unclear reason="undefined" cert="high">ἢ</unclear> <unclear reason="undefined" cert="high">τ</unclear>ῇ αἰσίᾳ <supplied reason="lost" cert="high">ἐμ</supplied>βολῇ ἢ <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">ῖ</unclear><supplied reason="lost" cert="high">ς ἄλλο</supplied><unclear reason="undefined" cert="high">ις</unclear> <unclear reason="undefined" cert="high">δη</unclear><supplied reason="lost" cert="high">μ</supplied>οσί<unclear reason="undefined" cert="high">ο</unclear>ις σχ<unclear reason="undefined" cert="high">ο</unclear>λά<unclear reason="undefined" cert="high">ζε</unclear> κ<gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="2" unit="character"/> μή<unclear reason="undefined" cert="high">τε</unclear> <supplied reason="lost" cert="high">ὅ</supplied><unclear reason="undefined" cert="high">λ</unclear>ως πάροδον 

    <lb n="7"/>αὐτοῦ ἔχοντος πρὸς ἡμᾶς, καθὼς προεῖπον· ἀλλ<g type="apostrophe"/> ε<unclear reason="undefined" cert="high">κ</unclear><gap reason="lost" extent="6" unit="character"/><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="3" unit="character"/><gap reason="illegible" extent="2" unit="character"/> κατ<unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">βο</supplied><unclear reason="undefined" cert="high">λ</unclear><supplied reason="lost" cert="high">ὴ</supplied><unclear reason="undefined" cert="high">ν</unclear> π<unclear reason="undefined" cert="high">έμ</unclear>π<unclear reason="undefined" cert="high">ον</unclear>τ<unclear reason="undefined" cert="high">ες</unclear> 

    <lb n="8"/>διὰ <hi rend="diaeresis">ἰ</hi>δίου <hi rend="diaeresis">ὑ</hi>ποδέκτου εἰς τὴν τάξιν καὶ μηδὲν ἐν τῇ Ἀντα<supplied reason="lost" cert="high">ιο</supplied>πολιτῶ<unclear reason="undefined" cert="high">ν</unclear> καταβάλλοντες κ<unclear reason="undefined" cert="high">αὶ</unclear> 

    <lb n="9"/>τὸ πρόσγραφον <unclear reason="undefined" cert="high">δ</unclear><supplied reason="lost" cert="high">ὲ τ</supplied><unclear reason="undefined" cert="high">ῆ</unclear><supplied reason="lost" cert="high">ς</supplied> <unclear reason="undefined" cert="high">ἐ</unclear>μβολῆς τοῦ εὐλαβεστάτου μονάζοντος πάλιν διὰ <hi rend="diaeresis">ἰ</hi>δίου <unclear reason="undefined" cert="high">ἀν</unclear>θρώπο<unclear reason="undefined" cert="high">υ</unclear> 

    <lb n="10"/>πέμποντες εἰ<unclear reason="undefined" cert="high">ς</unclear> <unclear reason="undefined" cert="high">τ</unclear>ὴν τάξιν <unclear reason="undefined" cert="high">ὡς</unclear> <unclear reason="undefined" cert="high">μη</unclear>δε<supplied reason="lost" cert="high">μ</supplied><unclear reason="undefined" cert="high">ίαν</unclear> <unclear reason="undefined" cert="high">ἔχειν</unclear> <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ὸν</supplied> ἐξπελλε<unclear reason="undefined" cert="high">υτ</unclear>ὴ<supplied reason="lost" cert="high">ν </supplied><gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/> <gap reason="lost" extent="5" unit="character"/> 

    <lb n="11"/>δι᾽ ἡμῶν. ἀλλ᾽ οὐδὲ <subst><add place="inline">ὠχλή<unclear reason="undefined" cert="high">θη</unclear><supplied reason="lost" cert="high">σ</supplied><unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">ν</supplied></add><del rend="corrected" status="unremarkable"><gap reason="illegible" extent="1" unit="character"/>χλη<unclear reason="undefined" cert="high">θη</unclear><supplied reason="lost" cert="high">σ</supplied><unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">ν</supplied></del></subst><supplied reason="lost" cert="high"> ο</supplied>ὔποτε <hi rend="diaeresis">ὑ</hi>πὲρ τῆ<supplied reason="lost" cert="high">ς</supplied> ἡμῶν κ<unclear reason="undefined" cert="high">ώμ</unclear>η<unclear reason="undefined" cert="high">ς</unclear> ἢ αὐτὸς ἢ <subst><add place="inline">ἄλλοι</add><del rend="corrected" status="unremarkable">αλλος</del></subst> 

    <lb n="12"/><subst><add place="inline"><choice><corr>ἐξπελλευταὶ</corr><sic>εξπελλευτ<add place="supralinear"><unclear reason="undefined" cert="high">ο</unclear>ι</add></sic></choice></add><del rend="corrected" status="unremarkable">εξπελλευτης</del></subst> πρὸ αὐτοῦ. ἐπειδὴ δὲ ἀκήκοα ὡς καί τινα <choice><corr>ἀ<supplied reason="lost" cert="high">ν</supplied>ενήνεκται</corr><sic><unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">ν</supplied>ηνεκται</sic></choice> κατ᾽ ἐμοῦ 

    <lb n="13"/>παρὰ Νέπωτος τοῦ σιγγουλαρίου ἐξ <hi rend="diaeresis">ὑ</hi>πο<unclear reason="undefined" cert="high">βολῆ</unclear>ς, <unclear reason="undefined" cert="high">ὡς</unclear> εἴρηται, τοῦ αὐτοῦ αἰδεσί<unclear reason="undefined" cert="high">μο</unclear>υ Ἀφθονίου, ἑτοίμως 

    <lb n="14"/>ἔχ<unclear reason="undefined" cert="high">ω</unclear>, <unclear reason="undefined" cert="high">εἰ</unclear> κα<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="1" unit="character"/>η τὰ ἐντ<unclear reason="undefined" cert="high">α</unclear><gap reason="lost" extent="3" unit="character"/> <unclear reason="undefined" cert="high">ὁ</unclear> <unclear reason="undefined" cert="high">αὐ</unclear>τὸς <unclear reason="undefined" cert="high">αἰδ</unclear><supplied reason="lost" cert="high">έσ</supplied><unclear reason="undefined" cert="high">ι</unclear><supplied reason="lost" cert="high">μ</supplied>ος σιγ<supplied reason="lost" cert="high">γουλ</supplied>άρι<supplied reason="lost" cert="high">ος</supplied> Νέπως <unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">αὶ ἀπ</supplied>οδείξῃ τι 

    <lb n="15"/>τῶν παρ᾽ <unclear reason="undefined" cert="high">αὐτ</unclear>οῦ <choice><corr>ἀνενηνεγμένων</corr><sic>ανηνεγμενων</sic></choice> παρ᾽ ἐμοῦ γεγενημένο<unclear reason="undefined" cert="high">ν</unclear>, <supplied reason="lost" cert="high">ὑ</supplied><unclear reason="undefined" cert="high">π</unclear>ο<unclear reason="undefined" cert="high">κεῖ</unclear><supplied reason="lost" cert="high">σθαι</supplied> τοῖς προσταττομένοις 

    <lb n="16"/>παρὰ τῆς <hi rend="diaeresis">ὑ</hi>μετέρας ἐξουσίας. ὅτι γὰρ οὐδὲν τοιο<supplied reason="lost" cert="high">ῦ</supplied>το ἢ ἐνεθυμήθην πώποτε ἢ διεπραξάμη<supplied reason="lost" cert="high">ν</supplied>, 

    <lb n="17"/>ἐξ οὗ παρήγγειλ<unclear reason="undefined" cert="high">α</unclear> <unclear reason="undefined" cert="high">ε</unclear><supplied reason="lost" cert="high">ἰ</supplied>ς ἄνδρας, οὔπω λέγω εἰς τὸν πρωτοκωμήτην, θαρρῶ<unclear reason="undefined" cert="high">ν</unclear> ἐμαυτ<unclear reason="undefined" cert="high">ῷ</unclear> 

    <lb n="18"/>τοῦτο αὐτὸ κατατίθημι. καὶ παρακαλῶ, εἴπερ παρασταίη τῇ <hi rend="diaeresis">ὑ</hi>μετέρᾳ ἐξουσίᾳ, 

    <lb n="19"/>τὸν συνήθη λόγον κελεῦσαι δοθῆναί μοι, ὃ<unclear reason="undefined" cert="high">ν</unclear> πάντες ἔχουσιν οἱ τῆς ἐπαρχείας <unclear reason="undefined" cert="high">ἡμῶν</unclear> 


    <lb n="20"/><hi rend="diaeresis">ἰ</hi>διωτεύοντες, οὔπω λέγω δημόσια χειρίζοντες αὐτὸς γὰρ ὁ ταῦτα ἀνενεγκ<unclear reason="undefined" cert="high">ὼν</unclear> 

    <lb n="21"/>κατ᾽ ἐμοῦ <hi rend="diaeresis">ὑ</hi>πὸ λόγον τυγχάνων <unclear reason="undefined" cert="high">ἀν</unclear>ήνε<unclear reason="undefined" cert="high">γκεν</unclear> ψε<unclear reason="undefined" cert="high">υ</unclear>δῆ κατ᾽ ἐ<supplied reason="lost" cert="high">μο</supplied><unclear reason="undefined" cert="high">ῦ</unclear> βουλ<supplied reason="lost" cert="high">ό</supplied><unclear reason="undefined" cert="high">μενός</unclear> <supplied reason="lost" cert="high">μ</supplied><unclear reason="undefined" cert="high">ε</unclear> κα<unclear reason="undefined" cert="high">θ</unclear>᾽ <hi rend="diaeresis">ὑ</hi>π<unclear reason="undefined" cert="high">οβο</unclear><supplied reason="lost" cert="high">λ</supplied><unclear reason="undefined" cert="high">ὴν</unclear> 

    <lb n="22"/>Ἀφθονίου διωχθῆναι πρὸς περιγραφὴν τῆς αἰσί<supplied reason="lost" cert="high">ας</supplied> ἐμβο<unclear reason="undefined" cert="high">λ</unclear><supplied reason="lost" cert="high">ῆς κ</supplied><unclear reason="undefined" cert="high">αὶ</unclear> τῶν ἄλλων δη<unclear reason="undefined" cert="high">μο</unclear><supplied reason="lost" cert="high">σί</supplied><unclear reason="undefined" cert="high">ω</unclear><supplied reason="lost" cert="high">ν</supplied>, 

    <lb n="23"/>ὅπως δυνηθῶ τὰς προόδους ποιήσασθαι καὶ καταλαβεῖν τὸ <hi rend="diaeresis">ὑ</hi>μέτερον 

    <lb n="24"/>δικαστήριον καὶ ἀνεύθυνον ἐμαυτὸν ἀποδεῖξαι πάντων τῶν μάτην κατ᾽ ἐμοῦ 

    <lb n="25"/>κατηγορημένων παρὰ τῶν συκοφαντεῖν με ἐθελόντων καὶ τὸ δημόσιον 

    <lb n="26"/>διασκορπίζειν. </ab></div><div n="meta.15336" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.15336" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" precision="low">ca. 565 (?)</date></p></div><div n="meta.15336" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Zur Datierung vgl. Einleitung und ("Mitte VI") Études alexandrines 8, 2002, S. 34 mit Anm. 38. Gehört zum Dioskoros - Archiv. </p></div><div n="meta.15336" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">P.Hamb.</title><biblScope type="volume">3</biblScope><biblScope type="numbers">230</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">15336</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0128</series>; <biblScope type="volume">3</biblScope>; <biblScope type="numbers">230</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0128;3;230" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.15336" type="bibliography" subtype="corrections" org="uniform" part="N" sample="complete"><head>BL-Einträge nach BL-Konkordanz</head><bibl type="BL" default="NO"><biblScope type="volume">X</biblScope>, S. <biblScope type="pages">82</biblScope></bibl></div><div n="meta.15336" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p><bibl type="illustration" default="NO">Tafel XXIV</bibl></p></div><div n="meta.15336" type="history" subtype="locations" org="uniform" part="N" sample="complete"><p><placeName type="ancientFindspot" cert="high" full="yes">Antinoopolis</placeName></p></div></body></text></TEI.2>