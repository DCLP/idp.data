    <lb n="1"/>καὶ εἰς τὴ<unclear cert="high" reason="undefined">ν</unclear> <gap extent="4" reason="illegible" unit="character"/><gap extent="3" reason="illegible" unit="character"/> <unclear cert="high" reason="undefined">καὶ</unclear> <gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>παρούσης <expan>ἰνδικ<ex cert="high">τίωνος</ex></expan>, καὶ εἰς σὴν ἀσφάλειαν <supplied cert="high" reason="lost">ἐξέδωκά σοι ταύτην τὴν πληρωτικὴν</supplied> 

    <lb n="3"/>ἀποχὴν ἐφ᾽ ὑπογραφῆ<unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">τοῦ</unclear> <supplied cert="high" reason="lost">ὑπ</supplied><unclear cert="high" reason="undefined">ὲρ</unclear> <unclear cert="high" reason="undefined">ἐ</unclear><supplied cert="high" reason="lost">μο</supplied><unclear cert="high" reason="undefined">ῦ</unclear> <supplied cert="high" reason="lost">ὑπογράφοντος, ὡς πρόκειται, ἀκύρων</supplied> 

