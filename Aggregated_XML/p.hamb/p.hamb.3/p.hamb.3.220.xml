    <lb n="1"/><supplied cert="high" reason="lost">ἔτους τρίτου Αὐτοκράτορος Καίσαρος Μάρκου Αὐρηλίου Σεουήρου Ἀλεξ</supplied>άνδρου <unclear cert="high" reason="undefined">Εὐσ</unclear>εβο<unclear cert="high" reason="undefined">ῦς</unclear> Εὐ<unclear cert="high" reason="undefined">τυχ</unclear><supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ῦ</unclear>ς <unclear cert="high" reason="undefined">Σε</unclear>βασ<supplied cert="high" reason="lost">τοῦ μηνὸς </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐν Πτολεμαίδι Εὐεργέτιδι τοῦ Ἀρσινοίτου</supplied> 

    <lb n="2"/><supplied cert="low" reason="lost">νομοῦ. ὁμολογεῖ Αὐρήλιος Ἥρων </supplied><gap extent="unknown" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/> <unclear cert="high" reason="undefined">ἀ</unclear>να<unclear cert="high" reason="undefined">γ</unclear>ραφ<unclear cert="high" reason="undefined">ό</unclear><supplied cert="high" reason="lost">μ</supplied><unclear cert="high" reason="undefined">ε</unclear>ν<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ς ἐ</supplied>π᾽ <unclear cert="high" reason="undefined">ἀ</unclear><supplied cert="high" reason="lost">μ</supplied><unclear cert="high" reason="undefined">φ</unclear><supplied cert="high" reason="lost">όδου </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ὡς ἐτῶν </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> οὐλὴ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τῇ προούσῃ</supplied> 

    <lb n="3"/><supplied cert="high" reason="lost">καὶ συνούσῃ αὐτῷ ἀγράφως γυναικὶ Αὐρηλίᾳ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀπὸ κώ</supplied>μης Νείλου πόλεως τῆς Ἡρακλείδου <unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ερίδος τοῦ Ἀρσινοίτου νομοῦ ὡς ἐτῶν </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> οὐλὴ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> μετὰ</supplied> 

    <lb n="4"/><supplied cert="high" reason="lost">κυρίου </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἔχειν παρ᾽ αὐτῆς ἐφ᾽ ἑαυτῇ ἅμα τ</supplied><unclear cert="high" reason="undefined">ῇ</unclear> τοῦ γάμου <choice><corr>σ<supplied cert="high" reason="lost">υ</supplied>μβιώσει</corr><sic>σ<supplied cert="high" reason="lost">υ</supplied>νβιωσι</sic></choice> φερνὴν διὰ <unclear cert="high" reason="undefined">χε</unclear><supplied cert="high" reason="lost">ιρὸς χρυσίου καθαροῦ μναιαῖα ὀκτὼ <num value="8"/> ἐν κοσμαρίοις, ἅ ἐστιν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/><gap extent="57" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> τῶν λοιπῶν μναιαίων δύο <num value="2"/> καὶ <supplied cert="high" reason="lost">ἱμάτια ἐν συντιμήσει ἀργυρίου δραχμῶν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/><gap extent="57" reason="lost" unit="character"/> κόκκινα, <choice><corr>χιτὼν</corr><sic>κι<unclear cert="high" reason="undefined">τ</unclear>ων</sic></choice> καὶ μαφόρτης <choice><corr>σαπφείριν<supplied cert="high" reason="lost">οι</supplied></corr><sic>σαππιρ<unclear cert="high" reason="undefined">ιν</unclear><supplied cert="high" reason="lost">οι</supplied></sic></choice><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/><gap extent="57" reason="lost" unit="character"/>ν, ἅ ἐστιν Ἀφροδίτη, ἔσοπτ<unclear cert="high" reason="undefined">ρ</unclear>ον δίπ<unclear cert="high" reason="undefined">τυ</unclear>χ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> συμβιούτωσαν οὖν ἀλλήλοις οἱ γαμοῦντες ἀμέμπτως καθότι καὶ</supplied> 

    <lb n="8"/><supplied cert="high" reason="lost">πρότερον, τοῦ μὲν Αὐρηλίου Ἥρωνος ἐπιχορηγοῦντος τῇ Αὐρηλίᾳ </supplied><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ᾳ τὰ δέοντα πά<unclear cert="high" reason="undefined">ντα</unclear> καὶ τὸν ἱματισμὸν κ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ὶ τὰ ἄλλα ὅσα καθήκει γυναικὶ γαμετῇ κατὰ δύναμιν τοῦ βίου, τῆς δὲ Αὐρη</supplied>-

    <lb n="9"/><supplied cert="high" reason="lost">λίας </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἄμεμπτον καὶ ἀκατηγόρητον ἑαυτὴν παρεχομένης ἐν τῇ σ</supplied><unclear cert="high" reason="undefined">υ</unclear>μβιώσει ἐπὶ <supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ῷ</unclear> <unclear cert="high" reason="undefined">τὸν</unclear> Αὐρήλιον Ἥ<unclear cert="high" reason="undefined">ρωνα</unclear> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/><gap extent="57" reason="lost" unit="character"/><supplied cert="high" reason="lost"> κ</supplied><unclear cert="high" reason="undefined">α</unclear>ὶ συντάξ<unclear cert="high" reason="undefined">εω</unclear>ς, ἐφ᾽ ὃν χρόνον μένει αὐ<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">οῖς ἡ πρὸς ἀλλήλους συμβίωσις, </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐὰν δὲ διαφορᾶς αὐτοῖς γενομένης χ</supplied><unclear cert="high" reason="undefined">ω</unclear>ρίζωνται <unclear cert="high" reason="undefined">ἀπ</unclear>᾽ <unclear cert="high" reason="undefined">ἀ</unclear>λλήλων, ἀποδότω Αὐ<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ήλιος Ἥρων τῇ Αὐρηλίᾳ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τήν τε φερνὴν καὶ τὰ παράφερνα, τὸ μὲν χρυσί</supplied>-

    <lb n="12"/><supplied cert="high" reason="lost">ον ἐν τοῖς αὐτοῖς κοσμαρίοις καὶ ἴσῃ ὁλκῇ καὶ τὰ ἱμάτια ἐκλογῆς α</supplied>ὐτῇ οὔση<unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">ἤ</unclear>τοι τὰ ἱμάτια καὶ τὸ ἀποδέ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ν λαμβάνειν ἢ τὴν προκειμένην αὐτῶν συντίμησιν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐπὶ μὲν ἀποπομπῆς παραχρῆμα, ἐπὶ δὲ τῆς ἑκουσίο</supplied>υ ἀπαλλαγῆ<unclear cert="high" reason="undefined">ς</unclear> <supplied cert="high" reason="lost">ἐ</supplied>ν ἡμέραις ἑξήκοντα <num value="60"/>  <unclear cert="high" reason="undefined">τῆ</unclear><supplied cert="high" reason="lost">ς πράξεως τῇ Αὐρηλίᾳ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> γινομένης ἔκ τε τοῦ Αὐρη</supplied>-

    <lb n="14"/><supplied cert="high" reason="lost">λίου Ἥρωνος καὶ ἐκ τῶν ὑπαρχόντων αὐτῷ πάντων καθάπερ ἐκ δίκης.</supplied> περὶ δὲ τοῦ ταῦτα οὕτως ὀρθῶς κα<unclear cert="high" reason="undefined">λῶ</unclear><supplied cert="high" reason="lost">ς πεπρᾶχθαι Αὐρήλιος Ἥρων ἐπερωτηθεὶς ὡμολόγησεν.</supplied> 

