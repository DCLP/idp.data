    <lb n="1"/><handShift new="m1"/> Ἀ<unclear cert="high" reason="undefined">ν</unclear>τᾶς <supplied cert="high" reason="lost">Κ</supplied>απίτ<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">νι</supplied> τῷ ἀδ<supplied cert="high" reason="lost">ελφῷ</supplied> 

    <lb n="2"/>πλεῖστα χαίρειν. 

    <lb n="3"/>ἐχάρην λαβών σου τὸ ἐπιστόλιον 

    <lb n="4"/>κ<unclear cert="high" reason="undefined">α</unclear>λῆς φάσεως, ὅτι <app type="BL"><lem resp="5.40"><choice><corr>ἐγάμησας</corr><sic>εγαμησες</sic></choice></lem><rdg><choice><corr>ἐγαίσησας</corr><sic>εγαισησε?</sic></choice></rdg></app> 


    <lb n="5"/>καὶ ὅτι <app type="BL"><lem resp="5.40">ἀσθενήσασα ἐσώθη</lem><rdg><choice><corr>ἀσθενήσας</corr><sic>ασθενησασα</sic></choice> ἐ<unclear cert="high" reason="undefined">σ</unclear>ώθη<supplied cert="high" reason="omitted">ς</supplied></rdg></app>. 

    <lb n="6"/>λ<unclear cert="high" reason="undefined">οι</unclear>πόν, καθώς σοι <choice><corr>πρέπει</corr><sic>πρεπι</sic></choice>, πάντοτε 

    <lb n="7"/>κα<supplied cert="high" reason="lost">τ</supplied>άγεις σεαυτὸν καὶ νῦν ὡς ἄν-

    <lb n="8"/>θρωπος τέλειος γενάμενος. 

    <lb n="9"/>γράφεις μοι περὶ τῶν χρεωστῶν 

    <lb n="10"/>τοῦ ταλαιπώρου Ἰο<unclear cert="high" reason="undefined">υ</unclear><supplied cert="high" reason="lost">λ</supplied>ιανοῦ ἀπο-

    <lb n="11"/>χὰς προφέρειν <app type="BL"><lem resp="5.40">τινὰς αὐτοῦ</lem><rdg>τινὸς ἀστοῦ</rdg></app>. ἐὰν <choice><corr>ᾖ</corr><sic>ην</sic></choice> 

    <lb n="12"/>ἰδιόγραφα αὐτοῦ, <choice><corr>θέλοντας</corr><sic>θελοντες</sic></choice> καὶ <unclear cert="high" reason="undefined">μ</unclear>ὴ 

    <lb n="13"/><choice><corr>θέλοντας</corr><sic>θ<supplied cert="high" reason="lost">ε</supplied>λοντες</sic></choice> δεῖ ἡμᾶς <choice><corr>προσδέξασθαι</corr><sic>προσδεξεσθαι</sic></choice>. 

    <lb n="14"/><app type="BL"><lem resp="5.40">τὰ δὲ λοιπὰ</lem><rdg>τὰ χ<supplied cert="high" reason="omitted">ρ</supplied>έα, ἵνα</rdg></app> μετὰ τὰ<unclear cert="high" reason="undefined">ς</unclear> ἀποχὰς <choice><corr>ἀπαι-

    <lb n="15"/>τηθῇ</corr><sic>απαιτει<unclear cert="high" reason="undefined">τ</unclear>ωσαν</sic></choice>. σπουδαῖός ἐστιν ὁ Νῶνις 

    <lb n="16"/>καὶ <choice><corr>οὐδέποτε</corr><sic>ο<unclear cert="high" reason="undefined">υτ</unclear>εποτε</sic></choice> <choice><corr>ἀνταμείψομαι</corr><sic>ανταμειψωμαι</sic></choice> αὐτῷ 

    <lb n="17"/>τὴν χάριν, ἐάνπερ ζ<unclear cert="high" reason="undefined">ῶ</unclear>. <unclear cert="high" reason="undefined">μ</unclear>ελησάτω σ<unclear cert="high" reason="undefined">ο</unclear>ι 

    <lb n="18"/>γρά<supplied cert="high" reason="lost">ψα</supplied>ι καὶ Οὐαλερίῳ <supplied cert="high" reason="lost">τῷ</supplied> <unclear cert="high" reason="undefined">σ</unclear>ημε<unclear cert="high" reason="undefined">α</unclear>φόρῳ 

    <lb n="19"/>π<unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">ρὶ ο</supplied><unclear cert="high" reason="undefined">ὗ</unclear> αὐτῷ ἔδωκ<supplied cert="high" reason="lost">ας </supplied><gap extent="2" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="3" reason="illegible" unit="character"/> 

