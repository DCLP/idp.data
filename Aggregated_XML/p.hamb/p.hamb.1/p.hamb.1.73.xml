    <lb n="1"/>ἐν τῇ βιβλιοθήκῃ <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>χρῆσιν καρπίαν ἀ<supplied cert="high" reason="lost">γροῦ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>ωνα<add place="supralinear"><unclear cert="high" reason="undefined">ι</unclear></add> τὸν προ<supplied cert="high" reason="lost">γεγραμμένον </supplied><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="4"/>τῆς τελ<gap extent="4" reason="illegible" unit="character"/>ε<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>τὴν α<supplied cert="high" reason="lost">ὐτ</supplied>ήν. <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/>παρέ<unclear cert="high" reason="undefined">χεσθ</unclear>αι <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/>μου θέλω <unclear cert="high" reason="undefined">τ</unclear>ῷ <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>παρέχεσθαι κ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ὶ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τῷ δεῖνι καταλείπω δοθῆναί τε βούλομαι κατ᾽ ἔτος </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/><unclear cert="high" reason="undefined">κ</unclear>οντα ἐφ᾽ ὅνπερ <supplied cert="high" reason="lost">ζήσεται χρόνον. εἰ δέ τι ἐγὼ μετὰ ταῦτα γεγραμμένον καταλείπω τῇ</supplied> 

    <lb n="10"/>ἐμῇ <choice><corr>χειρὶ</corr><sic>χιρι</sic></choice> γραφ<unclear cert="high" reason="undefined">ό</unclear><supplied cert="high" reason="lost">μενον οἵῳ δήποτε τρόπῳ, βέβαιόν μοι εἶναι θέλω ἀκολούθως ταύτῃ μου τῇ</supplied> 

    <lb n="11"/>βουλήσει. Κορνηλίᾳ <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="12"/>τὸ πρᾶγμα, μὴ ἐλαττο<supplied cert="high" reason="lost">υμένης αὐτῆς περὶ ὧν ἄλλων ὀφείλεται. ταύτῃ τῇ διαθήκῃ δόλος πονηρὸς ἀπέσ</supplied>-

    <lb n="13"/>τω. οἰκετι<unclear cert="high" reason="undefined">κ</unclear>ὰ χρήματα τ<supplied cert="high" reason="lost">αύτης τῆς διαθήκης γενομένης ἐπρίατο </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="14"/><choice><corr>σηστερτίου</corr><sic><supplied cert="high" reason="lost">σ</supplied>ηστερτιων</sic></choice> νούμμ<supplied cert="high" reason="lost">ου ἑνός <num value="1"/>, ζυγοστατοῦντος τοῦ δεῖνος, ἀντεμαρτυρήθη </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="15"/>Ἀπρώνιος Νῖγερ ὁ κ<unclear cert="high" reason="undefined">αλ</unclear><supplied cert="high" reason="lost">ούμενος </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἡ διαθήκη ἐγένετο ἐν </supplied><gap extent="5" reason="lost" unit="character"/><supplied cert="high" reason="lost"> πόλει πρὸ </supplied><gap extent="2" reason="lost" unit="character"/> 

    <lb n="16"/>Καλανδῶν Ἰουνίων <gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ὑπάτοις, ἔτους </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Καίσαρος</supplied> 

    <lb n="17"/>τοῦ κυρίου μηνὶ Παχ<supplied cert="high" reason="lost">ὼν </supplied><gap extent="2" reason="lost" unit="character"/><supplied cert="high" reason="lost"> οἱ λοιποὶ σφραγισταί· </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐσφρά</supplied>-

    <lb n="18"/>γισα <foreign lang="la">?,</foreign> Γάιος Λικίννιος <gap extent="unknown" reason="lost" unit="character"/><supplied cert="low" reason="lost"> ἐσφράγισα <foreign lang="la">?, </foreign></supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐσφράγισα ,</supplied>

    <lb n="19"/>Λούκιος <unclear cert="high" reason="undefined">Ἐ</unclear>γνάτι<supplied cert="high" reason="lost">ο</supplied>ς Λό<supplied cert="high" reason="lost">νγος </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐσφράγισα <foreign lang="la">?. </foreign></supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost">  ἠνοίχθη καὶ ἀνεγνώσθη ἐν τῷ ἐπὶ </supplied><gap extent="5" reason="lost" unit="character"/><supplied cert="high" reason="lost"> πόλεως Και</supplied>-

    <lb n="20"/>σαρείῳ θεῶν Σεβα<supplied cert="high" reason="lost">στῶν ἐπὶ παρόντων τοῦ δεῖνος στρατηγοῦ τοῦ νομοῦ καὶ</supplied> 

    <lb n="21"/>π<unclear cert="high" reason="undefined">λείο</unclear>νος μέρους σφ<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">αγιστῶν, </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost">  ὑπάτοις, ἔτους </supplied><gap extent="2" reason="lost" unit="character"/> 

    <lb n="22"/>Αὐτοκράτορος Καί<unclear cert="high" reason="undefined">σ</unclear>αρο<supplied cert="high" reason="lost">ς </supplied><gap extent="unknown" reason="lost" unit="character"/> </ab></div><div n="meta.28697" part="N" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.28697" part="N" subtype="textDate" type="commentary"><p><date notAfter="200" notBefore="101" type="textDate">II</date></p></div><div n="meta.28697" part="N" subtype="general" type="commentary"><p>Zum Ort vgl. BL II  2, S. 75.</p></div><div n="meta.28697" part="N" subtype="principalEdition" type="bibliography"><listBibl><bibl subtype="principal" type="publication"><title level="s" type="abbreviated">P.Hamb.</title><biblScope type="volume">1</biblScope><biblScope type="numbers">73</biblScope></bibl><bibl type="Trismegistos"><biblScope type="numbers">28697</biblScope></bibl><bibl type="DDbDP"><series>0128</series>; <biblScope type="volume">1</biblScope>; <biblScope type="numbers">73</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0128;1;73" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.28697" part="N" subtype="corrections" type="bibliography"><head>BL-Einträge nach BL-Konkordanz</head><listBibl><bibl type="BL"><biblScope type="volume">VIII</biblScope>, S. <biblScope type="pages">145</biblScope></bibl><bibl type="BL"><biblScope type="volume">IX</biblScope>, S. <biblScope type="pages">99</biblScope></bibl></listBibl></div><div n="meta.28697" part="N" subtype="illustrations" type="bibliography"><p>keine</p></div><div n="meta.28697" part="N" subtype="otherPublications" type="bibliography"><head>Andere Publikation</head><bibl subtype="other" type="publication">SB III 6273</bibl></div><div n="meta.28697" part="N" subtype="locations" type="history"><p><placeName cert="low" type="ancientFindspot">Philadelphia (Arsinoites)</placeName></p></div></body></text></TEI.2>