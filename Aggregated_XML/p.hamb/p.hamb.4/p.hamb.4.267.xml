    <lb n="2"/>νον <expan><ex cert="high">ταλάντων</ex></expan> <num value="17">ιζ</num> καὶ <unclear reason="undefined" cert="high">ε</unclear>ἰς <choice><corr>ἔκθεσιν</corr><sic>εχθεσιν</sic></choice> τὸ <choice><corr>πρωτεῖον</corr><sic>πρωτιον</sic></choice>, τὸ δὲ

    <lb n="3"/>παπα <note lang="en" anchored="yes" place="not specified">?</note> τὸ <choice><corr>πρωτεῖον</corr><sic>πρωτιον</sic></choice> <expan><ex cert="high">ταλάντων</ex></expan> <num value="26">κϛ</num> <expan><ex cert="high">δραχμῶν</ex></expan> <num value="4000">Δ</num>, ἡ δὲ λίτρα τῶν

    <lb n="4"/>χωρικῶν <expan><ex cert="high">οὐγκίων</ex></expan> <num value="14">ι<unclear reason="undefined" cert="high">δ</unclear></num> <expan><ex cert="high">ταλάντων</ex></expan> <num value="2">β</num> <expan><ex cert="high">δραχμῶν</ex></expan> <num value="1200">Ασ</num>, τὸ δὲ σπάτιον

    <lb n="5"/>θελόμενον <expan><ex cert="high">ταλάντων</ex></expan> <num value="22">κβ</num> τὸ <choice><corr>πρωτεῖον</corr><sic>πρωτιον</sic></choice>, ἡ δὲ ἰ̈σάτις

    <lb n="6"/>τὸ κεντηνάρι<unclear reason="undefined" cert="high">ο</unclear>ν <expan><ex cert="high">ταλάντων</ex></expan> <num value="100">ρ</num> θελόμενον τὸ <choice><corr>μαζίον</corr><sic>μαζιν</sic></choice>,

    <lb n="7"/>τὸ δ<unclear reason="undefined" cert="high">ὲ</unclear> <choice><corr>κόγχισμα</corr><sic><unclear reason="undefined" cert="high">κ</unclear>ονχισμ<unclear reason="undefined" cert="high">α</unclear></sic></choice> <gap reason="illegible" extent="3" unit="character"/><unclear reason="undefined" cert="high">ακ</unclear>ωγον <expan><ex cert="high">ταλάντων</ex></expan> <num value="19">ιθ</num> εἰς <choice><corr>ἔκθεσιν</corr><sic>εχθεσιν</sic></choice>.

    <lb n="8"/>ἰ̈δοὺ δὲ πάντα σοι ἐδήλωσα, ὅπως <expan>γνό<ex cert="high">ν</ex></expan>-

    <lb n="9"/>τος <supplied reason="omitted" cert="high">σ</supplied>ου ὃ <choice><corr>θέλεις</corr><sic>θελις</sic></choice> πράξῃς. <choice><corr>ἀπέστειλά</corr><sic>απεστιλα</sic></choice> σοι δὲ

    <lb n="10"/>διὰ μὲν σκάφης Κορνηλίου διὰ Διονυσίου

    <lb n="11"/>ναύτου Σαΐτιον <choice><corr>ἔχον</corr><sic>εχων</sic></choice> <expan><ex cert="high">τάλαντα</ex></expan> <num value="2200">Βσ</num> καὶ διὰ τῆ<unclear reason="undefined" cert="high">ς</unclear>

    <lb n="12"/>ἄλλης σκάφης διὰ Βάνου ναύτου <choice><corr>ἀγγεῖον ἐλαίου</corr><sic>αγ᾽ γιον <unclear reason="undefined" cert="high">αι</unclear>λε<unclear reason="undefined" cert="high">ο</unclear>υ</sic></choice>

    <lb n="13"/>ξεστῶν <num value="8">η</num> <num value="1/2">𐅵</num>. καὶ νῦν κόμισον διὰ Τιμοθέου

    <lb n="14"/>καὶ Θέωνος υἱοῦ Ἱ¨έρακος ναυτῶν ἐκκλησίας

    <lb n="15"/>ἀργυρίου κνίδιον ἓν <num value="1"/> <choice><corr>ἔχον</corr><sic>εχων</sic></choice> τάλαντα ἑπτα-

    <lb n="16"/>κόσια, <expan>γί<ex cert="high">νεται</ex></expan> <expan><ex cert="high">τάλαντα</ex></expan> <num value="700">ψ</num>, κ<unclear reason="undefined" cert="high">α</unclear>ὶ ἐπιστολὴν διὰ <subst><add place="inline">τῶν</add><del rend="corrected" status="unremarkable">τ<del rend="erasure" status="unremarkable">ου</del>ων</del></subst> αὐτῶν

    <lb n="17"/>ναυτῶν. ταῦ<unclear reason="undefined" cert="high">τα</unclear> οὖν ἐὰν κομίσῃ, δήλωσόν

    <lb n="18"/>μοι π<unclear reason="undefined" cert="high">ε</unclear>ρὶ αὐτῶν, <choice><corr>ἐπεὶ</corr><sic>επι</sic></choice> πάνυ περικακῶ διὰ

    <lb n="19"/>τὸ <unclear reason="undefined" cert="high">μὴ</unclear> <unclear reason="undefined" cert="high">γ</unclear>ράμματά <unclear reason="undefined" cert="high">σ</unclear>ου δέξασθαι περὶ <expan>τούτω<ex cert="high">ν</ex></expan>.

