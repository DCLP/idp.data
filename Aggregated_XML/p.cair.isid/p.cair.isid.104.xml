    <lb n="1"/>Αὐρ<supplied reason="lost" cert="high">ή</supplied>λιος Χαιρήμων Πτολε<supplied reason="lost" cert="high">μα</supplied><unclear reason="undefined" cert="high">ί</unclear>ου <expan>μη<ex cert="high">τρὸς</ex></expan> Τα-

    <lb n="2"/>ήσεως ἀπὸ κώμης Καρ<supplied reason="lost" cert="high">α</supplied>νίδ<supplied reason="lost" cert="high">ος</supplied> ταῖς θυγα-

    <lb n="3"/>τράσιν τοῦ ἀδελφοῦ μου <choice><corr>Κοπρῆ</corr><sic>κοπρης</sic></choice> Αὐρηλίαις 

    <lb n="4"/>Ταησίῳ καὶ Κυριλλοῦτι <supplied reason="lost" cert="high">ἐ</supplied>κ μητρ<supplied reason="lost" cert="high">ὸ</supplied>ς <expan>Σαραπιάδο<ex cert="high">ς</ex></expan> 

    <lb n="5"/>ἀφήλιξιν διὰ φροντιστοῦ τοῦ πρὸς μητρὸς 

    <lb n="6"/>θείου Αὐρηλίου Ἀμμωνίου Ἰσιδώρ<supplied reason="lost" cert="high">ο</supplied>υ ἀπὸ τῆς 

    <lb n="7"/>αὐτῆς κώμης                    <choice><corr>χαίρειν</corr><sic>χαιριν</sic></choice>. 

    <lb n="8"/>ἐπειδὴ κατέσχον καὶ ἐγεώργησα <supplied reason="lost" cert="high">τρεισ</supplied>καιδεκαέτει 

    <lb n="9"/>χρόν<unclear reason="undefined" cert="high">ῳ</unclear> μετὰ τὴν τοῦ πατρ<supplied reason="lost" cert="high">ὸ</supplied>ς ὑμῶν Κο<supplied reason="lost" cert="high">π</supplied>ρῆ τελευτὴν 

    <lb n="10"/>τὸ ὑπάρχον ὑμῖν ἥμισυ μέρος σιτικ<supplied reason="lost" cert="high">ῶ</supplied>ν ἀρουρῶν 

    <lb n="11"/>δύο <num value="2"/> περὶ κώμην Καρανίδα κοινὸν <supplied reason="lost" cert="high">π</supplied>ρὸς ἐμὲ κατὰ 

    <lb n="12"/>τὸ λοιπὸν ἥμισυ μέρος οὐσῶν ἀπὸ λι<supplied reason="lost" cert="high">βὸς</supplied> <choice><corr>ἐλαιῶνος</corr><sic>ελεωνος</sic></choice> 

    <lb n="13"/>Ἁρπάλου ἄνευ ἐκφορίου ἀντὶ τῶν <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ελο</supplied>υμένων 

    <lb n="14"/>ὑπὲρ αὐτῶν δημοσίων καὶ ἀννωνῶν, <choice><corr>ταύτας</corr><sic>ταυτα</sic></choice> καὶ 

    <lb n="15"/><choice><corr>ὑμεῖς</corr><sic>υμις</sic></choice> <choice><corr>βουληθεῖσαι</corr><sic>βουληθεντες</sic></choice> κατέχειν καὶ γε<supplied reason="lost" cert="high">ωρ</supplied>γεῖν ἐπὶ τὸν 

    <lb n="16"/>αὐτὸν ἴ<supplied reason="lost" cert="high">σ</supplied>ον χρόνον, ἐντεῦθεν τ<supplied reason="lost" cert="high">ὸ</supplied> ἀπ<supplied reason="lost" cert="high">οσ</supplied>τάσιον 

    <lb n="17"/>ὑμῖν π<supplied reason="lost" cert="high">οιο</supplied>ῦμαι τῆς <choice><corr>γεωργίας</corr><sic>γεωργειας</sic></choice> ἀπὸ <supplied reason="lost" cert="high">τοῦ ἐ</supplied>νεστῶτος 

    <lb n="18"/><num value="2">β</num> <expan><ex cert="high">ἔτους</ex></expan> ἐπὶ <supplied reason="lost" cert="high">τ</supplied>ὰ ἴσα ἔτη δεκατρία <num value="13"/> το<unclear reason="undefined" cert="high">ῦ</unclear> <unclear reason="undefined" cert="high">ἐμοὶ</unclear> <choice><corr><unclear reason="undefined" cert="high">ἐ</unclear><supplied reason="omitted" cert="high">πι</supplied>βάλλοντος</corr><sic><unclear reason="undefined" cert="high">ε</unclear>βαλλοντος</sic></choice> 

