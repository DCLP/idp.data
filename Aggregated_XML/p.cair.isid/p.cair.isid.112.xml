    <lb n="11"/>[<num value="15">ιε</num> <expan><ex cert="high">ἔτους</ex></expan>] καὶ <num value="14">ιδ</num> <expan><ex cert="high">ἔτους</ex></expan> κα[ὶ <num value="7">ζ</num>] <expan><ex cert="high">ἔτους</ex></expan> καὶ <num value="1">α</num>[<num value="6">ϛ</num> <expan><ex cert="high">ἔτους</ex></expan> καὶ <num value="15">ιε</num> <expan><ex cert="high">ἔτους</ex></expan>] καὶ <num value="8">η</num> <expan><ex cert="high">ἔτους</ex></expan> 

