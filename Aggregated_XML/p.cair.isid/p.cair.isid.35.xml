    <lb n="12"/>καὶ Μεχεὶρ <num value="3">γ</num> <expan>ὁμοί<ex cert="high">ως</ex></expan> <expan>διέγρ<ex cert="high">αψεν</ex></expan> ὁ <expan>α<ex cert="high">ὐτὸς</ex></expan> 

