    <lb n="1"/>ἐ<supplied cert="high" reason="lost">πὶ χρόνο</supplied>ν ἔτη <gap extent="3" extentmax="5" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀπὸ τοῦ ἑπτακαιδεκάτου καὶ ἑκκαιδε</supplied>-

    <lb n="2"/><unclear cert="high" reason="undefined">κά</unclear><supplied cert="high" reason="lost">του κ</supplied>αὶ ἐνάτου ἔ<supplied cert="high" reason="lost">τους, ἐκ</supplied>φορίου <supplied cert="high" reason="lost">κατ᾽ ἔτος</supplied> ἕκα<supplied cert="high" reason="lost">στον τῆς</supplied> 

    <lb n="3"/>ἀρ<supplied cert="high" reason="lost">ούρης</supplied> πυροῦ ἀρταβ<supplied cert="high" reason="lost">ῶν </supplied><gap extent="4" reason="lost" unit="character"/> ἡμίσου<supplied cert="high" reason="lost">ς <num value=" 1/2 "/> ἀν</supplied>υπολόγ<supplied cert="high" reason="lost">ων καὶ ἀκιν</supplied>-

    <lb n="4"/>δ<supplied cert="high" reason="lost">ύνων, κα</supplied>ὶ ἐπάναγ<supplied cert="high" reason="lost">κον ἐπ</supplied>ιτελέσω τὰ <supplied cert="high" reason="lost">κατ᾽</supplied> ἔτος ἔρ<supplied cert="high" reason="lost">γα πάντα</supplied> 

    <lb n="5"/><supplied cert="high" reason="lost">περιχω</supplied>ματισμοὺς <supplied cert="high" reason="lost">ποτ</supplied>ισμοὺς ὑπο<supplied cert="high" reason="lost">σχισ</supplied>μοὺς διβ<supplied cert="high" reason="lost">ολητ</supplied>ὸν 

    <lb n="6"/><supplied cert="high" reason="lost">καὶ χωμ</supplied>άτων ἀν<supplied cert="high" reason="lost">αβ</supplied>ολὰς καὶ τ<supplied cert="high" reason="lost">ὰ ἄλλα π</supplied>άντα ὅσα <choice><corr>καθήκει</corr><sic>καθηκι</sic></choice> 

    <lb n="7"/>ἐ<supplied cert="high" reason="lost">κ τοῦ ἰδ</supplied>ίου τοῖς δ<supplied cert="high" reason="lost">έου</supplied>σι καιροῖς <supplied cert="high" reason="lost">βλάβος μ</supplied>ηδὲ ἓ<supplied cert="high" reason="lost">ν ποι</supplied>ῶν, 

    <lb n="8"/>τ<supplied cert="high" reason="lost">ὸ δ᾽ ἐκφό</supplied>ριον ἀπ<supplied cert="high" reason="lost">οδώσω</supplied> <choice><corr>κατ᾽</corr><sic>καθ</sic></choice> ἔ<supplied cert="high" reason="lost">τος ἕκασ</supplied>τον μη<supplied cert="high" reason="lost">νὶ Πα</supplied>ῦνι 

    <lb n="9"/>ἐν τ<supplied cert="high" reason="lost">ῇ αὐ</supplied>τῇ κώμῃ μ<supplied cert="high" reason="lost">έτρῳ τε</supplied>τραχοινίκῳ τῶν δη<supplied cert="high" reason="lost">μοσίω</supplied>ν 

    <lb n="10"/>πάντ<supplied cert="high" reason="lost">ων ὄ</supplied>ν<supplied cert="high" reason="lost">των</supplied> πρὸς <supplied cert="high" reason="lost">ὑμᾶς</supplied> τοὺς κτήτ<supplied cert="high" reason="lost">ορ</supplied>ας, καὶ μετὰ <supplied cert="high" reason="lost">τὸν</supplied> 

    <lb n="11"/>χρόν<supplied cert="high" reason="lost">ον</supplied> παραδώσω <supplied cert="high" reason="lost">τὰς ἀρο</supplied>ύρας ἀπὸ καλάμης κα<supplied cert="high" reason="lost">θὼς</supplied> 

    <lb n="12"/>καὶ <choice><corr>ἐγὼ</corr><sic>καγω</sic></choice> <supplied cert="high" reason="lost">πα</supplied><unclear cert="high" reason="undefined">ρ</unclear>έλαβα, ἐὰ<supplied cert="high" reason="lost">ν φαίν</supplied>εται μισθῶσαι. καὶ ἐπ<supplied cert="high" reason="lost">ερωτ</supplied>η-

    <lb n="13"/>θ<supplied cert="high" reason="lost">εῖσα ὡ</supplied>μολόγησα. <supplied cert="high" reason="lost">Αὐρ</supplied>ηλία Σερηνίλλα δι᾽ ἐμο<supplied cert="high" reason="lost">ῦ</supplied> 

    <lb n="14"/>Ζω<unclear cert="high" reason="undefined">ί</unclear><supplied cert="high" reason="lost">λου</supplied> <choice><corr>υἱοῦ</corr><sic>υιω</sic></choice> μεμί<supplied cert="high" reason="lost">σ</supplied>θωκα ὡς πρόκιται. <handShift new="m2"/> Αὐρή<supplied cert="high" reason="lost">λιο</supplied>ι 

    <lb n="15"/>Γέμελ<supplied cert="high" reason="lost">λος</supplied> καὶ Πασιγένης δι᾽ ἐμοῦ Ἰσιδώρου καὶ Π<supplied cert="high" reason="lost">τ</supplied>ολλᾶς καὶ Οὐαλᾶς μ<supplied cert="high" reason="lost">εμισ</supplied>-

    <lb n="16"/>θώ<supplied cert="high" reason="lost">καμεν</supplied> ὡς <expan>π<ex cert="high">ρόκειται</ex></expan>. Ἰσίδωρος ἔγραψα καὶ ὑπὲρ Πτολλᾶ καὶ <unclear cert="high" reason="undefined">Ο</unclear><supplied cert="high" reason="lost">ὐ</supplied><unclear cert="high" reason="undefined">αλᾶ</unclear> ἀγραμμάτων. 

