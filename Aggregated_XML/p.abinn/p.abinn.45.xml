    <lb n="1"/><supplied reason="lost" cert="high">Φ</supplied><unclear reason="undefined" cert="high">λ</unclear><supplied reason="lost" cert="high">αο</supplied><unclear reason="undefined" cert="high">υ</unclear>ίῳ Ἀβινναίῳ ἐπάρχῳ εἴλης 

    <lb n="2"/>σ<supplied reason="lost" cert="high">τρατι</supplied>ωτῶν κάστρων Διονυσιάδος παρὰ 

    <lb n="3"/>Φλα<unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">υ</supplied><unclear reason="undefined" cert="high">ί</unclear><supplied reason="lost" cert="high">ου</supplied> Πρίσκου οὐετρανοῦ τῶν <choice><corr>ἐντίμως</corr><sic>εντιμος</sic></choice> 

    <lb n="4"/><unclear reason="undefined" cert="high">ἀ</unclear>πολελυ<unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">έν</supplied>ων καὶ Ἀλίας <del rend="erasure" status="unremarkable">θυ</del> γυναικὸς Ἥρωνος 

