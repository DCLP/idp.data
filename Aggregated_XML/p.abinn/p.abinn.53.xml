    <lb n="10"/>ἀξιῶ κα<supplied reason="lost" cert="high">ὶ</supplied> δέομαί σου τῆς φι<supplied reason="lost" cert="high">λανθρωπίας</supplied> 

    <lb n="11"/>ὅπως τοῦτον <choice><corr>συλλαβόμε<supplied reason="lost" cert="high">νος</supplied></corr><sic>συνλαβ<unclear reason="undefined" cert="high">ω</unclear>μ<unclear reason="undefined" cert="high">ε</unclear><supplied reason="lost" cert="high">νος</supplied></sic></choice> <supplied reason="lost" cert="high"><sic>καὶ</sic> κατανα</supplied>-

