    <lb n="1"/>Φλαουίῳ Ἀβιν<supplied cert="high" reason="lost">ν</supplied>έῳ ἐ<unclear cert="high" reason="undefined">ξ</unclear> <choice><corr>ἀποπροτηκτόρων</corr><sic>αποπρο<unclear cert="high" reason="undefined">τ</unclear>ηκτωρων</sic></choice> ἐπάρ<unclear cert="high" reason="undefined">χ</unclear>ῳ 

    <lb n="2"/>εἴλης κάσ<unclear cert="high" reason="undefined">τρω</unclear>ν Διονυσιάδος.            παρὰ Αὐρηλίου 

    <lb n="3"/>Ἥρωνο<supplied cert="high" reason="lost">ς</supplied> διάκω<supplied cert="high" reason="lost">ν</supplied>ος ἀπὸ κώμης <choice><corr>Βερενικίδος</corr><sic>βερνικει<unclear cert="high" reason="undefined">δ</unclear>ος</sic></choice> τοῦ αὐτοῦ 

    <lb n="4"/>νομοῦ χαίρειν. εἰ μὴ ὑπῆρχεν <choice><corr>ἡμῖν</corr><sic>ημειν</sic></choice> ἡ τῶν 

    <lb n="5"/>νόμων <unclear cert="high" reason="undefined">ἀ</unclear>λήθει<unclear cert="high" reason="undefined">α</unclear> πάλαι δ᾽ ἂν ὑπὸ τῶν κακού<unclear cert="high" reason="undefined">ργω</unclear>ν 

    <lb n="6"/><choice><corr>ἀνῃρούμεθα</corr><sic>αναιλουμεθα</sic></choice>. Εὔπορος τοίνυν <hi rend="diaeresis">υ</hi>ἱὸς Ἑρμεία 

    <lb n="7"/>ἀπὸ κώμης <choice><corr>Φιλαγρείδος</corr><sic>φιλαγρειδος</sic></choice> τοῦ αὐτοῦ νομοῦ 

    <lb n="8"/>ἐσύλησέν με <choice><corr>ἔνδον</corr><sic>ενδων</sic></choice> τῆς <choice><corr>οἰκίας</corr><sic>οικειας</sic></choice>, ἐπιβὰς 

    <lb n="9"/><choice><corr>λῃστρικῷ</corr><sic>ληστρεικω</sic></choice> τρόπῳ, καὶ πᾶσαν τὴν <choice><corr>ἐσθῆτα</corr><sic>εσθηταν</sic></choice> 

    <lb n="10"/>συνελάβετο καὶ <unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">ἰς</supplied> τὸ <hi rend="diaeresis">ἴ</hi>διον <choice><corr>ἀνεστείλατο</corr><sic>ανεστιλατω</sic></choice> <choice><corr>μέχρις</corr><sic>μεχρεις</sic></choice> 

    <lb n="11"/><choice><corr>δ<supplied cert="high" reason="lost">εῦ</supplied>ρο</corr><sic>δ<supplied cert="high" reason="lost">ευ</supplied><unclear cert="high" reason="undefined">ρ</unclear>ω</sic></choice>, δυναμ<supplied cert="high" reason="lost">έν</supplied><unclear cert="high" reason="undefined">ου</unclear> μου καὶ <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ὰ</supplied><unclear cert="high" reason="undefined">ς</unclear> <choice><corr>ἀποδείξει<supplied cert="high" reason="lost">ς</supplied></corr><sic><unclear cert="high" reason="undefined">απο</unclear>διξει<supplied cert="high" reason="lost">ς</supplied></sic></choice> <supplied cert="high" reason="lost">ποι</supplied>εῖν 

    <lb n="12"/>ὡς τούτου τήνδε τὴν <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">α</supplied>κουργίαν π<supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">π</unclear>οιημένου. 

    <lb n="13"/><choice><corr>διὸ</corr><sic>διω</sic></choice> ἀξ<supplied cert="high" reason="lost">ι</supplied>ῶ, <choice><corr>πραιπόσιτε</corr><sic>πραιποσιται</sic></choice> κ<supplied cert="high" reason="lost">ύ</supplied><unclear cert="high" reason="undefined">ρι</unclear>ε, <choice><corr>ἀπραγμόνως</corr><sic>απρα<unclear cert="high" reason="undefined">γμω</unclear>νος</sic></choice> 

    <lb n="14"/>ἃ ἀφήρπασέν μου <supplied cert="high" reason="lost">π</supplied>α<unclear cert="high" reason="undefined">ρασ</unclear>χεθῆναί μ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ι</supplied>· διάκων γὰρ 

    <lb n="15"/><unclear cert="high" reason="undefined">τ</unclear>ετ<supplied cert="high" reason="lost">ύ</supplied>χηκα τῆ<unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">αθ</supplied>ολικῆς <choice><corr>ἐκ<supplied cert="high" reason="omitted">κ</supplied><supplied cert="high" reason="lost">λ</supplied>ησίας</corr><sic><unclear cert="high" reason="undefined">εκ</unclear><supplied cert="high" reason="lost">λ</supplied><unclear cert="high" reason="undefined">η</unclear>σιας</sic></choice>. καὶ τ<unclear cert="high" reason="undefined">ούτ</unclear>ο<unclear cert="high" reason="undefined">υ</unclear> 

    <lb n="16"/>τυχὼν <choice><corr>εἰσαεί</corr><sic>εισαειν</sic></choice> σοι <choice><corr>εὐχαριστήσω</corr><sic>ευχαρειστησω</sic></choice>.  διευτύχει. 

    <lb n="17"/><supplied cert="high" reason="lost">μετὰ τὴ</supplied><unclear cert="high" reason="undefined">ν</unclear> <unclear cert="high" reason="undefined">ὑπατ</unclear>είαν <expan>Φλ<ex cert="high">αουίων</ex></expan> <unclear cert="high" reason="undefined">Σ</unclear>εργίου καὶ Νειγρεινι<unclear cert="high" reason="undefined">αν</unclear><add place="supralinear"><unclear cert="high" reason="undefined">οῦ</unclear></add> 

    <lb n="18"/><unclear cert="high" reason="undefined">τῶ</unclear><supplied cert="high" reason="lost">ν λαμπροτά</supplied>των, Μεχεὶρ <num value="17">ιζ</num>. </ab></div><div n="meta.10046" org="uniform" part="N" sample="complete" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.10046" org="uniform" part="N" sample="complete" subtype="textDate" type="commentary"><p><date notAfter="351" notBefore="351" type="textDate" value="0351-02-11">11. Febr. 351</date></p></div><div n="meta.10046" org="uniform" part="N" sample="complete" subtype="general" type="commentary"><p>Zum Ort vgl. Z. 3 und Calderini-Daris, Dizionario II, S. 42.</p></div><div n="meta.10046" org="uniform" part="N" sample="complete" subtype="principalEdition" type="bibliography"><listBibl default="NO"><bibl default="NO" subtype="principal" type="publication"><title level="s" type="abbreviated">P.Abinn.</title><biblScope type="numbers">55</biblScope></bibl><bibl default="NO" type="Trismegistos"><biblScope type="numbers">10046</biblScope></bibl><bibl default="NO" type="DDbDP"><series>0052</series>; ; <biblScope type="numbers">55</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0052;;55" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.10046" org="uniform" part="N" sample="complete" subtype="illustrations" type="bibliography"><p>keine</p></div><div n="meta.10046" org="uniform" part="N" sample="complete" subtype="otherPublications" type="bibliography"><head>Andere Publikation</head><bibl default="NO" subtype="other" type="publication">P.Lond. II 412, S. 279</bibl></div><div n="meta.10046" org="uniform" part="N" sample="complete" subtype="locations" type="history"><p><placeName cert="high" full="yes" type="ancientFindspot">Berenikis Aigialu</placeName> (<geogName cert="high" full="yes" type="nome">Arsinoites</geogName>), <geogName cert="high" full="yes" key="Aegyptus" type="ancientRegion">Egypt</g