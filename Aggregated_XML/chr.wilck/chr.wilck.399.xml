    <lb n="8"/><choice><corr>ἔγγιστα</corr><sic>ενγισταν</sic></choice> πληρούντων εἰς Παῦν<supplied cert="high" reason="lost">ι </supplied><gap extent="2" reason="lost" unit="character"/> 

    <lb n="9"/>καὶ Ἰσχυρίωνος Πετεσ<supplied cert="high" reason="lost">ούχου καὶ </supplied><gap extent="4" reason="lost" unit="character"/> 

    <lb n="10"/>κουρ<supplied cert="high" reason="lost">έ</supplied>ως <expan>τετελ<ex cert="high">ευτηκότων</ex></expan> τῶν <app type="BL"><lem resp="cf. 6.11">[<num value="5">ε</num> <expan>πρ]α<unclear cert="high" reason="undefined">κ</unclear><ex cert="high">τόρων</ex></expan> <expan>ἀργ[υρικ<ex cert="high">ῶν</ex>]</expan></lem><rdg>[<num value="2">β</num> <gap extent="2" reason="illegible" unit="character"/>]α<gap extent="1" reason="illegible" unit="character"/> ἀπ<supplied cert="high" reason="lost">ὸ</supplied></rdg></app> 

