    <lb n="3"/>κους ἀπ<supplied reason="lost" cert="high">ὸ κ</supplied>ώμης Θερενο<supplied reason="lost" cert="high">ύθεως τοῦ Πρ</supplied>ο-

    <lb n="4"/>σωπείτου νομοῦ. τ<supplied reason="lost" cert="high">οῦ</supplied> πα<supplied reason="lost" cert="high">τρ</supplied>ό<supplied reason="lost" cert="high">ς μου</supplied>, κ<supplied reason="lost" cert="high">ύρ</supplied>ιε, 

