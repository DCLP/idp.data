    <lb n="2"/>παρὰ Μηνοδώρου τοῦ <expan>κ<ex cert="high">αὶ</ex></expan> <expan>Ἀμμω<ex cert="high">νίου</ex></expan> Διοσκόρου καὶ Εὐ<supplied cert="high" reason="lost">δα</supplied><unclear cert="high" reason="undefined">ί</unclear>μονος Κ<unclear cert="high" reason="undefined">ύ</unclear>νος καὶ <choice><corr>Ἑρμαίου</corr><sic>ερμαιωι</sic></choice> 

    <lb n="3"/>καὶ Μ<unclear cert="high" reason="undefined">ητ</unclear>όκου <expan><unclear cert="high" reason="undefined">ἀ</unclear>μφο<unclear cert="high" reason="undefined">τ</unclear><ex cert="high">έρων</ex></expan> Εὐδήμου καὶ Δί<supplied cert="high" reason="lost">ο</supplied>υ Ἥρων<supplied cert="high" reason="lost">ο</supplied>ς <supplied cert="high" reason="lost">καὶ Δ</supplied><unclear cert="high" reason="undefined">ι</unclear>δύμου Ἀμφίονος. οἱ ἓξ <num value="6"/> 

    <lb n="4"/><expan>Ἑρμ<supplied cert="high" reason="lost">οπο</supplied>λ<ex cert="high">ῖται</ex></expan>. βουλόμεθα ἑκουσ<supplied cert="high" reason="lost">ί</supplied><unclear cert="high" reason="undefined">ως</unclear> μισθώσασθ<supplied cert="high" reason="lost">αι</supplied> <unclear cert="high" reason="undefined">ε</unclear>ἰς <unclear cert="high" reason="undefined">ἔτ</unclear>η δύο <num value="2"/> ἀπὸ τοῦ ἐνεστῶτος 

    <lb n="5"/><num value="6">ϛ</num> <expan><ex cert="high">ἔτους</ex></expan> Ἁδρι<supplied cert="high" reason="lost">ανοῦ Καίσαρο</supplied>ς τοῦ κυρίου <unclear cert="high" reason="undefined">ἀπὸ</unclear> <supplied cert="high" reason="lost">τῶ</supplied>ν <choice><corr>ὑπαρχόντων</corr><sic><unclear cert="high" reason="undefined">υπα</unclear>ρχουσων</sic></choice> σοι ἐδαφῶν ἐν τῷ Λ<unclear cert="high" reason="undefined">ε</unclear>υ-

    <lb n="6"/><unclear cert="high" reason="undefined">κ</unclear>ο<unclear cert="high" reason="undefined">πυργ</unclear>ίτῃ κάτω <unclear cert="high" reason="undefined">περ</unclear><supplied cert="high" reason="lost">ὶ</supplied> <unclear cert="high" reason="undefined">Π</unclear>ανν<unclear cert="high" reason="undefined">α</unclear><gap extent="3" reason="illegible" unit="character"/> ἐκ τοῦ Πον<unclear cert="high" reason="undefined">α</unclear><gap extent="2" reason="lost" unit="character"/><unclear cert="high" reason="undefined">υ</unclear> <unclear cert="high" reason="undefined">τ</unclear>οῦ <unclear cert="high" reason="undefined">κ</unclear>αὶ Γλ<unclear cert="high" reason="undefined">α</unclear>ύκου <expan><ex cert="high">ἀρούρας</ex></expan> <num value="111">ρια</num> <num value="3/4">𐅸</num> <num value="1/16">ιϛ</num> προγεωργ<unclear cert="high" reason="undefined">ου</unclear>-


    <lb n="7"/>μένα<unclear cert="high" reason="undefined">ς</unclear> ὑπὸ Ἑρμησίωνος Ἑρμαίου, ὧν <abbr>χό</abbr> <abbr>εν<gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">υ</unclear></abbr> <expan><ex cert="high">ἀρουρ  </ex></expan> <gap extent="3" reason="illegible" unit="character"/> <expan>ἐκφο<ex cert="high">ρίου</ex></expan> κριθῆς <expan>ἀποτάκτο<ex cert="high">υ</ex></expan> <expan>ἀρταβ<ex cert="high">ῶν</ex></expan> ἓξ τε-

    <lb n="8"/>τάρτου <num value="6  1/4 "/> <expan>κ<ex cert="high">αὶ</ex></expan> τῶν <expan>ἀλλῶ<ex cert="high">ν</ex></expan> <expan><ex cert="high">ἀρουρῶν</ex></expan> τὰς ἐν ἀνα<supplied cert="high" reason="lost">π</supplied>αύματ<unclear cert="high" reason="undefined">ι</unclear> εἰς <supplied cert="high" reason="lost">σπ</supplied><unclear cert="high" reason="undefined">ο</unclear>ρὰν πυροῦ <expan>ὑ<ex cert="high">πὲρ</ex></expan> <expan><ex cert="high">πυροῦ</ex></expan> <expan>μέτ<ex cert="high">ρῳ</ex></expan> <expan>δη<ex cert="high">μοσίῳ</ex></expan> <expan>δοχ<ex cert="high">ικῷ</ex></expan> <expan><ex cert="high">ἀρτάβας</ex></expan> πέντε <num value="5"/> 

    <lb n="9"/>καὶ τὰς λοιπὰς εἰς <expan>ξυλαμὴ<ex cert="high">ν</ex></expan> <expan>ἄρακο<ex cert="high">ς</ex></expan> καὶ <expan>χόρ<unclear cert="high" reason="undefined">τ</unclear>ο<ex cert="high">υ</ex></expan> εἰς μὲ<unclear cert="high" reason="undefined">ν</unclear> <unclear cert="high" reason="undefined">βρ</unclear>ῶσ<supplied cert="high" reason="lost">ιν</supplied> <expan>προβάτ<ex cert="high">ων</ex></expan> <expan>κ<ex cert="high">αὶ</ex></expan> <expan><unclear cert="high" reason="undefined">κ</unclear>οίτη<ex cert="high">ς</ex></expan> <expan><ex cert="high">ἀρούρας</ex></expan> <num value="12">ιβ</num> καὶ εἰς 

    <lb n="10"/>βρῶσιν βο<hi rend="diaeresis">ϊ</hi>κῶν κτήνων <expan><ex cert="high">ἀρούρας</ex></expan> <num value="13">ιγ</num> καὶ εἰς κοπὴν τὰ<unclear cert="high" reason="undefined">ς</unclear> <expan>λ<supplied cert="high" reason="lost">ο</supplied>ι<ex cert="high">πὰς</ex></expan> φ<unclear cert="high" reason="undefined">όρ</unclear>ου <expan>ἑκάστη<ex cert="high">ς</ex></expan> <expan><ex cert="high">ἀρούρης</ex></expan> ἀνὰ <expan>ἀργ<ex cert="high">υρίου</ex></expan> <expan><ex cert="high">δραχμὰς</ex></expan> <num value="16">ιϛ</num> 

    <lb n="11"/>φορέτρου <del rend="erasure" status="unremarkable"><expan>κ<ex cert="high">αὶ</ex></expan> <gap desc="vestiges" extent="unknown" reason="illegible" unit="character"/></del> <app type="BL"><lem resp="cf. 2.2.83"><choice><corr><expan>ὀνη<ex cert="high">λατικοῦ</ex></expan></corr><sic>ωνη</sic></choice></lem><rdg><del rend="erasure" status="unremarkable"><expan>κ<ex cert="high">αὶ</ex></expan> <gap desc="vestiges" extent="unknown" reason="illegible" unit="character"/></del> <abbr>νη</abbr></rdg></app> τῆς <expan>κριθ<ex cert="high">ῆς</ex></expan> ὡς τῶν <num value="100">ρ</num> <gap extent="1" reason="illegible" unit="character"/> <expan><ex cert="high">ἀρταβῆς</ex></expan> <num value="1">α</num> <num value="2/3">𐅷</num>, αἷς προσάγομεν ὑπὲρ ἐπιθέ-

    <lb n="12"/>ματος <expan><ex cert="high">πυροῦ</ex></expan> <expan>δη<ex cert="high">μοσίῳ</ex></expan> <expan><supplied cert="high" reason="lost">δοχ<ex cert="high">ικῷ</ex></supplied></expan> ἀρτάβας πέντε <num value="5"/> τὰ δ᾽ ἐκφόρια ἀποδώσομεν κατ᾽ ἔτος ἐν τῷ Παῦνι <expan>κ<ex cert="high">αὶ</ex></expan> 

    <lb n="13"/>Ἐπεὶφ μησὶ ἐξ ἀλληλεγγύης, τὸν μὲν πυρὸν εἰς τοὺς δημοσίους θησαυρούς, τὸ δὲ ἀργύ-

    <lb n="14"/>ριον καὶ τὴν κριθὴν τοῖς τῆς οὐσίας οἰκονόμοις <unclear cert="high" reason="undefined">κ</unclear>αὶ <supplied cert="high" reason="lost">δώσ</supplied>ομεν <expan><unclear cert="high" reason="undefined">σ</unclear>πο<unclear cert="high" reason="undefined">ν</unclear>δῆ<ex cert="high">ς</ex></expan> κα<supplied cert="high" reason="lost">τ᾽</supplied> ἔτος ἀργυρίου 

    <lb n="15"/><expan>δραχ<ex cert="high">μὰς</ex></expan> δώδεκα <num value="12"/>. <expan><ex cert="high">ἔτους</ex></expan> ἕκτου Αὐτοκρά<unclear cert="high" reason="undefined">το</unclear>ρος Καίσαρος <supplied cert="high" reason="lost">Τραια</supplied>νοῦ Ἁ<supplied cert="high" reason="lost">δ</supplied>ριανοῦ Σεβαστοῦ Φαῶφι <num value="9">θ</num>. </ab></div><div n="meta.22846" org="uniform" part="N" sample="complete" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.22846" org="uniform" part="N" sample="complete" subtype="textDate" type="commentary"><p><date notAfter="121" notBefore="121" type="textDate" value="0121-10-06">6. Okt. 121</date></p></div><div n="meta.22846" org="uniform" part="N" sample="complete" subtype="principalEdition" type="bibliography"><listBibl default="NO"><bibl default="NO" subtype="principal" type="publication"><title level="s" type="abbreviated">P.Lond.</title><biblScope type="volume">3</biblScope><biblScope type="numbers">1223</biblScope><biblScope type="pages">S. 139</biblScope></bibl><bibl default="NO" type="Trismegistos"><biblScope type="numbers">22846</biblScope></bibl><bibl default="NO" type="DDbDP"><series>0008</series>; ; <biblScope type="numbers">370</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0008;;370" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.22846" org="uniform" part="N" sample="complete" subtype="illustrations" type="bibliography"><p><bibl default="NO" type="illustration">P.Lond. III, Plate 36</bibl></p></div><div n="meta.22846" org="uniform" part="N" sample="complete" subtype="otherPublications" type="bibliography"><head>Andere Publikation</head><bibl default="NO" subtype="other" type="publication">W.Chr. 370</bibl></div><div n="meta.22846" org="uniform" part="N" sample="complete" subtype="translations" type="bibliography"><head>Übersetzungen</head><listBibl default="NO" lang="en"><head lang="de"