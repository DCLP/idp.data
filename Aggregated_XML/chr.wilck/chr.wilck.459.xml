    <lb n="1"/><gap extent="4" reason="lost" unit="character"/> Ἀντινοέων. 

    <lb n="2"/><expan><supplied cert="high" reason="lost">ἀντ</supplied>ίγρα<ex cert="high">φον</ex></expan>. ἐκ τόμου ἐπικρίσεω<supplied cert="high" reason="lost">ν Μάρκου Πετρωνίου</supplied> 

    <lb n="3"/><supplied cert="high" reason="lost">Ὁνω</supplied>ράτου ἐπάρχου Αἰγύπτου, <supplied cert="high" reason="lost">οὗ προγραφή· οἱ ὑπο</supplied>-

    <lb n="4"/>γεγραμ<supplied cert="high" reason="lost">μένοι</supplied> οὐετρανοὶ στρατ<supplied cert="high" reason="lost">ευσάμενοι ἐν εἴλαις</supplied> 

    <lb n="5"/>καὶ σπείραις καὶ ἐν κλάσσῃ Συρι<supplied cert="high" reason="lost">α</supplied>κῇ ἔν<supplied cert="high" reason="lost">ε</supplied>ιοι μ<supplied cert="high" reason="lost">ὲν ἐπιτυχόντες</supplied> 

    <lb n="6"/>σὺν τέκνοις καὶ ἐγγόνοις, <choice><corr>ἕτεροι</corr><sic>ετερ<supplied cert="high" reason="lost">ο</supplied>ις</sic></choice> μ<unclear cert="high" reason="undefined">ό</unclear><supplied cert="high" reason="lost">νο</supplied>ι τῆ<supplied cert="high" reason="lost">ς Ῥωμαίων</supplied> 

    <lb n="7"/><choice><corr>πολιτείας</corr><sic>ποτειτειας</sic></choice> καὶ ἐπιγαμίας πρὸς γυν<supplied cert="high" reason="lost">αῖκ</supplied>ας, <supplied cert="high" reason="lost">ἃς τότε εἶχον,</supplied> 

    <lb n="8"/>ὅτε τούτοις ἡ <choice><corr>πολιτεία</corr><sic>πολειτεια</sic></choice> ἐδ<supplied cert="high" reason="lost">ό</supplied>θη <supplied cert="high" reason="omitted">ἢ</supplied> εἴ τ<supplied cert="high" reason="lost">ινες ἄγαμοι εἶεν,</supplied> 

    <lb n="9"/><supplied cert="high" reason="lost">π</supplied>ρὸς ἃς ἐὰν μεταξὺ <choice><corr>ἀγάγωσι</corr><sic>αγαγων</sic></choice> τοῦ μέχρ<supplied cert="high" reason="lost">ι μιᾶς <num value="1"/> ἕκαστος, ἔτι δὲ</supplied> 

    <lb n="10"/>καὶ ἕτεροι οὐετρανοὶ οἱ χωρὶς χαλκῶν οἱ ν<unclear cert="high" reason="undefined">ῦν</unclear> <supplied cert="high" reason="lost">καὶ αὐτοὶ ἐπι</supplied>-

    <lb n="11"/>τυχόντες μόνοι τῆς Ῥωμαίων πολ<supplied cert="high" reason="lost">ιτ</supplied>είας <supplied cert="high" reason="lost">ἐπεκρίθησαν</supplied> 

    <lb n="12"/>ἐξ ἐνκελεύσεως Μάρκου Πετρωνίου <supplied cert="high" reason="lost">Ὁ</supplied>ν<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">ράτου ἐπάρχου</supplied> 

    <lb n="13"/>Αἰγύπτου δ<supplied cert="high" reason="lost">ι</supplied>ὰ Μα<unclear cert="high" reason="undefined">γί</unclear>ου Σαβείνου <choice><corr>χιλιάρχου</corr><sic>χειλιαρ<supplied cert="high" reason="lost">χο</supplied>υ</sic></choice> λ<supplied cert="high" reason="lost">εγεῶνος <num value="2">β</num></supplied> 

    <lb n="14"/>Τραιανῆς <hi rend="diaeresis">Ἰ</hi>σχυρᾶς ἀπὸ Μεχεὶρ <num value="3">γ</num> ἕ<supplied cert="high" reason="lost">ως</supplied> <num value="2">β</num> τοῦ Παχ<supplied cert="high" reason="lost">ὼν μηνὸς</supplied> 

    <lb n="15"/><supplied cert="high" reason="lost">τοῦ ἐνε</supplied>στῶτος <num value="11">ια</num> <expan><ex cert="high">ἔτους</ex></expan> Αὐτοκράτορος <supplied cert="high" reason="lost">Καίσαρος Τίτου Αἰλίου</supplied> 

    <lb n="16"/><supplied cert="high" reason="lost">Ἁδρια</supplied>νοῦ Ἀντωνείνου Σεβ<supplied cert="high" reason="lost">αστοῦ Εὐσεβοῦς. ἃ δὲ παρέθεντο</supplied> 

    <lb n="17"/><supplied cert="high" reason="lost">δικαι</supplied>ώματα τῷ <choice><corr>προγεγραμμένῳ</corr><sic>προγεγρμ<supplied cert="high" reason="lost">μενω</supplied></sic></choice> <unclear cert="high" reason="undefined">Σ</unclear><supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">βε</unclear><supplied cert="high" reason="lost">ίνῳ, ἑκάσ</supplied>-

    <lb n="18"/><supplied cert="high" reason="lost">τῳ ὀνόμ</supplied>ατι παράκ<supplied cert="high" reason="lost">ει</supplied>ται. μεθ᾽ ἕτερα σελίδων <gap extent="8" reason="lost" unit="character"/> 

    <lb n="19"/><gap extent="6" reason="lost" unit="character"/> Σ<supplied cert="high" reason="lost">ε</supplied>μ<supplied cert="high" reason="lost">π</supplied>ρώνιος Μάξιμος βο<unclear cert="high" reason="undefined">υ</unclear><supplied cert="high" reason="lost">λ</supplied><unclear cert="high" reason="undefined">ό</unclear><supplied cert="high" reason="lost">με</supplied>νο<supplied cert="high" reason="lost">ς παρεπιδημεῖν</supplied> 

    <lb n="20"/><supplied cert="high" reason="lost">πρὸς καιρ</supplied>ὸν ἐ<supplied cert="high" reason="lost">ν</supplied> νομῷ Ἀρσινοεί<supplied cert="high" reason="lost">τ</supplied>ῃ <gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="9" reason="lost" unit="character"/> 

    <lb n="21"/><gap extent="9" reason="lost" unit="character"/> δέλτον χαλκῆν ἐκσφρα<supplied cert="high" reason="lost">γισθεῖσαν <app type="BL"><lem resp="cf. 5.10">ἐκ τάβλης</lem><rdg><gap extent="7" reason="illegible" unit="character"/></rdg></app></supplied> 

    <lb n="22"/><supplied cert="high" reason="lost"><app type="BL"><lem resp="cf. 5.10">χαλκῆς</lem><rdg><gap extent="5" reason="illegible" unit="character"/></rdg></app> παρα</supplied>κειμένης ἐν Ῥώμῃ, δι᾽ ἧς <app type="BL"><lem resp="cf. 1.34"><seg cert="low" part="N">ἐμα<supplied cert="low" reason="lost">ρτυρήθη</supplied></seg></lem><rdg>ἐ<gap extent="1" reason="illegible" unit="character"/>α<gap extent="9" reason="lost" unit="character"/></rdg></app> 

