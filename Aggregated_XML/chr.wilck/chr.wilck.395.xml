    <lb n="11"/>ὑπαρχόντ<supplied reason="lost" cert="high">ων πε</supplied>ρὶ κώμ<unclear reason="undefined" cert="high">α</unclear>[ς <expan>Βακχ<ex cert="high">ιάδα</ex></expan>] 

    <lb n="12"/>καὶ Ἡφαιστιάδα τῆς Ἡρακλ<supplied reason="lost" cert="high">είδου</supplied> 

    <lb n="13"/>μερίδος τοῦ Ἀρσινοίτου τ<supplied reason="lost" cert="high">ετραε</supplied>-

    <lb n="14"/>τεῖ ἤδη χρόνωι ἐν τῇ χρ<supplied reason="lost" cert="high">είαι</supplied> 

    <lb n="15"/>πονούμενος ἐξησθένησα [ὅλως <foreign lang="la">?,</foreign>] 

    <lb n="16"/>κύριε, ὅθεν ἀξιῶ <choice><corr>σε</corr><sic>σαι</sic></choice> τὸν σω<supplied reason="lost" cert="high">τῆρα</supplied> 

    <lb n="17"/>ἐλεῆσαί με καὶ κελεῦσαι ἤ<supplied reason="lost" cert="high">δη με</supplied> 

    <lb n="18"/>ἀπολυθῆναι τῆς χρείας ὅ<unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ως δυ</supplied>-

    <lb n="19"/>νηθῶ ἐμαυτὸν ἀνακτήσα<supplied reason="lost" cert="high">σθαι ἀ</supplied>-

    <lb n="20"/>πὸ τῶν καμάτων οὐδὲν δ[ὲ <unclear cert="high" reason="undefined">δεῖ</unclear> <gap reason="illegible" extent="2" unit="character"/>] 

