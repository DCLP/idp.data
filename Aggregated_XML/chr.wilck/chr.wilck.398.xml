    <lb n="16"/>με<supplied reason="lost" cert="high">μ</supplied>φθῆναι. <choice><corr>σεσημείωμαι</corr><sic>σεσημιωμαι</sic></choice>. <expan><ex cert="high">ἔτους</ex></expan> <num value="9">θ</num> Αὐρηλί<supplied reason="lost" cert="high">ο</supplied>υ 

    <lb n="17"/><choice><corr>Ἀντωνίνου</corr><sic>αντωνινινου</sic></choice> Καίσα<supplied reason="lost" cert="high">ρ</supplied>ος τοῦ κυρί<supplied reason="lost" cert="high">ο</supplied>υ Ἀρμ<supplied reason="lost" cert="high">εν</supplied>ιακοῦ 

    <lb n="18"/>Μηδικοῦ Πα<supplied reason="lost" cert="high">ρθ</supplied>ικοῦ Μεγίστου <supplied reason="lost" cert="high">Μ</supplied>ε<supplied reason="lost" cert="high">σορ</supplied>ὴ <num value="17">ιζ</num>. 

    <lb n="19"/>ἔστι δὲ· Γά<hi rend="diaeresis">ϊ</hi>ος <hi rend="diaeresis">Ἰ</hi>ούλιος Ἀπολινάριος γεουχῶν 

    <lb n="20"/>ἐν Καρανίδι ἔχων πόρον <expan><ex cert="high">δραχμῶν</ex></expan> <num value="4000">Δ</num> 

    <lb n="21"/>Μύσθης Κορνηλίου γεουχῶν ἐν Πτολεμα<hi rend="diaeresis">ΐ</hi>δι 

    <lb n="22"/>Νέᾳ ἔχων πόρον <expan><ex cert="high">ταλάντου</ex></expan> <num value="1">α</num>. 

    <lb n="23"/>Ἀντώ<supplied reason="lost" cert="high">ν</supplied>ιος Ἡρακλιανὸς γεουχῶν ἐν τῷ 

    <lb n="24"/>Νέστου ὁμοίως <expan><ex cert="high">δραχμῶν</ex></expan> <num value="4000">Δ</num> 

    <lb n="25"/>Γά<hi rend="diaeresis">ϊ</hi>ος <hi rend="diaeresis">Ἰ</hi>ούλιος Σατορνεῖλος γεουχ<supplied reason="lost" cert="high">ῶν</supplied> ἐν Τάνι 

