    <lb n="4"/><supplied reason="lost" cert="high">καὶ ἁπ</supplied><unclear reason="undefined" cert="high">οζ</unclear><supplied reason="lost" cert="high">υ</supplied><unclear reason="undefined" cert="high">γῆ</unclear>ς κ<unclear reason="undefined" cert="high">α</unclear>ὶ δ<unclear reason="undefined" cert="high">ι</unclear>[αλύσεως, <gap reason="illegible" extent="unknown" unit="character"/> ἐκ μὲν <expan>το<ex cert="high">ῦ</ex></expan> ἑνὸς] 

