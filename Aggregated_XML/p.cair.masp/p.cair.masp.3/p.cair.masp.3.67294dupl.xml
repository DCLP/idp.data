    <lb n="14"/><supplied reason="lost" cert="high">ἐπεὶ τοί</supplied>ν<supplied reason="lost" cert="high">υ</supplied>ν ἐκε<unclear reason="undefined" cert="high">ίνω</unclear><supplied reason="lost" cert="high">ν τῶν προδηλωθέν</supplied><unclear reason="undefined" cert="high">τω</unclear><supplied reason="lost" cert="high">ν Ἰα</supplied><unclear reason="undefined" cert="high">κὼβ</unclear>  <supplied reason="lost" cert="high">οφίας, ὡς ἀνωτέρω</supplied> 

