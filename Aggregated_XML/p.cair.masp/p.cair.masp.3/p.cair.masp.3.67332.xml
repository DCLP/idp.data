    <lb n="7"/><supplied cert="high" reason="lost">ὁμολογοῦμεν ἑκουσίως καὶ αὐθαιρέτως, ἐπομνύμε</supplied>νοι τόν <supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ε</unclear> παντοκράτ<unclear cert="high" reason="undefined">ορ</unclear><supplied cert="high" reason="lost">α Θ</supplied>εὸν κ<unclear cert="high" reason="undefined">αὶ</unclear> <supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ὴ</unclear><supplied cert="high" reason="lost">ν νί</supplied><unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">η</supplied>ν <unclear cert="high" reason="undefined">κ</unclear>αὶ σωτη<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ί</supplied><unclear cert="high" reason="undefined">α</unclear>ν 

