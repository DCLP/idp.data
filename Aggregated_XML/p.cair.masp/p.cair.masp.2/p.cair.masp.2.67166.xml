    <lb n="1"/><g type="chirho"/> β<supplied reason="lost" cert="high">ασιλ</supplied><unclear reason="undefined" cert="high">ε</unclear>ίας καὶ <hi rend="diaeresis">ὑ</hi>πατείας τοῦ θειο<unclear reason="undefined" cert="high">τά</unclear>του ἡμ<supplied reason="lost" cert="high">ῶν</supplied> 

    <lb n="2"/>δεσπότου <expan>Φλα<ex cert="high">υίου</ex></expan> <expan><hi rend="diaeresis">Ἰ</hi>ουστίνο<ex cert="high">υ</ex></expan> τοῦ αἰωνίου Αὐγού<unclear reason="undefined" cert="high">στο</unclear>υ 


    <lb n="3"/>Αὐτοκράτορ<supplied reason="lost" cert="high">ο</supplied>ς ἔτους τρίτου, Φαμενὼθ <expan>ἐννεακ<ex cert="high">αι</ex></expan>-

    <lb n="4"/>δεκάτῃ πρώτη<supplied reason="lost" cert="high">ς</supplied> <expan><hi rend="diaeresis">ἰ</hi>νδ<ex cert="high">ικτίονος</ex></expan>. ἐν <expan>Ἀ<unclear reason="undefined" cert="high">ν</unclear><supplied reason="lost" cert="high">τ</supplied>ινόο<ex cert="high">υ</ex></expan> πόλει τῇ <expan>λαμπρο<ex cert="high">τάτῃ</ex></expan> 

    <lb n="5"/>Αὐρήλιος Μαρτῖνος υ<hi rend="diaeresis">ἱ</hi>ὸς <expan>Σαβινιανο<ex cert="high">ῦ</ex></expan>, ἐκ μητρὸς 

    <lb n="6"/>Μάρθας, δοῦλος τοῦ <expan>ἐνδόξο<ex cert="high">υ</ex></expan> <expan>οἴκο<ex cert="high">υ</ex></expan> <expan>το<ex cert="high">ῦ</ex></expan> πανευφήμου 

