    <lb n="1"/><supplied reason="lost" cert="high"><g type="chirho"/></supplied> <unclear reason="undefined" cert="high">Δι</unclear>όσκ<supplied reason="lost" cert="high">ορος</supplied> <unclear reason="undefined" cert="high">π</unclear>ροσ<unclear reason="undefined" cert="high">ῆλθε</unclear>ν ἡμῖν τ<unclear reason="undefined" cert="high">ὸν</unclear> πατέρα <unclear reason="undefined" cert="high">τὸ</unclear>ν οἰκεῖ<unclear reason="undefined" cert="high">ον</unclear> λ<unclear reason="undefined" cert="high">έ</unclear><supplied reason="lost" cert="high">γ</supplied>ων τ<unclear reason="undefined" cert="high">ὴ</unclear><supplied reason="lost" cert="high">ν</supplied> πρ<supplied reason="lost" cert="low">ῶτ</supplied>ον

    <lb n="2"/><gap reason="lost" extent="20" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/>εξ<gap reason="illegible" extent="3" unit="character"/>αυ<gap reason="lost" extent="12" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/> 

    <lb n="3"/><unclear reason="undefined" cert="high">πρ</unclear>ὸς <supplied reason="lost" cert="high">δ</supplied>ευτέρ<supplied reason="lost" cert="high">ου</supplied><unclear reason="undefined" cert="high">ς</unclear> γάμους ἐλ<unclear reason="undefined" cert="high">θ</unclear>εῖ<unclear reason="undefined" cert="high">ν</unclear>, τὰ δὲ τῇ προτέρᾳ διενέγκοντα 

    <lb n="4"/>τ<supplied reason="lost" cert="high">ῇ δ</supplied><unclear reason="undefined" cert="high">ε</unclear>υτέρᾳ προσγ<unclear reason="undefined" cert="high">ρ</unclear>άψαι· καὶ ἐξε<unclear reason="undefined" cert="high">λθ</unclear>εῖν ἐξ ἀνθρώπ<supplied reason="lost" cert="high">ω</supplied><unclear reason="undefined" cert="high">ν</unclear> ὡς <expan>τ<unclear reason="undefined" cert="high">ο</unclear><ex cert="high">ὺ</ex></expan><unclear reason="undefined" cert="high">ς</unclear> 

    <lb n="5"/>ἐξ ἐκείνης παῖδας ἀφελέσθαι τε καὶ κατα<unclear reason="undefined" cert="high">δαπα</unclear>νᾶν <unclear reason="undefined" cert="high">ἐ</unclear>κεῖνα, 

    <lb n="6"/>καὶ πανταχόθεν ἔρημον εἶναι <unclear reason="undefined" cert="high">τὸν</unclear> <unclear reason="undefined" cert="high">ἡμέ</unclear><supplied reason="lost" cert="high">τε</supplied><unclear reason="undefined" cert="high">ρ</unclear><supplied reason="lost" cert="high">ον</supplied> <unclear reason="undefined" cert="high">ἱ</unclear><supplied reason="lost" cert="high">κέτ</supplied>η<supplied reason="lost" cert="high">ν, ὥστε</supplied> 

    <lb n="7"/>τῆς ἐξ ἡμ<unclear reason="undefined" cert="high">ῶν</unclear> κ<unclear reason="undefined" cert="high">α</unclear>ὶ τῶν νόμων δέεσθαι βο<unclear reason="undefined" cert="high">ηθ</unclear>ε<unclear reason="undefined" cert="high">ί</unclear>ας. θεσπίζο<unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">εν</supplied> 

    <lb n="8"/><supplied reason="lost" cert="high">τοίνυ</supplied>ν τὴν <expan>ἐνδοξ<ex cert="high">ότητα</ex></expan> τὴν σὴν ἀγαγεῖν αὐτοὺ<supplied reason="lost" cert="high">ς κα</supplied>ὶ τὰ <supplied reason="lost" cert="high">περὶ</supplied> <expan>τ<unclear reason="undefined" cert="high">ο</unclear><ex cert="high">ύ</ex>το<ex cert="high">υ</ex></expan> 

    <lb n="9"/>ζητῆσαι, καὶ εἰ οὕτ<unclear reason="undefined" cert="high">ως</unclear> <unclear reason="undefined" cert="high">ἔχοντ</unclear><supplied reason="lost" cert="low">α τὰ τῇ τοῦ ἡμετέρου ἱκέτου</supplied> 

    <lb n="10"/>μητρὶ διενηνοχότα ταῖς ἀληθείαις φαν<unclear reason="undefined" cert="high">είη</unclear>, <unclear reason="undefined" cert="high">τοῦ</unclear>το εἰσπρᾶξαι 

    <lb n="11"/>καὶ <unclear reason="undefined" cert="high">ἀποδοῦ</unclear>ναι τῷ δεομένῳ, ἐπι<gap reason="illegible" extent="4" unit="character"/> <unclear reason="undefined" cert="high">τοῖς</unclear> <unclear reason="undefined" cert="high">λοιποῖς</unclear> <unclear reason="undefined" cert="high">π</unclear>ράγμασι 

    <lb n="12"/>τοῖς τῷ τελευτήσαντι <choice><corr cert="low">διενεγκοῦσι</corr><sic>διενεγκοντα</sic></choice> <app type="editorial"><lem>τ<unclear reason="undefined" cert="high">ὸν</unclear> Λ<unclear reason="undefined" cert="high">έο</unclear><supplied reason="lost" cert="high">ντος το</supplied><unclear reason="undefined" cert="high">ῦ</unclear> <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ῆς θείας</supplied></lem><rdg>τ<unclear reason="undefined" cert="high">ῷ</unclear> λ<unclear reason="undefined" cert="high">ω</unclear><gap reason="lost" extent="13" unit="character"/></rdg></app> 

    <lb n="13"/>λήξεως φυλάξαι τῷ δεομένῳ, ὡς <unclear reason="undefined" cert="high">μὴ</unclear> πλέον τὴν δευτέραν 

    <lb n="14"/>γυναῖκα <unclear reason="undefined" cert="high">λ</unclear>αβεῖν ὧν <choice><corr>ἑνὶ</corr><sic>ενη</sic></choice> <num value="1"/> τῶν ἐκ τοῦ <expan>προτέρο<ex cert="high">υ</ex></expan> γάμου παιδὶ 

    <lb n="15"/>ἁρμόζει μέν. ἐπειδὴ δὲ καὶ χωρία περιελθεῖν εἰς <unclear reason="undefined" cert="high">τὸν</unclear> <supplied reason="lost" cert="high">οἰκεῖον</supplied> 

    <lb n="16"/><unclear reason="undefined" cert="high">π</unclear>ατέρα φησίν, δωρεὰν τῆς <expan>αὐτο<ex cert="high">ῦ</ex></expan> θείας <unclear reason="undefined" cert="high">γ</unclear>ρα<unclear reason="undefined" cert="high">ψά</unclear>σης, καὶ <unclear reason="undefined" cert="high">ταύτ</unclear><supplied reason="lost" cert="high">ην</supplied> 

    <lb n="17"/>τοὺς ἐκ τοῦ <expan>δευτέρο<ex cert="high">υ</ex></expan> <expan>γάμο<ex cert="high">υ</ex></expan> κρατεῖν δωρεὰν εἰς <expan>αὑτο<ex cert="high">ύ</ex></expan>ς, δευτέραν 

    <lb n="18"/><supplied reason="lost" cert="high">μ</supplied>ὲ<supplied reason="lost" cert="high">ν</supplied> αὐτοῖς γενέσθαι πα<unclear reason="undefined" cert="high">ρ</unclear>ασκευάσαντας, θεσπίζομεν 

    <lb n="19"/>τὴν <expan>ἐνδοξ<ex cert="high">ότητα</ex></expan> τὴν σὴν καὶ τὰ περὶ <expan>το<ex cert="high">ύ</ex>το<ex cert="high">υ</ex></expan> ζητῆσαι, καὶ εἰ οὕτως 


    <lb n="20"/>ἔχοντα εὕροις, μὴ προσχῇς δευτέραις δωρεαῖς ἐπὶ πρ<supplied reason="lost" cert="high">άγμασιν</supplied> 

    <lb n="21"/><subst><add place="inline">ἐκπεποιημένοις</add><del rend="corrected" status="unremarkable">εκπεποιημεναις</del></subst> γραφείσαις, <app type="BL"><lem resp="2.2.39; cf. 1.104"><supplied reason="lost" cert="high">ὡ</supplied>ς οὐδὲ</lem><rdg><seg cert="low" part="N"><supplied reason="lost" cert="low">μνή</supplied>σου δὲ</seg></rdg></app> τῶν ἡμετέρων 

    <lb n="22"/>νόμων κυρία τὰ τοιαῦτα ποιούντων <g type="chirho"/> 

    <lb n="23"/>ὁ νόμος οὕτως ἔχει ὁ Λέοντος· ὅσα <choice><corr>κερδαίνειν</corr><sic>κερδενειν</sic></choice> <choice><corr>ἔμελλεν</corr><sic>η<unclear reason="undefined" cert="high">μελλεν</unclear></sic></choice> ἡ προτέρα 

    <lb n="24"/>γαμετή, τοῦ ἀνδρὸς πρὸ αὐτῆς τελευτώντος, <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">οσαῦ</supplied>τα <unclear reason="undefined" cert="high">καὶ</unclear> 

