    <lb n="3"/>Μεσ<supplied cert="high" reason="lost">ο</supplied>ρὴ <num value="3">γ</num>  <num value="15">ιε</num> <expan><supplied cert="high" reason="lost">ἰ</supplied><unclear cert="high" reason="undefined">νδι</unclear>κ<ex cert="high">τίονος</ex></expan>. 

    <lb n="4"/>Αὐρηλ<supplied cert="high" reason="lost">ίῳ</supplied> Ἄπα Βίκτωρι Ἀπολλ<unclear cert="high" reason="undefined">ῶ</unclear>τος, 

    <lb n="5"/>διὰ <expan>Χα<supplied cert="high" reason="lost">ρ</supplied>ισίο<ex cert="high">υ</ex></expan> Ψιμανωβ<unclear cert="high" reason="undefined">ετ</unclear> <gap extent="6" reason="lost" unit="character"/> 

    <lb n="6"/>αὐτοῦ, ἀπὸ κώμης <expan>Ἀ<supplied cert="high" reason="lost">φ</supplied><unclear cert="high" reason="undefined">ρ</unclear>[οδ<ex cert="high">ίτης</ex></expan> τοῦ Ἀνταιο]-

