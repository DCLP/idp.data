    <lb n="9"/>τοῦ <expan>αὐτο<ex cert="high">ῦ</ex></expan> <expan>Ἀνταιοπ[ολ<ex cert="high">ίτου</ex></expan> <expan>ν]ομο<ex cert="high">ῦ</ex></expan>, <expan>χαίρ<ex cert="high">ειν</ex></expan>. σ<unclear reason="undefined" cert="high">ω</unclear><supplied reason="lost" cert="high">ματίσατε</supplied> 

