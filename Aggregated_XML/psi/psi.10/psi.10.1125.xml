    <lb n="16"/><gap extent="unknown" reason="lost" unit="character"/>ν ῥητὸν ἀριθμόν σοι ἐπιτάξας. ἐντυχὼν οὖν <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="17"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> πρ</supplied>οσέταξεν αὐτοῦ ἡ ἀρετὴ ἀποστεῖλαι. ὁποια γὰρ π<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="18"/><gap extent="unknown" reason="lost" unit="character"/> ἀπεστάλη γὰρ περὶ τούτου καὶ ἑκατόνταρχος <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="19"/><gap extent="unknown" reason="lost" unit="character"/> τῆς ἀπαιτήσεως τῶν προτέρων χρόνω<supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="20"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ν</unclear>ων προσέταξα καὶ ὅτι χρὴ ἀμφοτέρους αὐτ<unclear cert="high" reason="undefined">ο</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="21"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">τ</unclear>α σιδήρῳ πρός με παραπέμψαι. ἐὰν γὰρ τοὺς <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="22"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> δι᾽</supplied> ἑτέρων σοι γραμμάτων δῆλον κατέστη<supplied cert="high" reason="lost">σα </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="23"/><gap extent="unknown" reason="lost" unit="character"/> καὶ ἐπιφανεστάτων Καισάρων Κωνσταντ<supplied cert="high" reason="lost">ίου </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="24"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ν</unclear> ἀναβαλόμενος. σφόδρα γὰρ τοῦ εἴδους τούτο<supplied cert="low" reason="lost">υ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

