    <lb n="5"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ε</supplied>ὔχομαι. <expan><ex cert="high">ἔτους</ex></expan> <num value="18">ιη</num> <orig><g type="s-etous"/></orig> καὶ <num value="19">ιθ</num> <orig><g type="s-etous"/></orig> καὶ <num value="10">ι</num> <orig><g type="s-etous"/></orig>, Φαρμοῦθι <num value="18">ιη</num> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/><gap extent="unknown" reason="lost" unit="character"/>ι <num value="26">κϛ</num> Ἄννιος Διογένης στρατηγῷ Ἀρσινο<hi rend="diaeresis">ί</hi>τ<supplied cert="high" reason="lost">ου χαίρειν. </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ῳ</unclear> στρατιώτῃ τῷ εἰς τοῦτο ἀποσταλέντι, <hi rend="diaeresis">ὑ</hi>πὲρ δὲ το<unclear cert="high" reason="undefined">υ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/><gap extent="unknown" reason="lost" unit="character"/> παρέξεις τὰ ναῦλα πρὸς τὴν ἀνωγὴν τοῦ ὑπὸ σο<unclear cert="high" reason="undefined">ῦ</unclear> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Ἀρ</supplied>σινο<hi rend="diaeresis">ί</hi>του χαίρειν. καὶ ἄλλοτέ σοι ἐπεστειλα<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀποσταλέ</supplied>ντος ὑπ᾽ αὐτοῦ μετὰ πλοίου στρατιώτου· ἀλλ᾽ ἐπεὶ καὶ <gap extent="unknown" reason="lost" unit="character"/> 

