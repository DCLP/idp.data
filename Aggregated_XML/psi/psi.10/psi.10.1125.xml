    <lb n="12"/><gap extent="unknown" reason="lost" unit="character"/> ἐπηρτημένον κίνδυνον. ἐρρῶσθαί σε εὔχ<supplied cert="high" reason="lost">ομαι. </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐπεστ</supplied><unclear cert="high" reason="undefined">ά</unclear>λη τῶν τεριόνων ἐντέτακται τῷ καθ᾽ ἑ<unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">αστ  </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="14"/><gap extent="unknown" reason="lost" unit="character"/>σιν <unclear cert="high" reason="undefined">κε</unclear>ντην<unclear cert="high" reason="undefined">άρι</unclear>ον ἓν <num value="1"/> ποιήσας παραδοθῆν<supplied cert="high" reason="lost">αι </supplied><gap extent="unknown" reason="lost" unit="character"/> 

