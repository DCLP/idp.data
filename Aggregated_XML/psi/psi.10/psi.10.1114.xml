    <lb n="1"/><choice><corr>ὑπατεία<supplied reason="omitted" cert="high">ς</supplied></corr><sic>υπατεια</sic></choice> Φλαουίων Ἀετίου καὶ Στουδ<supplied reason="lost" cert="high">ίου τῶν <expan>λαμ<ex cert="high">προτάτων</ex></expan> </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>τῇ τάξει τοῦ <choice><corr>πρίγκιπος</corr><sic>πρινκεπος</sic></choice> τῆς ἐξου<unclear reason="undefined" cert="high">σ</unclear><supplied reason="lost" cert="high">ίας τοῦ κυρίου μου</supplied> 

    <lb n="3"/>τοῦ λαμπροτάτου ἡγεμόνος ἐπαρχί<supplied reason="lost" cert="high">ας Ἀρκαδίας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>εἰς τοῦτο ἀποσταλέντος <gap reason="lost" extent="2" unit="character"/><unclear reason="undefined" cert="high">ακι</unclear><gap reason="illegible" extent="3" unit="character"/> <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> Ὀξυρυγχειτῶν</supplied> 

    <lb n="5"/>πόλεως. προσελθ<unclear reason="undefined" cert="high">ὼ</unclear>ν <unclear reason="undefined" cert="high">τῇ</unclear> ξ<supplied reason="lost" cert="high">ουσ</supplied>ί<supplied reason="lost" cert="low">ᾳ τοῦ αὐτοῦ κυρίου μου</supplied> 

    <lb n="6"/>ἄρχοντος ᾐτιασάμην φανερῶ<unclear reason="undefined" cert="high">ν</unclear> <supplied reason="lost" cert="low">κεφαλαίων</supplied><supplied reason="lost" cert="high"> τὸν δεῖνα</supplied> 

    <lb n="7"/>Ἱερακίωνος καὶ ἀποφάσεως <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>καὶ τούτου παρασταθέντος καὶ τ<supplied reason="lost" cert="high">ὸ ἱκανόν μοι πεποιηκότος, ἀπέστην</supplied> 

    <lb n="9"/>τῆς κατ᾽ αὐτοῦ αἰτιάσεως, καὶ ὀ<supplied reason="lost" cert="high">μνύω θεὸν τὸν παντοκράτορα</supplied> 

    <lb n="10"/>καὶ τὴν εὐσέβειαν τῶν τὰ πάντα <supplied reason="lost" cert="high">νικώντων δεσποτῶν ἡμῶν</supplied> 

    <lb n="11"/>Οὐαλεντινιανοῦ <supplied reason="omitted" cert="high">καὶ</supplied> Μαρκιανοῦ τῶν αἰ<unclear reason="undefined" cert="high">ω</unclear><supplied reason="lost" cert="high">νίων Αὐγούστων ἐπὶ τῷ με μὴ ἐν</supplied>-

    <lb n="12"/>οχλεῖν τῇ <hi rend="diaeresis">ὑ</hi>μῶν ἐντρεχίᾳ π<supplied reason="lost" cert="high">ερὶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/><gap reason="lost" extent="3" unit="character"/><unclear reason="undefined" cert="high">τ</unclear>ε περὶ ἐκβιβασμοῦ διὰ τὸ ἡμ<supplied reason="lost" cert="low">ᾶς </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> διαλελύσθαι</supplied> 

