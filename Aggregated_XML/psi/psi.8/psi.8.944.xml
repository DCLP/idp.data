    <lb n="1"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">τ</unclear>η<gap extent="3" reason="lost" unit="character"/> <unclear cert="high" reason="undefined">Φλα</unclear>βιανῷ τῷ λαμ<unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ροτάτῳ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="2" reason="illegible" unit="character"/> <unclear cert="high" reason="undefined">Σ</unclear>εύθους οὐετρανοῦ τῶν <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/><supplied cert="high" reason="lost">ἀ</supplied><unclear cert="high" reason="undefined">π</unclear>λεονέκτητον <choice><corr>παρέχει</corr><sic>παρεχι</sic></choice> ἡ σὴ ἀρετή, ἡγεμὼν κύρι<supplied cert="high" reason="lost">ε, </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/><supplied cert="high" reason="lost">ἐγ</supplied><unclear cert="high" reason="undefined">ὼ</unclear> <unclear cert="high" reason="undefined">τ</unclear>οίνυν συνήφθην ἀνδρὶ <expan>βουλ<ex cert="low">ευτῇ</ex></expan> Σαρπαστο<unclear cert="high" reason="undefined">υ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>ἐμῇ προικὶ ἐδάφη καὶ ψυκτῆρας λευκαν<unclear cert="high" reason="undefined">ι</unclear>ων. <choice><corr cert="low">ἐπεὶ</corr><sic>επι</sic></choice> τοίν<unclear cert="high" reason="undefined">υ</unclear><supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/>βουλευτικῆς καταλαβούσης τὸν αὐτόν μου σύνβιον ενχ<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/>διε<gap extent="2" reason="illegible" unit="character"/>ασατο τὰ αὐτοῦ <hi rend="diaeresis">ὑ</hi>παλλάγματα κατ᾽ <g type="apostrophe"/> ἐμὴν ἄγνοιαν ὧν ενς<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>καὶ ἔστιν πολλὰ ἃ <choice><corr>ὀφείλει</corr><sic>οφιλι</sic></choice> διὰ τὰ <choice><corr>λειτουργήματα</corr><sic>λιτουργηματα</sic></choice>. καὶ <choice><corr>ἐγὼ</corr><sic>καγω</sic></choice> περιπν<unclear cert="high" reason="undefined">ε</unclear><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀξι</supplied>-

    <lb n="9"/><unclear cert="high" reason="undefined">ῶ</unclear> καὶ δέομαι τῆς σῆς δικαιοκρισίας ὡς εἰδὼς τὸ <choice><corr>γυναικεῖον</corr><sic>γυνεον</sic></choice> γένος <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀγο</supplied>-

    <lb n="10"/><unclear cert="high" reason="undefined">ραστὴν</unclear> προνοίᾳ Μακροβίου τοῦ <choice><corr>προπολιτευομένου</corr><sic>προπολιτευομαινου</sic></choice> τὴν του<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/><gap extent="4" reason="illegible" unit="character"/><gap extent="2" reason="illegible" unit="character"/> καὶ τοὺς φόρους καὶ τὰ ἐνοίκια ἐμοῦ παρεχούση<unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">α</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="12"/>τοὺς ν<supplied cert="high" reason="lost">όμ</supplied>ους, <subst><add place="inline">ἵνα</add><del rend="corrected"><hi rend="diaeresis">ι</hi>νας</del></subst> καὶ <choice><corr>ἐγὼ</corr><sic>καγω</sic></choice> δυνηθῶ πληρῶσαι τοὺς λοιποὺς <unclear cert="high" reason="undefined">δα</unclear>ν<supplied cert="high" reason="lost">ειστάς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/><gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">τ</unclear>ω<unclear cert="high" reason="undefined">ν</unclear> <choice><corr>παρέχειν</corr><sic>παρεχιν</sic></choice> αὐτὸν τὴν προῖκαν ἣν <choice><corr>κατανήλωσεν</corr><sic>κατηνηλωσεν</sic></choice> ὁ αὐ<supplied cert="low" reason="lost">τός μου σύμβιος </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="14"/>τῶν <choice><corr>λειτουργημάτων</corr><sic>λιτουργηματων</sic></choice> ἅτινα ἐγὼ <subst><add place="inline"><unclear cert="high" reason="undefined">κ</unclear>ρατοῦμαι</add><del rend="corrected"><unclear cert="high" reason="undefined">κυ</unclear>ρατουμαι</del></subst> περὶ τούτων ω<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="15"/>εὐπορίαν, καὶ <choice><corr>ἐμὲ</corr><sic>καμαι</sic></choice> δὲ τὸ ἀ<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">πηρέα</unclear>στον <choice><corr>ἔχειν</corr><sic>εχιν</sic></choice> <hi rend="diaeresis">ὑ</hi>πὸ τῶν <choice><corr>δανειστῶν</corr><sic>δανιστων</sic></choice> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="16"/>φρι<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ο</unclear>τηλα<unclear cert="high" reason="undefined">β</unclear><gap extent="unknown" reason="lost" unit="character"/> 

