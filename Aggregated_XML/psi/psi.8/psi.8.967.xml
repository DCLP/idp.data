    <lb n="1"/>Εὐάγγελος Διδυμᾶτι <choice><corr>χαίρειν</corr><sic>χαιριν</sic></choice>. ὡς ἔγραψάς 

    <lb n="2"/>μοι ὅ<unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ι</supplied> <unclear reason="undefined" cert="high">μὴ</unclear> ἀφήσ<supplied reason="lost" cert="low">εις</supplied> <supplied reason="lost" cert="low">αὐτ</supplied><unclear reason="undefined" cert="low">ὴ</unclear>ν <choice><corr>εἰς</corr><sic>ις</sic></choice> Ἀλεξάνδριαν 

    <lb n="3"/>τῷ Σ<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="1" unit="character"/>ουτι εμ<gap reason="lost" extent="5" unit="character"/>υ<unclear reason="undefined" cert="high">της</unclear> τὴν δαπάνην 

    <lb n="4"/>ἀλλὰ <supplied reason="lost" cert="high">χ</supplied>ρή με δο<gap reason="lost" extent="6" unit="character"/>μενος ἐπὶ τοῦ παρόν-

    <lb n="5"/><choice><corr>τος</corr><sic>παρονδο<unclear reason="undefined" cert="high">ς</unclear></sic></choice> <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="2" unit="character"/><unclear reason="undefined" cert="high">σ</unclear>ε<unclear reason="undefined" cert="high">γα</unclear><gap reason="lost" extent="9" unit="character"/>ς γ<unclear reason="undefined" cert="high">υ</unclear>ν<unclear reason="undefined" cert="high">αι</unclear>κ<supplied reason="lost" cert="high">ὸ</supplied>ς Σερήνο<unclear reason="undefined" cert="high">υ</unclear> 

    <lb n="6"/>το<unclear reason="undefined" cert="high">π</unclear><gap reason="lost" extent="3" unit="character"/><gap reason="illegible" extent="1" unit="character"/>υν<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="8" unit="character"/> <unclear reason="undefined" cert="high">α</unclear>ὐτὴν π<supplied reason="lost" cert="high">ά</supplied>λιν εὗ-

    <lb n="7"/>ρον τῇ <supplied reason="lost" cert="high">ἡ</supplied>μέρᾳ <supplied reason="lost" cert="high">ἐν ᾗ </supplied><choice><corr><supplied reason="lost" cert="high">ἀπ</supplied>έθανε</corr><sic><supplied reason="lost" cert="high">απ</supplied><unclear reason="undefined" cert="high">εθ</unclear>αναι</sic></choice> <choice><corr>ἐνεχείρησα</corr><sic><unclear reason="undefined" cert="high">ενεχι</unclear><add place="supralinear">ρ<unclear reason="undefined" cert="high">η</unclear>σα</add></sic></choice> <unclear reason="undefined" cert="high">τ</unclear>αύτῃ 

    <lb n="8"/>δραχμὰς ἑκ<unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">τόν <num value="100"/>. καὶ α</supplied>ὐτὴ μὲν <unclear reason="undefined" cert="high">λέ</unclear>γει ὅτ<unclear reason="undefined" cert="high">ι</unclear> 

    <lb n="9"/>εἴ τι <choice><corr>θέλεις</corr><sic><unclear reason="undefined" cert="high">θ</unclear>ε<unclear reason="undefined" cert="high">λι</unclear>ς</sic></choice> λα<add place="supralinear"><unclear reason="undefined" cert="high">μ</unclear></add><gap reason="lost" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="1" unit="character"/> <choice><corr>δύναμαι</corr><sic><unclear reason="undefined" cert="high">δ</unclear>υνομα<unclear reason="undefined" cert="high">ι</unclear></sic></choice> <choice><corr>λαβεῖν</corr><sic><unclear reason="undefined" cert="high">λ</unclear>αβιν</sic></choice> πλ<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="1" unit="character"/> 

    <lb n="10"/><choice><corr>δύο</corr><sic>δυω</sic></choice> <num value="2"/> μ<unclear reason="undefined" cert="high">ν</unclear>ᾶς <choice><corr>μέλλω</corr><sic>με<unclear reason="undefined" cert="high">λ</unclear>λο</sic></choice> <choice><corr>δαπανῆσαι</corr><sic><unclear reason="undefined" cert="high">δ</unclear>απανησε</sic></choice> <choice><corr>εἰς</corr><sic>ις</sic></choice> αὐτήν. 

    <lb n="11"/>πέμψον <choice><corr>καὶ</corr><sic>κε</sic></choice> σὺ ἀπὸ μέρους ἵνα κ<add place="supralinear">ε</add><del rend="erasure" status="unremarkable"><gap reason="illegible" extent="3" unit="character"/></del><add place="supralinear"><gap reason="illegible" extent="1" unit="character"/></add><gap reason="illegible" extent="1" unit="character"/> 

    <lb n="12"/>δυνασθῶ <choice><corr>ἀναπλ<supplied reason="omitted" cert="high">ε</supplied>ῦσαι</corr><sic>αναπλυσε</sic></choice> βουλεύομαι αὐτὴν 

    <lb n="13"/><choice><corr>ὥ<add place="supralinear">δ</add>ε</corr><sic>ωτε</sic></choice> <choice><corr>θάψαι</corr><sic>θαψε</sic></choice> <choice><corr>πρόσδεξαι</corr><sic>πρ<unclear reason="undefined" cert="high">ο</unclear>σδεξε</sic></choice> αὐτὴν ἐν τῷ ἄρτι ἐρ-

    <lb n="14"/>χομένῳ πλοίῳ τῶν νεκρῶν <add place="supralinear">πέμ</add>ψον μοι ταχύ-

    <lb n="15"/>τερον ἵνα <subst><add place="inline">ἰσχύσω</add><del rend="corrected" status="unremarkable">ισχουσω</del></subst> ἀναβῆναι μετὰ τὴν 


    <lb n="16"/>ἑορτήν. ταῦτα <choice><corr>λέγει</corr><sic>λεγι</sic></choice> Ἀφροδι<add place="supralinear">σί</add>α τῷ πατρὶ 

    <lb n="17"/>ὅτι στρατιώτης <choice><corr>θέλει</corr><sic><subst><add place="inline">θελι</add><del rend="corrected" status="unremarkable"><unclear reason="undefined" cert="high">μ</unclear>ελι</del></subst></sic></choice> <choice><corr>γαμῆσαι</corr><sic>γαμησε</sic></choice> μου τὴν ἀδελ-

    <lb n="18"/>φήν, καὶ οὐκ ε<unclear reason="undefined" cert="high">κ</unclear>δ<unclear reason="undefined" cert="high">υ</unclear>νη<unclear reason="undefined" cert="high">κ</unclear>α χωρὶς τῆς γνώμη<unclear reason="undefined" cert="high">ς</unclear> 

    <lb n="19"/>σου δοῦναι. <unclear reason="undefined" cert="high">ὁ</unclear> ὲ <choice><corr>στρ<supplied reason="omitted" cert="high">α</supplied>τιώτης</corr><sic>στρδι<unclear reason="undefined" cert="high">ω</unclear><supplied reason="lost" cert="high">τη</supplied>ς</sic></choice> Φεβηχίτης ἐστὶν 

    <lb n="20"/>εὐσχή<supplied reason="lost" cert="high">μ</supplied><unclear reason="undefined" cert="high">ω</unclear>ν. ἔ<supplied reason="lost" cert="high">ρ</supplied><unclear reason="undefined" cert="high">ρωσ</unclear><supplied reason="lost" cert="high">ο</supplied>. 
