    <lb n="1"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> πρ</supplied>οστατικῇ μεγαλοπρεπείᾳ ὅτι καὶ ἄλλοτε ἔπεμψα χ<unclear cert="high" reason="undefined">ρ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/><gap extent="unknown" reason="lost" unit="character"/> <unclear cert="high" reason="undefined">δ</unclear>ιὰ τὴν εὐγενεστάτην Εὐδοξίαν  <hi rend="diaeresis">ἵ</hi>να επ<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> α</supplied>ὐτοῦ καὶ ἐπανέλυσεν μετὰ τῆς αἰσχύν<unclear cert="high" reason="undefined">η</unclear><supplied cert="low" reason="lost">ς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/><gap extent="unknown" reason="lost" unit="character"/>ων καὶ ἐγενάμην εἰς τοὺς πόδας αὐτῆς ἐν τῷ εὐλογ<unclear cert="high" reason="undefined">η</unclear>μέν<supplied cert="high" reason="lost">ῳ  </supplied><gap extent="unknown" reason="lost" unit="character"/> 


    <lb n="5"/><gap extent="unknown" reason="lost" unit="character"/>ι αὐτὴν ὅπως ἔλθῃ πρὸς <hi rend="diaeresis">ὑ</hi>μᾶς καὶ ἀκούσητε τῶν με<unclear cert="high" reason="undefined">ταιτ</unclear><gap extent="2" reason="illegible" unit="character"/> <unclear cert="high" reason="undefined">κα</unclear>ὶ <unclear cert="high" reason="undefined">ἐπ</unclear>ιτιμήσητε 

    <lb n="6"/><gap extent="unknown" reason="lost" unit="character"/> <unclear cert="high" reason="undefined">α</unclear>ὐτὸς <choice><corr>κατηξίωσεν</corr><sic>κατεξιωσεν</sic></choice> ἐλθεῖν πρὸς <hi rend="diaeresis">ὑ</hi>μᾶς καὶ ἠναγκάσθην ἀπράκτως 

    <lb n="7"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/> <hi rend="diaeresis">ἰ</hi>δοὺ οὖν πάλιν ἔπεμψα τὸν εἰρημένον εὐδοκιμ<unclear cert="low" reason="undefined">ώτα</unclear><supplied cert="low" reason="lost">τον</supplied><gap extent="4" reason="lost" unit="character"/>φ<gap extent="4" reason="illegible" unit="character"/> 

    <lb n="8"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> παρακ</supplied>αλῶ ὑμᾶς εἴπ<unclear cert="high" reason="undefined">ερ</unclear> προῄρηται ἐλθεῖν ἔτι ἐ<unclear cert="high" reason="undefined">γ</unclear>γὺς τῶν τε<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">τα</unclear>του ἐπειδὴ ἐγὼ ἐξ ἁμαρτιῶν μου <choice><corr>ὠφθαλμίασα</corr><sic>οφθαλμιασα</sic></choice> ἐν τῇ <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/><gap extent="unknown" reason="lost" unit="character"/> ἐλθεῖν τῆς <choice><corr>ματερ<supplied cert="high" reason="omitted">τέρ</supplied>ας</corr><sic>ματερας</sic></choice> μου, παρακαλῶ δὲ τὴν <hi rend="diaeresis">ὑ</hi>μετέραν προστατι<unclear cert="high" reason="undefined">κὴν</unclear> <unclear cert="high" reason="undefined">μεγαλο</unclear>-

    <lb n="11"/><supplied cert="high" reason="lost">πρέπειαν </supplied><gap extent="unknown" reason="lost" unit="character"/> ἀποσχέσθαι ἐφεξῆς τῶν τοιούτων μιαρῶν ῥημάτων καὶ μὴ τῆς αὐτῆς 

    <lb n="12"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ς ἀλλὰ κατὰ τὴν δύναμιν τοῦ γαμικοῦ αὐτῆς τιμᾶν τὸν <hi rend="diaeresis">ἴ</hi>διον ἄνδρα 

    <lb n="13"/><gap extent="unknown" reason="lost" unit="character"/> ἀποκακήσας <hi rend="diaeresis">ὑ</hi>βριζόμενος παρ᾽ αὐτῆς ἔλθω εἰς ἄλλην <unclear cert="high" reason="undefined">ταξιμι</unclear>

    <lb n="14"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ομαι, παρακαλῶ δὲ αὐτὴν παραγγεῖλαι μηκέτι πεμφθῆναι ἐνταῦθα 

    <lb n="15"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ραμαν<unclear cert="low" reason="undefined">ν</unclear>αν πρὸς αὐτήν· ἐπεὶ ὡς οἶδεν κύριος τῆς δόξης ὅσας πλη<unclear cert="high" reason="undefined">γὰς</unclear> 

    <lb n="16"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">εν</unclear> τῷ <hi rend="diaeresis">ἴ</hi>σας παρ᾽ <hi rend="diaeresis">ὑ</hi>μῶν εἰς σῶμα οὐκ ἐπανέρχεται <gap extent="4" reason="illegible" unit="character"/><gap extent="5" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>υν δὲ 

    <lb n="17"/><gap extent="unknown" reason="lost" unit="character"/> ἐνταῦθα εἰς τὴν ἡ<unclear cert="high" reason="undefined">μέρ</unclear>αν τοῦ κοινοῦ προστάτου <supplied cert="low" reason="lost">ἡμῶν</supplied><supplied cert="high" reason="lost"> Θ</supplied>εοδώρου 

