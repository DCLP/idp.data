    <lb n="1"/>Φανίας <supplied reason="lost" cert="high">τῶι δεῖνα τῶι πρὸς τ</supplied>ῆι ἀγοραν<supplied reason="lost" cert="high">ομίαι τοῦ Ἀρσινοίτου.</supplied> 

    <lb n="2"/>τῆς γεγε<supplied reason="lost" cert="high">νημένης ὑπογραφῆς</supplied> ἐπὶ τῶν <supplied reason="lost" cert="high">τὰ προσπίπτοντα</supplied> 

    <lb n="3"/>κρινόν<supplied reason="lost" cert="high">των χρηματιστῶν ὑπόκ</supplied>ειται τὸ <supplied reason="lost" cert="high">ἀντίγραφον</supplied>. 

    <lb n="4"/>ἐπιτελε<supplied reason="lost" cert="high">σθήτω οὖν καθὼς σ</supplied>υγκέκριτα<supplied reason="lost" cert="high">ι. </supplied><gap reason="lost" extent="unknown" unit="character"/> 
