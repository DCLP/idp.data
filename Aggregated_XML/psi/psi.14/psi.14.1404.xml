    <lb n="1"/>Ἀπολλώνιος Μάκρω<unclear cert="high" reason="undefined">ν</unclear>ι 

    <lb n="2"/>τῷ υ<supplied cert="high" reason="lost">ἱ</supplied><unclear cert="high" reason="undefined">ῶ</unclear>ι <expan>χ<ex cert="high">αίρειν</ex></expan> καὶ <choice><corr>ὑγιαίν</corr><sic>υγιον</sic></choice><expan><ex cert="high">ειν</ex></expan>. 

    <lb n="3"/>ἐκομισάμην διὰ 

    <lb n="4"/>Θοώνιος ε<gap extent="2" reason="illegible" unit="character"/>ιας <expan><ex cert="high">δραχμὰς</ex></expan> <num value="40">μ</num>. 

    <lb n="5"/>οὐ πέμπω δέ σοι 

    <lb n="6"/>ἐπιμήνια διὰ <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ὸ</supplied> 

    <lb n="7"/>μὴ εὑρίσκειν ἀσφ<unclear cert="high" reason="undefined">α</unclear>-

    <lb n="8"/>λ<unclear cert="high" reason="undefined">ῆ</unclear> ἄνθρωπον <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">αὶ</supplied> 

    <lb n="9"/>δ<unclear cert="high" reason="undefined">ι</unclear>ὰ τὰ πράγμα<supplied cert="high" reason="lost">τα</supplied> 

    <lb n="10"/>ἅ μ᾽ <choice><corr>ἐλάμβανεν</corr><sic>ε<unclear cert="high" reason="undefined">μ</unclear>λαμβαν<supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">ν</unclear></sic></choice>. 

    <lb n="11"/><unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">λ</supplied>ήρης δὲ ἐ<unclear cert="high" reason="undefined">ν</unclear> τοῖς 

    <lb n="12"/>δημοσίοις ἐρ<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">τῶ</supplied> 

    <lb n="13"/>σε παραγείν<supplied cert="high" reason="lost">εσθαι</supplied> 

    <lb n="14"/>ἰδίᾳ· ὀλιγωρ<unclear cert="high" reason="undefined">ῶ</unclear> γὰρ 

    <lb n="15"/>περὶ σοῦ· μὴ ἵνα 

    <lb n="16"/>σκύλῃς με ἐλθεῖν 

    <lb n="17"/>ἐπὶ σέ.  Ἀμμώ-

    <lb n="18"/>νιος δὲ οὐκ ἠθέλη-

    <lb n="19"/>σεν <choice><corr>δέξασθαι</corr><sic>δεξασασθαι</sic></choice> 

    <lb n="20"/>ἐπιμήνια, ἐπεὶ ὑπῆ-

    <lb n="21"/>γεν <supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">ἰ</unclear>ς ἐμβολ<unclear cert="high" reason="undefined">ή</unclear>ν. 

    <lb n="22"/>ὅρα δὲ μὴ ἀπολέ-

    <lb n="23"/>σῃς <unclear cert="high" reason="undefined">σ</unclear>εαυτὸ<supplied cert="high" reason="lost">ν</supplied> εἰς 

    <lb n="24"/><supplied cert="high" reason="lost">τὰ ἔ</supplied><unclear cert="high" reason="undefined">ρ</unclear>γα επρα<gap extent="2" reason="lost" unit="character"/><unclear cert="high" reason="undefined">θ</unclear>α 

    <lb n="25"/><gap extent="3" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ε</unclear> ἐαν ε<gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ευ<gap extent="1" reason="illegible" unit="character"/> 

    <lb n="26"/><supplied cert="high" reason="lost">ἀσπ</supplied><unclear cert="high" reason="undefined">ά</unclear>ζου Ἀπο<unclear cert="high" reason="undefined">λ</unclear><supplied cert="high" reason="lost">λ</supplied><unclear cert="high" reason="undefined">ώ</unclear>νιον 

    <lb n="27"/><supplied cert="high" reason="lost">ἀδελ</supplied><unclear cert="high" reason="undefined">φό</unclear>ν.  ἐὰ<unclear cert="high" reason="undefined">ν</unclear> <unclear cert="high" reason="undefined">δ</unclear>ὲ 

    <lb n="28"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="29"/><gap extent="unknown" reason="lost" unit="line"/> 

    <lb n="29,ms"/>ἀσπάζου τοὺς <gap extent="4" reason="illegible" unit="character"/><gap extent="2" reason="illegible" unit="character"/> πάντας.  <expan><ex cert="high">ἔτους</ex></expan> <num value="2">β</num> Τιβερίου Κλαυδ<supplied cert="high" reason="lost">ί</supplied>ου <expan>κ<ex cert="high">αί</ex></expan>σαρος σεβαστοῦ <gap extent="6" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 
