    <lb n="9"/>δ<unclear reason="undefined" cert="high">ι</unclear>ὰ τὰ πράγμα<supplied reason="lost" cert="high">τα</supplied> 

    <lb n="10"/>ἅ μ᾽ <choice><corr>ἐλάμβανεν</corr><sic>ε<unclear reason="undefined" cert="high">μ</unclear>λαμβαν<supplied reason="lost" cert="high">ε</supplied><unclear reason="undefined" cert="high">ν</unclear></sic></choice>. 

    <lb n="11"/><unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">λ</supplied>ήρης δὲ ἐ<unclear reason="undefined" cert="high">ν</unclear> τοῖς 

    <lb n="12"/>δημοσίοις ἐρ<unclear reason="undefined" cert="high">ω</unclear><supplied reason="lost" cert="high">τῶ</supplied> 

    <lb n="13"/>σε παραγείν<supplied reason="lost" cert="high">εσθαι</supplied> 

    <lb n="14"/>ἰδίᾳ· ὀλιγωρ<unclear reason="undefined" cert="high">ῶ</unclear> γὰρ 

    <lb n="15"/>περὶ σοῦ· μὴ ἵνα 

    <lb n="16"/>σκύλῃς με ἐλθεῖν 

    <lb n="17"/>ἐπὶ σέ.  Ἀμμώ-

    <lb n="18"/>νιος δὲ οὐκ ἠθέλη-

    <lb n="19"/>σεν <choice><corr>δέξασθαι</corr><sic>δεξασασθαι</sic></choice> 

    <lb n="20"/>ἐπιμήνια, ἐπεὶ ὑπῆ-

    <lb n="21"/>γεν <supplied reason="lost" cert="high">ε</supplied><unclear reason="undefined" cert="high">ἰ</unclear>ς ἐμβολ<unclear reason="undefined" cert="high">ή</unclear>ν. 

