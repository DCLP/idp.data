    <lb n="3"/>τιο<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>ς<g rend="extension" type="filler"/> 

    <lb n="5"/>προ<unclear cert="high" reason="undefined">στετά</unclear><supplied cert="high" reason="lost">χασι δὲ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/><note lang="en" place="not specified">desunt <num>7</num>_<num>8</num> lin</note> 

