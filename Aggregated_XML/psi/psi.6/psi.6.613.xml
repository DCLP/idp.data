    <lb n="1"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Πανακέ</supplied>στορι χαίρε<unclear cert="high" reason="undefined">ι</unclear>ν. <unclear cert="high" reason="undefined">Ἐ</unclear>τεάρχωι καὶ Σωστράτω<supplied cert="high" reason="lost">ι </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/><gap extent="unknown" reason="lost" unit="character"/>ω<unclear cert="high" reason="undefined">ν</unclear> αὐτῶν προχρῆσαι <expan><ex cert="high">δραχμὰς</ex></expan> <num value="137">ρλζ</num> <expan><ex cert="high">ὀβολοὺς 3</ex></expan><num value="3"/>. τὸ μὲ<supplied cert="low" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/><gap extent="unknown" reason="lost" unit="character"/>εσθαι δὲ τ<supplied cert="high" reason="lost">ὰς</supplied> <expan><ex cert="high">δραχμὰς</ex></expan> <num value="37">λζ</num> <expan><ex cert="high">ὀβολοὺς 3</ex></expan><num value="3"/> καὶ ἕλκειν αὐτοὺς α<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/><gap extent="unknown" reason="lost" unit="character"/>αι σύμβολα δὲ ἐγράφη ἐν Κροκοδίλων πόλε<supplied cert="high" reason="lost">ι </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/><app type="BL"><lem resp="3.224"><gap extent="unknown" reason="lost" unit="character"/>κέλητος</lem><rdg><gap extent="1" reason="illegible" unit="character"/>ελητοϲ</rdg></app> μετρῆσαι πυρῶν <expan><ex cert="high">ἀρτάβας</ex></expan> <num value="20">κ</num> <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/><gap extent="unknown" reason="lost" unit="character"/>ν ἀλλὰ ἀξιῶσαι τοὺς ναύτας δοθῆναι α<unclear cert="high" reason="undefined">υ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> τὰ δὲ σύμβολα ἡμᾶς λαβεῖν καὶ κομίσα<unclear cert="high" reason="undefined">σ</unclear><supplied cert="low" reason="lost">θαι </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/><gap extent="unknown" reason="lost" unit="character"/> συνεθήκαμεν αὐτὰ Μάρωνι ὥστε ἀποδ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/><gap extent="unknown" reason="lost" unit="character"/> ἀντιγράφει καί φησιν συσταθῆναι Δᾶμιν <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/><gap extent="unknown" reason="lost" unit="character"/>των καὶ ἔτι καὶ νῦν οὐ κεκομίσθαι ἐπι<unclear cert="high" reason="undefined">μ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="low" reason="lost">ἀργ</supplied><unclear cert="low" reason="undefined">ύ</unclear>ριον ληφθῆι παρ᾽ αὐτῶν ἢ τιμὴν τῆς <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="12"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">τ</unclear>ελοῦμεν ἡμῖν αὐτοῖς. <space dim="horizontal" extent="unknown" unit="character"/> καὶ το<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> βασι</supplied>λικοῦ <foreign lang="la">?.</foreign> ἀπεστάλκαμεν δέ σοι καὶ ἐλαι<supplied cert="high" reason="lost">ο </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="14"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="15"/>ἔ<supplied cert="high" reason="lost">ρρωσο.  </supplied><gap extent="unknown" reason="lost" unit="character"/> 
