    <lb n="1"/>Διονύσιος Ὠφελίωνι χαίρειν. κα<unclear cert="high" reason="undefined">λ</unclear>[ῶς ποιήσεις <note anchored="yes" lang="en" place="not specified">?</note><gap extent="unknown" reason="illegible" unit="character"/> ] 

    <lb n="2"/>εἰ ὑγιαίνων ἥκεις <choice><corr>εἰ<supplied cert="high" reason="omitted">ς</supplied></corr><sic>ει</sic></choice> Ἀλεξ<supplied cert="high" reason="lost">ά</supplied><unclear cert="high" reason="undefined">νδ</unclear><supplied cert="high" reason="lost">ρειαν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>οὐκ ὠιόμην σε τοιοῦτον α<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>ἐμέ.  ἔτι καὶ νῦν δὲ <unclear cert="high" reason="undefined">ν</unclear><gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐπιστό</supplied>-

    <lb n="5"/><unclear cert="high" reason="undefined">λια</unclear> <choice><corr>ἡμῖν</corr><sic>ημειν</sic></choice>. γράφε δὲ <app type="BL"><lem resp="9.316">ὧν κα<supplied cert="high" reason="lost">ὶ </supplied><gap extent="unknown" reason="lost" unit="character"/></lem><rdg><seg cert="low" part="N"><unclear cert="high" reason="undefined">ὧ</unclear>ν ἂ<supplied cert="low" reason="lost">ν</supplied>[ χρείαν ἔχηις <note anchored="yes" lang="en" place="not specified">?</note><gap extent="unknown" reason="illegible" unit="character"/> ]</seg></rdg></app> 

    <lb n="6"/>ἀπὸ Μένφεως, γράφε δὲ τὰ <unclear cert="high" reason="undefined">περι</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/>καὶ δίδου Βαλλίωνι ὧν ἂν χρε<supplied cert="high" reason="lost">ίαν ἔχηι </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>οὐ γὰρ μὴ <choice><corr cert="low">ἀποτύχη<supplied cert="low" reason="omitted">ι</supplied>ς</corr><sic>αποτυχης</sic></choice>. ἀσπαζ[ <app type="BL"><lem resp="8.399"><gap extent="unknown" reason="illegible" unit="character"/> Μο]-

    <lb n="9"/>νιμον ὡς</lem><rdg><gap extent="unknown" reason="lost" unit="character"/>νιμονω?</rdg></app> παρ᾽ ἐμοῦ καὶ <app type="BL"><lem resp="8.399"><seg cert="low" part="N">Σ<unclear cert="high" reason="undefined">πινθῆ</unclear>[ρα καὶ τὸν ]</seg></lem><rdg>Σπ<gap extent="2" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/></rdg></app> 

