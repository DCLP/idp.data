    <lb n="4"/><gap extent="unknown" reason="lost" unit="character"/>αι τὴν ἄρουραν <expan><ex cert="high">δραχμὰς</ex></expan> <num value="3">γ</num> ἃ ἔσται φορτ<unclear cert="high" reason="undefined">ι</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/><gap extent="unknown" reason="lost" unit="character"/> δ<unclear cert="high" reason="undefined">ὲ</unclear> καὶ πλείονα ἔσεσθαι καὶ τὸ φορτίον <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ν</unclear>η. ἐὰν οὖν γράψηις πόσον δεῖ λαβεῖ<unclear cert="high" reason="undefined">ν</unclear> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">α</unclear>κειηται· εἰ δὲ μή, γράψον μοι ὅπως α<gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">κ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/><gap extent="unknown" reason="lost" unit="character"/>ι τοῦ καιροῦ μὴ ἔχειν χόρτον ἀλλὰ <unclear cert="high" reason="undefined">γ</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ν τούτων πρότερον εἴ τίς σοι ἀπέδ<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">κε </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/><gap extent="unknown" reason="lost" unit="character"/>ι τὸν μόσχον τὸν ἀπὸ τοῦ Εἰσιείου τῆι <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="low" reason="lost">ἐν</supplied>θυμῆι μόνος ὤν, καὶ ἐστὶν κεφαλ<gap extent="unknown" reason="lost" unit="character"/> 

