    <lb n="2"/><gap extent="3" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">δ</unclear><gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="illegible" unit="character"/> <expan>π<ex cert="high">αρὰ</ex></expan> Πα<gap extent="4" reason="lost" unit="character"/>ρα τοῦ ἀδελφοῦ </ab></div></div><div n="meta.31222" org="uniform" part="N" sample="complete" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.31222" org="uniform" part="N" sample="complete" subtype="textDate" type="commentary"><p><date notAfter="300" notBefore="201" type="textDate">III</date></p></div><div n="meta.31222" org="uniform" part="N" sample="complete" subtype="principalEdition" type="bibliography"><listBibl default="NO"><bibl default="NO" subtype="principal" type="publication"><title level="s" type="abbreviated">PSI</title><biblScope type="volume">3</biblScope><biblScope type="numbers">206</biblScope></bibl><bibl default="NO" type="Trismegistos"><biblScope type="numbers">31222</biblScope></bibl><bibl default="NO" type="DDbDP"><series>0238</series>; <biblScope type="volume">3</biblScope>; <biblScope type="numbers">206</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0238;3;206" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.31222" org="uniform" part="N" sample="complete" subtype="illustrations" type="bibliography"><p>keine</p></div><div n="meta.31222" org="uniform" part="N" sample="complete" subtype="locations" type="history"><p><placeName cert="high" full="yes" type="ancientFindspot">Oxyrhynchos</placeName></p></div></body></text></TEI.2>