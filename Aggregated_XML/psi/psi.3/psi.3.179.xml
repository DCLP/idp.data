    <lb n="5"/><expan><hi rend="diaeresis">ἰ</hi>νδ<ex cert="high">ικτίονος</ex></expan> <app type="BL"><lem resp="8.393"><num value="5">ε</num></lem><rdg><num value="6"><unclear reason="undefined" cert="high">ϛ</unclear></num></rdg></app>. 

