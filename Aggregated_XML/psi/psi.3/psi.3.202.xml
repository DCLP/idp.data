    <lb n="28"/>αὐτῆς πόλεως <supplied cert="high" reason="lost">διὰ</supplied> 

    <lb n="29"/>Ἱέρακος. 

    <lb n="30"/>προσφωνοῦμε<supplied cert="high" reason="lost">ν</supplied> 

    <lb n="31"/>ἰδίῳ τιμήματι <supplied cert="high" reason="lost">τὴν</supplied> 

    <lb n="32"/>ἑξῆς ἐγ<g type="apostrophe"/>γεγραμμ<supplied cert="high" reason="lost">ένην</supplied> 

    <lb n="33"/>τιμὴν ὧν χειρίζο-

    <lb n="34"/><choice><corr>μεν</corr><sic>χιρι<unclear cert="high" reason="undefined">ζο</unclear>μεν</sic></choice> ὠνίων εἶ<supplied cert="high" reason="lost">ναι</supplied> 

    <lb n="35"/>ἐπὶ τοῦδε τοῦ <expan>μη<ex cert="high">νός</ex></expan>, <supplied cert="high" reason="lost">καὶ</supplied> 

    <lb n="36"/>ὀμνύομεν τὸν θε<supplied cert="high" reason="lost">ῖον</supplied> 

    <lb n="37"/>ὅρκον μηδὲν διεψ<supplied cert="high" reason="lost">εῦσθαι</supplied>. 

    <lb n="38"/>ἐστὶ δέ· 

    <lb n="39"/><hi rend="diaeresis">ἰ</hi>χθύων παντοίω<supplied cert="high" reason="lost">ν</supplied> 

