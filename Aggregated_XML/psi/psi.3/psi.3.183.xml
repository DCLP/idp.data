    <lb n="2"/>Φλαουία Κ<supplied reason="lost" cert="high">αρτερ</supplied>ία ἡ καὶ <app type="BL"><lem resp="6.173">Κ<unclear reason="undefined" cert="high">υρα</unclear>πάντων</lem><rdg>Κ<gap reason="illegible" extent="3" unit="character"/>πάντων</rdg></app>, θυγάτη<supplied reason="lost" cert="high">ρ</supplied> τοῦ τῆς <unclear reason="undefined" cert="high">εὐ</unclear>λ<supplied reason="lost" cert="high">α</supplied>β<supplied reason="lost" cert="high">οῦ</supplied>ς μνήμης Αὐσονίου γεναμένου πρεσβυτέρου δια<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>Φλαουίῳ <gap reason="illegible" extent="2" unit="character"/>ι<gap reason="illegible" extent="2" unit="character"/> τῷ καθ<supplied reason="lost" cert="high">ωσιωμ</supplied>ένῳ στρατηλ<supplied reason="lost" cert="high">α</supplied>τιανῷ <supplied reason="lost" cert="high">ὁ</supplied>μογνησί<supplied reason="lost" cert="high">ῳ μου ἀδ</supplied>ελφ<supplied reason="lost" cert="high">ῷ </supplied><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">ι</unclear>ῳ τῷ αὐτῷ εὐλαβεστάτ<supplied reason="lost" cert="high">ῳ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>χαίρειν.  ὁμολογῶ διὰ <supplied reason="lost" cert="high">ταύτης μου</supplied> τῆς ἐγγράφου ἀσφ<supplied reason="lost" cert="high">α</supplied>λείας ἐσχηκέναι παρὰ σοῦ ἐν διαφόροις <choice><corr>κοσμίοι<supplied reason="omitted" cert="high">ς</supplied></corr><sic>κοσμιοι</sic></choice> τ<gap reason="lost" extent="unknown" unit="character"/> 

