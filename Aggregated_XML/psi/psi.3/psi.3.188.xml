    <lb n="1"/>χμγ 

    <lb n="2"/>μετὰ τὴν ὑπατεί<supplied reason="lost" cert="high">αν</supplied> Φλαυί<supplied reason="lost" cert="high">ο</supplied>υ Ἀπίωνος τοῦ ἐνδ<supplied reason="lost" cert="high">οξοτάτου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>Αὐρηλίῳ Ἰωσηφίῳ Ἀβρααμίου τῷ θα<supplied reason="lost" cert="high">υ</supplied>μ<supplied reason="lost" cert="high">ασιωτάτῳ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>καὶ Ὧρος ἀδελφὸς καὶ Θεοδόσιος οἰκέτα<supplied reason="lost" cert="high">ι </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἀπὸ</supplied> 

    <lb n="5"/>κώμης Τέρτον Σαμοού.  ὁμολογοῦμεν ἀλλ<supplied reason="lost" cert="low">ηλέγγυοι ὄντες εἰς ἔκτισιν ἑκουσίως καὶ αὐ</supplied>-

    <lb n="6"/>θαιρέτως μεμισθῶσθαι παρὰ <choice><corr>τῆ<supplied reason="omitted" cert="high">ς</supplied></corr><sic>τη</sic></choice> <choice><corr>σῆ<supplied reason="omitted" cert="high">ς</supplied></corr><sic>ση</sic></choice> θαυμ<supplied reason="lost" cert="high">ασιότητος  ἐπὶ χρόνον </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> λογιζόμενον ἀπὸ</supplied> 

    <lb n="7"/>καρπῶν τῆς σὺν θεῷ τετάρτης <expan><hi rend="diaeresis">ἰ</hi>νδ<ex cert="high">ικτίονος</ex></expan> τὸ ὑ<supplied reason="lost" cert="high">πάρχον σοι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>μέρος ὁλοκλήρου κτήματος <choice><corr>ἐφύδρου</corr><sic>επιυδρου</sic></choice> διακει<supplied reason="lost" cert="high">μένου ἐν </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> μετὰ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>καὶ λάκκου καὶ ξυλικοῦ ὀργάνου εἰς σπορὰν καὶ κα<supplied reason="lost" cert="low">τάθεσιν </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> φόρου κα</supplied>-

    <lb n="10"/>θ᾽ ἕκαστον ἐνιαυτὸν ἀποτάκτου ἐν τῷ συμβρόχ<supplied reason="lost" cert="high">ῳ καὶ ἀβρόχῳ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἀρτάβας</supplied> 

    <lb n="11"/>τριακονταπέντε <num value="35"/> νομισμάτια πέντε <num value="5"/> παρὰ κεράτια <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>ἄλλης ὑποθήκης πρὸς τὴν <choice><corr>δύναμιν</corr><sic>δυναμειν</sic></choice> αὐτῆς εἶναι βε<supplied reason="lost" cert="high">βαίαν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>καὶ <expan>ἐπερ<ex cert="low">ωτηθέντες</ex></expan> <expan>ὡμολ<ex cert="high">ογήσαμεν</ex></expan>. <handShift new="m2"/> <gap reason="illegible" extent="4" unit="character"/><unclear reason="undefined" cert="high">ι</unclear>ος Πέτρο<unclear reason="undefined" cert="high">υ</unclear> <expan><unclear reason="undefined" cert="high">πρ</unclear><ex cert="high">εσβύτερος</ex></expan> <unclear reason="undefined" cert="high">ἀπὸ</unclear> <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> μαρτυρῶ τῇ μισθώσει ἀκούσας</supplied> 

    <lb n="14"/>παρὰ τῶν θεμένων. Ὧρο<supplied reason="lost" cert="low">ς</supplied><supplied reason="lost" cert="high"> τοῦ δεῖνα ἀπὸ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> μαρτυρῶ τῇ μισθώσει ἀκού</supplied>-

    <lb n="15"/>σα<supplied reason="lost" cert="high">ς παρὰ τ</supplied>ῶν θεμένων.  <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> Ὡρ<unclear reason="undefined" cert="high">ο</unclear><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἔγραψα ὑπὲρ αὐτῶν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

