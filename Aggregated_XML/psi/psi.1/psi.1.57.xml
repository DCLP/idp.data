    <lb n="1"/>Ἔ<unclear cert="high" reason="undefined">ρ</unclear>ω<unclear cert="high" reason="undefined">τι</unclear> <unclear cert="high" reason="undefined">στρατ</unclear>ηγῶι 

    <lb n="2"/>Θεμίστου μερίδος 

    <lb n="3"/>παρὰ Σαμβᾶ τοῦ υἱοῦ 

    <lb n="4"/>Σωτηρίχου τῶν ἀπὸ 

    <lb n="5"/>Θεαδαλφέας τῆς <expan>προκ<ex cert="high">ειμένης</ex></expan> <expan>μερ<ex cert="high">ί</ex>δ<ex cert="high">ος</ex></expan>. 

    <lb n="6"/><choice><corr>γεωργῶ</corr><sic>γεωργωι</sic></choice> σὺν <unclear cert="high" reason="undefined">τ</unclear>ῶι ἀδελφῷ 

    <lb n="7"/>μου Θεφερ<supplied cert="high" reason="lost">ο</supplied>ῦτι κλῆρον 

    <lb n="8"/><expan>ἀρο<ex cert="high">υρῶν</ex></expan> πέντε <num value="5"/> π<supplied cert="high" reason="lost">ε</supplied>ρὶ τὴν <expan>αὐτὴ<ex cert="high">ν</ex></expan> 

    <lb n="9"/>κώμην, ἃ<supplied cert="high" reason="lost">ς ἐ</supplied>μισθώσατο 

    <lb n="10"/>ὁ πατὴρ ἡμῶν <expan>Σωτήριχ<ex cert="high">ος</ex></expan>, 

    <lb n="11"/>ὃς <expan>τετελ<ex cert="high">εύτηκεν</ex></expan>, π<supplied cert="high" reason="lost">α</supplied>ρὰ <expan>Ἀπολλω<ex cert="high">νίου</ex></expan> 

    <lb n="12"/>τοῦ Πτολε<unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">αί</supplied>ου. τοῦ οὖν 

    <lb n="13"/><unclear cert="high" reason="undefined">διὰ</unclear> <unclear cert="high" reason="undefined">τῆς</unclear> μ<supplied cert="high" reason="lost">ισ</supplied>θώσεως 

    <lb n="14"/><supplied cert="high" reason="lost">ἡμῖν ἐκδ</supplied>οθέντος 

    <lb n="15"/><supplied cert="high" reason="lost">κλήρου τετελεκότες</supplied> 

    <lb n="16"/><supplied cert="high" reason="lost">τὰ τοῦ διεληλυθό</supplied>-

    <lb n="17"/>τος <num value="11">ια</num> <expan><ex cert="high">ἔτους</ex></expan> <supplied cert="high" reason="lost">Τ</supplied>ιβερίου 

    <lb n="18"/>Κλαυδίου Καίσαρος 

    <lb n="19"/>Σεβαστοῦ Γερμανικοῦ 

    <lb n="20"/>Αὐτοκράτορος ἐκφόρια, 

    <lb n="21"/>προήγμ<supplied cert="high" reason="lost">ε</supplied>θα ἐγβῆ-

    <lb n="22"/>ναι τῆς τοῦ <expan>κλή<ex cert="high">ρου</ex></expan> <expan>γεωργ<ex cert="high">ίας</ex></expan> 

    <lb n="23"/>τὸ καθῆ<unclear cert="high" reason="undefined">κον</unclear> <choice><corr>ἐπιτελέσαντες</corr><sic>επιτελεσας</sic></choice>· 

    <lb n="24"/>δι᾽ ὃ <choice><corr>ἀξιῶ</corr><sic>αξιωι</sic></choice>, <choice><corr>καταχωρισθέντος</corr><sic>κατακωρισθεντος</sic></choice> 

    <lb n="25"/>παρά σοι τοῦδε τοῦ <expan>ὑπομνή<ex cert="high">ματος</ex></expan>, 

    <lb n="26"/>τὸ <expan>ἀντίγρα<unclear cert="high" reason="undefined">φ</unclear>ο<ex cert="high">ν</ex></expan> δι᾽ ὑπηρέτου 

    <lb n="27"/>μεταδοθῆναι τῷ τοῦ 

    <lb n="28"/>Ἀπολλωνίου <expan>ἀδελ<ex cert="high">φῷ</ex></expan> Ἀμέσῳ, 

    <lb n="29"/>διὰ τὸ τὸν <expan>Ἀπολ<ex cert="high">λώνιον</ex></expan> <expan>τετελ<ex cert="high">ευτηκέναι</ex></expan>, 

    <lb n="30"/>ὅπως εἰδῇ <choice><corr>ἐκβαίνοντα<supplied cert="high" reason="omitted">ς</supplied></corr><sic>εγβαινοντα</sic></choice> 

    <lb n="31"/>ἡμᾶς τῆς <unclear cert="high" reason="undefined">γ</unclear><supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">ω</unclear>ργίας ἑτοί-

    <lb n="32"/><unclear cert="high" reason="undefined">μ</unclear>ω<unclear cert="high" reason="undefined">ς</unclear> <supplied cert="high" reason="lost">ἔχειν </supplied><gap extent="unknown" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="illegible" unit="character"/> 
