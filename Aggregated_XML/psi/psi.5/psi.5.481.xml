    <lb n="3"/><g type="stauros"/> τῷ κυρίῳ μου <expan>ἀδελφ<ex cert="low">ῷ</ex></expan> <gap reason="illegible" extent="3" unit="character"/> </ab></div></div><div n="meta.35226" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.35226" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" notBefore="401" notAfter="600">V - VI</date></p></div><div n="meta.35226" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Rückseite: SB XVI 12254.</p></div><div n="meta.35226" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">PSI</title><biblScope type="volume">5</biblScope><biblScope type="numbers">481</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">35226</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0238</series>; <biblScope type="volume">5</biblScope>; <biblScope type="numbers">481</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0238;5;481" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.35226" type="bibliography" subtype="corrections" org="uniform" part="N" sample="complete"><head>BL-Einträge nach BL-Konkordanz</head><listBibl default="NO"><bibl type="BL" default="NO"><biblScope type="volume">VIII</biblScope>, S. <biblScope type="pages">398/399</biblScope></bibl><bibl type="BL" default="NO"><biblScope type="volume">XI</biblScope>, S. <biblScope type="pages">245</biblScope></bibl></listBibl></div><div n="meta.35226" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p><bibl type="illustration" default="NO">Pap.Flor. XXX, Tav. CLVII</bibl></p></div><div n="meta.35226" type="history" subtype="locations" org="unifor