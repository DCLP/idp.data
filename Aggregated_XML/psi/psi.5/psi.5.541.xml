    <lb n="1"/><supplied reason="lost" cert="high">β</supplied>ασιλεῖ μ<supplied reason="lost" cert="high">εγ</supplied>άλωι Πτολεμαίω<unclear reason="undefined" cert="high">ι</unclear> 

