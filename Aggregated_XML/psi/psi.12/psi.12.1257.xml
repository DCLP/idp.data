    <lb n="1"/><num value="104">ρδ</num> 

    <lb n="2"/>Αὐρηλίῳ Θέωνι τῷ καὶ Διο<unclear cert="high" reason="undefined">ν</unclear><gap extent="5" reason="lost" unit="character"/><supplied cert="high" reason="lost"> φυ</supplied>-

    <lb n="3"/>λάρχῃ τῆς Ὀξυρυγχειτῶν <supplied cert="high" reason="lost">πόλεως</supplied> 

    <lb n="4"/>τοῦ ἐνεστῶτος <num value="7">ζ</num> <expan><ex cert="high">ἔτους</ex></expan> 

    <lb n="5"/>παρὰ Αὐρηλίων Ἱέρακος υἱ<supplied cert="high" reason="lost">οῦ </supplied><gap extent="5" reason="lost" unit="character"/> 

    <lb n="6"/>τοῦ καὶ Ἡρακλᾶ ἀγορανομήσ<supplied cert="high" reason="lost">αντος τῆς</supplied> 

    <lb n="7"/>Ὀξυρυγχειτῶν πόλεως καὶ <gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Δι</supplied>-

    <lb n="8"/>οσκᾶτος μητρὸς Τανεχώ<supplied cert="low" reason="lost">τιδος</supplied><supplied cert="high" reason="lost"> ἀ</supplied>-

    <lb n="9"/>πὸ τῆς αὐτῆς πόλεως μετ<unclear cert="high" reason="undefined">ὰ</unclear> <supplied cert="high" reason="lost">συνεστῶ</supplied>-

    <lb n="10"/>τος τοῦ αὐτοῦ Ἱέρακος.  βουλό<supplied cert="high" reason="lost">μεθα πρώ</supplied>-

    <lb n="11"/>τως ἀπὸ τοῦ νῦν ἀναγρα<supplied cert="high" reason="lost">φῆναι ἐφ᾽ οὗ</supplied> 

    <lb n="12"/>ἔχει ὁ Ἱέραξ ἐν τῇ αὐτῇ πό<supplied cert="high" reason="lost">λει ἐπ᾽ ἀμ</supplied>-

    <lb n="13"/>φόδου Παμμένους Παραδείσ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">υ μέρους</supplied> 

    <lb n="14"/>οἰκίας τοὺς γεγονότας τ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="15"/>καὶ τῷ μετηλλαχό<unclear cert="high" reason="undefined">τ</unclear>ι αὐ<unclear cert="high" reason="undefined">τῆ</unclear><supplied cert="high" reason="lost">ς ἀνδρὶ</supplied> 

    <lb n="16"/>Διονυσίῳ τῷ καὶ Πλουτάρχῳ Π<supplied cert="high" reason="lost">λου</supplied>-

    <lb n="17"/>τάρχου μητρὸς Τααρπαήσιος ἀπ<supplied cert="high" reason="lost">ὸ τῆς</supplied> 

    <lb n="18"/>αὐτῆς πόλεως υἱοὺς δύο <num value="2"/> <expan><ex cert="high">δωδεκαδράχμους</ex></expan> ἀπ<supplied cert="high" reason="lost">ὸ γυ</supplied>-


    <lb n="19"/>μνασίου ὄντας πρὸς τὸ ἐνεστ<supplied cert="high" reason="lost">ὸς</supplied> 

    <lb n="20"/><num value="7">ζ</num> <expan><ex cert="high">ἔτος</ex></expan> Αὐρηλίους Ἀσκλᾶν <unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ὲν ἐτῶν</supplied> 

    <lb n="21"/>ἕξ <num value="6"/>, Ἰσίδωρον δὲ τὸν καὶ <supplied cert="high" reason="lost">Πανε</supplied>-

    <lb n="22"/>χώτην ἐτῶν δύο <num value="2"/>· <unclear cert="high" reason="undefined">διὸ</unclear> <unclear cert="high" reason="undefined">ἡ</unclear>μεῖ<unclear cert="high" reason="undefined">ς</unclear> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="23"/><unclear cert="high" reason="undefined">καὶ</unclear> ὀμνύομεν τὸν ἔθιμον<gap extent="unknown" reason="lost" unit="character"/> 

