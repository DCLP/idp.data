    <lb n="8"/><handShift new="m1"/> <gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> νομάρχῃ Ἀντινό</supplied>ου πόλεως 

    <lb n="9"/><supplied cert="high" reason="lost">παρὰ Ἑρμίου Διοσκόρου τοῦ καὶ Ἁ</supplied>ρπάλου Χαιρήμονος Ἁδριανείο<supplied cert="high" reason="lost">υ τοῦ κα</supplied>ὶ Μουσηγετείου βουλευτοῦ Ἀντινοέων η<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐνεχυρασί</supplied>ας χρηματισμοῦ ἀντίγραφον <supplied cert="high" reason="lost">ὑπόκειτ</supplied>αι, καὶ ἀξιῶ τὸ ἀκόλουθον γενέσθ<supplied cert="high" reason="lost">αι</supplied>. 

    <lb n="11"/><supplied cert="high" reason="lost"><expan><ex cert="high">ἔτους</ex></expan> <num value="2">β</num> Αὐτοκράτορος Καίσαρος Μάρκο</supplied>υ Αὐρηλίου Ἀντωνίνου σεβα<supplied cert="high" reason="lost">στοῦ κ</supplied>αὶ Αὐτοκράτορος Καίσαρος Λουκίου Αὐρη<supplied cert="high" reason="lost">λίου</supplied> 

    <lb n="12"/><supplied cert="high" reason="lost">Οὐήρου σεβαστοῦ, </supplied><gap extent="unknown" reason="lost" unit="character"/> Ἡρακλείδης ὁ ἱερεὺς καὶ ἀρχι<supplied cert="high" reason="lost">δικ</supplied>αστὴς Ἀντινόου νομάρχῃ χαίρειν.  <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/><gap extent="unknown" reason="lost" unit="character"/> ἵν᾽ εἰδῇς.  ἔρρωσο.  <expan><ex cert="high">ἔτους</ex></expan> <num value="2">β</num> Αὐτοκράτορος Καίσαρος Μάρκου Αὐρηλίου Ἀν<supplied cert="high" reason="lost">τω</supplied>-

    <lb n="14"/><supplied cert="high" reason="lost">νίνου σεβαστοῦ καὶ Αὐτοκράτορος Καίσα</supplied>ρος Λουκίου Αὐρηλίου Οὐήρου, Μεχεὶρ <num value="7">ζ</num>.  Οὔλπιος Δίδυμος <expan>σεσημ<ex cert="high">είωμαι</ex></expan><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="15"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/> <expan><ex cert="high">ἔτους</ex></expan> <num value="2">β</num> Αὐτοκράτορος Καίσαρος Μάρκου Αὐρηλίου Ἀντωνίνου σεβ<supplied cert="high" reason="lost">αστοῦ</supplied> 

    <lb n="16"/><supplied cert="high" reason="lost">καὶ Αὐτοκράτορος Καίσαρος Λουκίου</supplied> Αὐρηλίου Οὐήρου σεβαστοῦ, Τῦβι. ἀναγνωσθείσης ἐντεύξεως Ἑρ<supplied cert="high" reason="lost">μίου</supplied> 

    <lb n="17"/><supplied cert="high" reason="lost">Διοσκόρου τοῦ καὶ Ἁρπάλου, τῆς</supplied> δὲ δι᾽ αὐτῆς σημαινομένης καταγραφῆς ὑπογεγραμμένης ἐπενηνεγμέν<supplied cert="high" reason="lost">ης καὶ</supplied> 

    <lb n="17a"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="illegible" unit="character"/> ὑπογεγραφότος διαπεστάλθαι 

    <lb n="18"/><supplied cert="high" reason="lost">τοῦ λεγομένου διαπεστάλθαι </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> πρὸς τὴ</supplied>ν τοῦ χρηματισμοῦ τελείωσιν, <choice><corr>συνεκρίναμεν</corr><sic>συνεκρειναμεν</sic></choice> γραφῆναι καὶ πεμφθῆ<supplied cert="high" reason="lost">ναι</supplied> 

    <lb n="19"/><supplied cert="high" reason="lost">τῆς ἐντεύξεως ἀντίγραφον τῷ τῆς Ἀντινόου πόλεως</supplied> νομάρχῃ, <hi rend="diaeresis">ἵ</hi>ν᾽, ἐὰν παρὼν ἐπ᾽ αὐτὸν ὁ Ἑρμίας εὐδοκῇ τούτ<supplied cert="high" reason="lost">οις</supplied> 

    <lb n="20"/><supplied cert="high" reason="lost">καὶ προῆται τὴν καθήκουσαν χειρογραφί</supplied>αν περὶ τοῦ ἀληθῆ εἶναι τὰ διὰ τῆς ἐντεύξεως δεδηλωμένα καὶ μηδὲν <unclear cert="high" reason="undefined">ὑ</unclear><supplied cert="high" reason="lost">πεναν</supplied>-

    <lb n="21"/><supplied cert="high" reason="lost">τίον ᾠκονομῆσθαι, τὸ τηνικαῦτα, προδιαστει</supplied>λάμενος τοῖς οὖσι ἐν τοῖς καταγεγραμμένοις ἐκχωρεῖν ἐ<supplied cert="high" reason="lost">κ</supplied> 

    <lb n="22"/><supplied cert="high" reason="lost">τούτων ἐν ἡμέραις δέκα <num value="10"/>, μετὰ ταύτας ποιῆτ</supplied><unclear cert="high" reason="undefined">αι</unclear> τὰ τῆς <unclear cert="high" reason="undefined">ἐνβα</unclear>δείας ὃν τρόπον καθήκει τοῖς προστεταγμέν<supplied cert="high" reason="lost">οις</supplied> 

    <lb n="23"/><supplied cert="high" reason="lost">ἀκολούθως. </supplied><gap extent="unknown" reason="lost" unit="character"/> ὑπάρχοντα ἀνεξαλλοτρίωτα καὶ ἀκαταχρημάτιστα μέχρι τε οὗ οι οφ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="24"/><gap extent="unknown" reason="lost" unit="character"/> ἐνιαύσιος χρόνος.  Μάρκωι <supplied cert="high" reason="lost">Ἀ</supplied>ννί<unclear cert="high" reason="undefined">ω</unclear>ι Συριακῶι Ἑρμίας Διοσκόρου <supplied cert="high" reason="lost">τοῦ καὶ</supplied> 

    <lb n="25"/><supplied cert="high" reason="lost">Ἁρπάλου Χαιρήμονος Ἁδριάνειος ὁ καὶ Μο</supplied>υσηγέτειος βο<unclear cert="high" reason="undefined">υ</unclear>λευτὴς Ἀντινοέων.  κατὰ δημόσιον χρηματισμὸν <supplied cert="high" reason="lost">τελει</supplied>-

    <lb n="26"/><supplied cert="high" reason="lost">ωθέντα </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐν</supplied> τῷ ἐνεστῶτι μηνὶ Ἁδριανῷ καταγέγραμμαι κατὰ τὰ προστεταγμέν<supplied cert="high" reason="lost">α  </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="27"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/>ανους Ἁδριανείου τοῦ καὶ Μουσηγετείου τετελευτηκότος ε<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="28"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Δι</supplied>οσκόρου καὶ Σατορνείλῳ ἐκ μητρὸς Κορνηλίας τῆς <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="29"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀ</supplied>δελφοῦ αὐτοῦ μετηλλαχότος μετ᾽ ἐπιτρόπων καταστ<gap extent="unknown" reason="lost" unit="character"/> 

