    <lb n="1"/>Ἀδδαῖος Ζήνω<unclear reason="undefined" cert="high">νι</unclear> χ<unclear reason="undefined" cert="high">αίρειν</unclear>. τὴν ἐπιστολὴν ἐκομι<unclear reason="undefined" cert="high">σ</unclear><supplied reason="lost" cert="high">άμην </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>περὶ τῶν κιθαν<unclear reason="undefined" cert="high">αλλ</unclear>ων· <unclear reason="undefined" cert="high">νῦ</unclear>ν δὲ λαβὼν παράδειγμα εκ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>παιδίσκαις ἵνα κ<unclear reason="undefined" cert="high">ατα</unclear><supplied reason="lost" cert="high">σ</supplied>κε<unclear reason="undefined" cert="high">υ</unclear>ασ<unclear reason="undefined" cert="high">θ</unclear>ῆι· ἃ δὲ οὐκ ἔφασαν δ<unclear reason="undefined" cert="high">ύ</unclear>νασ<unclear reason="undefined" cert="high">θα</unclear><supplied reason="lost" cert="high">ι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>τεχνίταις ουδ<gap reason="illegible" extent="3" unit="character"/>τι δὲ ἔφα<unclear reason="undefined" cert="high">σαν</unclear> <gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="3" unit="character"/><unclear reason="undefined" cert="high">τ</unclear>α γενε<gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἔ</supplied>-

    <lb n="5"/>φατο ποιήσειν καὶ ηγ<gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="2" unit="character"/>ε<gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/> ἐ<unclear reason="undefined" cert="high">λαβ</unclear><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>ἀπέδωκεν οὐ φάμενος δ<unclear reason="undefined" cert="high">ύν</unclear>ασθαι ποιῆσαι. διὰ τὸ δὲ γενέ<supplied reason="lost" cert="low">σθαι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>τοῦ παρόντος οὐθένα η<unclear reason="undefined" cert="high">ὑ</unclear>ρήκαμεν. διὸ οὔπω δεδυνάσμεθα <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>πειράσομαι αὐτάρκειαν <unclear reason="undefined" cert="high">τοῦ</unclear> που ἐγδοῦναι. ἔγραψας δέ μ<unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">ι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>λωσιν. ἕτοιμαι οὖν εἰσιν. ἐὰν οὖν πλοῖον εὕρω ἀποσταλ<unclear reason="undefined" cert="high">ε</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>έταξας ποιή<unclear reason="undefined" cert="high">σω</unclear>. <unclear reason="undefined" cert="high">γί</unclear>νωσκε δὲ καὶ τὰς κιρίας τὰς παρὰ Ζ<unclear reason="undefined" cert="high">η</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>ἐνέλειπον δέ τι <unclear reason="undefined" cert="high">πα</unclear><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">α</unclear><gap reason="illegible" extent="2" unit="character"/><unclear reason="undefined" cert="high">ε</unclear><gap reason="illegible" extent="3" unit="character"/> <unclear reason="undefined" cert="high">Σα</unclear>ρ<unclear reason="undefined" cert="high">α</unclear>πί<unclear reason="undefined" cert="high">ωνι</unclear> ς γεγρα<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>Ζηνόδωρος δύν<unclear reason="undefined" cert="high">ασθαι</unclear> ὸ<unclear reason="undefined" cert="high">ς</unclear> διδάξ<unclear reason="undefined" cert="high">α</unclear>ι τὴν τέχνην οὐδ᾽ ἂν Ζη<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>ἀπειθοῦντα οὔτε τὸ ἔλαιον οὔτε τὸ ὀψώνιον αὐτῶι ἐδωκ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/>καὶ τὰς παιδίσκας κα<unclear reason="undefined" cert="high">σ</unclear>ιαι <unclear reason="undefined" cert="high">κ</unclear>αθάπερ μοι ἔγραψα<unclear reason="undefined" cert="high">ς</unclear> πορευθ<unclear reason="undefined" cert="high">ει</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/>τὰς παιδίσκας τὰς κ<unclear reason="undefined" cert="high">α</unclear><gap reason="illegible" extent="3" unit="character"/>ε<unclear reason="undefined" cert="high">ι</unclear>ας δεδεμένας καὶ ἔφη ἀπολωλε<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>κε δὲ ε<unclear reason="undefined" cert="high">μου</unclear><gap reason="illegible" extent="2" unit="character"/>τα<unclear reason="undefined" cert="high">τ</unclear>ου <gap reason="illegible" extent="unknown" unit="character" desc="vestiges"/> <unclear reason="undefined" cert="high">υπ</unclear>ε<unclear reason="undefined" cert="high">ρβ</unclear>αντα<gap reason="illegible" extent="4" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/>μαλακῶν ἀποστέ<unclear reason="undefined" cert="high">λλ</unclear>ων μο<unclear reason="undefined" cert="high">ι</unclear>, ἐάν σοι δο<unclear reason="undefined" cert="high">κῆι</unclear>, παιδίσκην μίαν <num value="1"/> <unclear reason="undefined" cert="high">ι</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/>αὐλῆι προσέχηι κα<unclear reason="undefined" cert="high">ὶ</unclear> <gap reason="illegible" extent="1" unit="character"/>ν<gap reason="illegible" extent="3" unit="character"/>ν <unclear reason="undefined" cert="high">πα</unclear>ρῆι. 
