    <lb n="1"/><g type="stauros"/> σὺν θεῷ παρακλητικὴ ἱκετ<unclear cert="high" reason="undefined">η</unclear><supplied cert="high" reason="lost">ρ</supplied>ία προσε<supplied cert="high" reason="lost">ν</supplied>εχθεῖσα τῷ <choice><corr>ἐνδοξοτάτῳ</corr><sic>ενδοξ<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">τ</supplied>ατω</sic></choice> καὶ εὐκλεε<supplied cert="low" reason="lost">στάτῳ δουκὶ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>πάντων μὲν προηγείσθω ἡ το<unclear cert="high" reason="undefined">ῦ</unclear> δεσπότου Χριστοῦ χάρις καὶ ἀλήθεια ἧς κ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>αἰτῶ τὰ τίμια <hi rend="diaeresis">ἴ</hi>χνη τῶν ποδῶν τῆς <hi rend="diaeresis">ὑ</hi>μετέρας ἐνδόξου φιλανθρωπίας κ<supplied cert="high" reason="lost">αὶ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>ἔνδοξον μέγ<supplied cert="high" reason="lost">εθο</supplied>ς μὴ παρ<hi rend="diaeresis">ι</hi>δεῖν τὴν ε<gap extent="5" reason="lost" unit="character"/>ένην μοι πανταχόθεν <unclear cert="high" reason="undefined">χ</unclear><gap extent="unknown" reason="lost" unit="character"/> 


    <lb n="5"/>ἐπαρχείαν κα<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">οι</supplied>κοῦσιν γονικῆς μο<unclear cert="high" reason="undefined">υ</unclear> <supplied cert="high" reason="lost">οὐσί</supplied>ας καὶ <hi rend="diaeresis">ὑ</hi>ποστάσεως ης <gap extent="unknown" reason="lost" unit="character"/> 

