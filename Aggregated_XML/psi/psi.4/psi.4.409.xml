    <lb n="1"/>ὑπόμνημα Ἀρτεμιδώρωι 

    <lb n="2"/>παρὰ Ἱναρωῦτος μοσχοτρόφου. 

    <lb n="3"/>τῶν περιόντων μόσχων, ὧν καὶ 

    <lb n="4"/>λόγον δεδώκαμε<supplied cert="high" reason="lost">ν Ἀπολλ</supplied>ωνίωι, 

    <lb n="5"/>τὸ καθ᾽ ἓν <num value="1"/> γραψ<unclear cert="high" reason="undefined">α</unclear><gap extent="10" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ν</unclear> 

    <lb n="6"/>ἐνεγκάντων <gap extent="13" reason="lost" unit="character"/> 

    <lb n="7"/>ὡσαύτως δὲ κ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ὶ </supplied><gap extent="3" reason="lost" unit="character"/>μιαι τ<unclear cert="high" reason="undefined">οῦ</unclear> 

    <lb n="8"/><gap extent="5" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> εἰσιν οἱ <unclear cert="high" reason="undefined">π</unclear>ε<supplied cert="high" reason="lost">ριόν</supplied>τες 

    <lb n="9"/>μόσχοι <num value="95">ϙε</num> <expan><ex cert="high">ὧν</ex></expan> <app type="editorial"><lem><supplied cert="high" reason="lost">εἰσι</supplied></lem><rdg><gap extent="4" reason="lost" unit="character"/></rdg></app> βασιλι-

    <lb n="10"/>κοὶ <num value="14">ιδ</num> Ἀπο<unclear cert="high" reason="undefined">λ</unclear><supplied cert="high" reason="lost">λ</supplied><unclear cert="high" reason="undefined">ω</unclear>νίου <num value="81">πα</num> <expan><ex cert="high">ὧν</ex></expan> 

    <lb n="11"/>ἐπέλεξεν εἰς τὴν Πεντα-

    <lb n="12"/>ετηρίδα <num value="42">μβ</num>· λοιποὶ περιε-

    <lb n="13"/>λείφθησαν εἰς τὰς θυσίας <num value="29">κθ</num> <expan><ex cert="high">ὧν</ex></expan> 

    <lb n="14"/>εἰσὶν πρεσβῦται ἀχρεῖοι <num value="11">ια</num> <expan><ex cert="high">ὧν</ex></expan> 

    <lb n="15"/>παρ᾽ Ἀπολλωνίου τοῦ ἑρμηνέως 

    <lb n="16"/><num value="6">ϛ</num>, παρὰ Ἡρακλεοδώρου <num value="1">α</num>, 

    <lb n="17"/>παρὰ Διοτίμου <num value="1">α</num>, παρὰ Ζω-

    <lb n="18"/>ίλου <num value="1">α</num> <del rend="erasure" status="unremarkable">α</del> παρὰ Ζήνωνος ἄγριοι 

    <lb n="19"/><num value="2">β</num>· <expan><ex cert="high">γίνονται</ex></expan> <num value="11">ια</num>· <expan><ex cert="high">λοιποὶ</ex></expan> <num value="28">κη</num> <expan><ex cert="high">ὧν</ex></expan> ἐπέλε-

    <lb n="20"/>ξεν τοῦ Ἁθὺρ <num value="26">κϛ</num> ὥστε εἰς 

    <lb n="21"/>Κάνωπον <num value="18">ιη</num> καὶ εἰς τὸ Κα-

    <lb n="22"/>ρικὸν ἐτύθη <num value="1">α</num>· <expan><ex cert="high">γίνονται</ex></expan> <num value="19"><unclear cert="high" reason="undefined">ιθ</unclear></num>· <expan><ex cert="high">λοιποὶ</ex></expan> <num value="9">θ</num>. 

    <lb n="23"/>τούτων εἰσὶ λευκοὶ <num value="3">γ</num> 

    <lb n="24"/>καὶ λεπτοὶ <num value="6">ϛ</num>. 

    <lb n="25"/>εὐτύχει. 
