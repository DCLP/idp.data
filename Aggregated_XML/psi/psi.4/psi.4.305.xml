    <lb n="9"/>καὶ πεμψάτ<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">σαν</supplied> τὰ ὀνόματα 

    <lb n="10"/><unclear cert="high" reason="undefined">ἐ</unclear>κεί<unclear cert="high" reason="undefined">ν</unclear>ων χα<unclear cert="high" reason="undefined">λ</unclear><gap extent="3" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ω</unclear>ν ἐπὶ τοῦ 

    <lb n="11"/>παρόντος <gap extent="1" reason="illegible" unit="character"/><gap extent="7" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ά</unclear>ψωμαι 

    <lb n="12"/>αυτ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/>τη<unclear cert="high" reason="undefined">η</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

