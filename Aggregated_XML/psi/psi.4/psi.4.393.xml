    <lb n="23"/><app type="BL"><lem resp="4.88">[τοῦτο ἐπεδείξαμέν <del rend="erasure" status="unremarkable">σοι] τῆι <num value="16">ιϛ</num> καὶ</del></lem><rdg><gap reason="lost" extent="unknown" unit="character"/> τῆι <num value="16">ιϛ</num> καὶ]</rdg></app> 

