    <lb n="8"/><supplied reason="lost" cert="high">εἰς </supplied><app type="editorial"><lem><supplied reason="lost" cert="high">Φιλαδ</supplied>έλφειαν</lem><rdg><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> Φιλαδ</supplied>έλφειαν</rdg></app> 

