    <lb n="7"/>ἔρρωσο. <expan><ex cert="high">ἔτους</ex></expan> <num value="29">κθ</num> <supplied cert="high" reason="lost">Δ</supplied>αισίου <num value="8">η</num>, Παχ<supplied cert="high" reason="lost">ὼν </supplied><gap extent="unknown" reason="lost" unit="character"/> 
