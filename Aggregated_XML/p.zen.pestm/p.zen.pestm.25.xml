    <lb n="1"/>Ἀπολλώνιος Ζήνωνι χαίρειν. τ<supplied cert="high" reason="lost">ὸ</supplied> ἔλαιον τὸ 

    <lb n="2"/>παραγεγενημένον ἡμῖν ἐξ <supplied cert="high" reason="lost">οἴ</supplied>κου κα<unclear cert="high" reason="undefined">τ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>εἰς Ἐμπόριον ἐξελοῦ καὶ λαβὼν <supplied cert="high" reason="lost">π</supplied>αρ᾽ Ἡγήμον<supplied cert="high" reason="lost">ος</supplied> 

    <lb n="4"/>ἀπόστασιν ὅ τι ἀσφαλεστάτην κατάστησ<supplied cert="high" reason="lost">ον</supplied> 


    <lb n="5"/>τῶν παίδων τοὺς τηρήσοντας ἕω<unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">ἂ</unclear><supplied cert="high" reason="lost">ν</supplied> 

