    <lb n="1"/><supplied reason="lost" cert="high">τε καὶ</supplied> <unclear reason="undefined" cert="high">ἀκάρπο</unclear><supplied reason="lost" cert="high">ις καὶ</supplied> <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">αῖς πε</supplied><unclear reason="undefined" cert="high">ρι</unclear><supplied reason="lost" cert="high">έξωθεν ἀρούραις καὶ δικαίοις π</supplied><unclear reason="undefined" cert="high">ᾶσι</unclear> <supplied reason="lost" cert="high">ἐ</supplied><unclear reason="undefined" cert="high">ν</unclear><supplied reason="lost" cert="high">τὸς</supplied> 

    <lb n="2"/>καὶ ἐκτὸς πρὸς τὰ παλαιὰ αὐτοῦ ὅρια κυκλ<unclear reason="undefined" cert="high">όθεν</unclear> <unclear reason="undefined" cert="high">ἐφ</unclear>᾽ <supplied reason="lost" cert="high">ᾧ με</supplied> <unclear reason="undefined" cert="high">τ</unclear>οῦτο ἔχειν 

