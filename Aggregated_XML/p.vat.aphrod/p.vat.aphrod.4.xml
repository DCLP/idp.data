    <lb n="7"/><supplied cert="high" reason="lost">τῶν μετ᾽ ἐμὲ</supplied> κ<supplied cert="high" reason="lost">λ</supplied>η<supplied cert="high" reason="lost">ρονόμων</supplied> ἢ <unclear cert="high" reason="undefined">συγκλη</unclear><supplied cert="high" reason="lost">ρο</supplied><unclear cert="high" reason="undefined">νόμων</unclear> ἢ τῶν <unclear cert="high" reason="undefined">ἐ</unclear>κ <supplied cert="high" reason="lost">γ</supplied><unclear cert="high" reason="undefined">έ</unclear><supplied cert="high" reason="lost">ν</supplied><unclear cert="high" reason="undefined">ους</unclear> <supplied cert="high" reason="lost">μο</supplied><unclear cert="high" reason="undefined">ι</unclear> <supplied cert="high" reason="lost">ἀνη</supplied>-

