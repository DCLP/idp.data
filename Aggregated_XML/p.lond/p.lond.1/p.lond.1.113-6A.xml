    <lb n="1"/><g type="stauros"/> ἐν ὀνό<supplied reason="lost" cert="high">ματ</supplied>ι <supplied reason="lost" cert="high">τ</supplied>ῆς <supplied reason="lost" cert="high">ἁγ</supplied>ίας <supplied reason="lost" cert="high">καὶ ὁ</supplied>μο-

    <lb n="2"/>ουσίου τρ<supplied reason="lost" cert="high">ιάδος</supplied> πατρ<supplied reason="lost" cert="high">ὸς καὶ</supplied> υἱοῦ 

    <lb n="3"/>καὶ ἁγίου πνεύματος καὶ τ<supplied reason="lost" cert="high">ῆς</supplied> <expan>δεσπο<ex cert="high">ίνης</ex></expan> 

    <lb n="4"/>ἡμῶν τῆς ἁγίας θεοτό<supplied reason="lost" cert="high">κου κ</supplied>αὶ πάν-

    <lb n="5"/>των τῶν ἁγίων Φαρμο<supplied reason="lost" cert="high">ῦθι</supplied> <num value="7">ζ</num> [<num value="15">ιε</num>] <expan>ἰν<ex cert="high">δικτίονος</ex></expan> ἐν <expan>Ἀρ<ex cert="high">σίνοῃ</ex></expan> 

    <lb n="6"/>Αὐρηλία Μαρία θυγάτηρ Ἀίωνος 

    <lb n="7"/>λαχανοπράτ<supplied reason="lost" cert="high">ου</supplied> ἀπὸ τῆς Ἀρσινοι-

