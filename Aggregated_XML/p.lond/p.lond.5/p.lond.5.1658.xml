    <lb n="1"/>τῷ <choice><corr>ἀειμνή<supplied reason="omitted" cert="high">σ</supplied>τῳ</corr><sic>αιμνητω</sic></choice> υ<hi rend="diaeresis">ἱ</hi>ῷ <app type="BL"><lem resp="6.65">Ἄ<unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">μωνι</supplied></lem><rdg>Α<unclear reason="undefined" cert="high">μ</unclear><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

