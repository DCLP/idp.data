    <lb n="1"/><g type="stauros"/> τὰ γράμματα τῆς ἀδελφικῆς <unclear reason="undefined" cert="high">ὑμῶ</unclear><supplied reason="lost" cert="high">ν </supplied><gap reason="lost" extent="6" unit="character"/><unclear reason="undefined" cert="high">ς</unclear> διαθέσεως δέ<unclear reason="undefined" cert="high">δ</unclear><supplied reason="lost" cert="high">ωκεν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>ὥστε με σημᾶναι. εἴη δὲ ὡς ἔχω τὰ π<supplied reason="lost" cert="high">α</supplied>ιδί<unclear reason="undefined" cert="high">α</unclear> <gap reason="lost" extent="5" unit="character"/><unclear reason="undefined" cert="high">ε</unclear><gap reason="illegible" extent="1" unit="character"/>θην<supplied reason="lost" cert="low">αι</supplied><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="2" unit="character"/>επι<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>τὴν <choice><corr>κατεπείγουσαν</corr><sic>κατεπιγουσαν</sic></choice> τοῦ<unclear reason="undefined" cert="high">τ</unclear>ο πρὸ τῆς ἐμ<unclear reason="undefined" cert="high">ῆς</unclear> [τὲ <unclear cert="high" reason="undefined">καὶ</unclear>] <unclear reason="undefined" cert="high">Ἡρ</unclear>ακλ<unclear reason="undefined" cert="high">εί</unclear><supplied reason="lost" cert="high">α</supplied>ς <unclear reason="undefined" cert="high">φ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>ως τῆ<unclear reason="undefined" cert="high">ς</unclear> ὐτῶν ἐξουσίας τὴν Ἡράκ<unclear reason="undefined" cert="high">λε</unclear><supplied reason="lost" cert="high">ιαν</supplied> <unclear reason="undefined" cert="high">ω</unclear><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="4" unit="character"/>ης καταλαβεῖν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>τὴν ἐμὴν <unclear reason="undefined" cert="high">πα</unclear>ρουσίαν. εἰ γὰρ σημανῶ πρότερον <unclear reason="undefined" cert="high">κ</unclear>α<unclear reason="undefined" cert="high">τ</unclear>ὰ τὴν Ὀξυρυγχ<unclear reason="undefined" cert="high">ι</unclear>[τῶν πόλιν <note lang="en" anchored="yes" place="not specified">?</note><gap reason="illegible" extent="unknown" unit="character"/> ] 

    <lb n="6"/>εὐλόγως ὀλιγωρῆσαι. χρῆ οὖν ὡς <unclear reason="undefined" cert="high">κ</unclear>αὶ α<unclear reason="undefined" cert="high">ὐ</unclear>τὴ συνορᾷ ἐλθεῖν με εἰς <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>ἐπὶ Ὀξύρυγχον τότε α<unclear reason="undefined" cert="high">ὐ</unclear>τὰ προπέμψαι. εἰ δὲ καὶ ὀρθὸν μέ<supplied reason="lost" cert="high">λ</supplied>λει ἡ α<unclear reason="undefined" cert="high">ὐ</unclear>τ<supplied reason="lost" cert="high">ὴ</supplied> <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>τὰ α<unclear reason="undefined" cert="high">ὐτ</unclear>όθι καταλαβεῖν σὺν Θεῷ καὶ αὐτὰ κάτω καταλαμβ<unclear reason="undefined" cert="high">ά</unclear>νομε<unclear reason="undefined" cert="high">ν</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>τῶν ἐνταῦθα ἐξελθεῖν ἐὰν ὁ Θεὸς συνέλθῃ μοι καὶ ἀπαλλάξῃ τ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>ὑμᾶς προσκυνῶ κύριε δὲ Σαραπίων νῦν ὁ λόγος μοι οὐκ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>τὸν εὐδόκιμον <expan>ὀρδινάρ<ex cert="high">ιον</ex></expan> ὀχλήσατε ἢ ἐλθεῖν ἢ γράψαι μοι <unclear reason="undefined" cert="high">ἀπό</unclear>κρι<unclear reason="undefined" cert="high">σιν</unclear> <gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 
