    <lb n="1"/><unclear cert="high" reason="undefined">ἀ</unclear>πὸ κώ<supplied cert="high" reason="lost">μης Ἐνσεῦ τοῦ Ἑρμουπολίτου νομοῦ. ὁμολογῶ</supplied> 

    <lb n="2"/>ἐγ<unclear cert="high" reason="undefined">ὼ</unclear> ὁ προ<unclear cert="high" reason="undefined">γ</unclear><supplied cert="high" reason="lost">εγραμ</supplied>μένο<unclear cert="high" reason="undefined">ς</unclear> <supplied cert="high" reason="lost">Μην</supplied><unclear cert="high" reason="undefined">ᾶς</unclear> γεω<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">γὸς ἑκουσίως</supplied> 

    <lb n="3"/>καὶ αὐθ<unclear cert="high" reason="undefined">αι</unclear><supplied cert="high" reason="lost">ρέτω</supplied>ς μεμ<supplied cert="high" reason="lost">ισ</supplied>θῶσ<unclear cert="high" reason="undefined">θα</unclear><supplied cert="high" reason="lost">ι παρ᾽ ὑμῶν</supplied> 

    <lb n="4"/>ἐπὶ πεν<supplied cert="high" reason="lost">ταετῆ</supplied> <unclear cert="high" reason="undefined">χ</unclear>ρόνον λογιζόμε<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">ον ἀπὸ καρπῶν τῆς</supplied> 

    <lb n="5"/>σὺν Θεῷ εἰσιούσης δω<unclear cert="high" reason="undefined">δ</unclear>εκάτης <expan>ἰνδ<ex cert="high">ικτίονος</ex></expan> <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ὴν ὑπάρχουσαν</supplied> 

