    <lb n="1"/>τὸν ἐπικείμενον τῆς ἐρ<g type="apostrophe"/>γασί<supplied reason="lost" cert="low">ας οὐ δυνάμενον</supplied> 

    <lb n="2"/>περαιῶσαι τοὺς αὐτοὺς καράβους κ<supplied reason="lost" cert="high">α</supplied>ὶ <supplied reason="lost" cert="high">ἀκάτια καὶ</supplied> 

    <lb n="3"/>δρομονάρια εἰ μὴ τὸ πρᾶγμα <subst><add place="inline">τῶ<supplied reason="lost" cert="high">ν</supplied></add><del rend="corrected" status="unremarkable">το<supplied reason="lost" cert="high">ν</supplied></del></subst><gap reason="lost" extent="unknown" unit="character"/> 

