    <lb n="5"/>σταλῆναι. <space extent="unknown" unit="character" dim="horizontal"/> δεχόμενος οὖν τὰ παρόντα γ<supplied reason="lost" cert="high">ράμματα εὐθέως</supplied>

    <lb n="6"/>κ<unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">ὶ</supplied> κ<supplied reason="lost" cert="high">α</supplied>τ᾽ αὐτὴν τὴν ὥραν ἀπόστειλον π<supplied reason="lost" cert="high">ρὸς ἡμᾶς</supplied> 

    <lb n="7"/>τὰ ἀνώτερα δηλωθέ<supplied reason="lost" cert="high">ν</supplied>τα ε<supplied reason="lost" cert="high">ἴ</supplied>δη <unclear reason="undefined" cert="high">ἔτ</unclear>ι <supplied reason="lost" cert="high">μὴν καὶ τὸ</supplied> 

    <lb n="8"/>λο<unclear reason="undefined" cert="high">ιπ</unclear>αζόμενον διὰ τῆς διοικήσεώ<supplied reason="lost" cert="high">ς σου μὴ ὑστερῶν</supplied> 

    <lb n="9"/>τι τὸ σύνολον μήτε μὴν <unclear reason="undefined" cert="high">πέ</unclear>μπων τ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>μὴ ἀποστραφῆναί σοι ἐπιτρέπων τοῖ<supplied reason="lost" cert="high">ς τῶν χωρίων</supplied> 

    <lb n="11"/>χρησίμως καὶ ὡς χρὴ καμεῖν τὰς δ<supplied reason="lost" cert="low">όσεις</supplied><supplied reason="lost" cert="high"> μὴ ἀνύων <expan>ἀπαργ<ex cert="high">υρισμὸν</ex></expan></supplied> 

