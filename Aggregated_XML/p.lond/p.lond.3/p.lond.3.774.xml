    <lb n="1"/><g type="stauros"/> βασιλ<supplied cert="high" reason="lost">εί</supplied>ας τοῦ θειοτάτου καὶ εὐσεβε<supplied cert="high" reason="lost">στ</supplied>άτου ἡμῶν δεσπότου μεγίστου 

    <lb n="2"/>εὐεργέτου <expan>Φλ<ex cert="high">αυίου</ex></expan> Τιβερίου <expan>Κωνσταντίνο<ex cert="high">υ</ex></expan> τοῦ αἰωνίου Αὐγούστου καὶ <expan>Αὐτοκρ<ex cert="high">άτορος</ex></expan> 

    <lb n="3"/>ἔτους <num value="8">η</num> <choice><corr>ὑπατείας</corr><sic><hi rend="diaeresis">υ</hi>πατιας</sic></choice> τοῦ <expan>αὐτ<ex cert="high">οῦ</ex></expan> <expan>εὐσεβ<ex cert="high">εστάτου</ex></expan> ἡμῶν δεσπότου ἔτους <num value="4">δ</num> Φαῶφι <num value="14">ιδ</num> 

    <lb n="4"/><space dim="horizontal" extent="unknown" unit="character"/> <expan>ἰνδ<ex cert="high">ικτίονος</ex></expan> <num value="1">α</num>. <space dim="horizontal" extent="unknown" unit="character"/> 

    <lb n="5"/>τ<supplied cert="high" reason="lost">οῖς ὑπερφυεστάτοις</supplied> κληρονόμοις τοῦ τῆς εὐκλεοῦς μνήμης 

    <lb n="6"/>Ἀπίων<supplied cert="high" reason="lost">ος γ</supplied>εουχοῦσιν καὶ ἐνταῦθα τῇ λαμπρᾷ <expan>Ὀξυρυγχ<ex cert="high">ιτῶν</ex></expan> πόλει 

    <lb n="7"/>διὰ Μηνᾶ οἰκέτου τοῦ ἐπερωτῶντος καὶ προσπορίζοντος τοῖς 

    <lb n="8"/>ἰδ<supplied cert="high" reason="lost">ί</supplied>οις δεσπόταις <supplied cert="high" reason="lost">το</supplied>ῖς αὐτοῖς πανευφήμοις ἀνδράσι<unclear cert="high" reason="undefined">ν</unclear> τὴν ἀγωγὴν 

    <lb n="9"/>καὶ ἐνοχὴν Αὐρήλιος Ἰωάννης <hi rend="acute">υ</hi>ἱὸς <unclear cert="high" reason="undefined">Ἄπα</unclear> Σίωνος μητρὸς Νό<unclear cert="high" reason="undefined">ν</unclear>νας 

    <lb n="10"/>ἀπὸ <expan>ἐποικίο<ex cert="high">υ</ex></expan> Θώλ<unclear cert="high" reason="undefined">θ</unclear>εως τοῦ Ὀξυρυγχίτου <expan>νομο<ex cert="high">ῦ</ex></expan> διαφέροντος τῇ 

    <lb n="11"/><hi rend="diaeresis">ὑ</hi>μῶν <choice><corr>ὑπερφυίᾳ</corr><sic><hi rend="acute">υ</hi>περφυεια</sic></choice> ἐναπόγραφος αὐτῆ<unclear cert="high" reason="undefined">ς</unclear> γεωργὸς χαίρ<unclear cert="high" reason="undefined">ει</unclear>ν. χρείας 

    <lb n="12"/>καὶ νῦν <expan>γεν<unclear cert="high" reason="undefined">α</unclear>μ<ex cert="high">ένης</ex></expan> εἰς τὰς <hi rend="diaeresis">ὑ</hi>π᾽ ἐμὲ γεο<unclear cert="high" reason="undefined">υ</unclear>χικὰς μηχανὰς τοῦ <expan><unclear cert="high" reason="undefined">π</unclear>λαγίο<ex cert="high">υ</ex></expan> 

    <lb n="13"/><expan>ποταμο<ex cert="high">ῦ</ex></expan> Και<hi rend="supraline">αν</hi>ες μεγάλων ἐργατῶν <hi rend="supraline">δύο</hi> <num value="2"/> ἀνελθὼν ἐπὶ τῆς πόλεως 

