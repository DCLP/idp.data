    <lb n="10"/>ὄν<supplied cert="high" reason="lost">τ</supplied>ας εὐπόρου<supplied cert="high" reason="lost">ς καὶ </supplied><choice><corr><supplied cert="high" reason="lost">ἐπιτη</supplied>δείους</corr><sic><supplied cert="high" reason="lost">επιτη</supplied>διους</sic></choice> εἰς <supplied cert="high" reason="lost">τοῦ</supplied>το <supplied cert="high" reason="lost">ἐ</supplied>φ᾽ <supplied cert="high" reason="lost">ᾧ</supplied> 

    <lb n="11"/>αὐ<supplied cert="high" reason="lost">τ</supplied>οὺς ἐντεῦθεν α<unclear cert="high" reason="undefined">ι</unclear><gap extent="4" reason="lost" unit="character"/><supplied cert="high" reason="lost"> σ</supplied>θαι κ<supplied cert="high" reason="lost">αὶ</supplied> ἀντιλαβέσθαι τ<supplied cert="high" reason="lost">ῆ</supplied>ς 

