    <lb n="5"/><supplied cert="high" reason="lost">ἀ</supplied>μφοτέρ<supplied cert="high" reason="lost">ων κωμάρχων ἀπὸ ἐπ</supplied>οικ[ίου <app type="BL"><lem resp="8.187-8">Πατελκίου</lem><rdg><supplied cert="high" reason="lost">Πα </supplied><gap extent="unknown" reason="lost" unit="character"/></rdg></app> τοῦ] 

    <lb n="6"/>αὐτο<add place="supralinear">ῦ</add> νομο<supplied cert="high" reason="lost">ῦ. δίδομεν καὶ εἰσαγ</supplied>γέλ<supplied cert="high" reason="lost">λομεν εἰς τὴν ὑδροφυλα</supplied>-

    <lb n="7"/>κίαν <app type="BL"><lem resp="1.290">διακό<supplied cert="high" reason="lost">που</supplied></lem><rdg>διὰ κό<supplied cert="high" reason="lost">που</supplied></rdg></app><gap extent="2" reason="lost" unit="character"/><supplied cert="high" reason="lost">οα </supplied><gap extent="3" reason="lost" unit="character"/><supplied cert="high" reason="lost">ως </supplied><gap extent="2" reason="lost" unit="character"/> καὶ τῶν ὁρ<supplied cert="high" reason="lost">ίων </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>ἰδίῳ ἡμῶν κιν<supplied cert="high" reason="lost">δύνῳ καὶ</supplied> πάντων τῶν <supplied cert="high" reason="lost">ἀπὸ τῆς κ</supplied>ώ-

