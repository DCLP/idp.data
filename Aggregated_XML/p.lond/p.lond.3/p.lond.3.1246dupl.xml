    <lb n="13"/>Αὐρήλιοι Σερῆνος Πυ<gap extent="1" reason="lost" unit="character"/>ιτο<add place="supralinear">υ</add>  Ὡρίων Ταυρίνο<add place="supralinear">υ</add> 

    <lb n="14"/><app type="BL"><lem resp="9.142">Λεῦς</lem><rdg>Λευ<unclear cert="high" reason="undefined">ᾶ</unclear><supplied cert="high" reason="lost">ς</supplied></rdg></app> <supplied cert="high" reason="lost">Ἀ</supplied>κῆτος                Σερῆνος Χώνσιος 

    <lb n="15"/>Ἀ<supplied cert="high" reason="lost">λ</supplied>έξ<supplied cert="high" reason="lost">αν</supplied>δρος Σιλβ<supplied cert="high" reason="lost">ανο</supplied><add place="supralinear">ῦ</add>   Φ<supplied cert="high" reason="lost">ῖ</supplied>βις Μελ<supplied cert="high" reason="lost">ᾶ</supplied>τος 

    <lb n="16"/>Ἠλίας Σαλαμ<unclear cert="high" reason="undefined">ῶ</unclear><supplied cert="high" reason="lost">νο</supplied><unclear cert="high" reason="undefined">ς</unclear>            Ἀροοῦς Προοῦτος 

    <lb n="17"/>Σόεις Κάστορος                Ἀπόλλων Πρεμμεώτο<add place="supralinear">υ</add> 

    <lb n="18"/>οὕσπερ ἐγ<g type="apostrophe"/>γυώμ<supplied cert="high" reason="lost">εθα</supplied> ἀμέμπτως ἀποπληροῦντας 

    <lb n="19"/>τὴν <choice><corr>ἐνχειρισθεῖσαν</corr><sic>ενχιρισθ<supplied cert="high" reason="lost">εισαν</supplied></sic></choice> αὐτοῖς ὑδροφυλακίαν. ἐὰν δέ 

    <lb n="20"/>τις αὐτῶν <choice><corr>ἀφυστερήσῃ</corr><sic>αφυσ<supplied cert="high" reason="lost">τερ</supplied>ησιεν</sic></choice> καὶ μὴ παραστή<supplied cert="high" reason="lost">σ</supplied>ωμεν 

