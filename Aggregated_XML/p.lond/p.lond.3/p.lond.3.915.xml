    <lb n="1"/>Κεφάλωνι <expan>βασιλ<ex cert="high">ικῷ</ex></expan> <expan>γρ<ex cert="high">αμματεῖ</ex></expan> <expan>Μεμφ<ex cert="high">ίτου</ex></expan> 

    <lb n="2"/>διαδεχομένῳ καὶ τὰ κατὰ 

    <lb n="3"/>τὴν <expan><unclear cert="high" reason="undefined">στρ</unclear><ex cert="high">ατηγίαν</ex></expan> 

    <lb n="4"/>π<supplied cert="high" reason="lost">αρὰ</supplied> Ἀπολλωνίου Ἁρφα-

    <lb n="5"/>ήσιος τοῦ καὶ Ἡφαιστί-

    <lb n="6"/>ωνος μητρὸς Θερμού-

    <lb n="7"/>θιος τῶν ἀπὸ Μέμφε-

    <lb n="8"/>ως ἀ<unclear cert="high" reason="undefined">ρ</unclear>γῶν ἀναγραφό-

    <lb n="9"/>μενος ἐπὶ <app type="BL"><lem resp="9.132">τοῦ <num value="1">α</num> <expan><unclear cert="high" reason="undefined">ἀ</unclear>μφ<ex cert="high">όδου</ex></expan></lem><rdg>τ<unclear cert="high" reason="undefined">οῦ</unclear> <abbr>α</abbr></rdg></app>. 

    <lb n="10"/>ἀπογράφομαι ἐμαυτόν 

    <lb n="11"/>τε καὶ τὸν ἀφήλικά 

    <lb n="12"/>μου υἱὸν Σωτήριχον 

    <lb n="13"/>μητρὸς Πτολέμας 

    <lb n="14"/>Ἁρχώσιος <choice><corr>Μεμφίτι-

    <lb n="15"/>δος</corr><sic>μεμφειτιδος</sic></choice> ἔτι πάλαι ἀπηλ-

    <lb n="16"/>λαγμένης ἐνοίκους 

    <lb n="17"/>εἰς ὃ οἰκοῦμεν ἐπὶ τοῦ 

    <lb n="18"/><app type="BL"><lem resp="9.132"><expan>α<ex cert="high">ὐ</ex></expan>τοῦ <expan>ἀμφ<ex cert="high">όδου</ex></expan></lem><rdg><expan>α<ex cert="high">ὐτοῦ</ex></expan> <sic>τοῦ</sic> <abbr>α</abbr></rdg></app> μέρος οἰκίας 

    <lb n="19"/>Σαραπίωνος <expan>Σαραπίωνο<ex cert="high">ς</ex></expan> 

    <lb n="20"/>εἰς τὴν τοῦ ἐνεστῶ-

    <lb n="21"/>τος <num value="23">κγ</num> <expan><ex cert="high">ἔτους</ex></expan> Ἀντωνίνου 

    <lb n="22"/>Καίσαρος τοῦ κυρίου 

    <lb n="23"/>κατ᾽ οἰκίαν <expan>ἀπογρ<ex cert="high">αφὴν</ex></expan> 

    <lb n="24"/>ἐμαυτὸν Ἀπολλώνιον 

    <lb n="25"/>τὸ<unclear cert="high" reason="undefined">ν</unclear> <expan>προγε<ex cert="high">γραμμένον</ex></expan> <expan><ex cert="high">ἐτῶν</ex></expan> <num value="37">λζ</num>, 

    <lb n="26"/>Σωτήριχον υἱὸν <expan>μη<ex cert="high">τρὸς</ex></expan> 

    <lb n="27"/>τῆς <expan>προγε<ex cert="high">γραμμένης</ex></expan> Πτολέμας 

    <lb n="28"/><expan>ἀναγρ<ex cert="high">αφόμενον</ex></expan> δι<unclear cert="high" reason="undefined">ὰ</unclear> <expan><supplied cert="high" reason="lost">γ</supplied><unclear cert="high" reason="undefined">ρ</unclear><ex cert="high">αφῆς</ex></expan> <app type="BL"><lem resp="1.274"><expan>ἀφηλ<ex cert="high">ίκων</ex></expan></lem><rdg><expan>ἀφηλ<ex cert="high">ικίασ</ex></expan></rdg></app> εἰς 

    <lb n="29"/>τὸ <expan>α<ex cert="high">ὐτὸ</ex></expan> <num value="23"><unclear cert="high" reason="undefined">κγ</unclear></num> <expan><ex cert="high">ἔτος</ex></expan> <expan><ex cert="high">ἐτῶν</ex></expan> <num value="5">ε</num> <app type="BL"><lem resp="1.274"><expan><unclear cert="high" reason="undefined">μ</unclear>η<ex cert="high">νῶν</ex></expan> <gap extent="1" reason="lost" unit="character"/></lem><rdg><gap desc="vestiges" extent="unknown" reason="illegible" unit="character"/></rdg></app> 

    <lb n="30"/>παρὼν δὲ ὁ <expan><unclear cert="high" reason="undefined">στα</unclear>θμο<supplied cert="high" reason="lost">ῦ</supplied><unclear cert="high" reason="undefined">χ</unclear><ex cert="high">ος</ex></expan> 

    <lb n="31"/><expan>Σαραπ<ex cert="high">ίωνος</ex></expan> <unclear cert="high" reason="undefined">πρὸς</unclear> τοῦ <app type="BL"><lem resp="cf. 8.181">Ἁρτ<supplied cert="high" reason="lost">ώ</supplied>-

    <lb n="32"/>του <expan><ex cert="high">ἑκατοντάρχου</ex></expan></lem><rdg>τ<gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">τ</unclear>ου</rdg></app> <unclear cert="high" reason="undefined">ἐγγ</unclear>υᾶ<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">αί</supplied> με τῶν 

    <lb n="33"/><app type="BL"><lem resp="9.132">ἐπικεφαλίω<supplied cert="high" reason="lost">ν</supplied></lem><rdg><unclear cert="high" reason="undefined">επικ</unclear><gap extent="3" reason="illegible" unit="character"/>ε<unclear cert="high" reason="undefined">ν</unclear>ω<gap extent="5" reason="lost" unit="character"/></rdg></app> 

    <lb n="34"/>καὶ ὀμνύω τὴν τοῦ 

    <lb n="35"/>κυρίου Αὐτοκράτορος 

    <lb n="36"/>Καίσαρος Τίτου Αἰλίου 

    <lb n="37"/>Ἁδριανοῦ Ἀντωνίνου 

    <lb n="38"/>Σεβατοῦ Εὐσεβοῦς 

    <lb n="39"/>τύχην ἀληθῆ εἶναι 

    <lb n="40"/>τὰ <expan>προκ<ex cert="high">είμενα</ex></expan>. 

    <lb n="41"/><expan><ex cert="high">ἔτους</ex></expan> <num value="23">κγ</num> Αὐτοκράτορος Καίσαρος 

    <lb n="42"/>Τίτου Αἰλίου Ἁδριανοῦ 

    <lb n="43"/>Ἀντωνίνου Σεβαστοῦ 

    <lb n="44"/>Εὐσεβοῦς <gap desc="vestiges" extent="unknown" reason="illegible" unit="character"/> 

    <lb n="45"/><expan>Ἀπολλ<ex cert="high">ώνιος</ex></expan> <expan>στ<unclear cert="high" reason="undefined">ρ</unclear><ex cert="high">ατηγὸς</ex></expan> διὰ 

    <lb n="46"/><app type="BL"><lem resp="8.181">Διδύμου <expan>βοηθ<ex cert="high">οῦ</ex></expan> <expan>ἐσημ<ex cert="high">ειωσάμην</ex></expan></lem><rdg><gap extent="4" reason="illegible" unit="character"/><gap extent="3" reason="illegible" unit="character"/></rdg></app> 
