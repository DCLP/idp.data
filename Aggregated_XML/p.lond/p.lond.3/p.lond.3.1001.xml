    <lb n="1"/><g type="stauros"/> 

    <lb n="1"/><g type="stauros"/> μετὰ τὴν <hi rend="diaeresis">ὑ</hi>πατείαν <expan>Φλ<ex cert="high">αυίου</ex></expan> <expan>Ἰωάννο<ex cert="high">υ</ex></expan> <expan>το<ex cert="high">ῦ</ex></expan> <expan>ἐ<unclear cert="high" reason="undefined">ν</unclear>[δοξο<ex cert="high">τάτου</ex>]</expan> 

    <lb n="2"/>καὶ <expan>ὑπερφυεστάτο<ex cert="high">υ</ex></expan> Μ<supplied cert="high" reason="lost">ε</supplied>χεὶρ <num value="20">κ</num>  <num value="2">β</num> ἰνδι<supplied cert="high" reason="lost">κτίονος</supplied>. 

    <lb n="3"/>Αὐρήλιος <expan>Κόλλο<ex cert="high">υ</ex></expan>θος <expan>Λυθί<supplied cert="high" reason="lost">ο</supplied><ex cert="high">υ</ex></expan> μητρὸς <app type="BL"><lem resp="1.298"><unclear cert="high" reason="undefined">Τσ</unclear>α<gap extent="3" reason="illegible" unit="character"/></lem><rdg><gap extent="2" reason="illegible" unit="character"/>αν<gap extent="1" reason="illegible" unit="character"/></rdg></app> 

    <lb n="4"/>ἀμπελουργὸς ἀπὸ <choice><corr><expan>ἐποικίο<ex cert="high">υ</ex></expan></corr><sic>εποικειο</sic></choice> <expan>καλο<ex cert="high">υ</ex><unclear cert="high" reason="undefined">μ</unclear>έν<supplied cert="high" reason="lost">ο</supplied><ex cert="high">υ</ex></expan> 

    <lb n="5"/>Τ<unclear cert="high" reason="undefined">η</unclear>υπαρὰτ πεδίων κώμης Ἐνσεῦ <expan>το<ex cert="high">ῦ</ex></expan> <expan>Ἑρμο<ex cert="high">υ</ex></expan>-

    <lb n="6"/>πολίτου <expan>νομο<ex cert="high">ῦ</ex></expan> Φλαου<hi rend="diaeresis">ΐ</hi>ῳ Γε<unclear cert="high" reason="undefined">ω</unclear>ργίῳ <expan>Σιλ<supplied cert="high" reason="lost">β</supplied>ανο<ex cert="high">ῦ</ex></expan> 

    <lb n="7"/><unclear cert="high" reason="undefined">ἀ</unclear>πὸ στρατιωτῶν ἀπὸ τῆς <expan>Ἑρμο<ex cert="high">υ</ex></expan>πολιτῶν 

    <lb n="8"/><expan>χ<ex cert="high">αίρειν</ex></expan>· ὁμολογῶ ἐσχηκέναι καὶ πεπληρῶσθαι 

    <lb n="9"/>παρ[ὰ <expan>σ]ο<ex cert="high">ῦ</ex></expan> τῆ<supplied cert="high" reason="lost">ς</supplied> πρὸς ἀλλήλους συμπεφω-

    <lb n="10"/>νημ<supplied cert="high" reason="lost">έ</supplied>νης τελείας τιμῆς <expan>σίτο<ex cert="high">υ</ex></expan> ἀρτάβης μιᾶς 

    <lb n="11"/>τ<supplied cert="high" reason="lost">ρίτο</supplied>υ καὶ <expan>οἴνο<ex cert="high">υ</ex></expan> νέου μούστου κνιδίων 

    <lb n="12"/><unclear cert="high" reason="undefined">δ</unclear>έκα <expan>γί<ex cert="high">νονται</ex></expan> <expan>σί<ex cert="high">του</ex></expan> <expan><ex cert="high">ἀρτάβη</ex></expan> <num value="1">α</num> <num value="1/3">γ</num> καὶ <expan>οἴν<ex cert="high">ου</ex></expan> <app type="BL"><lem resp="1.298"><expan>κνίδ<ex cert="high">ια</ex></expan> <num value="10">ι</num>, ἅπερ <gap extent="4" reason="illegible" unit="character"/> ἀ-

    <lb n="13"/>ποδώσω</lem><rdg><expan>κ<supplied cert="high" reason="lost">νί</supplied>δ<ex cert="high">ια</ex></expan> <num value="10">ι</num> ἅπερ ἀντιδώσω</rdg></app> σοι οὕτως· τὸν μὲν σῖτον τῷ 

    <lb n="14"/>Πα<hi rend="diaeresis">ῧ</hi>νι μηνὶ καιρῷ συγκομιδῆς <supplied cert="high" reason="lost">κα</supplied><unclear cert="high" reason="undefined">ρ</unclear>π<supplied cert="high" reason="lost">ῶ</supplied><unclear cert="high" reason="undefined">ν</unclear> 

    <lb n="15"/>τῆς σὺν θεῷ εἰσ<hi rend="diaeresis">ϊ</hi>ούσης τρίτης ἰνδικτίονος 

    <lb n="16"/>ἐν σίτῳ νέῳ καθαρῷ κεκοσκινευμένῳ 

    <lb n="17"/>μέτρῳ σῷ Ἀθηναίῳ, τὸν δὲ οἶνον τῷ 

    <lb n="18"/>Μεσορὴ μηνὶ καιρῷ τρύγης καρπῶν τῆς <expan><ex cert="high">αὐτῆς</ex></expan> 

    <lb n="19"/><expan>ἰνδικ<ex cert="high">τίονος</ex></expan> ἀνυπερθέτως ἐν οἴνῳ νέῳ <choice><corr>καλλίστῳ</corr><sic>κ<unclear cert="high" reason="undefined">α</unclear>λ<unclear cert="high" reason="undefined">ισ</unclear>τω</sic></choice> 

    <lb n="20"/>καὶ εὐαρέστῳ μέτρῳ <expan>το<ex cert="high">ῦ</ex></expan> ὑπ᾽ ἐμὲ <expan>χωρίο<ex cert="high">υ</ex></expan> <expan>ἀμπελικ[ο<ex cert="high">ῦ</ex>]</expan> 

    <lb n="21"/><expan>σο<ex cert="high">ῦ</ex></expan> παρέχοντος τὰ <expan>το<ex cert="high">ύ</ex></expan>των <choice><corr>κοῦφα</corr><sic>κουφων</sic></choice>. εἰ δὲ μὴ 

    <lb n="22"/>δυ<unclear cert="high" reason="undefined">ν</unclear>ηθῶ διδόναι σοι τὸ<unclear cert="high" reason="undefined">ν</unclear> <expan>α<ex cert="high">ὐ</ex></expan>τὸν σῖτον καὶ οἶνο<unclear cert="high" reason="undefined">ν</unclear> 

    <lb n="23"/><app type="BL"><lem resp="1.298">ἐν <supplied cert="high" reason="lost">τ</supplied>ῇ αὐτῇ</lem><rdg>ε<unclear cert="high" reason="undefined">ξ</unclear><gap extent="2" reason="illegible" unit="character"/>β<gap extent="1" reason="illegible" unit="character"/>τη</rdg></app> π<unclear cert="high" reason="undefined">ρο</unclear>θεσμία παρέξω σοι ὑπὲρ <app type="BL"><lem resp="1.298">τῆ<unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">τούτω</unclear><supplied cert="high" reason="lost">ν</supplied></lem><rdg>τῆ<supplied cert="high" reason="lost">ς</supplied> πρ<unclear cert="high" reason="undefined">ώ</unclear><supplied cert="high" reason="lost">της</supplied></rdg></app> 

    <lb n="24"/>τιμῆς <expan>χρυσο<ex cert="high">ῦ</ex></expan> κεράτια ἓξ <num value="6"/> εὐθὺς <app type="BL"><lem resp="1.298">μετὰ τὴ<unclear cert="high" reason="undefined">ν</unclear> <gap extent="3" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">α</unclear>δ<gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/></lem><rdg>μ<supplied cert="high" reason="lost">ετὰ</supplied> τῆς <gap desc="vestiges" extent="unknown" reason="illegible" unit="character"/></rdg></app> 

    <lb n="25"/><app type="BL"><lem resp="1.298"><unclear cert="high" reason="undefined">τ</unclear>ῆς <expan>α<ex cert="high">ὐτῆς</ex></expan></lem><rdg>σῆσ</rdg></app> προθεσμίας <hi rend="diaeresis">ἰ</hi>δίῳ <expan><unclear cert="high" reason="undefined">μ</unclear>ο<ex cert="high">υ</ex></expan> κινδύνῳ καὶ πάση<supplied cert="high" reason="lost">ς</supplied> 

    <lb n="26"/>ὑποστάσεως. τὸ γραμματεῖον τοῦτο κύρι<unclear cert="high" reason="undefined">ον</unclear> <supplied cert="high" reason="lost">καὶ βέβαιον</supplied> 

    <lb n="27"/>καὶ <expan>ἐπερ<ex cert="high">ωτηθεὶς</ex></expan> <expan><unclear cert="high" reason="undefined">ὡμολ</unclear><ex cert="high">όγησα</ex></expan>. <handShift new="m2"/> <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> Κόλλουθος <expan>Λυθίο<ex cert="high">υ</ex></expan> ὁ <expan>προκ<ex cert="high">είμενος</ex></expan> ἐθέ<supplied cert="high" reason="lost">μην τοῦ</supplied>-

    <lb n="28"/>το τὸ γραμματεῖον ὡς <expan>πρόκ<ex cert="high">ειται</ex></expan>. <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> <app type="BL"><lem resp="1.298">Ἄπα Λεῦς</lem><rdg>Ἀπαλεῦσ</rdg></app>  Ἑρμου<supplied cert="high" reason="lost">πολίτης</supplied> 

    <lb n="29"/><unclear cert="high" reason="undefined">ἀξ</unclear>ιωθεὶς ἔγραψα ὑπὲρ αὐτοῦ γράμματα <supplied cert="high" reason="lost">μὴ εἰδότος</supplied>. 

    <lb n="30"/><handShift new="m3"/> Φλάυιος Ἴσακος Φοιβάμωνος ἀπὸ <unclear cert="high" reason="undefined">Μ</unclear>ω<gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> μαρτυρῶ τῷ</supplied> 

    <lb n="31"/><choice><corr>γραμματείῳ</corr><sic>γραματιω</sic></choice> ἀκούσας παρὰ τοῦ θεμένου <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="32"/><handShift new="m4"/> <unclear cert="high" reason="undefined">ἀ</unclear>πὸ <expan>Ἑ[ρ<ex cert="high">μουπόλεως</ex>]</expan> μαρτυρῶ τῷ <choice><corr>γραμματείῳ</corr><sic>γραμματιω</sic></choice> ἀκούσας παρὰ το<add place="supralinear">ῦ</add> θε<supplied cert="high" reason="lost">μένου</supplied> 

