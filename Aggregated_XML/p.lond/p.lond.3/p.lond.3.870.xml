    <lb n="1"/>ὑπατεί<supplied reason="lost" cert="high">α</supplied>ς τῶ<supplied reason="lost" cert="high">ν δεσποτῶν ἡμῶν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>Αὐρήλιο<supplied reason="lost" cert="high">ς</supplied> <app type="BL"><lem resp="1.292">Κοτ<gap reason="lost" extent="unknown" unit="character"/>ρη υ<unclear reason="undefined" cert="high">π</unclear><gap reason="lost" extent="unknown" unit="character"/></lem><rdg>Κωτ<gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="3"/>ὀρνιθᾶ<supplied reason="lost" cert="high">ς</supplied> ἀπὸ Πανοσπό<unclear reason="undefined" cert="high">λε</unclear>ως <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ῆς λαμπρο</supplied>τάτης Αὐρηλί<supplied reason="lost" cert="high">α Ἀπολλωνία </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>ἀπὸ τῆς α<supplied reason="lost" cert="high">ὐ</supplied>τῆς πόλεως χαίρειν· <supplied reason="lost" cert="high">ὁμολογ</supplied>ῶ ἐσχηκέναι παρ<supplied reason="lost" cert="high">ὰ σοῦ διὰ χειρὸς ἐξ οἴκου χρῆσιν ἔντοκον</supplied> 

    <lb n="5"/>χρυσίνο<supplied reason="lost" cert="high">υ</supplied>ς πέντε <expan>γί<ex cert="high">νονται</ex></expan> <expan>χρ<ex cert="high">υσίνοι</ex></expan> <num value="5">ε</num> εἰς διάπρασιν καὶ εἰς καταβολ<supplied reason="lost" cert="high">ὴν </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> οὓς καί σοι</supplied> 

    <lb n="6"/>ἀποδώσω ὁπόταν εὐπορήσω ἀνυπερθέτως ἐπάναγκ<unclear reason="undefined" cert="high">ε</unclear><supplied reason="lost" cert="high">ς </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> καὶ</supplied> 

    <lb n="7"/>ὑποτίθημί σοι τὸ ὑπάρχον μοι καὶ <app type="BL"><lem resp="1.292"><choice><corr>ἐπιβάλλον</corr><sic>επειβαλ<supplied reason="lost" cert="high">λ</supplied>ον</sic></choice></lem><rdg>επει ἐὰ<supplied reason="lost" cert="high">ν</supplied> ἦν</rdg></app> μέρος ἀ<unclear reason="undefined" cert="high">πὸ</unclear> <supplied reason="lost" cert="high">μέρους ἡμίσους τῆς ὅλης οἰκίας κοινῆς</supplied> 

    <lb n="8"/>ἐν ῥυμίῳ τυφλῷ ὃ ἔσχον ἀπὸ <expan>κλήρο<ex cert="high">υ</ex></expan> τοῦ προκειμένου κατὰ <supplied reason="lost" cert="high">μετοχὴν τοῦ ἄλλου μέρους ἡμίσους</supplied> 

    <lb n="9"/>τῆς ὅλης οἰκίας κοινῆς πρὸς σὲ τὴν προκειμένην Ἀπολλω<supplied reason="lost" cert="high">νίαν, γείτονες ἀπηλιώτου τὸ προκεί</supplied>-

    <lb n="10"/>μενόν σοι μέρος, νότου οἰκία Τασάιτος <app type="BL"><lem resp="7.87">ἀπὸ Ψώνεως</lem><rdg>Απ<gap reason="illegible" extent="1" unit="character"/>τωνατο?</rdg></app>, βο<supplied reason="lost" cert="high">ρρᾶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 


    <lb n="11"/>Πτολεμαίῳ, λιβὸς τὸ αὐτὸ ῥυμίον τυφλὸν ἢ οἳ ἐὰν ὦσι γ<supplied reason="lost" cert="high">είτονες πάντοθεν <app type="BL"><lem resp="1.292">καὶ μὴ ἐξείη μοι</lem><rdg>κυριεύειν σε</rdg></app> ἀπὸ τῆς</supplied> 

    <lb n="12"/>σήμερον τὸ προκείμενον <del rend="erasure" status="unremarkable">μοι</del> μέρο<supplied reason="lost" cert="high">ς</supplied> ἀπὸ μέρους ἡμίσο[υς τῆς ὅλης οἰκίας <app type="BL"><lem resp="1.292"><gap reason="illegible" extent="16" unit="character"/></lem><rdg><supplied reason="lost" cert="high">κοινῆς καὶ μὴ ἐξείη</supplied> σ<unclear reason="undefined" cert="high">οι</unclear></rdg></app> ]

    <lb n="13"/>σαι <supplied reason="lost" cert="high">ὑ</supplied>ποθέ<unclear reason="undefined" cert="high">σθα</unclear>ι ἢ <choice><corr>πωλῆσαι</corr><sic>πωλλησαι</sic></choice> <unclear reason="undefined" cert="high">ἢ</unclear> ἄλλην τινα <app type="BL"><lem resp="1.292"><choice><corr>οἰκονομίαν</corr><sic>οικον<supplied reason="lost" cert="high">ομ</supplied>ε<supplied reason="lost" cert="high">ι</supplied>αν</sic></choice></lem><rdg>οἰ<unclear reason="undefined" cert="high">κ</unclear>ο<unclear reason="undefined" cert="high">ν</unclear><supplied reason="lost" cert="high">ομίαν</supplied> <unclear reason="undefined" cert="high">ἀντι</unclear><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="14"/><supplied reason="lost" cert="high">π</supplied>ληρ<unclear reason="undefined" cert="high">ώ</unclear>σω, εἰ δὲ μὴ πληρώσω <app type="BL"><lem resp="1.292">κυρ<supplied reason="lost" cert="high">ιεύειν σε τῆς</supplied> πρ<supplied reason="lost" cert="high">ο</supplied>κει<supplied reason="lost" cert="high">μένης ὑποθήκης</supplied></lem><rdg>κυρ<supplied reason="lost" cert="high">ίαν εἶναι τὴν</supplied> πρ<supplied reason="lost" cert="high">ο</supplied>κει<supplied reason="lost" cert="high">μένην ὑποθήκην</supplied></rdg></app><supplied reason="lost" cert="high"> καὶ βεβαιώσω σοι διὰ παν</supplied>-

    <lb n="15"/>τὸς κατὰ πᾶσαν βεβαίωσιν ἀπὸ παντὸς <app type="BL"><lem resp="1.292"><gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="2" unit="character"/> του</lem><rdg><supplied reason="lost" cert="high">κινδύ</supplied><unclear reason="undefined" cert="high">ν</unclear>ου</rdg></app> ἐπε<unclear reason="undefined" cert="high">λ</unclear><supplied reason="lost" cert="high">ευσομένου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>ἐ<supplied reason="lost" cert="high">π</supplied>άναγκες. τὸ δὲ γράμμα τοῦτο τῆς ὑπο<supplied reason="lost" cert="high">θήκη</supplied>ς ἁπλοῦ<supplied reason="lost" cert="high">ν σοι ἐξεδόμην πρὸς ἀσφάλειαν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/>τ<gap reason="lost" extent="2" unit="character"/>ου ὑπὲρ ἐμοῦ <app type="BL"><lem resp="1.292">ὑπογράφοντος</lem><rdg>ὑπογράψαντοσ</rdg></app> κύριον καὶ βέβ<unclear reason="undefined" cert="high">α</unclear>ι<unclear reason="undefined" cert="high">ον</unclear> <unclear reason="undefined" cert="high">ὡ</unclear>ς <app type="BL"><lem resp="1.292">ἐν <supplied reason="lost" cert="high">δημοσίῳ κατακείμενον</supplied></lem><rdg>επ<gap reason="lost" extent="unknown" unit="character"/></rdg></app><supplied reason="lost" cert="high"> καὶ ἐπερωτη</supplied>-

