    <lb n="1"/>Ἥρωνι βασιλικῷ <expan>γρ<ex cert="high">αμματεῖ</ex></expan> <expan>Φθεμφουθ<ex cert="high">ίτου</ex></expan> <gap extent="unknown" reason="lost" unit="character"/> 

