    <lb n="15"/>τοίχου τοῦ αὐτοῦ ἀμπελῶνος καὶ <app type="BL"><lem resp="1.295"><del rend="erasure" status="unremarkable"><gap reason="illegible" extent="1" unit="character"/></del> <unclear reason="undefined" cert="high">ἐ</unclear>μ<unclear reason="undefined" cert="high">οῦ</unclear> σπ<unclear reason="undefined" cert="high">ορ</unclear>ί<unclear reason="undefined" cert="high">μης</unclear> ἀρ<unclear reason="undefined" cert="high">ούρ</unclear>ας, β<unclear reason="undefined" cert="high">ορ</unclear><supplied reason="lost" cert="high">ρ</supplied><unclear reason="undefined" cert="high">ᾶ</unclear>.</lem><rdg><gap reason="illegible" extent="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> <note lang="en" anchored="yes" place="not specified">vestig</note></rdg></app> 

