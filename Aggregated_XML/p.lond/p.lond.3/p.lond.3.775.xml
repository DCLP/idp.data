    <lb n="1"/><g type="stauros"/> βασιλείας <supplied reason="lost" cert="high">κ</supplied>αὶ <choice><corr>ὑπατείας</corr><sic><hi rend="diaeresis">υ</hi>πατιας</sic></choice> τοῦ θειοτάτου καὶ <expan>εὐ<unclear reason="undefined" cert="high">σεβεστ</unclear><supplied reason="lost" cert="high">άτ<ex cert="high">ου</ex></supplied></expan> 

    <lb n="2"/>ἡμῶν δ<supplied reason="lost" cert="high">εσ</supplied>πότο<supplied reason="lost" cert="high">υ</supplied> <expan>Φλ<ex cert="high">αυίου</ex></expan> Ἰ<supplied reason="lost" cert="high">ο</supplied>υστίν<supplied reason="lost" cert="high">ο</supplied>υ το<supplied reason="lost" cert="high">ῦ αἰ</supplied>ωνί<supplied reason="lost" cert="high">ου Αὐ</supplied>γού<supplied reason="lost" cert="high">σ</supplied>τ<supplied reason="lost" cert="high">ου</supplied> 

    <lb n="3"/>καὶ <expan>Αὐτοκρ<ex cert="high">άτορος</ex></expan> ἔτους <num value="2">β</num> Θὼθ <num value="14">ιδ</num> <expan>ἰνδ<ex cert="high">ικτίονος</ex></expan> <num value="1">α</num> ἐν <app type="BL"><lem resp="7.86"><expan>Ὀξυρ<ex cert="high">ύγχων</ex></expan></lem><rdg><expan>Ὀξυρ<ex cert="high">υγχιτῶν</ex></expan></rdg></app> <expan>πόλ<ex cert="high">ει</ex></expan>. 

    <lb n="4"/>Φλαουίῳ Ἀπ<supplied reason="lost" cert="high">ί</supplied>ωνι τῷ πανευφήμῳ καὶ <expan>ὑπερφ<ex cert="high">υεστάτῳ</ex></expan> 

    <lb n="5"/>ἀπὸ ὑπάτων ὀρδιναρίων καὶ πατρικίῳ <expan>γεουχ<ex cert="high">οῦντι</ex></expan> 

    <lb n="6"/>καὶ <expan>ἐνταῦθ<ex cert="high">α</ex></expan> τῇ λαμπρᾷ Ὀξυρυγχιτῶν πόλει διὰ 

    <lb n="7"/>Μηνᾶ οἰκέτου τοῦ ἐπερωτῶντος καὶ <expan>προσπορί<unclear reason="undefined" cert="high">ζον</unclear><supplied reason="lost" cert="high">το<ex cert="high">ς</ex></supplied></expan> 

    <lb n="8"/>τῷ <hi rend="diaeresis">ἰ</hi>δίῳ δεσπότῳ τῷ αὐτῷ πανευφήμῳ ἀνδρὶ τὴν ἀγωγὴν 

    <lb n="9"/>καὶ ἐνοχὴν <supplied reason="lost" cert="high">Αὐ</supplied>ρήλιος Ἐπίμαχος ὁ καὶ Ἀπίμα <app type="BL"><lem resp="1.299"><seg cert="low" part="N">υἱ<supplied reason="lost" cert="high">ὸ</supplied>ς <unclear reason="undefined" cert="low">Λ</unclear>αμ<unclear reason="undefined" cert="low">ί</unclear>ωνος</seg></lem><rdg>υἱ<supplied reason="lost" cert="high">ὸς</supplied> <unclear reason="undefined" cert="high">Φ</unclear>λαι<supplied reason="lost" cert="high">ου </supplied><gap reason="lost" extent="2" unit="character"/>ωνο<supplied reason="lost" cert="high">ς</supplied></rdg></app> 

