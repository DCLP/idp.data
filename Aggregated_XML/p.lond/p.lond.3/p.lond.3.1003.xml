    <lb n="2"/><g type="stauros"/> μετὰ τὴν ὑπατείαν <expan>Φλ<ex cert="high">αυίου</ex></expan> <supplied cert="high" reason="lost">Βασιλείου</supplied> <unclear cert="high" reason="undefined">τ</unclear>οῦ <expan>ἐνδοξοτ<supplied cert="high" reason="lost">ά</supplied>το<ex cert="high">υ</ex></expan> ἔτους <app type="BL"><lem resp="1.295"><choice><corr>εἰκ<supplied cert="high" reason="omitted">ο</supplied>στοῦ</corr><sic>εικστου<unclear cert="high" reason="undefined">τ</unclear>ο<unclear cert="high" reason="undefined">υ</unclear></sic></choice> πρώτο<supplied cert="high" reason="lost">υ </supplied><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/></lem><rdg>ε<unclear cert="high" reason="undefined">ἰ</unclear><supplied cert="high" reason="lost">κ</supplied><unclear cert="high" reason="undefined">ο</unclear>στοῦ <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ρώ</supplied>του επ<gap extent="5" reason="lost" unit="character"/></rdg></app> ἑνδε<supplied cert="high" reason="lost">κ</supplied>άτης <expan>ἰνδ<ex cert="high">ικτίονος</ex></expan>. 

    <lb n="3"/>Αὐρηλίαι Ἀρχοντίᾳ Α<gap extent="1" reason="illegible" unit="character"/><gap extent="6" reason="lost" unit="character"/> καὶ Οὐανθίᾳ <expan>Ἰωάννο<ex cert="high">υ</ex></expan> ἀπὸ τῆς Ἑρμου<supplied cert="high" reason="lost">πολι</supplied>τῶν <expan>π<ex cert="high">αρὰ</ex></expan> <expan>Α<supplied cert="high" reason="lost">ὐ</supplied>ρηλίο<ex cert="high">υ</ex></expan> Ὡρ<unclear cert="high" reason="undefined">ίτο</unclear>υ <expan>Ἰσιδώρο<ex cert="high">υ</ex></expan> 

    <lb n="4"/>μητρὸς Ἡραείδος ἀμ<supplied cert="high" reason="lost">φοτέρων</supplied> ἀπὸ κώμης Σινα<unclear cert="high" reason="undefined">κυ</unclear>ρέως τοῦ Ἑρμουπ<supplied cert="high" reason="lost">ολίτ</supplied>ου <expan>νομο<ex cert="high">ῦ</ex></expan> χαίρειν· ὁμολογῶ ἐγὼ ὁ <expan>προγεγραμ<ex cert="high">μένος</ex></expan> 

    <lb n="5"/>Ὥριτος διὰ ταύτης μου <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ῆς ἐγγράφ</supplied>ου ἀσφαλείας ἑκουσίως καὶ  αὐθαιρέτως <unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">μ</unclear>ισθῶσθαι παρὰ <expan>σο<ex cert="high">ῦ</ex></expan> ἐφ᾽ <supplied cert="high" reason="lost">ὅσ</supplied>ον ἐ<unclear cert="high" reason="undefined">ὰ</unclear>ν <expan>βο<ex cert="high">υ</ex></expan>λη-

    <lb n="6"/>θῇς χρόνον λογιζό<supplied cert="high" reason="lost">μενον ἀπὸ</supplied> καρπῶν τῆς εὐτυχοῦς εἰσιούσης δωδεκ<supplied cert="high" reason="lost">άτη</supplied>ς <expan>ἰνδ<ex cert="high">ικτίονος</ex></expan> καὶ αὐτῆς τὸ ὑπάρχον <app type="BL"><lem resp="1.296">σ<unclear cert="high" reason="undefined">οι</unclear></lem><rdg><choice><corr>σοι</corr><sic>σ<unclear cert="high" reason="undefined">ω</unclear></sic></choice></rdg></app> ἀρούρας 

    <lb n="7"/><choice><corr>τρεῖς</corr><sic>τρια</sic></choice> <num value="3"/> χέρσας χαρ<gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀμπ</supplied>ελικῷ πλέω <choice><corr>ἐλάττω</corr><sic>ελλαττ<supplied cert="high" reason="lost">ω</supplied></sic></choice> <supplied cert="high" reason="lost">ἀ</supplied>ποκ<unclear cert="high" reason="undefined">α</unclear>λ<unclear cert="high" reason="undefined">α</unclear>μουργο<supplied cert="high" reason="lost">υμ</supplied>ένων <unclear cert="high" reason="undefined">ἐ</unclear>κ το<unclear cert="high" reason="undefined">ῦ</unclear> <expan><unclear cert="high" reason="undefined">τ</unclear><gap extent="1" reason="lost" unit="character"/>χο<ex cert="high">υ</ex></expan> ἐκ τ<unclear cert="high" reason="undefined">ε</unclear><gap extent="2" reason="illegible" unit="character"/>τρα<unclear cert="high" reason="undefined">ν</unclear>ωτος 

    <lb n="8"/>κατὰ <expan>τρόπο<ex cert="high">υ</ex></expan> σὺν τῷ ὁλο<unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">λήρ</supplied><unclear cert="high" reason="undefined">ῳ</unclear> <choice><corr>λάκκῳ</corr><sic><expan>λακκο<ex cert="high">υ</ex></expan></sic></choice> καὶ <choice><corr>ὑδρεύματι</corr><sic>υδρευμ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">τ</supplied>ος</sic></choice> κα<unclear cert="high" reason="undefined">ὶ</unclear> ξυλικῷ ὀρ<unclear cert="high" reason="undefined">γ</unclear><supplied cert="high" reason="lost">ά</supplied><unclear cert="high" reason="undefined">ν</unclear>ῳ κ<supplied cert="high" reason="lost">αὶ</supplied> χρηστηρίοις <choice><corr>πᾶσι</corr><sic>πασει</sic></choice> καὶ παντὶ 

    <lb n="9"/>δικαίῳ διακειμένας ἐν <supplied cert="high" reason="lost">π</supplied>εδιάδι κώμης <app type="BL"><lem resp="1.296">Πέσλα</lem><rdg>Τεσλᾶ</rdg></app> τοῦ Ἑρμουπολίτου νομο<supplied cert="high" reason="lost">ῦ ἐ</supplied>κ <expan>το<ex cert="high">ῦ</ex></expan> <expan>λ<unclear cert="high" reason="undefined">εγ</unclear><supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">μ</unclear>ένο<ex cert="high">υ</ex></expan> <expan>ὀργάνο<ex cert="high">υ</ex></expan> Ἀφοῦτος 

    <lb n="10"/>ἐφ᾽ ᾧ τε τῆς <choice><corr>ἀρδείας</corr><sic>αρτιας</sic></choice> γιγνόμενα διὰ τῶν ἐμῶν <app type="BL"><lem resp="1.296">ζῴων τῶν</lem><rdg>ζ<unclear cert="high" reason="undefined">ώ</unclear>ντ<unclear cert="high" reason="undefined">ω</unclear>ν</rdg></app> καὶ τρεφομένω<unclear cert="high" reason="undefined">ν</unclear> μέχρι <supplied cert="high" reason="lost">τ</supplied>ὴν <app type="BL"><lem resp="1.296">ἀμπελουργικὴν</lem><rdg>ἀμπελουργ<unclear cert="high" reason="undefined">ί</unclear><supplied cert="high" reason="lost">αν </supplied><gap extent="1" reason="lost" unit="character"/>η <unclear cert="high" reason="undefined">οὖ</unclear>ν</rdg></app> 

    <lb n="11"/>ἐργασίαν καὶ πᾶσαν <choice><corr>ἐπιμέλειαν</corr><sic>επι<supplied cert="high" reason="lost">μ</supplied>ελιαν</sic></choice> καὶ φιλοκαλίαν καὶ σκάψιν πέντε <num value="5"/> τοῦ <expan>ἐν<supplied cert="high" reason="lost">ι</supplied>αυτ<supplied cert="high" reason="lost">ο<ex cert="high">ῦ</ex></supplied></expan> <gap extent="2" reason="illegible" unit="character"/>λιδομιον καὶ <gap extent="3" reason="illegible" unit="character"/>ιδομιον 

    <lb n="12"/><app type="BL"><lem resp="1.296"><gap extent="unknown" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ος</unclear></lem><rdg><gap extent="unknown" reason="lost" unit="character"/> ο</rdg></app> ἐκτελεῖ σοι καὶ τὰ <app type="BL"><lem resp="1.296">περιγιγνόμενα</lem><rdg>περιπ<gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ρ</unclear>ομενα</rdg></app> ἐν τῇ σου αφ<gap extent="1" reason="lost" unit="character"/>η<unclear cert="high" reason="undefined">ς</unclear> διαι<supplied cert="high" reason="lost">τ</supplied>ηθῆναι τὰ οι κη<unclear cert="high" reason="undefined">τα</unclear>σομένων 

