    <lb n="1"/><expan><ex cert="high">ἔτους</ex></expan> <num value="12">ιβ</num> <expan>Φαμ<ex cert="high">ενὼθ</ex></expan> <num value="15">ι<unclear reason="undefined" cert="high">ε</unclear></num>. <expan>κ<ex cert="high">ριθῆς</ex></expan> <expan>ἀν<ex cert="high">ηλωτικῶι</ex></expan> <expan><ex cert="high">ἀρτάβαι</ex></expan> <num value="12">ιβ</num>. 

    <lb n="2"/>Ἀπολλώνιος Ἀπολλωνίωι 

    <lb n="3"/>χαίρειν. μετρήσατε 

    <lb n="4"/>μετὰ τοῦ ἐπακολουθοῦντος 

    <lb n="5"/>ἀπὸ τῶν <expan>γενη<ex cert="high">μάτων</ex></expan> τοῦ <num value="12">ιβ</num> <expan><ex cert="high">ἔτους</ex></expan> Ἀπολλωνίωι 

    <lb n="6"/>ἐφόδωι εἴς τε αὐτὸν καὶ ἄλλους <num value="7">ζ</num> 

    <lb n="7"/>ὥστ᾽ εἶναι <num value="8">η</num> τοῖς ἐκ τοῦ 

    <lb n="8"/><choice><corr>Ἑρ<supplied reason="omitted" cert="high">μο</supplied>πολίτου</corr><sic>ερπολιτου</sic></choice> συναπεσταλ-

    <lb n="9"/>μένοις τοῖς ἐκ τοῦ αὐτοῦ 

    <lb n="10"/>νομοῦ πορείοις τοῖς 

    <lb n="11"/>μεταγομένοις εἰς τὸ μετὰ 

    <lb n="12"/>τοῦ βασιλέως στρατόπεδον 

    <lb n="13"/>τὴν καθήκουσαν τροφὴν 

    <lb n="14"/>τοῖς ἵπποις ἀπὸ <num value="16">ιϛ</num> τοῦ Φαμενὼθ 

    <lb n="15"/>ἕως <num value="5">ε</num> τοῦ Φαρμοῦθι, τῆς 

    <lb n="16"/><expan>ἡμέ<ex cert="high">ρας</ex></expan> εἰς <expan>ἕκα<ex cert="high">στον</ex></expan> <expan>ἵπ<ex cert="high">πον</ex></expan> <expan>χ<ex cert="high">οίνικες</ex></expan> <num value="3">γ</num>, <num value="1/2">𐅵</num> <expan>χ<ex cert="high">οίνικες</ex></expan> <num value="4">δ</num>, τὰς πάσας 

    <lb n="17"/>κριθῆς <expan>ἀνη<ex cert="high">λωτικῶι</ex></expan> <expan><ex cert="high">ἀρτάβας</ex></expan> δεκαδύο <expan><ex cert="high">γίνεται</ex></expan> <num value="12">ιβ</num>, καὶ <expan>σύ<ex cert="high">μβολον</ex></expan> 

    <lb n="18"/><expan>πο<ex cert="high">ίησον</ex></expan> πρὸς <expan>αὐ<ex cert="high">τὸν</ex></expan> ὡς εἴθισται.

    <lb n="19"/><expan>ἔρρ<ex cert="high">ωσο</ex></expan>. <expan><ex cert="high">ἔτους</ex></expan> <num value="12">ιβ</num>, Φαμενὼθ <num value="16">ιϛ</num>. 

    <lb n="20"/>Ἀπολλώνιος. <expan>μέ<ex cert="high">τρησον</ex></expan> <expan>κ<ex cert="high">ριθῆς</ex></expan> δεκαδύο, 

