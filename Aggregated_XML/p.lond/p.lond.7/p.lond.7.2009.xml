    <lb n="2"/>καὶ ἐν τ<unclear cert="high" reason="undefined">ῶ</unclear>ι <num value="2">β</num> <expan><ex cert="high"><unclear cert="high" reason="undefined">ἔτει</unclear></ex></expan> <gap extent="5" reason="lost" unit="character"/> 

    <lb n="3"/>τῆι <num value="7">ζ</num> εἰς τὴν <num value="8"><unclear cert="high" reason="undefined">η</unclear></num> <supplied cert="high" reason="lost">ἦλθεν μετά</supplied> 

    <lb n="3a"/>με<unclear cert="high" reason="undefined">θυό</unclear><supplied cert="high" reason="lost">ντων </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>τινων καὶ <unclear cert="high" reason="undefined">ἔ</unclear>κρ<supplied cert="high" reason="lost">ουσεν τὴν</supplied> 

    <lb n="5"/>θύραν κε<unclear cert="high" reason="undefined">λεύων</unclear> <choice><corr>ἄ<supplied cert="high" reason="lost">νοιξον</supplied></corr><sic>α<supplied cert="high" reason="lost">νυξον</supplied></sic></choice><supplied cert="high" reason="lost"> ἵνα</supplied> 

    <lb n="6"/>πίωμεν. ἐμο<unclear cert="high" reason="undefined">ῦ</unclear> <unclear cert="high" reason="undefined">δὲ</unclear> <choice><corr>προσ<supplied cert="high" reason="lost">φώ</supplied>-

    <lb n="7"/>νήσαντος</corr><sic>προσ<unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">φω</supplied>νησα</sic></choice> αὐτῶι οὐκ ἔχω οἶνον 

    <lb n="8"/>οὐδὲ <choice><corr>ἀνοίγω</corr><sic>ανυγω</sic></choice> νύκτ<unclear cert="high" reason="undefined">α</unclear>, <unclear cert="high" reason="undefined">ἔλε</unclear>γέν 

    <lb n="9"/>μοι <choice><corr>ἀνοῖξαι</corr><sic>ανυξαι</sic></choice>, καὶ ἄλλου <choice><corr>προπεμ-

    <lb n="10"/>πομένου</corr><sic>πρ<unclear cert="high" reason="undefined">οπ</unclear>ε<unclear cert="high" reason="undefined">μ</unclear>ψομενου</sic></choice> προσεῖχον <choice><corr>ἀνοῖξαι</corr><sic>ανυξαι</sic></choice>, 

    <lb n="11"/>ἐξεμόχλευεν τὴν θύραν 

    <lb n="12"/><unclear cert="high" reason="undefined">τ</unclear>αῖς μαχαίραις καὶ ῥάβδο<unclear cert="high" reason="undefined">ις</unclear>. 

    <lb n="13"/>ἀναπηδήσαντος δέ μου 

    <lb n="14"/>ἄνω ἐπὶ <choice><corr>το<supplied cert="high" reason="omitted">ῦ</supplied></corr><sic>το</sic></choice> δώματος 

    <lb n="15"/>βοῶντος οὕτως ἀπηλλάγη. 

    <lb n="16"/>ἀξιῶ οὖν σε <unclear cert="high" reason="undefined">γ</unclear>ράψαι ὧι <unclear cert="high" reason="undefined">κα</unclear>-

    <lb n="17"/><unclear cert="high" reason="undefined">θή</unclear>κει ἀποστεῖλαι πρός σε 

    <lb n="18"/>ἵνα ἐὰν ἃ γράφω ἀληθῆ, <unclear cert="high" reason="undefined">τύχω</unclear> 

    <lb n="19"/>τοῦ δικαίου. 

