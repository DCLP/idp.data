    <lb n="16"/>μέρη πέντε <num value="5"/> ἀπὸ μέρων <supplied cert="high" reason="lost">ἓ</supplied>ξ <num value="6"/> ἐκ <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="17"/>ενοντων μέτρων <expan>κ<ex cert="high">αὶ</ex></expan> πηχῶ<supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="18"/>θυρὰν εἰς τὴν <choice><corr>ἐκ</corr><sic>εγ</sic></choice> λιβὸς ῥύμην βασιλικὴ<supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τὸ ἐπι</supplied>-

