    <lb n="1"/>Αὐρηλίοις Κορακιῶνι <choice><corr>ἐκ</corr><sic>εγ</sic></choice> μέρους <expan>κ<unclear reason="undefined" cert="high">οσ</unclear><ex cert="high">μητεύσαντι</ex></expan> καὶ Ἡρακλείδῃ 

    <lb n="2"/><choice><corr>ἐκ</corr><sic><unclear reason="undefined" cert="high">εγ</unclear></sic></choice> <unclear reason="undefined" cert="high">μ</unclear>έρους <expan>ἀρχι<ex cert="high">ερατεύσαντι</ex></expan> τῆς <expan>Ἀρσι<ex cert="high">νοιτῶν</ex></expan> πόλεως αἱρεθεῖσι ὑπὸ τῆς 

    <lb n="3"/><expan>κρα<ex cert="high">τίστης</ex></expan> βουλῆς ἐπί <unclear reason="undefined" cert="high">τ</unclear>ε λιμνα<unclear reason="undefined" cert="high">σ</unclear>μοῦ <supplied reason="lost" cert="high">κα</supplied>ὶ <app type="BL"><lem resp="5.29"><seg cert="low" part="N">ἀρδιῶν</seg></lem><rdg><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="2" unit="character"/></rdg></app> κατασπορᾶς 

    <lb n="4"/>καὶ <choice><corr>τῆ<supplied reason="omitted" cert="high">ς</supplied></corr><sic>τη</sic></choice> τῶν σπερμάτ<unclear reason="undefined" cert="high">ων</unclear> <app type="BL"><lem resp="1.136"><app type="BL"><lem resp="6.38"><unclear reason="undefined" cert="high">ἀν</unclear><supplied reason="lost" cert="high">αδόσεω</supplied><unclear reason="undefined" cert="high">ς</unclear></lem><rdg>δ<unclear reason="undefined" cert="high">ι</unclear><supplied reason="lost" cert="high">αδόσεω</supplied><unclear reason="undefined" cert="high">ς</unclear></rdg></app> <expan><unclear reason="undefined" cert="high">Θεμ</unclear><ex cert="high">ίστου</ex></expan> <unclear reason="undefined" cert="high">μερί</unclear>δος</lem><rdg>δ<unclear reason="undefined" cert="high">ι</unclear><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/>δοσ</rdg></app> 

    <lb n="5"/>παρὰ Αὐρ<unclear reason="undefined" cert="high">ηλί</unclear>ω<unclear reason="undefined" cert="high">ν</unclear> Μ<unclear reason="undefined" cert="high">έλ</unclear>αν<unclear reason="undefined" cert="high">ος</unclear> <gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="4" unit="character"/> κ<supplied reason="lost" cert="high">α</supplied>ὶ Εὐπο-

    <lb n="6"/>ρᾶ Σαραπίωνος καὶ Παλ<unclear reason="undefined" cert="high">έμπιος</unclear> <gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="2" unit="character"/> τῶν <num value="2">β</num> 

    <lb n="7"/>καὶ τῶν <expan>λο<unclear reason="undefined" cert="high">ι</unclear><supplied reason="lost" cert="high">π<ex cert="high">ῶν</ex></supplied></expan> <expan><unclear reason="undefined" cert="high">π</unclear>ρεσβ<ex cert="high">υτέρων</ex></expan> <unclear reason="undefined" cert="high">κα</unclear>ὶ <gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="3" unit="character"/><unclear reason="undefined" cert="high">ω</unclear>ς 

    <lb n="8"/><choice><corr>ὁριοδ<supplied reason="omitted" cert="high">ε</supplied>ίκτου</corr><sic>οριοδικτου</sic></choice> τῶν <unclear reason="undefined" cert="high">π</unclear><gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="3" unit="character"/>οης. 

    <lb n="9"/>αἰτούμεθα ἐπι<unclear reason="undefined" cert="high">σ</unclear>τα<unclear reason="undefined" cert="high">λ</unclear><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="3" unit="character"/><gap reason="illegible" extent="3" unit="character"/> <unclear reason="undefined" cert="high">σπέρ</unclear>ματα 

    <lb n="10"/>τῇ ἐπ᾽ ἀγαθοῖς γειν<unclear reason="undefined" cert="high">ο</unclear>μ<gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="4" unit="character"/> <unclear reason="undefined" cert="high">το</unclear>ῦ <expan>ἐνεσ<unclear reason="undefined" cert="high">τῶ</unclear><ex cert="high">τος</ex></expan> 

    <lb n="11"/><num value="3">γ</num> <expan><ex cert="high">ἔτους</ex></expan> ἀπὸ <expan>γενή<ex cert="high">ματος</ex></expan> τοῦ <expan>δι<unclear reason="undefined" cert="high">ελ</unclear><ex cert="high">ηλυθότος</ex></expan> <num value="2">β</num> <expan><ex cert="high">ἔτους</ex></expan> <gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="4" unit="character"/> <unclear reason="undefined" cert="high">ἐξ</unclear> <unclear reason="undefined" cert="high">α</unclear>ὐτῆς 

    <lb n="12"/>πυροῦ ἀρτάβας ἑκατὸν <unclear reason="undefined" cert="high">τριάκοντα</unclear> <unclear reason="undefined" cert="high">ἓξ</unclear>, <expan><ex cert="high">γίνονται</ex></expan> <expan><ex cert="high">πυροῦ</ex></expan> <expan><ex cert="high">ἀρτάβαι</ex></expan> <num value="136">ρλϛ</num>, 

    <lb n="13"/>ἃς καὶ καταθησόμεθα <app type="BL"><lem resp="1.136"><unclear reason="undefined" cert="high">σ</unclear>α<unclear reason="undefined" cert="high">φ</unclear>ῶς</lem><rdg><gap reason="illegible" extent="3" unit="character"/>ωσ</rdg></app> καὶ πιστῶς εἰς τὴν 

    <lb n="14"/>ἐν πυρῷ σπειρομέν<supplied reason="lost" cert="high">η</supplied>ν γὴν καὶ εἰσενεγ<g type="apostrophe"/>κοῦμεν 

    <lb n="15"/>ἐν πρώτοις ἅμα τοῖς τῆς κώμης δημοσίοις 

    <lb n="16"/>πᾶσι. Αὐρηλίοι Μέλας καὶ Εὐπορᾶς καὶ Παλέμπις 

    <lb n="17"/>αἰτούμεθα τὰ σπέρματα ὡς <expan>π<ex cert="high">ρόκειται</ex></expan>. Αὐρήλιος Πρω-

    <lb n="18"/>τᾶς <expan>ἔγρα<ex cert="high">ψα</ex></expan> ὑπὲρ αὐτῶν <expan>ἀγρα<ex cert="high">μμάτων</ex></expan>. 

