    <lb n="2"/><supplied cert="high" reason="lost">ν</supplied>έας <hi rend="diaeresis">Ἰ</hi>ουστίνου π<supplied cert="high" reason="lost">ό</supplied>λεως Αὐ<unclear cert="high" reason="undefined">ρή</unclear>λ<unclear cert="high" reason="undefined">ι</unclear><supplied cert="high" reason="lost">ος Ἀ</supplied>γένι<supplied cert="high" reason="lost">ς υἱὸ</supplied>ς Ἀβάρωνο<unclear cert="high" reason="undefined">ς</unclear> 

    <lb n="3"/>μητ<supplied cert="high" reason="lost">ρ</supplied>ὸς Θραησιοισμα<unclear cert="high" reason="undefined">β</unclear><gap extent="2" reason="illegible" unit="character"/>ροσ<gap extent="3" reason="illegible" unit="character"/>θρυσκ<gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ν</unclear> ἀπὸ <expan>ἐποικ<ex cert="high">ίου</ex></expan> 

    <lb n="4"/><choice><corr>Εὐνούχου</corr><sic>ευνουχοι</sic></choice> τοῦ <hi rend="diaeresis">Ἰ</hi>ουστινουπολείτ<supplied cert="high" reason="lost">ου ν</supplied>ομοῦ. ὁμολογῶ ἐσχηκέν<supplied cert="high" reason="lost">αι</supplied> 

    <lb n="5"/>παρά σου ἐντεῦθεν τὴν συναρέσασάν μοι τιμὴν 

    <lb n="6"/>πλῆρες οἴνου ἐξαξεστιαίων <choice><corr>σηκωμάτων</corr><sic>σηκοματων</sic></choice> <expan>τριακοσίω<ex cert="high">ν</ex></expan> 

    <lb n="7"/>ἑξήκοντα, <expan>γί<ex cert="high">νεται</ex></expan> <expan>οἴ<ex cert="high">νου</ex></expan> ἐξαξεστιαίων <expan>σηκ<ex cert="high">ωμάτων</ex></expan> <num value="360">τξ</num>, ὅνπερ οἶνον 

    <lb n="8"/>παρὰ <choice><corr>ληνὸν</corr><sic>ληλων</sic></choice> ἀπὸ <choice><corr>γλεύκους</corr><sic>γλαυκο<add place="supralinear">υ</add>ς</sic></choice> ἀδόλου σου παρέχοντος 

    <lb n="9"/>τὰ κοῦφα ἐπάναγκες ἀποδώσω σοι ἐν τῇ τρύγῃ 

    <lb n="10"/>τοῦ Μεσορὴ μηνὸς τῆς παρούσης τετάρτης <expan><hi rend="diaeresis">ἰ</hi>νδι<ex cert="high">κτίονος</ex></expan> 

    <lb n="11"/>οἴνου ῥύσεως τῆς σὺν θεῷ πέμπτης ἐπινεμήσεω<supplied cert="high" reason="lost">ς</supplied> 

    <lb n="12"/>ἀνυπερθ<supplied cert="high" reason="lost">έτω</supplied>ς <choice><corr>ἀναδέχομαι</corr><sic>αναδεχομε</sic></choice> δ<unclear cert="high" reason="undefined">ὲ</unclear> τὴν τοῦ ο<supplied cert="high" reason="lost">ἴ</supplied>νου καλλονὴν 

    <lb n="13"/>καὶ παραμονὴν μέχρι ὅλου τοῦ Τῦβι μηνὸς καὶ εἴ γε 

    <lb n="14"/>εὑρηθείη ἐν αὐτῷ μέρη ἢ πάντ<supplied cert="high" reason="lost">α</supplied> φαῦλα ἐμὲ ταῦτα 

    <lb n="15"/>ἀλλάξαι καὶ παρασχεῖν σοι <app type="BL"><lem resp="2.2.58">ἐ<unclear cert="high" reason="undefined">ν</unclear> <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ρ</supplied><unclear cert="high" reason="undefined">ωτ</unclear>ίῳ <choice><corr>οἴνῳ</corr><sic>οι<add place="supralinear">ν</add>ο<unclear cert="high" reason="undefined">ν</unclear></sic></choice></lem><rdg>Ε<gap extent="2" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">σ</unclear>ιω οἶ<add place="supralinear">ν</add>ο<unclear cert="high" reason="undefined">ν</unclear></rdg></app> <unclear cert="high" reason="undefined">τ</unclear>ὸν <unclear cert="high" reason="undefined">δ</unclear><supplied cert="high" reason="lost">ὲ</supplied> 

    <lb n="16"/>αὐτὸν οἶνον ὁμολογῶ διδόναι σοι ἐκ τῆς μηχανῆς 

    <lb n="17"/>καλουμένης <unclear cert="high" reason="undefined">Τζ</unclear>ηρὰς καὶ λογ<unclear cert="high" reason="undefined">ο</unclear><gap extent="2" reason="lost" unit="character"/>αλομενος 

    <lb n="18"/>κνίδια <num value="4">δ</num> καὶ μουστάρια <num value="4">δ</num> τὴν δὲ μεταφορὰν 

    <lb n="19"/>τῶν κούφων τοῦ οἴνου ἐμὲ π<unclear cert="high" reason="undefined">λ</unclear>ήσασθαι δίχα 

    <lb n="20"/><choice><corr>βλάβης</corr><sic>αβραβης</sic></choice> <choice><corr>ἀνυπερθέτως</corr><sic>ανυπερθετος</sic></choice> κύριον τ<unclear cert="high" reason="undefined">ὸ</unclear> <expan><unclear cert="high" reason="undefined">π</unclear><ex cert="high">αρὸν</ex></expan> <choice><corr><expan>γραμμά<ex cert="high">τιον</ex></expan></corr><sic>γλαμμα</sic></choice> <expan>ἁπλ<ex cert="high">οῦν</ex></expan> 

    <lb n="21"/>γραφὲν καὶ <expan>ἐπερωτηθ<ex cert="high">εὶς</ex></expan> <expan>ὡμολ<ex cert="high">όγησα</ex></expan>. 
<milestone rend="paragraphos" unit="undefined"/>
    <lb n="22"/><handShift new="m2"/> Αὐρήλιος Ἀγένις <hi rend="diaeresis">υ</hi>ἱὸς Ἀβάρ<unclear cert="high" reason="undefined">ω</unclear>νος <choice><corr>στοιχεῖ</corr><sic>στοιχι</sic></choice> 

    <lb n="23"/>τοῦτο τὸ γρ<supplied cert="high" reason="lost">άμματιον ὡς π</supplied>ρόκειται 

    <lb n="24"/>Παπνούθι<supplied cert="high" reason="lost">ο</supplied>ς ἀ<supplied cert="high" reason="lost">ξιωθεὶς</supplied> ἔγραψα ὑπὲρ 

    <lb n="25"/>αὐτοῦ ἀγραμμάτου ὄντος. <g type="stauros"/> 

