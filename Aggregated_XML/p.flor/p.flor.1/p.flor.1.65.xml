    <lb n="5"/>παρά σου ἐντεῦθεν τὴν συναρέσασάν μοι τιμὴν 

    <lb n="6"/>πλῆρες οἴνου ἐξαξεστιαίων <choice><corr>σηκωμάτων</corr><sic>σηκοματων</sic></choice> <expan>τριακοσίω<ex cert="high">ν</ex></expan> 

    <lb n="7"/>ἑξήκοντα, <expan>γί<ex cert="high">νεται</ex></expan> <expan>οἴ<ex cert="high">νου</ex></expan> ἐξαξεστιαίων <expan>σηκ<ex cert="high">ωμάτων</ex></expan> <num value="360">τξ</num>, ὅνπερ οἶνον 

    <lb n="8"/>παρὰ <choice><corr>ληνὸν</corr><sic>ληλων</sic></choice> ἀπὸ <choice><corr>γλεύκους</corr><sic>γλαυκο<add place="supralinear">υ</add>ς</sic></choice> ἀδόλου σου παρέχοντος 

    <lb n="9"/>τὰ κοῦφα ἐπάναγκες ἀποδώσω σοι ἐν τῇ τρύγῃ 

    <lb n="10"/>τοῦ Μεσορὴ μηνὸς τῆς παρούσης τετάρτης <expan><hi rend="diaeresis">ἰ</hi>νδι<ex cert="high">κτίονος</ex></expan> 

    <lb n="11"/>οἴνου ῥύσεως τῆς σὺν θεῷ πέμπτης ἐπινεμήσεω<supplied reason="lost" cert="high">ς</supplied> 

    <lb n="12"/>ἀνυπερθ<supplied reason="lost" cert="high">έτω</supplied>ς <choice><corr>ἀναδέχομαι</corr><sic>αναδεχομε</sic></choice> δ<unclear reason="undefined" cert="high">ὲ</unclear> τὴν τοῦ ο<supplied reason="lost" cert="high">ἴ</supplied>νου καλλονὴν 

    <lb n="13"/>καὶ παραμονὴν μέχρι ὅλου τοῦ Τῦβι μηνὸς καὶ εἴ γε 

    <lb n="14"/>εὑρηθείη ἐν αὐτῷ μέρη ἢ πάντ<supplied reason="lost" cert="high">α</supplied> φαῦλα ἐμὲ ταῦτα 

    <lb n="15"/>ἀλλάξαι καὶ παρασχεῖν σοι <app type="BL"><lem resp="2.2.58">ἐ<unclear reason="undefined" cert="high">ν</unclear> <unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ρ</supplied><unclear reason="undefined" cert="high">ωτ</unclear>ίῳ <choice><corr>οἴνῳ</corr><sic>οι<add place="supralinear">ν</add>ο<unclear reason="undefined" cert="high">ν</unclear></sic></choice></lem><rdg>Ε<gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">σ</unclear>ιω οἶ<add place="supralinear">ν</add>ο<unclear reason="undefined" cert="high">ν</unclear></rdg></app> <unclear reason="undefined" cert="high">τ</unclear>ὸν <unclear reason="undefined" cert="high">δ</unclear><supplied reason="lost" cert="high">ὲ</supplied> 

    <lb n="16"/>αὐτὸν οἶνον ὁμολογῶ διδόναι σοι ἐκ τῆς μηχανῆς 

    <lb n="17"/>καλουμένης <unclear reason="undefined" cert="high">Τζ</unclear>ηρὰς καὶ λογ<unclear reason="undefined" cert="high">ο</unclear><gap reason="lost" extent="2" unit="character"/>αλομενος 

    <lb n="18"/>κνίδια <num value="4">δ</num> καὶ μουστάρια <num value="4">δ</num> τὴν δὲ μεταφορὰν 

    <lb n="19"/>τῶν κούφων τοῦ οἴνου ἐμὲ π<unclear reason="undefined" cert="high">λ</unclear>ήσασθαι δίχα 

    <lb n="20"/><choice><corr>βλάβης</corr><sic>αβραβης</sic></choice> <choice><corr>ἀνυπερθέτως</corr><sic>ανυπερθετος</sic></choice> κύριον τ<unclear reason="undefined" cert="high">ὸ</unclear> <expan><unclear reason="undefined" cert="high">π</unclear><ex cert="high">αρὸν</ex></expan> <choice><corr><expan>γραμμά<ex cert="high">τιον</ex></expan></corr><sic>γλαμμα</sic></choice> <expan>ἁπλ<ex cert="high">οῦν</ex></expan> 

    <lb n="21"/>γραφὲν καὶ <expan>ἐπερωτηθ<ex cert="high">εὶς</ex></expan> <expan>ὡμολ<ex cert="high">όγησα</ex></expan>. 
