    <lb n="1"/>Αὐρηλίῳ Ὠριγένει <expan>στρα<ex cert="high">τηγῷ</ex></expan> <expan>Ἑρμοπ<supplied reason="lost" cert="high">ολ<ex cert="high">είτου</ex></supplied></expan> 

    <lb n="2"/>παρὰ <expan>Αὐρ<ex cert="high">ηλίων</ex></expan> Παησίος Στεφάνου μητρὸς <unclear reason="undefined" cert="high">Α</unclear><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/> 

    <lb n="3"/>καὶ Σιλβανοῦ Πακυμίος μητρὸς Ἑλένης <unclear reason="undefined" cert="high">ἀμφοτέ</unclear>-

    <lb n="4"/>ρων κωμαρχῶν κώμης Σενομβώ. <supplied reason="lost" cert="high">ἀν</supplied>αδίδο-

    <lb n="5"/>μεν καὶ προσαγ<g type="apostrophe"/>γέλλομεν τοὺς ἑξῆς ἐγ<g type="apostrophe"/>γε<supplied reason="lost" cert="high">γρ</supplied>αμμένους 

    <lb n="6"/>ἐργάτας ἀπελευ<supplied reason="lost" cert="high">σο</supplied>μένους εἰς τὸ κατὰ Ἀλα<supplied reason="lost" cert="high">βα</supplied>στρίν<unclear reason="undefined" cert="high">ην</unclear> 

    <lb n="7"/>μέταλλον πρὸς ἄμειψιν τῶν ἐκεῖσε προαπ<supplied reason="lost" cert="high">οστ</supplied>αλέντων 

    <lb n="8"/>ἐργατῶν ὄντ<supplied reason="lost" cert="high">α</supplied>ς εὐθέτους καὶ ἐπιτηδείους κ<supplied reason="lost" cert="high">ιν</supplied>-

    <lb n="9"/>δ<supplied reason="lost" cert="high">ύν</supplied>ῳ ἡμῶν κ<supplied reason="lost" cert="high">αὶ</supplied> πάντων τῶν ἀπὸ τῆς κώμη<supplied reason="lost" cert="high">ς</supplied> 

    <lb n="10"/>ἐξ ἀλληλεγ<g type="apostrophe"/>γύης. εἰσὶ δέ· <expan>Αὐρ<ex cert="high">ήλιοι</ex></expan> 

    <lb n="11"/>Εὖτις Σιλβανοῦ 

