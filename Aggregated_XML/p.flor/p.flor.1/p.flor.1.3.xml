    <lb n="13"/>οὕσ<supplied reason="lost" cert="high">πε</supplied>ρ <supplied reason="lost" cert="high">ἐγγυ</supplied>ώμεθα καὶ παρ<supplied reason="lost" cert="high">α</supplied>στή<supplied reason="lost" cert="high">σ</supplied>ο<supplied reason="lost" cert="high">με</supplied>ν ἀμέμ-

    <lb n="14"/><unclear reason="undefined" cert="high">πτ</unclear><supplied reason="lost" cert="high">ως</supplied> ἀπ<supplied reason="lost" cert="high">οπλή</supplied>ρουντας τὴν ἐγ<g type="apostrophe"/>χειρισθεῖσαν 

    <lb n="15"/>α<supplied reason="lost" cert="high">ὐτο</supplied>ῖς ἐργατεί<unclear reason="undefined" cert="high">ας</unclear> χρείαν καὶ μὴ ἀπολιμ<supplied reason="lost" cert="high">π</supplied><unclear reason="undefined" cert="high">ανομένους</unclear> 

    <lb n="16"/>τῶν ἐκεῖσε <supplied reason="lost" cert="high">ἄχ</supplied>ρι κελεύσεως ἤτοι ἀμ<supplied reason="lost" cert="high">εί</supplied>ψ<supplied reason="lost" cert="high">εως</supplied>. ἐὰν δὲ 

    <lb n="17"/>ἀφυστερή<supplied reason="lost" cert="high">σ</supplied><unclear reason="undefined" cert="high">ω</unclear>σι καὶ μὴ παραστήσωμε<supplied reason="lost" cert="high">ν ἡ</supplied>μεῖς 

    <lb n="18"/>αὐτοὶ τὸν <supplied reason="lost" cert="high">ὑπὲρ</supplied> αὐτῶν λόγον ὑπομ<supplied reason="lost" cert="high">εν</supplied>οῦμεν. 

