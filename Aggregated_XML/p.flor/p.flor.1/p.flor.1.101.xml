    <lb n="2"/>Καίσαρ<supplied cert="high" reason="lost">ος τ</supplied>οῦ κυρίου ἀρούρας πεντηκοντατέσσαρας· 

    <lb n="3"/><expan><ex cert="high">γίνονται</ex></expan> <expan><ex cert="high">ἄρουραι</ex></expan> <num value="5"><unclear cert="high" reason="undefined">ε</unclear></num><supplied cert="high" reason="lost"><num value="4">δ</num></supplied>, ἑκάστῳ <expan><ex cert="high">ἄρουραι</ex></expan> <num value="6">ϛ</num>· καὶ δώσεις ἡμῖν ὑπὲρ μισθοῦ 

    <lb n="4"/>θερισμο<supplied cert="high" reason="lost">ῦ ἑ</supplied>κάστης ἀρού<unclear cert="high" reason="undefined">ρ</unclear>ης πυροῦ ἥμισυ τρίτον· <expan><ex cert="high">γίνεται</ex></expan> <expan>ἑκ<ex cert="high">άστης</ex></expan> <expan><ex cert="high">ἀρούρης</ex></expan> <expan><ex cert="high">πυροῦ</ex></expan> <num value="1/2">𐅵</num> <num value="1/3">γ</num>· 

    <lb n="5"/>καὶ μετρ<unclear cert="high" reason="undefined">ή</unclear><supplied cert="high" reason="lost">σ</supplied>εις ἡμῖν μετὰ τὸν θερισμὸν τῶν προκειμέ-

    <lb n="6"/>νων <unclear cert="high" reason="undefined">ἀρ</unclear><supplied cert="high" reason="lost">ουρῶν</supplied> τοὺς <expan>προκείμενο<ex cert="high">υς</ex></expan> <expan>μισθο<ex cert="high">ύς</ex></expan>, ἐφ᾽ ᾧ ἀναλέξῃς 

    <lb n="7"/>σὺ ὁ Εὐ<unclear cert="high" reason="undefined">δ</unclear><supplied cert="high" reason="lost">αίμων τὰ</supplied> <unclear cert="high" reason="undefined">δ</unclear>ράγματα· καὶ χ<unclear cert="high" reason="undefined">ορ</unclear>ηγήσεις ἡμῖν ὕδωρ 

    <lb n="8"/>εἰς <choice><corr>πιεῖν</corr><sic>πειν</sic></choice> ἄ<unclear cert="high" reason="undefined">χ</unclear><supplied cert="high" reason="lost">ρι</supplied> οὗ ἐκβῶμεν τοῦ <expan>θερισμο<ex cert="high">ῦ</ex></expan> <unclear cert="high" reason="undefined">τῶ</unclear>ν <expan>α<ex cert="high">ὐτῶν</ex></expan> <expan>ἀρουρῶ<ex cert="high">ν</ex></expan>· ἐφ᾽ ᾧ, ἐ-

    <lb n="9"/>ὰν <app type="BL"><lem resp="1.148">ἀργήσῃ <supplied cert="low" reason="lost">ποτὲ</supplied></lem><rdg><seg cert="low" part="N">ἀργήση<supplied cert="low" reason="lost">ται</supplied></seg></rdg></app> <unclear cert="high" reason="undefined">ε</unclear>ἷς <num value="1"/> ἐξ ἡμῶν, <expan>ἐπάναγκο<ex cert="high">ν</ex></expan> στήσωμεν ἀντ᾽ <expan>αὐ<unclear cert="high" reason="undefined">το</unclear><ex cert="high">ῦ</ex></expan> 

    <lb n="10"/><unclear cert="high" reason="undefined">ἐν</unclear> τῇ αὐτῇ <supplied cert="high" reason="lost">ἡμέ</supplied>ρᾳ. τὸν δὲ <expan>προκ<ex cert="high">είμενον</ex></expan> <expan>πυρὸ<ex cert="high">ν</ex></expan> μετρήσεις ἡμῖν μέτρωι 

    <lb n="11"/><choice><corr><supplied cert="high" reason="lost">Ἀ</supplied>θηναίῳ</corr><sic><supplied cert="high" reason="lost">α</supplied>θηναιο</sic></choice> ἑ<supplied cert="high" reason="lost">κτημ</supplied>όρωι, καὶ <supplied cert="high" reason="lost">δ</supplied>ώσεις ἡμ<supplied cert="high" reason="lost">ῖν</supplied> τῇ ἐσχάτῃ ἡμέρᾳ ζύ-

    <lb n="12"/>του κερ<supplied cert="high" reason="lost">άμιον ἕ</supplied>ν <num value="1"/>. ἀρξώμεθα δὲ το<supplied cert="high" reason="lost">ῦ</supplied> θερισμοῦ <expan>ἐπάναγκο<ex cert="high">ν</ex></expan> 

    <lb n="13"/>τῇ τριακά<supplied cert="high" reason="lost">δι τοῦ</supplied> <expan>ἐνεστῶτο<ex cert="high">ς</ex></expan> <expan>μηνὸ<ex cert="high">ς</ex></expan> Φαρμοῦθι τοῦ ἐνεστῶ-

    <lb n="14"/>τος δεκά<supplied cert="high" reason="lost">του <expan><ex cert="high">ἔτους</ex></expan></supplied>. <expan>ὑπ<ex cert="high">ὲρ</ex></expan> δὲ τῆς <expan>εὑρεθησομέν<ex cert="high">ης</ex></expan> ἐν σπόρωι με-

    <lb n="15"/>τρήσεις <supplied cert="high" reason="lost">ἡμῖ</supplied>ν, καθὼ<supplied cert="high" reason="lost">ς πρόκειται, <expan>ἑκ<ex cert="high">άστης</ex></expan> <expan><ex cert="high">ἀρούρης</ex></expan></supplied> <expan><ex cert="high">πυροῦ</ex></expan> <num value="1/2">𐅵</num> <num value="1/3">γ</num>. <supplied cert="high" reason="lost"><expan><ex cert="high">ἔτους</ex></expan> δε</supplied><unclear cert="high" reason="undefined">κάτου</unclear> 

