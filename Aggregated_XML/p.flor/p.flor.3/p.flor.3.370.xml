    <lb n="8"/>Σαραπιήου <expan><ex cert="high">ἀρουρῶν</ex></expan> ἑξήκοντα τριῶν ἡμί<supplied reason="lost" cert="high">σ</supplied>ους <num value="63  1/2 "/> <g type="unintelligible"/>, 

    <lb n="9"/>ἔξω ἀρουρῶν ἓξ <num value="6"/> ἅς γ<unclear reason="undefined" cert="high">ε</unclear> ί<unclear reason="undefined" cert="high">σ</unclear>θωσας ἑ<unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">έ</supplied><unclear reason="undefined" cert="high">ρ</unclear>οις 

    <lb n="10"/>πρὸς ἃς οὐχ ἕξω πρᾶγ<unclear reason="undefined" cert="high">μ</unclear>α, ὡς εἶναι τὰς κοιν<supplied reason="lost" cert="high">ὰς</supplied> 

    <lb n="11"/>ἐμοῦ <expan>κ<ex cert="high">αὶ</ex></expan> σοῦ ὡς πρόκειται <expan><ex cert="high">ἀρούρας</ex></expan> <num value="57">νζ</num> <num value="1/2">𐅵</num> <g type="unintelligible"/> <expan><ex cert="high">καὶ</ex></expan> περὶ <supplied reason="lost" cert="high">Τ</supplied>ε<unclear reason="undefined" cert="high">μσεῦ</unclear> 

    <lb n="12"/>ἐκ τοῦ <supplied reason="lost" cert="low">Μ</supplied>ικύλου <expan><ex cert="high">ἀρουρῶν</ex></expan> τρίακοντα ἑπτά <num value="37"/>, <expan><ex cert="high">γίνονται</ex></expan> <expan>ἐπ<ex cert="high">ὶ τὸ αὐτὸ</ex></expan> <expan><ex cert="high">ἄρουραι</ex></expan> <num value="94">ϙδ</num> <num value="1/2"><unclear reason="undefined" cert="high">𐅵</unclear></num> <g type="unintelligible"/> 

