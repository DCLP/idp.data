    <lb n="15"/><supplied cert="high" reason="lost">τοῦ μνημείου </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> χώρημα ἐκ βορρᾶ τῶν</supplied> <unclear cert="high" reason="undefined">αὐ</unclear>τῶν πηχῶν τεσσάρων <num value="4"/>, <add place="supralinear">εἰς ἀπηλιώτην</add>, οσαν ἐστὶν πηχισμοῦ 

    <lb n="16"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">η</unclear>σα τὴν πρᾶσιν. <handShift new="m3"/> Αὐρήλιος Κολλοῦθος Παμουνίου 

    <lb n="17"/><gap extent="unknown" reason="lost" unit="character"/><choice><corr><supplied cert="high" reason="lost">τοῦτό ἐστι</supplied>ν</corr><sic>τουτεστιν</sic></choice> ἀπὸ τοῦ βορινοῦ ἄκρου τοῦ <choice><corr>μνημείου</corr><sic>μνημιου</sic></choice> 

    <lb n="18"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> χώρ</supplied>ημα ἐκ βορρᾶ τῶν αὐτῶν πηχῶν 

    <lb n="19"/><supplied cert="high" reason="lost">τεσσάρων <num value="4"/> </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> καὶ βεβαι</supplied>ώσω, ὡς <expan>πρόκ<ex cert="high">ειται</ex></expan>, καὶ πληρωθεὶς ἀπέ-

    <lb n="20"/><supplied cert="high" reason="lost">σχηκα </supplied><gap extent="unknown" reason="lost" unit="character"/> 

