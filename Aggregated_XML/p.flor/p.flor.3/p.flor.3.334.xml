    <lb n="2"/><expan>Τανυ<ex cert="high">αίθεως</ex></expan> <app type="BL"><lem resp="7.54"><expan>τά<ex cert="high">λαντον</ex></expan> <num value="1">α</num> <expan><ex cert="high">δραχμαὶ</ex></expan></lem><rdg><abbr>ζα</abbr></rdg></app>  <num value="3503">Γ<unclear cert="high" reason="undefined">φ</unclear>γ</num> 

    <lb n="3"/>Πώεως <num value="4923">Δϡκγ</num> 

    <lb n="4"/><expan>Τερύθ<ex cert="high">εως</ex></expan> <num value="4771">Δψοα</num> 

    <lb n="5"/><abbr><unclear cert="high" reason="undefined">Δι</unclear><gap extent="1" reason="illegible" unit="character"/>ο</abbr> <num value="3615">Γχιε</num> <expan><ex cert="high">ὀβολοὶ 4</ex></expan><num value="4"/> 

    <lb n="6"/><expan><supplied cert="high" reason="lost">Ἰ</supplied><unclear cert="high" reason="undefined">β</unclear>ιῶ<ex cert="high">νος</ex></expan> <abbr>ζα</abbr> <num value="3279">Γσοθ</num> <expan><ex cert="high"><unclear cert="high" reason="undefined">ὀβολοὶ</unclear> 2</ex></expan><num value="2"/> 

