    <lb n="5"/>κέναι σοι ὄ<unclear cert="high" reason="undefined">νον</unclear> <choice><corr>μελαίνην</corr><sic>μελενην</sic></choice> ε<unclear cert="high" reason="undefined">γ</unclear>χ<gap extent="4" reason="illegible" unit="character"/><gap extent="3" reason="illegible" unit="character"/> 

    <lb n="6"/>τιμῆς δραχμῶ<unclear cert="high" reason="undefined">ν</unclear> ἑξακοσίω<unclear cert="high" reason="undefined">ν</unclear> <num value="600"/> <unclear cert="high" reason="undefined">ἅσπερ</unclear> 

