    <lb n="1"/>Α<unclear reason="undefined" cert="high">ὐρηλί</unclear>ῳ Διδύμῳ <expan>στρ<ex cert="high">ατηγῷ</ex></expan> <expan>Ἀρσ<ex cert="high">ινοίτου</ex></expan> <expan>Ἡρακ<ex cert="high">λείδου</ex></expan> μερίδος 

    <lb n="2"/>πα<unclear reason="undefined" cert="high">ρὰ</unclear> <supplied reason="lost" cert="high">Μ</supplied><unclear reason="undefined" cert="high">ά</unclear>ρκου Λουκρητίου Διογένους Ἀντινοέως. οὗ ἐπέδωκα Κλαυδ<supplied reason="lost" cert="high">ίῳ</supplied> Ἑρ<supplied reason="lost" cert="high">ε</supplied><unclear reason="undefined" cert="high">νν</unclear>ιανῷ τῷ κρατίστῳ δι<unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">αιο</supplied><unclear reason="undefined" cert="high">δ</unclear>ότῃ διέποντι καὶ τὰ 

    <lb n="3"/>κατὰ <unclear reason="undefined" cert="high">τὴ</unclear>ν ἡγεμονίαν βιβλιδίου καὶ ἧς ἔτυχον ὑπογραφῆς ἀντίγρα<unclear reason="undefined" cert="high">φ</unclear><supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">ν</unclear> <supplied reason="lost" cert="high">ὑ</supplied><unclear reason="undefined" cert="high">πό</unclear>κ<unclear reason="undefined" cert="high">ει</unclear>ται, καὶ ἀξιῶ τὰ ἀκό<unclear reason="undefined" cert="high">λου</unclear><supplied reason="lost" cert="high">θ</supplied>α π<unclear reason="undefined" cert="high">ρ</unclear>αχθῆναι, <unclear reason="undefined" cert="high">ἐ</unclear>πί-

    <lb n="4"/>τ<unclear reason="undefined" cert="high">ροπ</unclear><supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">ν</unclear> καταστῆσαί σε τοῖς ὑπογεγραμμένοις ἀφήλιξι <choice><corr>εἰς</corr><sic>ις</sic></choice> τὸ μηδὲν <supplied reason="lost" cert="high">αὐ</supplied>το<supplied reason="lost" cert="high">ῖς</supplied> παρ<supplied reason="lost" cert="high">α</supplied>πολέσθαι, τοῖς <unclear reason="undefined" cert="high">ν</unclear>όμ<supplied reason="lost" cert="high">ο</supplied>ι<unclear reason="undefined" cert="high">ς</unclear> <unclear reason="undefined" cert="high">ἀκ</unclear>ολο<unclear reason="undefined" cert="high">ύ</unclear>θως. ἔσ<unclear reason="undefined" cert="high">τ</unclear>ι δέ· 

    <lb n="5"/>Τι<unclear reason="undefined" cert="high">βε</unclear><supplied reason="lost" cert="high">ρ</supplied><unclear reason="undefined" cert="high">ί</unclear>ῳ Κλαυδίῳ Ἑρεννιανῷ τῷ κρατίστῳ δικαιοδότῃ διέποντ<unclear reason="undefined" cert="high">ι</unclear> <supplied reason="lost" cert="high">κ</supplied><unclear reason="undefined" cert="high">α</unclear>ὶ <supplied reason="lost" cert="high">τὰ κ</supplied>α<unclear reason="undefined" cert="high">τ</unclear>ὰ τὴν ἡγεμον<unclear reason="undefined" cert="high">ί</unclear>α<unclear reason="undefined" cert="high">ν</unclear> <unclear reason="undefined" cert="high">ἐ</unclear>κ <sic><unclear reason="undefined" cert="high">κ</unclear></sic> θείας κελεύσεως, 

    <lb n="6"/>παρὰ <unclear reason="undefined" cert="high">Μά</unclear>ρκου Λουκρητίου Διογένους Ἀντινοέως. δέησι<unclear reason="undefined" cert="high">ν</unclear> ἁρμόζουσ<supplied reason="lost" cert="high">αν</supplied> τ<unclear reason="undefined" cert="high">ῇ</unclear> <unclear reason="undefined" cert="high">σῇ</unclear> <choice><corr>καλοκἀγαθίᾳ</corr><sic>καλ<unclear reason="undefined" cert="high">ο</unclear>καγα<unclear reason="undefined" cert="high">θ</unclear>ειαν</sic></choice> <unclear reason="undefined" cert="high">π</unclear>ροσφέρων εὔελ<supplied reason="lost" cert="high">π</supplied>ίς <choice><corr>εἰμι</corr><sic>ιμοι</sic></choice> τῶν 

    <lb n="7"/>ἀπὸ σ<supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">ῦ</unclear> δικαίων τυχεῖν. τὸ δὲ πρᾶγμα διὰ βραχέων ὁριῶ. <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ῷ</supplied> δ<unclear reason="undefined" cert="high">ι</unclear><supplied reason="lost" cert="high">ε</supplied><unclear reason="undefined" cert="high">λ</unclear>ηλυθότι <unclear reason="undefined" cert="high">μη</unclear>νὶ Φαῶφ<unclear reason="undefined" cert="high">ι</unclear> ἡ ἀδ<unclear reason="undefined" cert="high">ελ</unclear>φή μου Ὀκ<unclear reason="undefined" cert="high">τ</unclear>αου-

    <lb n="8"/>ί<unclear reason="undefined" cert="high">α</unclear> Λ<unclear reason="undefined" cert="high">ου</unclear>κρητία <supplied reason="omitted" cert="high">ἐτελεύτησεν</supplied> ἀδιάθετος ἐπὶ κληρονόμοις τρισὶν <num value="3"/> υἱοῖς ἐκ διαφόρ<supplied reason="lost" cert="high">ων</supplied> <unclear reason="undefined" cert="high">γ</unclear>ά<supplied reason="lost" cert="high">μ</supplied>ων, Μάρκοις Αὐρ<unclear reason="undefined" cert="high">η</unclear>λίοις Ἰο<unclear reason="undefined" cert="high">υ</unclear>λιᾷ κα<unclear reason="undefined" cert="high">ὶ</unclear> Λουκρη-

    <lb n="9"/>τ<unclear reason="undefined" cert="high">ί</unclear>ῳ <unclear reason="undefined" cert="high">καὶ</unclear> Ῥούφῳ ἀφήλιξι. ὁ μὲν οὖν εἷς αὐτῶν Ἰουλιᾶς ὑποχείριος ὢν <supplied reason="lost" cert="high">τ</supplied>ῷ <unclear reason="undefined" cert="high">ἰδ</unclear>ίῳ πατρὶ Μάρκ<supplied reason="lost" cert="high">ῳ</supplied> Α<unclear reason="undefined" cert="high">ὐ</unclear>ρηλίῳ Ἥρωνι <choice><corr>ἀπείληφεν</corr><sic>απιλ<unclear reason="undefined" cert="high">η</unclear>φεν</sic></choice> 

    <lb n="10"/>τ<unclear reason="undefined" cert="high">ὸ</unclear> <unclear reason="undefined" cert="high">ἐπι</unclear>βάλλον αὐτῷ μέρος <supplied reason="omitted" cert="high">τῶν</supplied> <choice><corr>ἀπολελειμμένων</corr><sic>απολελιμμενων</sic></choice>. οἱ δὲ ἕτεροι ὁ μὲ<supplied reason="lost" cert="high">ν</supplied> <unclear reason="undefined" cert="high">Λο</unclear>υκ<unclear reason="undefined" cert="high">ρή</unclear>τιος ἀπάτω<unclear reason="undefined" cert="high">ρ</unclear> <choice><corr>τυγχάνει</corr><sic>τυγχα<unclear reason="undefined" cert="high">ν</unclear>ι</sic></choice>, τοῦ δὲ Ῥούφου 

    <lb n="11"/>ὁ π<unclear reason="undefined" cert="high">ατὴ</unclear>ρ ἐτελεύτησεν, καὶ <choice><corr>οὐδεὶς</corr><sic>ουδις</sic></choice> αὐτοῖς ὑπάρχει ἐγγυτέρ<unclear reason="undefined" cert="high">ω</unclear> <choice><corr>γένει</corr><sic>γενι</sic></choice> <supplied reason="omitted" cert="high">ἢ</supplied> ἐγ<unclear reason="undefined" cert="high">ὼ</unclear> <supplied reason="lost" cert="high">Λο</supplied>υκρήτιος Δ<unclear reason="undefined" cert="high">ιο</unclear><supplied reason="lost" cert="high">γ</supplied><unclear reason="undefined" cert="high">ένης</unclear>, δι᾽ ο<unclear reason="undefined" cert="high">ὕ</unclear>σπερ τήνδε τὴν <sic>δε την</sic> <add place="supralinear"><gap reason="illegible" extent="4" unit="character"/></add> 

    <lb n="12"/>δ<supplied reason="lost" cert="high">έ</supplied><unclear reason="undefined" cert="high">η</unclear><supplied reason="lost" cert="high">σι</supplied><unclear reason="undefined" cert="high">ν</unclear> ποιοῦμαι, καὶ παρακαλῶ, ἐάν σου τῇ τύχῃ δόξῃ, κελεῦσαι δι᾽ <choice><corr>ἱερ<supplied reason="lost" cert="high">ᾶς</supplied></corr><sic>ειερ<supplied reason="lost" cert="high">ας</supplied></sic></choice> <supplied reason="lost" cert="high">σ</supplied>ου ὑπογραφῆς τῷ τῆς Ἡρακλείδου <unclear reason="undefined" cert="high">μ</unclear>ερίδος 

    <lb n="13"/>τ<unclear reason="undefined" cert="high">οῦ</unclear> <unclear reason="undefined" cert="high">Ἀρ</unclear>σινοείτου νομοῦ στρατηγῷ, ἔνθα ἐστὶν αὐτῶν τὰ πράγμα<supplied reason="lost" cert="high">τ</supplied><unclear reason="undefined" cert="high">α</unclear>, καταστῆσαί με ἐπίτ<unclear reason="undefined" cert="high">ροπον</unclear> τῆς <unclear reason="undefined" cert="high">ὀ</unclear>ρφα<unclear reason="undefined" cert="high">ν</unclear>είας 

    <lb n="14"/>τῶ<unclear reason="undefined" cert="high">ν</unclear> <unclear reason="undefined" cert="high">π</unclear>αίδων, <choice><corr>ἵνα</corr><sic>εινα</sic></choice> δυνηθῶ τὴν διοίκησιν τῶν καταλειφθέντων α<supplied reason="lost" cert="high">ὐτ</supplied><unclear reason="undefined" cert="high">ο</unclear>ῖς ποιήσασθαι, πρὸς τὸ ταῦτα φυλαχθῆναι αὐ-

    <lb n="15"/>τοῖς <supplied reason="lost" cert="high">εἰς</supplied> <unclear reason="undefined" cert="high">τελ</unclear>είωσιν. διευτύχει. Μᾶρκος Λουκρήτιος Διογένης δι᾽ ἐμοῦ Αὐ<supplied reason="lost" cert="high">ρηλί</supplied>ου Σαραπίωνος ἐπιδέδωκα. ὁ στρατηγὸς πρὸ ὀ-

    <lb n="16"/>φθαλ<supplied reason="lost" cert="high">μ</supplied><unclear reason="undefined" cert="high">ῶ</unclear>ν ἔχων τὸν <choice><corr>ἴδιον</corr><sic>ειδιον</sic></choice> κίνδυνον <choice><corr>ἐπίτροπον</corr><sic>επιτροφον</sic></choice> τοῖς ἀφή<unclear reason="undefined" cert="high">λιξ</unclear>ι καταστῆ<unclear reason="undefined" cert="high">σ</unclear>αι <choice><corr>φροντίσει</corr><sic>φροντισι</sic></choice>. <expan>κολλ<ex cert="high">ημάτων</ex></expan> <num value="68">ξη</num>. </ab></div><div n="meta.10693" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.10693" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" value="0225-01-12" notBefore="225" notAfter="225">12. Jan. 225</date></p></div><div n="meta.10693" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Duplikat von P.Harr. I  68.</p></div><div n="meta.10693" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">P.Diog.</title><biblScope type="numbers">18</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">10693</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0100</series>; ; <biblScope type="numbers">18tripl</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0100;;18tripl" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.10693" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p><bibl type="illustration" default="NO">Planche XIII</bibl></p></div><div n="meta.10693" type="history" subtype="locations" org="uniform" part="N" sample="complete"><p><placeName type="ancientFindspot" cert="high" full="yes">Philadelphia</placeName> (<geogN