<placeName cert="high" full="yes">Philadelphia</placeName>
<xref type="reprint from" targOrder="U" from="ROOT" to="DITTO" n="p.harr;1;83" evaluate="all">PHarr1,83</xref></head><div type="edition" lang="grc" org="uniform" part="N" sample="complete"><div type="textpart" n="r" org="uniform" part="N" sample="complete"><ab part="N">

    <lb n="1"/>Μᾶρκος Λουκρήτι<unclear reason="undefined" cert="high">ο</unclear>ς Διογένης καὶ <unclear reason="undefined" cert="high">ἡ</unclear> ἀ<unclear reason="undefined" cert="high">δε</unclear>λ-

    <lb n="2"/>φή μου Λουκρητία Ὀκταυία Ἀντ<unclear reason="undefined" cert="high">ινο</unclear><supplied reason="lost" cert="high">εῖς</supplied> 

    <lb n="3"/>καὶ ὡς <expan>χρη<ex cert="high">ματίζομεν</ex></expan> Ἰγνατίῳ Ἀπολιναρίῳ Ἀ<unclear reason="undefined" cert="high">ν</unclear><supplied reason="lost" cert="high">τινοεῖ</supplied> 

    <lb n="4"/>καὶ ὡς <expan>χρη<ex cert="high">ματίζει</ex></expan> χαίρειν. ὁμολογοῦμε<unclear reason="undefined" cert="high">ν</unclear>  -

    <lb n="5"/>δε τὸ χειρόγραφον εἰληφέναι παρ<unclear reason="undefined" cert="high">ὰ</unclear>  -

    <lb n="6"/>ρίου σεβαστοῦ νομίσματος δραχμὰς τετρ<unclear reason="undefined" cert="high">ακο</unclear>-

    <lb n="7"/>σίας <num value="400"/> ἀριθμοῦ πλήρης καὶ πυροῦ <expan><ex cert="high">ἀρτάβας</ex></expan> <num value="10">ι</num> <expan><unclear reason="undefined" cert="high">γίν</unclear><ex cert="high">ονται</ex></expan> <unclear reason="undefined" cert="high">πυρ</unclear>-

    <lb n="8"/>οῦ <choice><corr>ἀρτάβαι</corr><sic>αρταβας</sic></choice> δέκα <num value="10"/>, μέτρῳ ὀγδόῳ σοῦ Ἀ<unclear reason="undefined" cert="high">πολι</unclear>-

    <lb n="9"/>ναρίου, αἷς οὐδὲν <choice><corr>προσεγράφη</corr><sic>προσεγραφησαν</sic></choice>, ἅσ<unclear reason="undefined" cert="high">πε</unclear>ρ ἀ<unclear reason="undefined" cert="high">πο</unclear>-

    <lb n="10"/>δώσομέν σοι ἐν μηνὶ Παῦνι ἀνυπερθέτ<unclear reason="undefined" cert="high">ω</unclear><supplied reason="lost" cert="high">ς.</supplied> 

    <lb n="11"/>ἐὰν δὲ μὴ ἀποδῶμεν, γινομένης <unclear reason="undefined" cert="high">σ</unclear><supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">ι</unclear> 

    <lb n="12"/>τῆς πράξεως ἔκ τε ἡμῶν καὶ ἐκ τῶν ὑ<unclear reason="undefined" cert="high">π</unclear>α<unclear reason="undefined" cert="high">ρχόν</unclear>-

    <lb n="13"/>των ἡμῖν πάντων καθάπερ ἐκ δίκης. 

    <lb n="14"/>τὸ χειρόγραφον τ<unclear reason="undefined" cert="high">οῦ</unclear>το ἁπλοῦν γραφ<unclear reason="undefined" cert="high">ὲ</unclear>ν <unclear reason="undefined" cert="high">χωρὶς</unclear> 

    <lb n="15"/><choice><corr>ἀλείφατος</corr><sic>αλιφαδος</sic></choice> καὶ ἐπιγραφῆ<unclear reason="undefined" cert="high">ς</unclear> κύρι<unclear reason="undefined" cert="high">ο</unclear>ν ἔστω <unclear reason="undefined" cert="high">παν</unclear>-

    <lb n="16"/>ταχῇ καὶ παντὶ τῷ ἐπιφέροντι ὡς ἐν δη<supplied reason="lost" cert="high">μο</supplied>-

    <lb n="17"/>σίῳ κατακεχωρι<unclear reason="undefined" cert="high">σμ</unclear><supplied reason="lost" cert="high">ένον.</supplied> <handShift new="m2"/> <choice><corr>Λουκρήτιος</corr><sic>λουκρητις</sic></choice> Δ<unclear reason="undefined" cert="high">ιογένης</unclear> 

    <lb n="18"/>καὶ Λουκρητί<unclear reason="undefined" cert="high">α</unclear> <supplied reason="lost" cert="high">λ</supplied><unclear reason="undefined" cert="high">άβο</unclear>μεν τὰς δραχ<unclear reason="undefined" cert="high">μὰς</unclear> 

    <lb n="19"/>τετρακοσίας <num value="400"/> <supplied reason="lost" cert="high">καὶ τ</supplied><unclear reason="undefined" cert="high">ὰ</unclear>ς <unclear reason="undefined" cert="high">τ</unclear>οῦ πυροῦ <unclear reason="undefined" cert="high">ἀρ</unclear>τ<unclear reason="undefined" cert="high">ά</unclear>-

    <lb n="20"/>βας δέκα <num value="10"/> κα<supplied reason="lost" cert="high">ὶ </supplied><choice><corr><supplied reason="lost" cert="high">ἀποδ</supplied>ώσομεν</corr><sic><supplied reason="lost" cert="high">αποδ</supplied><unclear reason="undefined" cert="high">ο</unclear>σωμεν</sic></choice> ὡς π<unclear reason="undefined" cert="high">ρ</unclear>ό-

    <lb n="21"/>κιται. Λουκρήτι<unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">ς ἔγρ</supplied><unclear reason="undefined" cert="high">α</unclear>ψα ὑπὲρ τῆς <unclear reason="undefined" cert="high">ἀδελ</unclear>-

    <lb n="22"/>φῆς μου, γράμ<unclear reason="undefined" cert="high">μ</unclear>ατα μὴ <choice><corr>εἰδυίας</corr><sic>ιδυιης</sic></choice>. 

    <lb n="23"/><handShift new="m1"/> <expan><ex cert="high">ἔτους</ex></expan> <num value="22">κ<unclear reason="undefined" cert="high">β</unclear></num> <unclear reason="undefined" cert="high">Μάρ</unclear>κου Αὐρηλίου Σεο<unclear reason="undefined" cert="high">υ</unclear>ήρου Ἀντωνί<unclear reason="undefined" cert="high">νου</unclear> 

    <lb n="24"/>Παρθικοῦ Μεγίστου <unclear reason="undefined" cert="high">Βρε</unclear>ταννικοῦ <unclear reason="undefined" cert="high">Με</unclear>γίστ<unclear reason="undefined" cert="high">ου</unclear> 

    <lb n="25"/><unclear reason="undefined" cert="high">Εὐσεβοῦς</unclear> Σεβαστοῦ, Ἁθὺρ <num value="16">ι<unclear reason="undefined" cert="high">ϛ</unclear></num>. 
