    <lb n="1"/><g type="stauros"/> μετὰ τὴν ὑπατ<supplied cert="high" reason="lost">είαν</supplied> <unclear cert="high" reason="undefined">το</unclear>ῦ τῆς <unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ακα</supplied><unclear cert="high" reason="undefined">ρί</unclear>ας μνήμης 

    <lb n="2"/>Ἀρμάτου, Παῦν<supplied cert="high" reason="lost">ι</supplied> <num value="2"><unclear cert="high" reason="undefined">β</unclear></num>. τῆς εὐ<supplied cert="high" reason="lost">τυ</supplied><unclear cert="high" reason="undefined">χο</unclear><supplied cert="high" reason="lost">ῦ</supplied><unclear cert="high" reason="undefined">ς</unclear> πρώτης <expan>ἰνδικ<ex cert="high">τίωνος</ex></expan> 

    <lb n="3"/>ἐ<unclear cert="high" reason="undefined">ν</unclear> κώμης Φεβίχεως μεγάλη<supplied cert="high" reason="lost">ς</supplied> τοῦ Ἡρακλεοπολίτου νομοῦ. 

    <lb n="4"/>Φλαουίῳ Ὀλυμπίῳ τῷ περιβλέπτῳ κόμετι τοῦ 

    <lb n="5"/>θείου κονσιστωρίου γεουχοῦ<unclear cert="high" reason="undefined">ντ</unclear>ι ἐν τῷ Ἡρακλεοπολίτῃ 

    <lb n="6"/>Αὐρήλιος Ἁτρῆς Ἀπόλλωνος μητρὸς Ἀβ<unclear cert="high" reason="undefined">ό</unclear>κης μετ᾽ <expan>ἐγ<ex cert="high">γυητοῦ</ex></expan> 

    <lb n="7"/><unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ῆς</supplied> <choice><corr>ἀποδόσεως</corr><sic>αποδωσεως</sic></choice> τοῦ δηλουμένου χρέους ἐμοῦ 

    <lb n="8"/><supplied cert="high" reason="lost">Αὐρ</supplied><unclear cert="high" reason="undefined">ηλί</unclear>ου Ἀνοῦπ Παμουθίου Ἀμμωνιανοῦ <expan>πρωτοκ<ex cert="high">ωμήτου</ex></expan> 

    <lb n="9"/><supplied cert="low" reason="lost">τῶν</supplied> <unclear cert="high" reason="undefined">ἀ</unclear>πὸ κώμης Φεβίχεως χαίρειν. 

    <lb n="10"/><supplied cert="high" reason="lost">ὁμολ</supplied><unclear cert="high" reason="undefined">ο</unclear>γοῦμεν διὰ ταύτης ἡμῶν τῆς ἐγγράφου 

    <lb n="11"/><supplied cert="high" reason="lost">ἀσφ</supplied><unclear cert="high" reason="undefined">αλε</unclear>ίας ἐσχηκέναι παρὰ τῆς σῆς λαμπρότητος 

    <lb n="12"/><supplied cert="high" reason="lost">τὴ</supplied><unclear cert="high" reason="undefined">ν</unclear> μεταξὺ συμφωνηθεῖσαν πρὸς ἀλλήλους τιμὴν 

    <lb n="13"/><supplied cert="high" reason="lost">κεραμί</supplied><unclear cert="high" reason="undefined">ων</unclear> ἐλαί<unclear cert="high" reason="undefined">ου</unclear> <unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">έ</supplied>ου <choice><corr>ἐκλεκτοῦ</corr><sic>εκκλεκτου</sic></choice> <choice><corr>ὀκτακισχιλίων</corr><sic>οκτακισχιλειων</sic></choice> ὀκτα-


    <lb n="14"/><supplied cert="high" reason="lost">κοσί</supplied>ων, <expan><ex cert="high">γίνεται</ex></expan> <expan><unclear cert="high" reason="undefined">κε</unclear>ρ<ex cert="high">άμια</ex></expan> <num value="8800">Η<unclear cert="high" reason="undefined">ω</unclear></num> <supplied cert="high" reason="lost">μόνα</supplied>, <unclear cert="high" reason="undefined">ὅ</unclear>περ ἐ<unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ά</supplied><unclear cert="high" reason="undefined">ν</unclear>αγκες <choice><corr>ἀποδώσομεν</corr><sic>αποδωσωμεν</sic></choice> 

    <lb n="15"/><supplied cert="high" reason="lost">τῇ σῇ</supplied> λαμ<supplied cert="high" reason="lost">πρότητι τῷ</supplied> <unclear cert="high" reason="undefined">ἐπι</unclear><supplied cert="high" reason="lost">όν</supplied><unclear cert="high" reason="undefined">τ</unclear>ι μην<supplied cert="high" reason="lost">ὶ</supplied> Μεχεὶρ τῆς εὐτυχούσης 

    <lb n="16"/>[δευτέρας <expan>ἰνδι]<unclear cert="high" reason="undefined">κ</unclear><ex cert="high">τίωνος</ex></expan>, <unclear cert="high" reason="undefined">γε</unclear>ωργίας τρίτης <expan>ἰνδικ<ex cert="high">τίωνος</ex></expan> ἀνυπερθέτως καὶ 

    <lb n="17"/><supplied cert="high" reason="lost">ἄνευ πάσης ἀντιλογ</supplied><unclear cert="high" reason="undefined">ία</unclear>ς <choice><corr>ὑποκειμένων</corr><sic>υποκιμενων</sic></choice> εἰς τοῦτο πάντων 

    <lb n="18"/><supplied cert="high" reason="lost">τῶν ἡμῖν ὑπαρχόν</supplied><unclear cert="high" reason="undefined">τ</unclear>ων καὶ ὑπαρξόντων ἐνεχύρου λόγῳ καὶ 

    <lb n="19"/><supplied cert="high" reason="lost">ὑποθήκης δικαίῳ</supplied> <unclear cert="high" reason="undefined">ἐ</unclear>ν παντὶ <choice><corr>εἴδει</corr><sic><unclear cert="high" reason="undefined">ι</unclear><supplied cert="high" reason="lost">δ</supplied><unclear cert="high" reason="undefined">ι</unclear></sic></choice> καὶ <choice><corr>γένει</corr><sic>γενι</sic></choice>. τὸ <choice><corr>γραμματεῖον</corr><sic>γραμματιον</sic></choice> κύρι<unclear cert="high" reason="undefined">ον</unclear> καὶ 

    <lb n="20"/>[<expan>ἐπερ<ex cert="high">ωτηθέντες</ex></expan> <expan>ὡμολ<ex cert="high">ογήσαμεν</ex></expan>. <handShift new="m2"/>  Αὐρήλιος] Ἁτρῆς Ἀπό<supplied cert="high" reason="lost">λλ</supplied>ωνος ὁ προκίμενος ἔσχον 

    <lb n="21"/><supplied cert="high" reason="lost">τὴν τιμὴν κεραμίων </supplied><choice><corr><supplied cert="high" reason="lost">ἐλα</supplied>ίου</corr><sic><supplied cert="high" reason="lost">ελα</supplied><unclear cert="high" reason="undefined">ι</unclear>ος</sic></choice> <choice><corr>ὀκτακισχιλίων</corr><sic>οκτα<unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">ισχι</supplied><unclear cert="high" reason="undefined">λ</unclear>ιον</sic></choice> <choice><corr>ὀκτακοσίων</corr><sic>οκτακουσιων</sic></choice> <num value="8800"/> καὶ ἀπο-

    <lb n="22"/><supplied cert="high" reason="lost">δώσω ἐν τῷ καιρῷ</supplied> ὡς πρόκιται. <handShift new="m3"/> Αὐρήλιος Ἀνοῦπ Ἀμμωνιανοῦ 

    <lb n="23"/><supplied cert="high" reason="lost">ὁ προκείμενος </supplied><choice><corr><supplied cert="high" reason="lost">ἐ</supplied>γγυητὴς</corr><sic><supplied cert="high" reason="lost">ε</supplied>ντηγης</sic></choice> <choice><corr>ἐγγυῶμαι</corr><sic><unclear cert="high" reason="undefined">ε</unclear>καιομεν</sic></choice> τὴν <choice><corr>ἀπόδοσιν</corr><sic>αποδισιν</sic></choice> 

