    <lb n="10"/>βλέπω <gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">χ</unclear>ει πάντα 

    <lb n="11"/>ποιῆσαι <gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> καὶ οὐδαμοῦ 

