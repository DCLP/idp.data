    <lb n="1"/><supplied cert="high" reason="lost">Αὐ</supplied><unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ηλί</supplied><unclear cert="high" reason="undefined">ῳ</unclear> <space dim="horizontal" extent="unknown" unit="character"/> Σερήνῳ ἐξάκτορι Πανοπολείτου 

    <lb n="2"/>πα<supplied cert="high" reason="lost">ρὰ Αὐ</supplied>ρ<supplied cert="high" reason="lost">ηλί</supplied>ου Πασνῶτος Ἀλώπηκος ἀπὸ τῆς αὐτῆ<supplied cert="high" reason="lost">ς</supplied> Πανὸς πόλεως. πρώην 

    <lb n="3"/><unclear cert="high" reason="undefined">ὀν</unclear><supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">μ</unclear>α<unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">θ</supplied>εὶς ὑπὸ Βησοῦτος <hi rend="diaeresis">ἱ</hi>ερίσσης τῆς <expan>α<ex cert="high">ὐτῆς</ex></expan> πόλεως εἰς <choice><corr>κτηναρχ<supplied cert="high" reason="lost">ί</supplied>αν</corr><sic>κτηναρχε<supplied cert="high" reason="lost">ι</supplied>αν</sic></choice> καὶ κατέφυ-

    <lb n="4"/><supplied cert="high" reason="lost">γον</supplied> ἐπὶ τὴν ἀρε<supplied cert="high" reason="lost">τ</supplied>ὴν τοῦ κυρίου μου διασημοτάτου ἡγεμόνος Φλαυου<hi rend="diaeresis">ί</hi>ου Κυ<hi rend="diaeresis">ι</hi>ντιλλινιανοῦ 

    <lb n="5"/>περ<supplied cert="high" reason="lost">ὶ τούτου</supplied> καὶ <supplied cert="high" reason="lost">ἔ</supplied>τυχον ἀπ<supplied cert="high" reason="lost">ο</supplied>φάσεως αὐτοτελοῦ<supplied cert="high" reason="lost">ς</supplied> δι᾽ <hi rend="diaeresis">ὑ</hi>πομνημάτων δι᾽ <choice><corr>ἧς</corr><sic>ου</sic></choice> ἐκέλευσεν τὸ μεγα-

    <lb n="6"/>λε<supplied cert="high" reason="lost">ῖ</supplied>ον <supplied cert="high" reason="lost">αὐτο</supplied><unclear cert="high" reason="undefined">ῦ</unclear> ἕτερο<supplied cert="high" reason="lost">ν ἀ</supplied>ντ᾽ ἐμοῦ προβληθῆναι διὰ τὴν <supplied cert="high" reason="lost">τ</supplied>οῦ γήρους μου 

    <lb n="7"/>ἡλικίαν βοηθείᾳ τοῦ ἐκδίκου 

    <lb n="8"/><gap extent="2" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="5" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> <unclear cert="high" reason="undefined">οὐ</unclear>κ οἶδ᾽ ὅπ<supplied cert="high" reason="lost">ω</supplied>ς ἀπὸ ἕκτης τοῦ ὄντος μηνὸς Ἐπεὶφ μέχρι σήμερόν σοι ὁ αὐτ<supplied cert="high" reason="lost">ὸ</supplied>ς 

    <lb n="9"/><gap extent="1" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/> <unclear cert="high" reason="undefined">ἐπ</unclear>ὶ τῶν τόπ<supplied cert="high" reason="lost">ων</supplied> δυναστεύων κατακλείσας με εἰς τὸ δημόσιον λογιστήριον 

    <lb n="10"/><unclear cert="high" reason="undefined">καὶ</unclear> <supplied cert="high" reason="lost">μὴ ὑπα</supplied><unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ύ</unclear><supplied cert="high" reason="lost">σα</supplied><unclear cert="high" reason="undefined">ς</unclear> τῇ κελεύ<supplied cert="high" reason="lost">σ</supplied>ει τοῦ αὐτοῦ κυρίου μου ἡγεμόνος ἠνάγκασέν με βου<unclear cert="high" reason="undefined">λό</unclear>μενός με 

    <lb n="11"/><unclear cert="high" reason="undefined">ἐ</unclear><supplied cert="high" reason="lost">κ</supplied>τελέ<supplied cert="high" reason="lost">σ</supplied>αι <unclear cert="high" reason="undefined">τὸ</unclear> φρόντισμα τῆς <choice><corr>κτηναρχίας</corr><sic>κτηναρχειας</sic></choice>. συνκροτῆται οὖν τὰ δικαστήρια τοῦ αὐτοῦ κυρίου μου 

    <lb n="12"/>διασημ<supplied cert="high" reason="lost">οτ</supplied>άτ<supplied cert="high" reason="lost">ο</supplied>υ ἡγεμό<supplied cert="high" reason="lost">νος</supplied> <unclear cert="high" reason="undefined">πα</unclear>ρὰ τῇ σῇ μετριότητι ἐμ<unclear cert="high" reason="undefined">οῦ</unclear> <unclear cert="high" reason="undefined">τε</unclear> <unclear cert="high" reason="undefined">καὶ</unclear> σοῦ καὶ κατὰ τῶν προσόντων μοι 

    <lb n="13"/><unclear cert="high" reason="undefined">δι</unclear><supplied cert="high" reason="lost">κ</supplied><unclear cert="high" reason="undefined">αίω</unclear><supplied cert="high" reason="lost">ν</supplied> ἐξεκαλεσάμην ἀπὸ σοῦ ἐπιβοώμενος τὸ μεγαλεῖον τοῦ αὐτοῦ κυρίου μου 

    <lb n="14"/><unclear cert="high" reason="undefined">δια</unclear><supplied cert="high" reason="lost">σημοτάτ</supplied>ου ἡγεμόν<supplied cert="high" reason="lost">ο</supplied>ς ἐπιδίδωμί σοι ταύτην τὴν ἔκκλητον ἀξιῶν δοθῆναί μοι 


    <lb n="15"/><unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ὴν συνή</supplied><unclear cert="high" reason="undefined">θ</unclear>ην <choice><corr>ὑποσημείωσιν</corr><sic>υποσημιωσιν</sic></choice> καὶ τοὺς διδομένους διδόσθαι ἀποστόλους πρὸς τὸ δύ-

    <lb n="16"/><supplied cert="high" reason="lost">νασθαι δι</supplied><unclear cert="high" reason="undefined">αγω</unclear>νίσασθαι <unclear cert="high" reason="undefined">ἐπ</unclear><supplied cert="high" reason="lost">ὶ</supplied> τοῦ ῥητοῦ τοῦ μεγαλείο<supplied cert="high" reason="lost">υ</supplied> αὐ<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">οῦ</supplied> τὴν ἔκκλητον καὶ <choice><corr>ἐκδικίας</corr><sic>εκδικειας</sic></choice> τυχεῖν 

    <lb n="17"/>καί, <supplied cert="high" reason="lost">εἰ βο</supplied>υληθείης προέσθαι μοι τὴν συνήθην <choice><corr>ὑποσημείωσιν</corr><sic>υποσημιωσιν</sic></choice>, γινώσκειν σε ἐκ 

    <lb n="18"/><unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ῆς προθ</supplied>έσεως τῶν κολλημάτων <choice><corr>ταύτης</corr><sic>ταυτην</sic></choice> μου τῆς ἐκκλήτου μὴ εἴργεσθαι τὸν ἀγῶνα 

    <lb n="19"/><supplied cert="high" reason="lost">τόνδε μο</supplied><unclear cert="high" reason="undefined">υ</unclear> τοῦ ἀχράντου δικαστηρίου αὐτοῦ ἀξιώσοντα τὸν τῆς βουλῆς σκρίβαν 

    <lb n="20"/><supplied cert="high" reason="lost">ποιῆσα</supplied><unclear cert="high" reason="undefined">ι</unclear> <unclear cert="high" reason="undefined">τὸ</unclear> πρ<unclear cert="high" reason="undefined">ο</unclear>θέσιμ<supplied cert="high" reason="lost">ο</supplied>ν τῆς ἐκκλήτου. 

    <lb n="21"/>ὑ<supplied cert="high" reason="lost">πατείας Πα</supplied><unclear cert="high" reason="undefined">π</unclear>ί<supplied cert="high" reason="lost">ο</supplied>υ Πακατιανοῦ τοῦ λαμπροτάτου ἐπάρχου τοῦ ἱεροῦ πραιτωρίου καὶ Μεκιλίου Ἱλαριανοῦ τοῦ λαμπροτάτου 

