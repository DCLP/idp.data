    <lb n="2"/>πα<supplied reason="lost" cert="high">ρὰ Αὐ</supplied>ρ<supplied reason="lost" cert="high">ηλί</supplied>ου Πασνῶτος Ἀλώπηκος ἀπὸ τῆς αὐτῆ<supplied reason="lost" cert="high">ς</supplied> Πανὸς πόλεως. πρώην 

    <lb n="3"/><unclear reason="undefined" cert="high">ὀν</unclear><supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">μ</unclear>α<unclear reason="undefined" cert="high">σ</unclear><supplied reason="lost" cert="high">θ</supplied>εὶς ὑπὸ Βησοῦτος <hi rend="diaeresis">ἱ</hi>ερίσσης τῆς <expan>α<ex cert="high">ὐτῆς</ex></expan> πόλεως εἰς <choice><corr>κτηναρχ<supplied reason="lost" cert="high">ί</supplied>αν</corr><sic>κτηναρχε<supplied reason="lost" cert="high">ι</supplied>αν</sic></choice> καὶ κατέφυ-

    <lb n="4"/><supplied reason="lost" cert="high">γον</supplied> ἐπὶ τὴν ἀρε<supplied reason="lost" cert="high">τ</supplied>ὴν τοῦ κυρίου μου διασημοτάτου ἡγεμόνος Φλαυου<hi rend="diaeresis">ί</hi>ου Κυ<hi rend="diaeresis">ι</hi>ντιλλινιανοῦ 

    <lb n="5"/>περ<supplied reason="lost" cert="high">ὶ τούτου</supplied> καὶ <supplied reason="lost" cert="high">ἔ</supplied>τυχον ἀπ<supplied reason="lost" cert="high">ο</supplied>φάσεως αὐτοτελοῦ<supplied reason="lost" cert="high">ς</supplied> δι᾽ <hi rend="diaeresis">ὑ</hi>πομνημάτων δι᾽ <choice><corr>ἧς</corr><sic>ου</sic></choice> ἐκέλευσεν τὸ μεγα-

    <lb n="6"/>λε<supplied reason="lost" cert="high">ῖ</supplied>ον <supplied reason="lost" cert="high">αὐτο</supplied><unclear reason="undefined" cert="high">ῦ</unclear> ἕτερο<supplied reason="lost" cert="high">ν ἀ</supplied>ντ᾽ ἐμοῦ προβληθῆναι διὰ τὴν <supplied reason="lost" cert="high">τ</supplied>οῦ γήρους μου 

    <lb n="7"/>ἡλικίαν βοηθείᾳ τοῦ ἐκδίκου 

    <lb n="8"/><gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="5" unit="character"/><gap reason="illegible" extent="1" unit="character"/> <unclear reason="undefined" cert="high">οὐ</unclear>κ οἶδ᾽ ὅπ<supplied reason="lost" cert="high">ω</supplied>ς ἀπὸ ἕκτης τοῦ ὄντος μηνὸς Ἐπεὶφ μέχρι σήμερόν σοι ὁ αὐτ<supplied reason="lost" cert="high">ὸ</supplied>ς 

    <lb n="9"/><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="3" unit="character"/><gap reason="lost" extent="1" unit="character"/><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="1" unit="character"/> <unclear reason="undefined" cert="high">ἐπ</unclear>ὶ τῶν τόπ<supplied reason="lost" cert="high">ων</supplied> δυναστεύων κατακλείσας με εἰς τὸ δημόσιον λογιστήριον 

    <lb n="10"/><unclear reason="undefined" cert="high">καὶ</unclear> <supplied reason="lost" cert="high">μὴ ὑπα</supplied><unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">ύ</unclear><supplied reason="lost" cert="high">σα</supplied><unclear reason="undefined" cert="high">ς</unclear> τῇ κελεύ<supplied reason="lost" cert="high">σ</supplied>ει τοῦ αὐτοῦ κυρίου μου ἡγεμόνος ἠνάγκασέν με βου<unclear reason="undefined" cert="high">λό</unclear>μενός με 

    <lb n="11"/><unclear reason="undefined" cert="high">ἐ</unclear><supplied reason="lost" cert="high">κ</supplied>τελέ<supplied reason="lost" cert="high">σ</supplied>αι <unclear reason="undefined" cert="high">τὸ</unclear> φρόντισμα τῆς <choice><corr>κτηναρχίας</corr><sic>κτηναρχειας</sic></choice>. συνκροτῆται οὖν τὰ δικαστήρια τοῦ αὐτοῦ κυρίου μου 

    <lb n="12"/>διασημ<supplied reason="lost" cert="high">οτ</supplied>άτ<supplied reason="lost" cert="high">ο</supplied>υ ἡγεμό<supplied reason="lost" cert="high">νος</supplied> <unclear reason="undefined" cert="high">πα</unclear>ρὰ τῇ σῇ μετριότητι ἐμ<unclear reason="undefined" cert="high">οῦ</unclear>   σοῦ καὶ κατὰ τῶν προσόντων μοι 

    <lb n="13"/><unclear reason="undefined" cert="high">δι</unclear><supplied reason="lost" cert="high">κ</supplied><unclear reason="undefined" cert="high">αίω</unclear><supplied reason="lost" cert="high">ν</supplied> ἐξεκαλεσάμην ἀπὸ σοῦ ἐπιβοώμενος τὸ μεγαλεῖον τοῦ αὐτοῦ κυρίου μου 

    <lb n="14"/><unclear reason="undefined" cert="high">δια</unclear><supplied reason="lost" cert="high">σημοτάτ</supplied>ου ἡγεμόν<supplied reason="lost" cert="high">ο</supplied>ς ἐπιδίδωμί σοι ταύτην τὴν ἔκκλητον ἀξιῶν δοθῆναί μοι 


    <lb n="15"/><unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ὴν συνή</supplied><unclear reason="undefined" cert="high">θ</unclear>ην <choice><corr>ὑποσημείωσιν</corr><sic>υποσημιωσιν</sic></choice> καὶ τοὺς διδομένους διδόσθαι ἀποστόλους πρὸς τὸ δύ-

    <lb n="16"/><supplied reason="lost" cert="high">νασθαι δι</supplied><unclear reason="undefined" cert="high">αγω</unclear>νίσασθαι <unclear reason="undefined" cert="high">ἐπ</unclear><supplied reason="lost" cert="high">ὶ</supplied> τοῦ ῥητοῦ τοῦ μεγαλείο<supplied reason="lost" cert="high">υ</supplied> αὐ<unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">οῦ</supplied> τὴν ἔκκλητον καὶ <choice><corr>ἐκδικίας</corr><sic>εκδικειας</sic></choice> τυχεῖν 

    <lb n="17"/>καί, <supplied reason="lost" cert="high">εἰ βο</supplied>υληθείης προέσθαι μοι τὴν συνήθην <choice><corr>ὑποσημείωσιν</corr><sic>υποσημιωσιν</sic></choice>, γινώσκειν σε ἐκ 

    <lb n="18"/><unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ῆς προθ</supplied>έσεως τῶν κολλημάτων <choice><corr>ταύτης</corr><sic>ταυτην</sic></choice> μου τῆς ἐκκλήτου μὴ εἴργεσθαι τὸν ἀγῶνα 

    <lb n="19"/><supplied reason="lost" cert="high">τόνδε μο</supplied><unclear reason="undefined" cert="high">υ</unclear> τοῦ ἀχράντου δικαστηρίου αὐτοῦ ἀξιώσοντα τὸν τῆς βουλῆς σκρίβαν 

    <lb n="20"/><supplied reason="lost" cert="high">ποιῆσα</supplied><unclear reason="undefined" cert="high">ι</unclear>  πρ<unclear reason="undefined" cert="high">ο</unclear>θέσιμ<supplied reason="lost" cert="high">ο</supplied>ν τῆς ἐκκλήτου. 

    <lb n="21"/>ὑ<supplied reason="lost" cert="high">πατείας Πα</supplied><unclear reason="undefined" cert="high">π</unclear>ί<supplied reason="lost" cert="high">ο</supplied>υ Πακατιανοῦ τοῦ λαμπροτάτου ἐπάρχου τοῦ ἱεροῦ πραιτωρίου καὶ Μεκιλίου Ἱλαριανοῦ τοῦ λαμπροτάτου 

