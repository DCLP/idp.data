    <lb n="17"/>τῆς πράξ<unclear cert="high" reason="undefined">ε</unclear>ως <unclear cert="high" reason="undefined">οὔ</unclear>σ<unclear cert="high" reason="undefined">ης</unclear> ἔκ τε ἐμοῦ 

    <lb n="18"/>καὶ ἐκ τῶ<unclear cert="high" reason="undefined">ν</unclear> ὑπαρχ<unclear cert="high" reason="undefined">όν</unclear>τ<unclear cert="high" reason="undefined">ων</unclear> <add place="supralinear">μοι</add> 

    <lb n="19"/>πάντων <sic>πάντων</sic> <choice><corr>παντοίων</corr><sic>παντ<unclear cert="high" reason="undefined">ο</unclear>ιουν</sic></choice> σοι ἀπο-

    <lb n="20"/><choice><corr>πράσσοντι</corr><sic>αποπρασσωντι</sic></choice> <unclear cert="high" reason="undefined">κ</unclear>αθάπε<unclear cert="high" reason="undefined">ρ</unclear> <unclear cert="high" reason="undefined">ἐ</unclear>κ <unclear cert="high" reason="undefined">δί</unclear>κης. τ<unclear cert="high" reason="undefined">ό</unclear>δ<unclear cert="high" reason="undefined">ε</unclear> <unclear cert="high" reason="undefined">τὸ</unclear> 

