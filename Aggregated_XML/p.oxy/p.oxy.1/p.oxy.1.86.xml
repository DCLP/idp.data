    <lb n="14"/><supplied cert="high" reason="lost">ὑπη</supplied>ρετήσασθαι τῇ δημοσίᾳ <choice><corr>σιτοπομπίᾳ</corr><sic>σ<unclear cert="high" reason="undefined">ιτ</unclear><supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ποι</unclear>α</sic></choice>. 

    <lb n="15"/><supplied cert="high" reason="lost">οὗτο</supplied>ς δὲ μίαν <num value="1"/> ἐκ μιᾶς <num value="1"/> ὑπερτιθέμεν<supplied cert="high" reason="lost">ο</supplied>ς 

    <lb n="16"/><supplied cert="high" reason="lost">οὐ π</supplied>αρέσχεν, καὶ τούτου χάριν τὸ βιβλί<supplied cert="high" reason="lost">ον</supplied> 

    <lb n="17"/><supplied cert="high" reason="lost">ἐπι</supplied>δίδωμι ἀξιῶν τοῦτον με<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">π</unclear>ε<unclear cert="high" reason="undefined">μ</unclear>-

