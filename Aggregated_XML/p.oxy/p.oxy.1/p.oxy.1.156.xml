    <lb n="6"/><g type="stauros"/> Θεόδωρος <expan>χαρτου<ex cert="high">λάριος</ex></expan> <expan><ex cert="high">καὶ</ex></expan> σὺν <expan>θ<ex cert="high">εῷ</ex></expan> ἀντιγεοῦχος. </ab></div></div><div n="meta.37145" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.37145" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" notBefore="501" notAfter="600">VI</date></p></div><div n="meta.37145" type="commentary" subtype="mentionedDates" org="uniform" part="N" sample="complete"><head>Erwähnte Daten</head></div><div n="meta.37145" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">P.Oxy.</title><biblScope type="volume">1</biblScope><biblScope type="numbers">156</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">37145</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0181</series>; <biblScope type="volume">1</biblScope>; <biblScope type="numbers">156</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0181;1;156" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.37145" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p>keine</p></div><div n="meta.37145" type="history" subtype="locations" org="uniform" part="N" sample="complete"><p><placeName type="ancientFindspot" cert="high" full="yes">Oxyrhynchos</placeName></p></div><div n="meta.37145" type="figure" org="uniform" part="N" sample="complete"><p><figure href="http://ipap.csad.ox.ac.uk/4DLink4/4DACTION/IPAPwebquery?vPub=P.Oxy.&amp;vVol=1&amp;vNum=156"><figDesc>Abbildung im Internet</figDesc></figure></p></div></body></text></TEI.2>