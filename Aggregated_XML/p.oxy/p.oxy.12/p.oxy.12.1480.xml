    <lb n="15"/>τοῦ τοῦ ἐκλογισ<supplied reason="lost" cert="high">τοῦ</supplied> 

    <lb n="16"/>ὡς Ἑρμογέν<unclear reason="undefined" cert="high">ε</unclear>ι, ἵν<supplied reason="lost" cert="high">α</supplied> 

