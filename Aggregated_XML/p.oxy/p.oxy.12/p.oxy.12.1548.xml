    <lb n="2"/><handShift new="m1"/> παρὰ Πλουτίωνος Πλουτίωνος 

    <lb n="3"/>τοῦ Πλουτίωνος <expan>μητ<ex cert="high">ρὸς</ex></expan> Ταψόιτος 

    <lb n="4"/>ἀπ᾽ <expan>Ὀξ<ex cert="high">υρύγχων</ex></expan> <expan>πόλ<ex cert="high">εως</ex></expan>. κατὰ τὰ <expan>κελευσθ<ex cert="high">έντα</ex></expan> ὑπὸ Μαικίου 

    <lb n="5"/>Λαίτου τοῦ λαμπροτάτου <expan>ἡγεμ<ex cert="high">όνος</ex></expan> 

    <lb n="6"/><expan>ἀπογρά<ex cert="high">φομαι</ex></expan> πρὸς τὴν τοῦ <expan>δι<supplied cert="high" reason="lost">ε</supplied>λ<ex cert="high">θόντος</ex></expan> <num value="10">ι</num> <expan><ex cert="high">ἔτους</ex></expan> 

    <lb n="7"/>Καισάρων τῶν κυρίων Σεουήρου 

    <lb n="8"/>καὶ Ἀντωνίνου <del rend="erasure" status="unremarkable">καὶ Γέτα Καίσαρος</del> 

    <lb n="9"/>κατ᾽ οἰκίαν <expan>ἀπογρα<ex cert="high">φὴν</ex></expan> <subst><add place="inline">τὸ</add><del rend="corrected" status="unremarkable">τη</del></subst> <expan><hi rend="diaeresis">ὑ</hi>πάρχ<ex cert="high">ον</ex></expan> μοι ἐπὶ ἀμ-

    <lb n="10"/><expan>φόδ<ex cert="high">ου</ex></expan> Βορρᾶ <expan>Κρηπ<ex cert="high">ῖδος</ex></expan> <expan><ex cert="high">τέταρτον</ex></expan> μέρος <expan>οἰκ<ex cert="high">ίας</ex></expan> καὶ <expan>αἰθ<ex cert="high">ρίου</ex></expan> 

    <lb n="11"/>καὶ τῶν ταύτης <expan>χρηστ<ex cert="high">ηρίων</ex></expan>, εἰς ἃ <expan>ἀπογρά<ex cert="high">φομαι</ex></expan>· 

    <lb n="12"/>αὐτὸς ἐγὼ <supplied cert="high" reason="lost">ὁ</supplied> Πλουτίων <expan>ἄτ<ex cert="high">εχνος</ex></expan> <expan>ἄση<ex cert="high">μος</ex></expan> <expan><ex cert="high">ἐτῶν</ex></expan> <num value="48">μη</num>, 

    <lb n="13"/>Δόσκο<supplied cert="high" reason="lost">ρος</supplied> <expan>δοῦλ<ex cert="high">ός</ex></expan> μ<supplied cert="high" reason="lost">ο</supplied>υ <expan>ἄτ<ex cert="high">εχνος</ex></expan> <expan>ἄση<ex cert="high">μος</ex></expan> <expan><ex cert="high">ἐτῶν</ex></expan> <gap extent="1" reason="lost" unit="character"/><num value="8">η</num>. 

    <lb n="14"/><expan>γ<ex cert="high">υναῖκες</ex></expan> Ταψόις ἡ <expan>κ<ex cert="high">αὶ</ex></expan> <expan>Εὐδαιμ<ex cert="high">ονὶς</ex></expan> <expan>θυγά<ex cert="high">τηρ</ex></expan> μου 

    <lb n="15"/>συνοικοῦσα τῷ <expan>ἀνδ<ex cert="high">ρὶ</ex></expan> Ἀπολλω-

    <lb n="16"/>νίῳ <expan>ἄτ<ex cert="high">εχνος</ex></expan> <expan>ἄση<ex cert="high">μος</ex></expan> ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <num value="20">κ</num>, 

    <lb n="17"/>Γα<hi rend="diaeresis">ι</hi>ανὴ ὁμογνησία <expan>αὐτ<ex cert="high">ῆς</ex></expan> <expan>ἀδελ<ex cert="high">φὴ</ex></expan> <expan>ἄτεχ<ex cert="high">νος</ex></expan> 

    <lb n="18"/>ἄσημος ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <num value="15">ιε</num>, 

    <lb n="19"/>Ἀφροδείτη <expan>ἐπικεκλημ<ex cert="high">ένη</ex></expan> Ἰσιδώρα 

    <lb n="20"/><expan>ἀγο<ex cert="high">ραστὴ</ex></expan> <expan>δούλ<ex cert="high">η</ex></expan> τῆς <expan>θυγα<ex cert="high">τρός</ex></expan> μου Ταψόιτος 

    <lb n="21"/>τῆς καὶ <expan>Εὐδαιμονίδο<ex cert="high">ς</ex></expan> γένει <expan>Ὀασεῖτ<ex cert="high">ις</ex></expan> 

    <lb n="22"/>συνοῦσα τῇ <expan>δεσπ<ex cert="high">οίνῃ</ex></expan> <expan>ἄτ<ex cert="high">εχνος</ex></expan> <expan>ἄση<ex cert="high">μος</ex></expan> ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <num value="13">ιγ</num>, 

    <lb n="23"/>Ταέρως δούλη μου καὶ τῶν <expan>ἀδελ<ex cert="high">φῶν</ex></expan> 

    <lb n="24"/>μου καὶ ἄλλων <expan>ἄτ<ex cert="high">εχνος</ex></expan> <expan>ἄση<ex cert="high">μος</ex></expan> ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <num value="35">λε</num>, 

    <lb n="25"/>Ταειπίμαχος ἑτέρα <expan>δούλ<ex cert="high">η</ex></expan> μου 

    <lb n="26"/>καὶ τῶν <expan>ἀδελ<ex cert="high">φῶν</ex></expan> καὶ ἄλλων ἐκ δούλης 

    <lb n="27"/>Ταέρωτος <expan>ἄτ<ex cert="high">εχνος</ex></expan> <expan>ἄση<ex cert="high">μος</ex></expan> ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <num value="9">θ</num>. 

    <lb n="28"/>καὶ ὀμνύω τὴν Λουκίου 

    <lb n="29"/>Σεπτιμίου Σεουήρου Εὐσεβοῦς Περτίνακος 

    <lb n="30"/>καὶ Μάρκου Αὐρηλίου Ἀντωνίνου 

