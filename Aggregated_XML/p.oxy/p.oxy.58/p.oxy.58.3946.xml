    <lb n="1"/><g type="chirho"/> ἐν ὀνόματι τῆς ἀχράντου καὶ ὁμοουσίου 

    <lb n="2"/>τριάδος πατρὸς καὶ <hi rend="diaeresis">υ</hi>ἱοῦ καὶ ἁγίου πνεύματο<add place="supralinear">ς</add>. 

    <lb n="3"/>βασιλείας τοῦ θειοτάτου καὶ <expan>εὐσεβ<ex cert="high">εστάτου</ex></expan> ἡμῶν 

    <lb n="4"/><expan>δεσπό<ex cert="high">του</ex></expan> μεγίστου εὐεργέτου <expan>Φλ<ex cert="high">αουίου</ex></expan> Φωκᾶ τοῦ 

    <lb n="5"/>αἰωνίου <choice><corr>Αὐγούστου</corr><sic>αγουστου</sic></choice> καὶ Αὐτοκράτορος 

    <lb n="6"/>ἔτους <num value="5">ε</num>, Ἁθὺρ <num value="22">κβ</num>, <expan><hi rend="diaeresis">ἰ</hi>νδ<ex cert="high">ικτίωνος</ex></expan> <num value="10">ι</num>. 

    <lb n="7"/>Φλαουίῳ Ἀπίω<supplied cert="high" reason="lost">ν</supplied><unclear cert="high" reason="undefined">ι</unclear> τῷ πανευφήμῳ 

    <lb n="8"/>καὶ ὑπερφυε<unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">τά</supplied><unclear cert="high" reason="undefined">τ</unclear>ῳ ἀπὸ ὑπάτω<add place="supralinear">ν</add> καὶ 

    <lb n="9"/>πατρικίῳ γεου<supplied cert="high" reason="lost">χοῦ</supplied>ντι καὶ ἐνταῦθα 

    <lb n="10"/>τῇ λ<unclear cert="high" reason="undefined">α</unclear>μπρᾷ <expan>Ὀ<supplied cert="high" reason="lost">ξυρυγ</supplied>χ<ex cert="high">ιτῶν</ex></expan> πόλε<unclear cert="high" reason="undefined">ι</unclear> διὰ Μη<supplied cert="high" reason="lost">ν</supplied>ᾶ

    <lb n="11"/><unclear cert="high" reason="undefined">οἰ</unclear><supplied cert="high" reason="lost">κέτου</supplied> <unclear cert="high" reason="undefined">τ</unclear>ο<unclear cert="high" reason="undefined">ῦ</unclear> <expan><supplied cert="high" reason="lost">ἐπερ<ex cert="high">ωτῶντος</ex></supplied></expan> κ<unclear cert="high" reason="undefined">αὶ</unclear> <supplied cert="high" reason="lost">προσ</supplied>πο<supplied cert="high" reason="lost">ρί</supplied><unclear cert="high" reason="undefined">ζ</unclear>οντ<supplied cert="high" reason="lost">ος</supplied> 

