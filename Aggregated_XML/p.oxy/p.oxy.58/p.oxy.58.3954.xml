    <lb n="2"/><g type="stauros"/> ἐν ὀνό<supplied reason="lost" cert="high">μα</supplied><unclear reason="undefined" cert="high">τι</unclear> τοῦ δεσ<unclear reason="undefined" cert="high">πό</unclear>του ἡμῶν Ἰησοῦ 

    <lb n="3"/>Χριστοῦ τοῦ θεοῦ καὶ σωτῆρος. <expan>βασιλ<ex cert="high">είας</ex></expan> 

    <lb n="4"/>τοῦ θειοτάτου καὶ <expan>εὐσεβ<ex cert="high">εστάτου</ex></expan> ἡμῶν δεσπότου 

    <lb n="5"/><expan>Φλ<ex cert="high">αουίου</ex></expan> Ἡρακλείου τοῦ αἰωνίου Αὐγούστου 

    <lb n="6"/>καὶ Αὐτ<unclear reason="undefined" cert="high">ο</unclear>κράτορος ἔτους πρώτου, 

    <lb n="7"/><space extent="unknown" unit="character" dim="horizontal"/> Μεχεὶρ <num value="18">ιη</num>, <expan>ἰνδ<ex cert="high">ικτίωνος</ex></expan> <num value="14">ιδ</num> <space extent="unknown" unit="character" dim="horizontal"/> 

    <lb n="8"/>Φλαου<hi rend="diaeresis">ί</hi>ου Γεωργίου τῷ <expan>λαμπρ<unclear reason="undefined" cert="high">οτ</unclear><ex cert="high">άτῳ</ex></expan> 

    <lb n="9"/>διοικητῇ τοῦ ἐνδόξου οἴκου 

    <lb n="10"/>τοῦ πανευφήμου πατρικίου 

    <lb n="11"/>Ἀπίωνος ταύτης τῆς Ἄνω <expan>Κυνοπ<unclear reason="undefined" cert="high">ολ</unclear><ex cert="high">ιτῶν πόλεως</ex></expan> 

    <lb n="12"/>ἐκ πατρὸς τοῦ μακαρίου <space extent="unknown" unit="character" dim="horizontal"/> 

    <lb n="13"/><unclear reason="undefined" cert="high">ἀπὸ</unclear>  λαμπρᾶς Ὀξυρυγχιτῶν 


    <lb n="14"/><unclear reason="undefined" cert="high">πόλ</unclear>ε<unclear reason="undefined" cert="high">ως</unclear> <space extent="unknown" unit="character" dim="horizontal"/> Αὐρήλιοι Μακᾶρι<unclear reason="undefined" cert="high">ς</unclear> 

    <lb n="15"/>μείζ<unclear reason="undefined" cert="high">ων</unclear> υἱὸς Ἰερεμίου μητρὸς 

    <lb n="16"/><unclear reason="undefined" cert="high">Μα</unclear>ρ<unclear reason="undefined" cert="high">ίας</unclear> καὶ Ἰωάννης υἱὸς 

    <lb n="17"/>Ὡριγ<unclear reason="undefined" cert="high">έ</unclear>νους μητρὸς Μαρίας καὶ 

    <lb n="18"/>Μηνᾶ<unclear reason="undefined" cert="high">ς</unclear> υἱὸς Ἀνοῦπ μητρὸς Μαρτυρίας 

    <lb n="19"/>καὶ Ἀβραὰμ υἱὸς Φουνσίου μητρὸς 

    <lb n="20"/>Ἐρεβ<unclear reason="undefined" cert="high">έ</unclear>κκας καὶ Μηνᾶς υἱὸς Γεωρ<unclear reason="undefined" cert="high">γίο</unclear>υ 

    <lb n="21"/>μητρ<unclear reason="undefined" cert="high">ὸς</unclear> Σοφίας καὶ Ὡριγένης υἱὸ<unclear reason="undefined" cert="high">ς</unclear> 

    <lb n="22"/>Στεφ<unclear reason="undefined" cert="high">άν</unclear>ου μητρὸς Ἐρεβέκκας, 

    <lb n="23"/>οἱ π<unclear reason="undefined" cert="high">άντες</unclear> ἀπὸ κώμης Ἀψέμψεως 

    <lb n="24"/>τοῦ <unclear reason="undefined" cert="high">Ἄνω</unclear> <supplied reason="lost" cert="high">Κ</supplied><unclear reason="undefined" cert="high">υ</unclear>νοπολίτου νομοῦ, <expan>χ<ex cert="high">αίρειν</ex></expan>. 

    <lb n="25"/>ὁμολ<supplied reason="lost" cert="high">ο</supplied>γοῦμεν εἰληφέναι καὶ 

    <lb n="26"/>δεδέχ<unclear reason="undefined" cert="high">θ</unclear>αι καὶ πεπληρῶσθαι 

    <lb n="27"/>παρὰ τῆς ὑμῶν λαμπρότητος 

    <lb n="28"/><unclear reason="undefined" cert="high">τὴν</unclear> τιμὴν τοῦ διαπραθέντος 

    <lb n="29"/>αὐτῇ παρ᾽ ἡμῶν οἴνου ἄχρι ἑνὸς <num value="1"/> ὀβολοῦ, 

    <lb n="30"/>τουτέστιν οἴνου Σάμια διακοσία <num value="200"/> 

    <lb n="31"/>ἐπὶ καρπῶν τῆς παρούσης 

    <lb n="32"/><choice><corr>τεσσαρεσκαιδεκάτης</corr><sic>τεσσαρασκαιδεκατης</sic></choice> <expan>ἰνδ<ex cert="high">ικτίωνος</ex></expan> 

    <lb n="33"/>ἄχρι ἑνὸς <num value="1"/> ὀβολοῦ, τουτέστιν <choice><corr><supplied reason="omitted" cert="high">οἴν</supplied>ου</corr><sic>ου</sic></choice> Σάμια 

    <lb n="34"/>διακοσία <num value="200"/>. ὁμολογοῦμεν τοῦ λοιποῦ οὐδένα 

    <lb n="35"/>λόγ<unclear reason="undefined" cert="high">ον</unclear> χειν πρὸς τὴν ὑμῶν <expan>μεγαλ<ex cert="high">οπρέπειαν</ex></expan> διὰ τὸ 

    <lb n="36"/>ἡμᾶς εἰληφέναι καὶ δεδέχθαι 

    <lb n="37"/>καὶ πεπληρῶσθαι παρ᾽ αὐτῆς 

    <lb n="38"/>τὴν τιμὴν τοῦ διαπραθέντος 

    <lb n="39"/>αὐτῇ παρ᾽ ἡμῶν <supplied reason="omitted" cert="high">οἴνου</supplied> ἄχρι ἑνὸς <num value="1"/> ὀβολοῦ 

    <lb n="40"/>καὶ πρὸς ἀπόδειξιν τῆς <supplied reason="omitted" cert="high">τῆς</supplied> τιμῆς 

    <lb n="41"/>τοῦ εἰρημένου οἴνου καταβολῆς 

    <lb n="42"/>τήνδε τὴν πληρωτικὴν 

    <lb n="43"/>ἀποχὴ<supplied reason="lost" cert="high">ν</supplied> ἐξεθέμεθα αὐτῇ, ἥτις 

