    <lb n="2"/><g type="stauros"/> ἐν ὀνό<supplied cert="high" reason="lost">μα</supplied><unclear cert="high" reason="undefined">τι</unclear> τοῦ δεσ<unclear cert="high" reason="undefined">πό</unclear>του ἡμῶν Ἰησοῦ 

    <lb n="3"/>Χριστοῦ τοῦ θεοῦ καὶ σωτῆρος. <expan>βασιλ<ex cert="high">είας</ex></expan> 

    <lb n="4"/>τοῦ θειοτάτου καὶ <expan>εὐσεβ<ex cert="high">εστάτου</ex></expan> ἡμῶν δεσπότου 

    <lb n="5"/><expan>Φλ<ex cert="high">αουίου</ex></expan> Ἡρακλείου τοῦ αἰωνίου Αὐγούστου 

    <lb n="6"/>καὶ Αὐτ<unclear cert="high" reason="undefined">ο</unclear>κράτορος ἔτους πρώτου, 

    <lb n="7"/><space dim="horizontal" extent="unknown" unit="character"/> Μεχεὶρ <num value="18">ιη</num>, <expan>ἰνδ<ex cert="high">ικτίωνος</ex></expan> <num value="14">ιδ</num> <space dim="horizontal" extent="unknown" unit="character"/> 

    <lb n="8"/>Φλαου<hi rend="diaeresis">ί</hi>ου Γεωργίου τῷ <expan>λαμπρ<unclear cert="high" reason="undefined">οτ</unclear><ex cert="high">άτῳ</ex></expan> 

    <lb n="9"/>διοικητῇ τοῦ ἐνδόξου οἴκου 

    <lb n="10"/>τοῦ πανευφήμου πατρικίου 

    <lb n="11"/>Ἀπίωνος ταύτης τῆς Ἄνω <expan>Κυνοπ<unclear cert="high" reason="undefined">ολ</unclear><ex cert="high">ιτῶν πόλεως</ex></expan> 

    <lb n="12"/>ἐκ πατρὸς τοῦ μακαρίου <space dim="horizontal" extent="unknown" unit="character"/> 

    <lb n="13"/><unclear cert="high" reason="undefined">ἀπὸ</unclear> <unclear cert="high" reason="undefined">τῆς</unclear> λαμπρᾶς Ὀξυρυγχιτῶν 


    <lb n="14"/><unclear cert="high" reason="undefined">πόλ</unclear>ε<unclear cert="high" reason="undefined">ως</unclear> <space dim="horizontal" extent="unknown" unit="character"/> Αὐρήλιοι Μακᾶρι<unclear cert="high" reason="undefined">ς</unclear> 

    <lb n="15"/>μείζ<unclear cert="high" reason="undefined">ων</unclear> υἱὸς Ἰερεμίου μητρὸς 

    <lb n="16"/><unclear cert="high" reason="undefined">Μα</unclear>ρ<unclear cert="high" reason="undefined">ίας</unclear> καὶ Ἰωάννης υἱὸς 

    <lb n="17"/>Ὡριγ<unclear cert="high" reason="undefined">έ</unclear>νους μητρὸς Μαρίας καὶ 

    <lb n="18"/>Μηνᾶ<unclear cert="high" reason="undefined">ς</unclear> υἱὸς Ἀνοῦπ μητρὸς Μαρτυρίας 

    <lb n="19"/>καὶ Ἀβραὰμ υἱὸς Φουνσίου μητρὸς 

    <lb n="20"/>Ἐρεβ<unclear cert="high" reason="undefined">έ</unclear>κκας καὶ Μηνᾶς υἱὸς Γεωρ<unclear cert="high" reason="undefined">γίο</unclear>υ 

    <lb n="21"/>μητρ<unclear cert="high" reason="undefined">ὸς</unclear> Σοφίας καὶ Ὡριγένης υἱὸ<unclear cert="high" reason="undefined">ς</unclear> 

    <lb n="22"/>Στεφ<unclear cert="high" reason="undefined">άν</unclear>ου μητρὸς Ἐρεβέκκας, 

    <lb n="23"/>οἱ π<unclear cert="high" reason="undefined">άντες</unclear> ἀπὸ κώμης Ἀψέμψεως 

    <lb n="24"/>τοῦ <unclear cert="high" reason="undefined">Ἄνω</unclear> <supplied cert="high" reason="lost">Κ</supplied><unclear cert="high" reason="undefined">υ</unclear>νοπολίτου νομοῦ, <expan>χ<ex cert="high">αίρειν</ex></expan>. 

    <lb n="25"/>ὁμολ<supplied cert="high" reason="lost">ο</supplied>γοῦμεν εἰληφέναι καὶ 

    <lb n="26"/>δεδέχ<unclear cert="high" reason="undefined">θ</unclear>αι καὶ πεπληρῶσθαι 

    <lb n="27"/>παρὰ τῆς ὑμῶν λαμπρότητος 

    <lb n="28"/><unclear cert="high" reason="undefined">τὴν</unclear> τιμὴν τοῦ διαπραθέντος 

    <lb n="29"/>αὐτῇ παρ᾽ ἡμῶν οἴνου ἄχρι ἑνὸς <num value="1"/> ὀβολοῦ, 

    <lb n="30"/>τουτέστιν οἴνου Σάμια διακοσία <num value="200"/> 

    <lb n="31"/>ἐπὶ καρπῶν τῆς παρούσης 

    <lb n="32"/><choice><corr>τεσσαρεσκαιδεκάτης</corr><sic>τεσσαρασκαιδεκατης</sic></choice> <expan>ἰνδ<ex cert="high">ικτίωνος</ex></expan> 

    <lb n="33"/>ἄχρι ἑνὸς <num value="1"/> ὀβολοῦ, τουτέστιν <choice><corr><supplied cert="high" reason="omitted">οἴν</supplied>ου</corr><sic>ου</sic></choice> Σάμια 

    <lb n="34"/>διακοσία <num value="200"/>. ὁμολογοῦμεν τοῦ λοιποῦ οὐδένα 

    <lb n="35"/>λόγ<unclear cert="high" reason="undefined">ον</unclear> <unclear cert="high" reason="undefined">ἔ</unclear>χειν πρὸς τὴν ὑμῶν <expan>μεγαλ<ex cert="high">οπρέπειαν</ex></expan> διὰ τὸ 

    <lb n="36"/>ἡμᾶς εἰληφέναι καὶ δεδέχθαι 

    <lb n="37"/>καὶ πεπληρῶσθαι παρ᾽ αὐτῆς 

    <lb n="38"/>τὴν τιμὴν τοῦ διαπραθέντος 

    <lb n="39"/>αὐτῇ παρ᾽ ἡμῶν <supplied cert="high" reason="omitted">οἴνου</supplied> ἄχρι ἑνὸς <num value="1"/> ὀβολοῦ 

    <lb n="40"/>καὶ πρὸς ἀπόδειξιν τῆς <supplied cert="high" reason="omitted">τῆς</supplied> τιμῆς 

    <lb n="41"/>τοῦ εἰρημένου οἴνου καταβολῆς 

    <lb n="42"/>τήνδε τὴν πληρωτικὴν 

    <lb n="43"/>ἀποχὴ<supplied cert="high" reason="lost">ν</supplied> ἐξεθέμεθα αὐτῇ, ἥτις 

    <lb n="44"/><gap extent="4" reason="illegible" unit="character"/><gap extent="3" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> <unclear cert="high" reason="undefined">κ</unclear>αὶ <expan>ἐπερ<ex cert="high">ωτηθέντες</ex></expan> <expan>ὡμολογ<ex cert="high">ήσαμεν</ex></expan>. <expan>γί<ex cert="high">νεται</ex></expan> 

    <lb n="45"/><expan><ex cert="high">ἔτους</ex></expan> <num value="287">σπζ</num>  <num value="256"><unclear cert="high" reason="undefined">σνϛ</unclear></num>, <num value="14">ιδ</num> <expan>ἰνδ<ex cert="high">ικτίωνος</ex></expan>. <handShift new="m2"/> <choice><corr>Αὐρήλιοι</corr><sic>αυρηλιος</sic></choice> Μακᾶρι<unclear cert="high" reason="undefined">ς</unclear> 

    <lb n="46"/><expan>μείζ<ex cert="high">ων</ex></expan> κα<supplied cert="high" reason="lost">ὶ Ἰωά</supplied><unclear cert="high" reason="undefined">νν</unclear>ης καὶ Μ<supplied cert="high" reason="lost">η</supplied>νᾶς κ<supplied cert="high" reason="lost">α</supplied>ὶ Ἀβραὰμ 

    <lb n="47"/><supplied cert="high" reason="lost">καὶ Μηνᾶς</supplied> <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">αὶ Ὡ</supplied><unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ι</supplied>γέν<supplied cert="high" reason="lost">ης οἱ</supplied> <expan>προ<unclear cert="high" reason="undefined">γ</unclear><supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">γραμμ</unclear><supplied cert="high" reason="lost"><ex cert="high">ένοι</ex></supplied></expan> 

