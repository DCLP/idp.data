    <lb n="45"/><expan><ex cert="high">ἔτους</ex></expan> <num value="287">σπζ</num>  <num value="256"><unclear reason="undefined" cert="high">σνϛ</unclear></num>, <num value="14">ιδ</num> <expan>ἰνδ<ex cert="high">ικτίωνος</ex></expan>. <handShift new="m2"/> <choice><corr>Αὐρήλιοι</corr><sic>αυρηλιος</sic></choice> Μακᾶρι<unclear reason="undefined" cert="high">ς</unclear> 

