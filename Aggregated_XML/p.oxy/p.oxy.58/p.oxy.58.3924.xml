    <lb n="16"/>μεμφθῆναι <supplied cert="high" reason="lost">ἢ</supplied> ἔ<supplied cert="high" reason="lost">νο</supplied><unclear cert="high" reason="undefined">χο</unclear><supplied cert="high" reason="lost">ς ε</supplied>ἴην 

    <lb n="17"/>τῷ ὅρκῳ. π<supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">ρ</unclear>έσχον δὲ ἐμαυ-

    <lb n="18"/>τοῦ <choice><corr>ἐγγυητὴν</corr><sic>ενγυητην</sic></choice> Γά<hi rend="diaeresis">ι</hi>ον <hi rend="diaeresis">Ἰ</hi>ούλιον 

    <lb n="19"/>Ἀντώνιον παρόντα καὶ εὐδο-

    <lb n="20"/>κοῦντα. ἔτους <num value="2">β</num> Αὐτοκράτορος 

    <lb n="21"/>Καίσ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ρ</supplied><unclear cert="high" reason="undefined">ο</unclear>ς Μάρκου Αὐρηλίου Σεουήρο<unclear cert="high" reason="undefined">υ</unclear> 

    <lb n="22"/>Ἀλεξάνδρου Εὐσεβοῦς Εὐτυχοῦς 

    <lb n="23"/>Σεβαστοῦ, Μεσορὴ ἐπαγομένων <num value="1">α</num>. 

    <lb n="24"/><handShift new="m2"/> Γά<hi rend="diaeresis">ι</hi>ος Πουλφέρνιος Τιβερεῖνος 

    <lb n="25"/>ὤμοσα τὸν ὃ<unclear cert="high" reason="undefined">ρ</unclear>κον καὶ ἐκτε<unclear cert="high" reason="undefined">λέσω</unclear> 

    <lb n="26"/>τὴν χρεί<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ν ὡς π</supplied><unclear cert="high" reason="undefined">ρ</unclear>όκειται. 

    <lb n="27"/><handShift new="m3"/> Γά<hi rend="diaeresis">ι</hi>ος <hi rend="diaeresis">Ἰ</hi>ο<unclear cert="high" reason="undefined">ύλιο</unclear><supplied cert="high" reason="lost">ς</supplied> <unclear cert="high" reason="undefined">Ἀ</unclear>ντώνιο<supplied cert="high" reason="lost">ς</supplied> 

