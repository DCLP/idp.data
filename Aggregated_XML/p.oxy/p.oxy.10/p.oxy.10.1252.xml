    <lb n="1"/><gap extent="16" reason="lost" unit="character"/> ἀπὸ <unclear cert="high" reason="undefined">Π</unclear><gap extent="2" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ε</unclear>ίμεω<unclear cert="high" reason="undefined">ς</unclear> ἑτέρα· 

