    <lb n="1"/>ὑπατείας τῶν δεσποτῶν <supplied cert="high" reason="lost">ἡμῶν Κωνσταντίνου καὶ Λικιννίου</supplied> 

    <lb n="2"/><space dim="horizontal" extent="unknown" unit="character"/>       Σεβα<supplied cert="high" reason="lost">στῶν τὸ <num value="4">δ</num> </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>Αὐρηλίᾳ <subst><add place="inline">Εὐτροπίῃ</add><del rend="corrected">ευτροπιον</del></subst> θυγατρὶ <supplied cert="high" reason="lost">Θ</supplied>εοδώρου τοῦ καὶ Χαιρ<supplied cert="high" reason="lost">ήμο</supplied>-

    <lb n="4"/>νος <expan>γυμ<ex cert="high">νασιαρχήσαντος</ex></expan> <expan>πρυ<ex cert="high">τανεύσαντος</ex></expan> <expan>γενομ<ex cert="high">ένου</ex></expan> <expan>βουλ<ex cert="high">ευτοῦ</ex></expan> τῆς <expan><supplied cert="high" reason="lost">λ</supplied>αμ<ex cert="high">πρᾶς</ex></expan> καὶ <expan>λαμ<ex cert="high">προτάτης</ex></expan> Ὀξυρυγχιτ<supplied cert="high" reason="lost">ῶν</supplied> 

    <lb n="5"/>πόλεως <space dim="horizontal" extent="unknown" unit="character"/> 

    <lb n="6"/>παρὰ Αὐρηλίου Διοσκόρου Ἀμμωνίου ἀπὸ τῆς αὐτῆ<supplied cert="high" reason="lost">ς <expan>πόλ<ex cert="high">εως</ex></expan>.</supplied> 

    <lb n="7"/>ἑκουσίω<supplied cert="high" reason="lost">ς</supplied> ἐπιδέχομαι μισθώσασθαι πρὸς μόν<supplied cert="high" reason="lost">ον τὸ</supplied> 

    <lb n="8"/>ἐνεστὸς <num value="10">ι</num> <supplied cert="high" reason="lost">κ</supplied>αὶ <num value="8">η</num> ἔτος ἀπὸ τῶν ὑπαρχόντων σοι περὶ τὸ <hi rend="diaeresis">Ἰ</hi><supplied cert="high" reason="lost">σῖον</supplied> 

    <lb n="9"/>Παγ<g type="apostrophe"/>γᾶ ἐ<unclear cert="high" reason="undefined">ν</unclear> <unclear cert="high" reason="undefined">π</unclear>εριχώματι Πέκτυ ἐν τόπῳ Τέλ<unclear cert="high" reason="undefined">κε</unclear> καλουμ<supplied cert="high" reason="lost">ένῳ</supplied> 

    <lb n="10"/>ἀρουρῶν δεκαέπτα <num value="17"/> κοινωνίας Πανάρους κατὰ τὸ τ<supplied cert="high" reason="lost">έταρτον</supplied> 

    <lb n="11"/>ὄγδοον μέ<unclear cert="high" reason="undefined">ρος</unclear> ὅ ἐστι ἄρουραι ἓξ τέταρτον ὄγδοον <num value="6  1/4   1/8 "/> <supplied cert="high" reason="lost">ἐκ γεωμε</supplied>-

    <lb n="12"/>τρίας <choice><corr>εἰς <supplied cert="high" reason="lost">σπορ</supplied>ὰ<unclear cert="high" reason="undefined">ν</unclear></corr><sic>εισ<supplied cert="high" reason="lost">πορ</supplied>ὰν</sic></choice> <supplied cert="high" reason="lost">λι</supplied>νοκαλάμης καὶ <choice><corr>τελέσειν</corr><sic>τελεσιν</sic></choice> ὑπὲρ φόρου τῆ<unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">μὲ</unclear><supplied cert="high" reason="lost">ν ἡμισείας</supplied> 

    <lb n="13"/>ἑκάστη<supplied cert="high" reason="lost">ς ἀρούρης</supplied> ἀνὰ ἀργυρί<unclear cert="high" reason="undefined">ου</unclear> τάλαντα <choice><corr>τέσσαρα</corr><sic>τεσσαρ<gap extent="1" reason="illegible" unit="character"/>ς</sic></choice> <num value="4"/> ἀ<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">τὶ δὲ φό</supplied>-

    <lb n="14"/>ρου <supplied cert="high" reason="lost">τὴς λοιπῆ</supplied><unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">ἡμ</unclear><supplied cert="high" reason="lost">ισεία</supplied>ς <choice><corr>ἥμισυ</corr><sic><unclear cert="high" reason="undefined">η</unclear><supplied cert="high" reason="lost">μ</supplied><unclear cert="high" reason="undefined">ι</unclear>σοι</sic></choice> μ<unclear cert="high" reason="undefined">έρος</unclear> τῆς ἐκβησομέ<supplied cert="high" reason="lost">νης</supplied> 

    <lb n="15"/><gap extent="1" reason="illegible" unit="character"/>ης λιν<supplied cert="high" reason="lost">οκαλάμ</supplied>ης, <choice><corr>καὶ ἐμὲ</corr><sic>καμε</sic></choice> δ<supplied cert="high" reason="lost">ὲ</supplied> <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ὸν</supplied> μεμισθωμένον ἀνθ᾽ <supplied cert="high" reason="lost">ὧν παρέχω</supplied> 

    <lb n="16"/>σπερμ<supplied cert="high" reason="lost">άτων κα</supplied>ὶ ἧς ποιοῦμα<supplied cert="high" reason="lost">ι γεωρ</supplied>γίας τὸ λοιπὸν <choice><corr>ἥμισυ</corr><sic>ημισο<supplied cert="high" reason="lost">ι</supplied></sic></choice> <supplied cert="high" reason="lost">μέρος</supplied> 

    <lb n="17"/>καὶ ἐξ ὁ<supplied cert="high" reason="lost">λοκλήρου τ</supplied><unclear cert="high" reason="undefined">ὸ</unclear> λινόσπε<supplied cert="high" reason="lost">ρμον ἀ</supplied>κίνδυνα πάντα παντὸς <supplied cert="high" reason="lost">κινδύνου,</supplied> 

    <lb n="18"/>τῶν τῆς <supplied cert="high" reason="lost">γῆς δη</supplied>μοσίων ὄ<supplied cert="high" reason="lost">ντω</supplied>ν πρὸς σὲ τὴν γεοῦχον κυριεύ<supplied cert="high" reason="lost">ουσαν</supplied> 

    <lb n="19"/>τῶν κα<supplied cert="high" reason="lost">ρπῶν</supplied> ἑ̂ως τὰ <supplied cert="high" reason="lost">ὀφ</supplied>ειλόμενα ἀπολάβῃ<supplied cert="high" reason="lost">ς</supplied>· βεβαιουμ<supplied cert="high" reason="lost">έ</supplied>-

    <lb n="20"/>νης δέ <supplied cert="high" reason="lost">μοι τῆς ἐ</supplied>πιδοχῆς ἐπάναγκες <choice><corr>ἀποδώσω</corr><sic>αποδοσω</sic></choice> τοὺς φ<unclear cert="high" reason="undefined">ό</unclear>ρου<unclear cert="high" reason="undefined">ς</unclear> 

    <lb n="21"/>καὶ τὰ ἐ<unclear cert="high" reason="undefined">κ</unclear>φ<unclear cert="high" reason="undefined">όρια</unclear> <supplied cert="high" reason="lost">τ</supplied>ὸν μὲν ἀργυρικὸν φόρον τῷ Παῦνι μηνὶ <unclear cert="high" reason="undefined">τὸ</unclear> δὲ 

    <lb n="22"/><unclear cert="high" reason="undefined">ἐκφό</unclear><supplied cert="high" reason="lost">ριον τῆς λι</supplied>νοκαλάμης ἐπὶ τῆς λίμνου σεσυνβρ<supplied cert="high" reason="lost">ο</supplied>χι<supplied cert="high" reason="lost">σμένης</supplied> 

    <lb n="23"/><unclear cert="high" reason="undefined">τ</unclear>α<gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="illegible" unit="character"/> ἐνεστῶτος ἔτους ἀνυπερθέτως, γεινομένης σ<unclear cert="high" reason="undefined">οι</unclear> 

    <lb n="24"/>τῆς πράξεως παρά τε ἐμοῦ ὡς <unclear cert="high" reason="undefined">καθήκε</unclear>ι. κυρία ἡ ἐπιδοχὴ 

    <lb n="25"/>καὶ ἐπερ<supplied cert="high" reason="lost">ωτ</supplied>ηθεὶς ὡμολόγησα. 

    <lb n="26"/>ὑπατεία<supplied cert="high" reason="lost">ς τ</supplied>ῆς προκειμένης  <handShift new="m2"/> Ἁθὺρ <num value="1"><unclear cert="high" reason="undefined">α</unclear></num><gap extent="1" reason="illegible" unit="character"/> 

    <lb n="27"/>Αὐρηλία Εὐτρόπιον δι᾽ ἐμοῦ Πτο<unclear cert="high" reason="undefined">λ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="28"/>ἔσχον τούτου τὸ ἴσον. </ab></div><div n="meta.20895" part="N" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.20895" part="N" subtype="textDate" type="commentary"><p><date notAfter="315" notBefore="315" type="textDate" value="0315-11-07">7. Nov. 315</date></p></div><div n="meta.20895" part="N" subtype="general" type="commentary"><p>Gehört zum Leonides - Archiv. Zur Datierung vgl. BL VIII, S. 85.</p></div><div n="meta.20895" part="N" subtype="principalEdition" type="bibliography"><listBibl><bibl subtype="principal" type="publication"><title level="s" type="abbreviated">P.Coll. Youtie</title><biblScope type="volume">2</biblScope><biblScope type="numbers">80</biblScope></bibl><bibl type="Trismegistos"><biblScope type="numbers">20895</biblScope></bibl><bibl type="DDbDP"><series>0181</series>; <biblScope type="volume">45</biblScope>; <biblScope type="numbers">3255</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0181;45;3255" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.20895" part="N" subtype="corrections" type="bibliography"><head>BL-Einträge nach BL-Konkordanz</head><bibl type="BL"><biblScope type="volume">VIII</biblScope>, S. <biblScope type="pages">85</biblScope></bibl></div><div n="meta.20895" part="N" subtype="illustrations" type="bibliography"><p><bibl type="illustration">Plate XXVIII</bibl></p></div><div n="meta.20895" part="N" subtype="otherPublications" type="bibliography"><head>Andere Publikation</head><bibl subtype="other" type="publication">P.Oxy. XLV 3255</bibl></div><div n="meta.20895" part="N" subtype="locations" type="history"><p><placeName cert="high" type="ancientFindspot">Oxyrhynchos</placeName></p></div><div n="meta.20895" part="N" type="figure"><p><figure href="http://163.1.169.40/cgi-bin/library?e=d-000-00---0POxy--00-0-0-0prompt-10---4---Document---0-1l--1-en-50---20-about---001-001-1-0utfZz-8-00031-001-1-0utfZz-8-