    <lb n="3"/>σίου ἀδελφοῦ <unclear reason="undefined" cert="high">Σ</unclear><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="1" unit="character"/> καὶ Σεύθης <expan>Διο<unclear reason="undefined" cert="high">γέ</unclear><ex cert="high">νους</ex></expan> 

    <lb n="4"/>τοῦ Διονυσίου γέ<supplied reason="lost" cert="high">ρ</supplied>διος ἡ μὲν Τασεῦς 

    <lb n="5"/><choice><corr>ἐκδεδόσθαι</corr><sic>εγδεδοσθαι</sic></choice> τ<unclear reason="undefined" cert="high">ῶ</unclear><supplied reason="lost" cert="high">ι Σε</supplied>ύθῃ τὸν ἑαυτῆς <expan>υ<ex cert="high">ἱὸν</ex></expan> 

    <lb n="6"/>Ἡρακλᾶ<unclear reason="undefined" cert="high">ν</unclear> Ἀπο<unclear reason="undefined" cert="high">λλω</unclear><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="1" unit="character"/> οὐδέπω ὄντα 

    <lb n="7"/>τῶν ἐτῶν πρὸς μάθησιν τῆς γερδι-

    <lb n="8"/>ακῆς <subst><add place="inline">τέχνης</add><del rend="corrected" status="unremarkable"><gap reason="illegible" extent="1" unit="character"/>εχνης</del></subst> ἐ<unclear reason="undefined" cert="high">π</unclear>᾽ ἔτη δύο <num value="2"/> καὶ μῆνας 

    <lb n="9"/>ἓξ <num value="6"/> ἀπὸ τῆς ἐνεστώσης ἡμέρας ποι-

    <lb n="10"/>οῦντα πάντα τὰ ἐπιτασσόμενα αὐτῶι 

    <lb n="11"/>ὑπὸ τοῦ Σεύθου κατὰ τὴν γερδιακὴν τέ-

    <lb n="12"/>χνην. ὁ δὲ Σεύθης καὶ αὐτὸς ἐκδιδάξει 

    <lb n="13"/>τοῦτον καθὼς ἐπίσταται, τοῦ παιδὸς 

    <lb n="14"/>τρεφομένου καὶ ἱματιζομένου ὑπὸ 

    <lb n="15"/>τοῦ <choice><corr>διδασκάλου</corr><sic>διδεσκαλου</sic></choice> Σεύθου, ὁ δὲ αὐτὸς Σεύ-

    <lb n="16"/>θης διορθώσεται τὸ ὑπὲρ τοῦ παιδὸς 

    <lb n="17"/>χειρωνάξιον <subst><add place="inline">σὺν</add><del rend="corrected" status="unremarkable"><gap reason="illegible" extent="3" unit="character"/></del></subst> κατακρίματι τῶν 

    <lb n="18"/>δύο ἡμίσους <num value="2  1/2 "/> ἐτῶν. ἐὰν δὲ καὶ ἐν τῶι 

    <lb n="19"/>αὐτῶι χρόνωι ὁ παῖς ἀπαιτηθῇ λαο-

    <lb n="20"/>γραφίαν ἢ χωματικὸν ἢ ὑικὴν ἔσται 

    <lb n="21"/>πρὸς τὸν <unclear reason="undefined" cert="high">αὐτ</unclear><supplied reason="lost" cert="high">ὸ</supplied><unclear reason="undefined" cert="high">ν</unclear> <supplied reason="lost" cert="high">Σ</supplied>εύθην. ὁ δὲ αὐτὸς 

    <lb n="22"/>Σεύθης, ἐὰν μὴ βούληται ὁ παῖς ὑπ<unclear reason="undefined" cert="high">ὸ</unclear> 

    <lb n="23"/>τοῦ <choice><corr>διδασκάλου</corr><sic>διδεσκαλου</sic></choice> τρέφεσθαι, χωρηγή-

    <lb n="24"/>σει τῇ Τα<supplied reason="lost" cert="high">σ</supplied><unclear reason="undefined" cert="high">ε</unclear>ῦτι ὑπὲρ τῶν τούτου τρο-

    <lb n="25"/>φείων κατὰ μῆνα ἕκαστον ἀργυρίου 

    <lb n="26"/>δραχμὰς <supplied reason="lost" cert="high">π</supplied>έντε <num value="5"/> καὶ μετὰ τὸν χρόνον 

    <lb n="27"/><unclear reason="undefined" cert="high">δώ</unclear>σει τ<supplied reason="lost" cert="high">ῶ</supplied>ι παιδὶ ὑ<unclear reason="undefined" cert="high">π</unclear>άγοντι <subst><add place="inline">χιτῶνα</add><del rend="corrected" status="unremarkable">χι<gap reason="illegible" extent="1" unit="character"/>ωνα</del></subst> 

    <lb n="28"/><supplied reason="lost" cert="high">ἄξι</supplied>ον δρ<supplied reason="lost" cert="high">αχ</supplied><unclear reason="undefined" cert="high">μ</unclear>ῶν δεκαδύο <num value="12"/> ἢ αὐτὰς τὰς 

    <lb n="29"/><supplied reason="lost" cert="high">δραχμὰς δεκαδύο <num value="12"/>.</supplied> ἀργήσει δὲ ὁ παῖς 

    <lb n="30"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="30"/><gap reason="lost" extent="15" unit="character"/><gap reason="illegible" extent="4" unit="character"/> <unclear reason="undefined" cert="high">Ἀ</unclear><supplied reason="lost" cert="high">με</supplied><unclear reason="undefined" cert="high">σ</unclear>υ<unclear reason="undefined" cert="high">σίοι</unclear><supplied reason="lost" cert="high">ς</supplied> 

    <lb n="31"/><gap reason="lost" extent="11" unit="character"/><gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="1" unit="character"/> <supplied reason="lost" cert="high">ὅ</supplied>σας δ᾽ ἂ<supplied reason="lost" cert="high">ν</supplied> με<unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ὰ</supplied> <unclear reason="undefined" cert="high">τ</unclear>α<unclear reason="undefined" cert="high">ύ</unclear>-

    <lb n="32"/><supplied reason="lost" cert="high">τας ὁ παῖς ἀργ</supplied>ήσῃ, ἐπὶ τὰς ἴσας αὐτὸν π<unclear reason="undefined" cert="high">αρ</unclear>-

    <lb n="33"/><supplied reason="lost" cert="high">έξεται ἡ Τασε</supplied><unclear reason="undefined" cert="high">ῦς</unclear> μετὰ τὸν χρόνον ᾒ ἀ<supplied reason="lost" cert="high">πο</supplied>-

    <lb n="34"/><supplied reason="lost" cert="high">τεισάτω ἑκάστη</supplied><unclear reason="undefined" cert="high">ς</unclear> ἡμέ<supplied reason="lost" cert="high">ρα</supplied><unclear reason="undefined" cert="high">ς</unclear> δραχμὴν μία<supplied reason="lost" cert="high">ν</supplied> <num value="1"/>, 

    <lb n="35"/><supplied reason="lost" cert="high">τοῦ δὲ ἀποσπ</supplied><unclear reason="undefined" cert="high">α</unclear>σθῆν<supplied reason="lost" cert="high">α</supplied>ι <unclear reason="undefined" cert="high">π</unclear>ρὸ τοῦ χρόν<unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">υ</supplied> 

    <lb n="36"/><choice><corr>ἐπ<supplied reason="lost" cert="high">ί</supplied>τιμ<supplied reason="lost" cert="high">ο</supplied>ν</corr><sic>ε<unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ι</supplied><unclear reason="undefined" cert="high">τειμ</unclear><supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">ν</unclear></sic></choice> <expan><supplied reason="lost" cert="high">ἀρ</supplied>γ<ex cert="high">υρίου</ex></expan> <expan><ex cert="high">δραχμὰς</ex></expan> ἑκατ<supplied reason="lost" cert="high">ὸ</supplied>ν <num value="100"/> καὶ εἰς τὸ <expan>δημόσιο<ex cert="high">ν</ex></expan> 


    <lb n="37"/>τὰς ἴσας. ἐὰν <unclear reason="undefined" cert="high">δὲ</unclear> καὶ αὐτὸς ὁ Σεύθης μὴ <choice><corr>ἐκ-

    <lb n="38"/>διδάξῃ</corr><sic>εγδιδαξη</sic></choice> τὸν παῖδα <choice><corr>ἐνεχέσθω</corr><sic>ενεχεσθωι</sic></choice> τῷ ἴσωι 

    <lb n="39"/><choice><corr>ἐπιτίμῳ</corr><sic>ε<unclear reason="undefined" cert="high">πι</unclear>τειμωι</sic></choice>. κυρία ἡ <choice><corr>διδασκαλική</corr><sic>διδεσκαλικη</sic></choice>. <expan><ex cert="high">ἔτους</ex></expan> <num value="12">ιβ</num> 

    <lb n="40"/>Νέρωνος Κ<supplied reason="lost" cert="high">λαυ</supplied>δ<supplied reason="lost" cert="high">ίου</supplied> Κ<supplied reason="lost" cert="high">α</supplied><unclear reason="undefined" cert="high">ί</unclear>σαρος Σεβαστοῦ 

    <lb n="41"/>Γερμανικοῦ <supplied reason="lost" cert="high">Αὐ</supplied><unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">οκ</supplied><unclear reason="undefined" cert="high">ρ</unclear>άτορος, Φ<supplied reason="lost" cert="high">α</supplied>μεν<supplied reason="lost" cert="high">ὼ</supplied>θ <num value="15">ιε</num>. 

    <lb n="42"/><handShift new="m2"/> Σεύθης <choice><corr>Δ<supplied reason="lost" cert="high">ιο</supplied>γένους</corr><sic>δ<supplied reason="lost" cert="high">ιο</supplied><unclear reason="undefined" cert="high">γε</unclear>νης</sic></choice> <choice><corr>ἐκδιδάξω</corr><sic>εγδιδαξω</sic></choice> 

    <lb n="43"/>τὸν παῖδ<supplied reason="lost" cert="high">α</supplied> καὶ ποιήσω καθότι 

    <lb n="44"/>πρόκιτα<supplied reason="lost" cert="high">ι</supplied>. <expan><ex cert="high">ἔτους</ex></expan> <num value="12">ιβ</num> Νέρωνος <expan>Κλαυδ<unclear reason="undefined" cert="high">ί</unclear>ο<ex cert="high">υ</ex></expan> 

