    <lb n="1"/>ὑπατείας τῶν δεσποτῶν ἡμῶν Κων<supplied cert="high" reason="lost">σταντίνου Σεβαστοῦ τὸ <num value="6">ϛ</num></supplied> 

    <lb n="2"/>καὶ Κωνσταντίνου τοῦ ἐπιφανεστάτου <supplied cert="high" reason="lost">Καίσαρος τὸ <num value="1">α</num>, </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>Οὐαλερίῳ Ἀμμωνιανῷ τῷ καὶ Γερο<supplied cert="high" reason="lost">ντίῳ διοικοῦντι τὴν</supplied> 

    <lb n="4"/><choice><corr>λογιστείαν</corr><sic>λογιστιαν</sic></choice> Ὀξυρυγ<g type="apostrophe"/>χείτου 

    <lb n="5"/>παρὰ Αὐρηλίας Τα<hi rend="diaeresis">ΰ</hi>ριος Ὡρίων<supplied cert="high" reason="lost">ος </supplied><gap extent="22" reason="lost" unit="character"/> 

    <lb n="6"/>ἀπὸ τῆς λαμπρᾶς καὶ λαμπροτάτης Ὀ<supplied cert="high" reason="lost">ξυρυγχιτῶν πόλεως. βούλομαι</supplied> 

    <lb n="7"/>πρώτως ἀναγραφῆναι τὸν γενό<unclear cert="high" reason="undefined">με</unclear><supplied cert="low" reason="lost">νον τῷ ἡμετέρῳ υἱῷ Αὐρηλίῳ</supplied> 

    <lb n="8"/>Θεοδώρῳ ἐν <choice><corr>στρατείᾳ</corr><sic>στρατια</sic></choice> ἐξεταζομέν<supplied cert="high" reason="lost">ῳ υἱὸν </supplied><gap extent="18" reason="lost" unit="character"/> 

    <lb n="9"/><unclear cert="high" reason="undefined">μ</unclear>ου τοῦ καὶ Ἡρακλάμμωνος ἀπὸ <unclear cert="high" reason="undefined">τ</unclear><supplied cert="low" reason="lost">ῆς αὐτῆς πόλεως ἐφ᾽ ἧς ἔχει</supplied> 

    <lb n="10"/>ὁ ἡμέτερος <hi rend="diaeresis">ὑ</hi><hi rend="diaeresis">ι</hi>ὸς τοῦ δὲ <hi rend="diaeresis">υ</hi>ἱωνοῦ <unclear cert="high" reason="undefined">μ</unclear><supplied cert="low" reason="lost">ου πατὴρ </supplied><gap extent="6" reason="lost" unit="character"/><supplied cert="high" reason="lost"> οἰκίας ἐπὶ</supplied> 

    <lb n="11"/>ἀμφόδου <unclear cert="high" reason="undefined">Παμμέν</unclear><supplied cert="high" reason="lost">ου</supplied><unclear cert="high" reason="undefined">ς</unclear> <choice><corr>Παραδείσου</corr><sic><unclear cert="high" reason="undefined">πα</unclear>ραδιζου</sic></choice> <gap extent="1" reason="illegible" unit="character"/><gap extent="22" reason="lost" unit="character"/> 

