    <lb n="1"/>ὑπατείας <supplied cert="high" reason="lost">τῶν δεσποτῶν ἡμῶν</supplied> 

    <lb n="2"/>Κωνσταντ<supplied cert="high" reason="lost">ίνου Σεβαστοῦ τὸ <num value="5">ε</num> καὶ</supplied> 

    <lb n="3"/>Λικινίου Καί<unclear cert="high" reason="undefined">σαρ</unclear><supplied cert="high" reason="lost">ος τὸ <num value="1">α</num>.</supplied> 

