    <lb n="85"/><expan><ex cert="high">ἔτους</ex></expan> <num value="20">κ</num> Τραιανοῦ Καίσαρος <expan>τ<ex cert="high">οῦ</ex></expan> <expan>κ<ex cert="high">υρίου</ex></expan> <expan>μη<ex cert="high">νὸς</ex></expan> 

    <lb n="86"/>Κ<unclear cert="high" reason="undefined">αι</unclear>σαρείο<unclear cert="high" reason="undefined">υ</unclear> <num value="2">β</num> 

    <lb n="87"/>ἐγδέδωκα ἀποχὴν τῶι Τοτοῇ καὶ <expan>Παχνο<ex cert="high">ῦμει</ex></expan> 

    <lb n="88"/><unclear cert="high" reason="undefined">τῶ</unclear>ν <expan>ἐκφο<ex cert="high">ρίων</ex></expan> <supplied cert="high" reason="omitted">καὶ</supplied> <expan><unclear cert="high" reason="undefined">λο</unclear><ex cert="high">ιπογραφίας</ex></expan> φόρων <expan>μη<ex cert="high">νὸς</ex></expan> <expan>Σεβ<ex cert="high">αστοῦ</ex></expan> 

    <lb n="89"/><abbr><unclear cert="high" reason="undefined">περ</unclear>ιδ<gap extent="1" reason="illegible" unit="character"/>ηδ</abbr> <expan><ex cert="high">αὐτοῦ</ex></expan> <expan><ex cert="high">ἔτους</ex></expan> </ab></div></div><div n="meta.22222" part="N" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.22222" part="N" subtype="textDate" type="commentary"><p><date notAfter="112" notBefore="112" type="textDate" value="0112-10-07">7. Okt. 112</date></p></div><div n="meta.22222" part="N" subtype="general" type="commentary"><p>Rückseite: 26. Juli 116.</p></div><div n="meta.22222" part="N" subtype="principalEdition" type="bibliography"><listBibl><bibl subtype="principal" type="publication"><title level="s" type="abbreviated">P.Oxy.</title><biblScope type="volume">22</biblScope><biblScope type="numbers">2351</biblScope></bibl><bibl type="Trismegistos"><biblScope type="numbers">22222</biblScope></bibl><bibl type="DDbDP"><series>0181</series>; <biblScope type="volume">22</biblScope>; <biblScope type="numbers">2351</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0181;22;2351" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.22222" part="N" subtype="corrections" type="bibliography"><head>BL-Einträge nach BL-Konkordanz</head><bibl type="BL"><biblScope type="volume">VIII</biblScope>, S. <biblScope type="pages">256/257</biblScope></bibl></div><div n="meta.22222" part="N" subtype="illustrations" type="bibliography"><p>keine</p></div><div n="meta.22222" part="N" subtype="locations" type="history"><p><placeName cert="high" type="ancientFindspot">Oxyrhynchos</placeName></p></div><div n="meta.22222" part="N" type="figure"><p><figure href="http://163.1.169.40/cgi-bin/library?e=d-000-00---0POxy--00-0-0-0prompt-10---4---Document---0-1l--1-en-50---20-about---001-001-1-0utfZz-8-00031-001-1-0utfZz-8-00&amp;a=d&amp;c=POxy&amp;cl=CL5.1.14&amp;d=HASH163eb5e519e6786f4