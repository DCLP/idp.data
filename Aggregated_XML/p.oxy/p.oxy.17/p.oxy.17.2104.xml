    <lb n="16"/>τας εἰ μὴ προσδεχθείη αὐτῶν <gap reason="lost" extent="12" unit="character"/> 

    <lb n="17"/><gap reason="illegible" extent="2" unit="character"/>εισα διασφα<supplied reason="lost" cert="high">λ</supplied>ίσασθ<supplied reason="lost" cert="high">α</supplied><unclear reason="undefined" cert="high">ι</unclear> μὴ δυνηθ<gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="10" unit="character"/> 

