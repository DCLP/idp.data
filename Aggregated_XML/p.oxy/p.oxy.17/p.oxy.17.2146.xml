    <lb n="1"/>δευτέρας στέγης νοτινῆς συ<unclear cert="high" reason="undefined">μπ</unclear><gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>τοῦ <unclear cert="high" reason="undefined">π</unclear>ε<supplied cert="high" reason="lost">σσο</supplied><unclear cert="high" reason="undefined">ῦ</unclear> <unclear cert="high" reason="undefined">σὺ</unclear><supplied cert="high" reason="lost">ν </supplied><gap extent="1" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">λωτη</unclear> <num value="1">α</num>, βορινῆ<supplied cert="high" reason="lost">ς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>ηλ<unclear cert="high" reason="undefined">α</unclear><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> θυρίδες</supplied> 

    <lb n="4"/>ἐπιφωτίζουσαι τὰ ὑποκείμενα <gap extent="1" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>χαλκῆ <num value="1">α</num>, καὶ <supplied cert="high" reason="lost">ἐ</supplied>πὶ τοίχου τοῦ αὐτοῦ χαλκοθεσίου κλεῖδες χαλκαῖ <gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> σὺν </supplied><gap extent="3" reason="lost" unit="character"/> 

    <lb n="6"/>καὶ ἐξομβρι<unclear cert="high" reason="undefined">σ</unclear>τῆρσι <num value="2">β</num> χαλκαῖ, καὶ ἀφετήριαι χαλκαῖ <num value="2">β</num> ὧν <num value="1">α</num> βάλλουσα <unclear cert="high" reason="undefined">εἰ</unclear><supplied cert="high" reason="lost">ς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/>ἐν ὑπερῴοις τοῦ χαλκοθεσίου φελλὸς <num value="1">α</num> σὺν κλειδίῳ χαλκῷ καὶ ζυγώ<supplied cert="low" reason="lost">ματι </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>Ἀπιάδος βορινὴ καὶ κατόπιθεν στρογγύλη βορινὴ πρ<supplied cert="high" reason="lost">ὸ</supplied><unclear cert="high" reason="undefined">ς</unclear> ἀφετηρία<supplied cert="high" reason="lost">ις </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>τῇ αὐτῇ παραδρομίδι, θύρα κέλλας σὺν κερατέᾳ καὶ πήγμασι καὶ ἐπιστά<supplied cert="low" reason="lost">θμῳ</supplied><supplied cert="low" reason="lost"> ἄλλη θύρα βάλλουσα</supplied> 

    <lb n="10"/>εἰς πλατεῖαν, ἄλλη θύρα βάλλουσα εἰς παραδρομίδα πεσσοῦ σὺν κερατέᾳ <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/><gap extent="3" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>υκ<unclear cert="high" reason="undefined">λ</unclear>ακησίων καὶ ὠχνων ἔχοντα δίθυρα <num value="2">β</num> σὺν κερατέαις καὶ <gap extent="unknown" reason="lost" unit="character"/> 

