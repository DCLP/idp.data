    <lb n="1"/>Αὐρήλιος Ἀμμώνι<supplied cert="high" reason="lost">ο</supplied>ς 

    <lb n="2"/>Ἀμμωνίου ναύκληρος 

    <lb n="3"/>χειρισμοῦ Νέας πόλεως πλ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ίω</supplied><unclear cert="high" reason="undefined">ν</unclear> 

    <lb n="4"/><num value="3">γ</num> ἀγωγῆς <expan><ex cert="high">ἀρταβῶν</ex></expan> <expan>μ<ex cert="high">υριάδος</ex></expan> <num value="1">α</num>  <num value="5000"><unclear cert="high" reason="undefined">Ε</unclear></num> <num value="15000"/> Αὐρηλίῳ 

    <lb n="5"/>Σαραπίωνι <choice><corr>σιτολόγῳ</corr><sic>σειτολογω</sic></choice> ἄνω <expan>τοπ<ex cert="high">αρχίας</ex></expan> 

    <lb n="6"/>Σκὼ <expan>τόπ<ex cert="high">ων</ex></expan> χαίρειν. παρέλα-

    <lb n="7"/>βον καὶ παραμεμέτρημαι παρὰ 

    <lb n="8"/>σοῦ τὰς <supplied cert="high" reason="lost">ἐπ</supplied><unclear cert="high" reason="undefined">ι</unclear>σταλείσας μοι ὑπὸ τοῦ 

    <lb n="9"/><unclear cert="high" reason="undefined">τ</unclear>ε <unclear cert="high" reason="undefined">στρατη</unclear><supplied cert="high" reason="lost">γοῦ</supplied> Αὐρηλίου Ἁρποκρα-

    <lb n="10"/>τίωνος καὶ Αὐρηλίου Νεμ<unclear cert="high" reason="undefined">ε</unclear>σί<supplied cert="high" reason="lost">ω</supplied>νος 

    <lb n="11"/>τοῦ καὶ Διονυσίου βασιλικοῦ <expan>γρα<ex cert="high">μματέως</ex></expan>, 

    <lb n="12"/>ἐπακολουθούντων τῶν ἐπὶ 

    <lb n="13"/>τῆς ἐμβολῆς τεταγμένων 

    <lb n="14"/>καὶ ὧν ἄλλων δέον ἐστίν, <supplied cert="high" reason="lost">ἀ</supplied><unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ὸ</supplied> 

    <lb n="15"/>δημοσίων θ<supplied cert="high" reason="lost">η</supplied>σαυρῶν τῆς π<supplied cert="high" reason="lost">ρ</supplied>ο-

    <lb n="16"/>κειμένης <choice><corr>σιτολογίας</corr><sic>σειτολογιας</sic></choice> εἰς ὅρ<supplied cert="high" reason="lost">μ</supplied>ον 

    <lb n="17"/>Σατύρου τοῦ μεγάλου ποτα-

    <lb n="18"/>μοῦ <unclear cert="high" reason="undefined">πυροῦ</unclear> γενήματος τοῦ 

    <lb n="19"/>διελθόντος <num value="3">γ</num> <expan><ex cert="high">ἔτους</ex></expan> καθαροῦ ἀδόλου 

    <lb n="20"/>ἀβώλ<supplied cert="high" reason="lost">ου </supplied><subst><add place="inline"><supplied cert="high" reason="lost">ἀκρί</supplied>θ<unclear cert="high" reason="undefined">ου</unclear></add><del rend="corrected"><supplied cert="high" reason="lost">ακρι</supplied>θ<gap extent="2" reason="illegible" unit="character"/></del></subst> ἀ<unclear cert="high" reason="undefined">διπα</unclear>τ<unclear cert="high" reason="undefined">ή</unclear>του 

    <lb n="21"/>κεκοσκινευμένου μέτρῳ δη-

    <lb n="22"/>μοσίῳ ἡμιαρταβίῳ <sic>δημοσίῳ</sic> 

    <lb n="23"/>μετρήσε<supplied cert="high" reason="lost">ι</supplied> τῇ κελευσθείσῃ σὺν 

    <lb n="24"/><expan><ex cert="high">ἑκατοστῇ</ex></expan> <num value="1">α</num> καὶ ἡμιαρταβίῳ ἀρτάβας

    <lb n="25"/>ἑβδομήκοντα ἑπτά, <expan>γ<ex cert="high">ίνονται</ex></expan> <expan><ex cert="high">ἀρτάβαι</ex></expan> <num value="77">οζ</num>, 

    <lb n="26"/>ἃς καὶ κατάξω εἰς τὴν Ἀλεξάν-

    <lb n="27"/>δρειαν καὶ παραδώσω εἰς τὸν ἐν 

    <lb n="28"/>τῇ Νέᾳ πόλει χειρισμὸν πλή-

    <lb n="29"/>ρη <choice><corr>καὶ ἀ<supplied cert="high" reason="omitted">κα</supplied>κούργητον</corr><sic>κακουργ<unclear cert="high" reason="undefined">η</unclear>τον</sic></choice> τὸν <unclear cert="high" reason="undefined">γόμον</unclear>. 

    <lb n="30"/>κυρία ἡ ἀποχὴ <expan><ex cert="high">τρισσὴ</ex></expan> γραφεῖσα, σοὶ 

    <lb n="31"/>μὲν τῷ σιτολόγῳ ἁπλῆ τῷ δὲ <expan>στρα<ex cert="high">τηγῷ</ex></expan> 

    <lb n="32"/>δισσή, καὶ ἐπ<supplied cert="high" reason="lost">ε</supplied>ρωτηθεὶς ὡμολόγησα. 

    <lb n="33"/><expan><ex cert="high">ἔτους</ex></expan> <num value="4">δ</num> Αὐτοκραότορος Καίσαρος 

    <lb n="34"/>Μάρκου Αὐρηλίου Σεουήρου 

    <lb n="35"/>Ἀλεξάνδρου Εὐσεβοῦς Εὐτυχοῦς 

    <lb n="36"/>Σεβαστοῦ, Φαρμοῦθι <num value="11">ια</num>. 

    <lb n="37"/><handShift new="m2"/> Αὐρήλιος <choice><corr>Ἀμ<supplied cert="high" reason="omitted">μ</supplied>ώνιος</corr><sic>αμωνιος</sic></choice> <choice><corr>Ἀμ<supplied cert="high" reason="omitted">μ</supplied>ωνίου</corr><sic>αμωνιου</sic></choice> 

    <lb n="38"/>ναύκληρος <choice><corr>χειρισμοῦ</corr><sic>χιρισμου</sic></choice> Νέας 

    <lb n="39"/>πόλεως <choice><corr>μεμέτρημαι</corr><sic>μεμετρημε</sic></choice> καὶ <choice><corr>ἐμ-

    <lb n="40"/>βέβλημαι</corr><sic>εμβεβλημε</sic></choice> τὰς τ<supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ῦ</unclear> <supplied cert="high" reason="lost">πυ</supplied>ροῦ 

    <lb n="41"/>σὺν ἡμιαρταβί<supplied cert="high" reason="lost">ῳ καὶ</supplied> ἑκα-

    <lb n="42"/>τοστῇ <supplied cert="high" reason="lost">μιᾷ ἀρτάβας ἑβδο</supplied>-

