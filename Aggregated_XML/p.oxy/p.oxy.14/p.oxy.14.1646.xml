    <lb n="57"/><handShift new="m4"/> <unclear cert="high" reason="undefined">ἀ</unclear><supplied cert="high" reason="lost">ποχ</supplied><unclear cert="high" reason="undefined">ὴ</unclear> φόρων Οὐιβίου Πουπλίου <gap extent="unknown" reason="lost" unit="character"/> </ab></div></div><div n="meta.21955a" part="N" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.21955a" part="N" subtype="textDate" type="commentary"><p><date notAfter="268" notBefore="268" type="textDate" value="0268-01-30">30. Jan. 268</date></p></div><div n="meta.21955a" part="N" subtype="principalEdition" type="bibliography"><listBibl><bibl subtype="principal" type="publication"><title level="s" type="abbreviated">P.Oxy.</title><biblScope type="volume">14</biblScope><biblScope type="numbers">1646</biblScope><biblScope type="parts">Kol. I</biblScope></bibl><bibl type="Trismegistos"><biblScope type="numbers">21955</biblScope><biblScope type="parts">a</biblScope></bibl><bibl type="DDbDP"><series>0181</series>; <biblScope type="volume">14</biblScope>; <biblScope type="numbers">1646</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0181;14;1646" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.21955a" part="N" subtype="illustrations" type="bibliography"><p><bibl type="illustration">ZPE 26, 1977, Tafel I (nach S. 230)</bibl></p></div><div n="meta.21955a" part="N" subtype="locations" type="history"><p><placeName cert="high" type="ancientFindspot">Oxyrhynchos</placeName></p></div><div n="meta.21955b" part="N" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.21955b" part="N" subtype="textDate" type="commentary"><p><date notAfter="0270-10-11" notBefore="0270-09-28" type="textDate">28. Sept. - 11. Okt. 270</date></p></div><div n="meta.21955b" part="N" subtype="general" type="commentary"><p>Descr. Zur Datierung vgl. BL VII, S. 140.</p></div><div n="meta.21955b"