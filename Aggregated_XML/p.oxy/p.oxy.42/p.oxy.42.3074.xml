    <lb n="6"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">υ</unclear>ς καὶ γενόμενος εἰς τὸ ἄνω πεδί<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/><gap extent="unknown" reason="lost" unit="character"/>αρμωτου ἀπὸ τοῦ <unclear cert="high" reason="undefined">Ἀραβι</unclear>κοῦ διερμη<unclear cert="high" reason="undefined">ν</unclear><gap extent="unknown" reason="lost" unit="character"/> 

