    <lb n="14"/>ὧν τὰ ὀνόματα ἐπὶ τ<unclear reason="undefined" cert="high">οῦ</unclear> ῥη-

    <lb n="15"/>τοῦ δηλώσω. διὸ ἐπιδι<unclear reason="undefined" cert="high">δ</unclear>ο<unclear reason="undefined" cert="high">ὺ</unclear><supplied reason="lost" cert="high">ς</supplied> 

    <lb n="16"/>τὸ ἀναφόριον, ἀξιῶ ἐκδ<supplied reason="lost" cert="high">ι</supplied>-

    <lb n="17"/>κηθῆναι ὅπως ε<supplied reason="lost" cert="high">ἰ</supplied>ς τὸ <unclear reason="undefined" cert="high">μ</unclear>έ<unclear reason="undefined" cert="high">λ</unclear>-

    <lb n="18"/>λ<unclear reason="undefined" cert="high">ον</unclear> ἀνεπηρέα<supplied reason="lost" cert="high">στος σὺν</supplied> 

    <lb n="19"/>τοῖς ἐμοῖς φυλαχθ<supplied reason="lost" cert="low">ῶ. <expan><ex cert="high">ἔτους</ex></expan> <num value="14">ιδ</num> </supplied> 

    <lb n="20"/>Αὐτοκράτοος Καίσα<supplied reason="lost" cert="high">ρος</supplied> 

