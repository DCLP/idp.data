    <lb n="13"/>πλείστων ἀξιοχρέων <choice><corr>ἀ<supplied cert="high" reason="omitted">ν</supplied>δρῶ<supplied cert="high" reason="lost">ν</supplied></corr><sic>αδρω<supplied cert="high" reason="lost">ν</supplied></sic></choice> 

    <lb n="14"/>ὧν τὰ ὀνόματα ἐπὶ τ<unclear cert="high" reason="undefined">οῦ</unclear> ῥη-

    <lb n="15"/>τοῦ δηλώσω. διὸ ἐπιδι<unclear cert="high" reason="undefined">δ</unclear>ο<unclear cert="high" reason="undefined">ὺ</unclear><supplied cert="high" reason="lost">ς</supplied> 

    <lb n="16"/>τὸ ἀναφόριον, ἀξιῶ ἐκδ<supplied cert="high" reason="lost">ι</supplied>-

    <lb n="17"/>κηθῆναι ὅπως ε<supplied cert="high" reason="lost">ἰ</supplied>ς τὸ <unclear cert="high" reason="undefined">μ</unclear>έ<unclear cert="high" reason="undefined">λ</unclear>-

    <lb n="18"/>λ<unclear cert="high" reason="undefined">ον</unclear> ἀνεπηρέα<supplied cert="high" reason="lost">στος σὺν</supplied> 

    <lb n="19"/>τοῖς ἐμοῖς φυλαχθ<supplied cert="low" reason="lost">ῶ. <expan><ex cert="high">ἔτους</ex></expan> <num value="14">ιδ</num> </supplied> 

    <lb n="20"/>Αὐτοκράτοος Καίσα<supplied cert="high" reason="lost">ρος</supplied> 

    <lb n="21"/>Νέρουα Τρ<unclear cert="high" reason="undefined">α</unclear>ιανοῦ Σ<supplied cert="high" reason="lost">εβαστοῦ</supplied> 

