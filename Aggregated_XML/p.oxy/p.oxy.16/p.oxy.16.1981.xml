    <lb n="1"/><g type="stauros"/> ἐν ὀνόματι τοῦ κυρίου καὶ δεσπότ<supplied reason="lost" cert="high">ου</supplied> <hi rend="diaeresis">Ἰ</hi>ησοῦ 

    <lb n="2"/>Χριστοῦ τοῦ θεοῦ καὶ σωτῆρος ἡμῶν, 

    <lb n="3"/>βασιλείας τοῦ θειοτάτου καὶ <expan>εὐσ<ex cert="high">εβεστάτου</ex></expan> ἡμῶν 

    <lb n="4"/>δεσπότου μεγίστου εὐεργέτου <expan>Φλ<ex cert="high">αουίου</ex></expan> Ἡρακλεί<supplied reason="lost" cert="high">ου</supplied> 

    <lb n="5"/>τοῦ αἰωνίου Αὐγούστου καὶ <expan>Αὐτοκρ<ex cert="high">άτορος</ex></expan> ἔτους <num value="2">β</num> 

    <lb n="6"/>Φαῶφι <num value="28">κη</num> <expan><hi rend="diaeresis">ἰ</hi>νδ<ex cert="high">ικτίονος</ex></expan> <num value="1">α</num>. 

    <lb n="7"/><expan>Φλ<ex cert="high">αουίῳ</ex></expan> Ἀπίωνι τῷ πανευφήμῳ καὶ <expan>ὑπερφ<ex cert="high">υεστάτῳ</ex></expan> 

    <lb n="8"/>ἀπὸ ὑπάτων καὶ πατρικίῳ, <expan>γεουχ<ex cert="high">οῦντι</ex></expan> <gap reason="illegible" extent="unknown" unit="character" desc="vestiges"/> 

    <lb n="9"/><gap unit="line" extent="4" reason="illegible"/> 

    <lb n="13"/>Αὐρήλιοι Φὶβ <supplied reason="lost" cert="high">υἱ</supplied>ὸς Διοσκόρου 

    <lb n="14"/>καὶ Ἀπολλῶς υἱὸς Ἀνοῦπ <subst><add place="inline"><expan><unclear reason="undefined" cert="high">μη</unclear>τ<ex cert="high">ρὸς</ex></expan></add><del rend="corrected" status="unremarkable"><gap reason="illegible" extent="2" unit="character"/>τ</del></subst> <expan>Φ<supplied reason="lost" cert="high">ι</supplied>λοξ<ex cert="high">ένης</ex></expan> 

    <lb n="15"/>ἀπὸ κώμης Ὤφεως τοῦ <expan>Ὀξυρ<ex cert="high">υγχίτου</ex></expan> νομοῦ 

    <lb n="16"/><expan>παγαρχουμέ<ex cert="high">νης</ex></expan> παρὰ τοῦ <app type="editorial"><lem><expan>ὑμ<ex cert="high">ῶν</ex></expan> <supplied reason="lost" cert="high">ἐν</supplied>δόξου</lem><rdg><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="2" unit="character"/><gap reason="illegible" extent="3" unit="character"/><unclear reason="undefined" cert="high">ου</unclear></rdg></app> 

    <lb n="17"/>οἴκου. ὁμολο<supplied reason="lost" cert="high">γ</supplied>ῶ τῇ ὑμετέρ<supplied reason="lost" cert="high">ᾳ</supplied> <expan><hi rend="diaeresis">ὑ</hi>περ<supplied reason="lost" cert="high">φ<ex cert="high">υείᾳ</ex></supplied></expan> 

    <lb n="18"/>διὰ τῶν αὐτῇ προσηκόντων ὡς εἴ 

    <lb n="19"/>ποτε καιρῷ ἢ χρόνῳ <supplied reason="omitted" cert="high">φανῶμεν</supplied> κλέψαντες 

    <lb n="20"/>ἢ μηχα<supplied reason="lost" cert="high">ν</supplied>ικὰ σκεύη ἢ βο<hi rend="diaeresis">ί</hi>δια 

    <lb n="21"/>ἢ οἱανδήποτε κλοπὴν <expan>π<supplied reason="lost" cert="high">οι</supplied>ήσα<supplied reason="lost" cert="high">ν<ex cert="high">τες</ex></supplied></expan> 

    <lb n="22"/>ἢ <hi rend="diaeresis">ὑ</hi>ποδέξασθ<unclear reason="undefined" cert="high">αι</unclear> <choice><corr>λῃστάς</corr><sic>λιστας</sic></choice>, <expan><supplied reason="lost" cert="high">ὁ</supplied>μολογο<supplied reason="lost" cert="high">ῦμε</supplied><ex cert="high">ν</ex></expan> 

    <lb n="23"/>παρασ<supplied reason="lost" cert="high">χ</supplied>εῖν τῇ ὑμετέρᾳ <expan>ὑπερφ<ex cert="high">υείᾳ</ex></expan> 

    <lb n="24"/>ὑπὲρ <supplied reason="lost" cert="high">ἑ</supplied>κάστου ἐγχειρήματος 

    <lb n="25"/>χρυ<supplied reason="lost" cert="high">σοῦ</supplied> <expan>νομίσμ<ex cert="high">ατα</ex></expan>  εἴκοσι τέ<supplied reason="lost" cert="high">σ</supplied>σερα <num value="24"/> 

    <lb n="26"/><expan>κινδ<ex cert="high">ύνῳ</ex></expan> <supplied reason="lost" cert="high">ἡ</supplied>μῶν καὶ τῆς ἡμῶν 


    <lb n="27"/><hi rend="diaeresis">ὑ</hi>ποστάσεως. <expan>κυρ<ex cert="high">ία</ex></expan> ἡ <expan>ὁμολ<ex cert="high">ογία</ex></expan> <expan>ἁπλ<ex cert="high">ῆ</ex></expan> 

    <lb n="28"/><expan>γραφ<ex cert="high">εῖσα</ex></expan> κ<unclear reason="undefined" cert="high">αὶ</unclear> <expan>ἐπερ<ex cert="high">ωτηθεὶς</ex></expan> <expan>ὡμολ<ex cert="high">όγησα</ex></expan>. <g type="stauros"/> τὸ κοιν<supplied reason="lost" cert="high">ὸ</supplied><unclear reason="undefined" cert="high">ν</unclear> 

    <lb n="29"/>τῶν ὀνομάτων <choice><corr>στοιχεῖ</corr><sic>στοιχι</sic></choice> ἡμῖν 

    <lb n="30"/>ὡς <expan>πρ<ex cert="high">όκειται</ex></expan>. <hi rend="diaeresis">Ἰ</hi>ωάννης <expan>ἔγρ<ex cert="high">αψα</ex></expan> <expan><ex cert="high">ὑπὲρ</ex></expan> αὐτῶν 

    <lb n="31"/><expan>ἀγρ<ex cert="high">αμμάτων</ex></expan> ὄντων. 

