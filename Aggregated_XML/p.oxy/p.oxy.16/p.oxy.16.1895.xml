    <lb n="2"/><supplied reason="lost" cert="high">Αὐτοκράτο</supplied><unclear reason="undefined" cert="high">ρος</unclear> <unclear reason="undefined" cert="high">ἔτους</unclear> <num value="28"><unclear reason="undefined" cert="high">κη</unclear></num> τοῖς τὸ <num value="13">ιγ</num> μετὰ τὴν <choice><corr>ὑπατείαν</corr><sic><hi rend="diaeresis">υ</hi>πατιαν</sic></choice> <expan>Φλ[<ex cert="high">αουίου</ex></expan> Βασιλίου τοῦ <expan>λαμπρ<ex cert="high">οτάτου</ex></expan> <gap reason="illegible" extent="6" unit="character"/> <expan>ἰνδ<ex cert="high">ικτίονος</ex></expan> <gap reason="illegible" extent="1" unit="character"/>] 

    <lb n="3"/><supplied reason="lost" cert="low">Αὐρηλία</supplied><supplied reason="lost" cert="high"> Ἡρα</supplied>ῒς θυγάτηρ <hi rend="diaeresis">Ἰ</hi>ωάννου μητρὸς Σωσάννας ἀπὸ <gap reason="lost" extent="25" unit="character"/> 

    <lb n="4"/><gap reason="lost" extent="10" unit="character"/><unclear reason="undefined" cert="high">α</unclear>πώλῃ υ<hi rend="diaeresis">ἱ</hi>ῷ Μηνᾶ καὶ <subst><add place="inline">Μαξίμᾳ</add><del rend="corrected" status="unremarkable">μαξιμ<gap reason="illegible" extent="1" unit="character"/></del></subst> <subst><add place="inline">τῇ</add><del rend="corrected" status="unremarkable"><gap reason="illegible" extent="2" unit="character"/></del></subst> σῇ γαμετῇ, <unclear reason="undefined" cert="high">θ</unclear>[υγατρὶ <gap reason="illegible" extent="4" unit="character"/> ἀπὸ <gap reason="illegible" extent="5" unit="character"/> χαίρειν.] 

    <lb n="5"/><supplied reason="lost" cert="high">πρὸ </supplied><gap reason="lost" extent="4" unit="character"/><supplied reason="lost" cert="high"> ἐνι</supplied>αυτῶν πλέον ἔλατ<g type="apostrophe"/>τον ὁ ἐμὸς σύμβιος ἐτελ<supplied reason="lost" cert="high">εύτησεν, ἐγὼ δὲ ἀπελείφθην</supplied> 

    <lb n="6"/><supplied reason="lost" cert="high">ὑπὲρ τῆς ἐμῆ</supplied><unclear reason="undefined" cert="high">ς</unclear> ἐξ αὐτοῦ θυγατρὸς κάμνουσα καὶ <choice><corr>δυστυχο<supplied reason="lost" cert="high">ῦσα</supplied></corr><sic>δυστηχο<supplied reason="lost" cert="high">υσα</supplied></sic></choice><supplied reason="lost" cert="high"> εἰς τὸ χορηγεῖν αὐτῇ τὰς</supplied> 

    <lb n="7"/><supplied reason="lost" cert="high">δεούσας τρο</supplied>φάς, καὶ τανῦν μὴ εὐποροῦσα ἀποθρέψαι <supplied reason="lost" cert="high">αὐτὴν </supplied><gap reason="lost" extent="18" unit="character"/> 

    <lb n="8"/><gap reason="lost" extent="6" unit="character"/><supplied reason="lost" cert="high"> οὖσα</supplied><unclear reason="undefined" cert="high">ν</unclear> ἐτῶν ἐννέα <num value="9"/> πλέον ἔλατ<g type="apostrophe"/>τον ἠξίωσα <unclear reason="undefined" cert="high">ὑ</unclear><supplied reason="lost" cert="high">μᾶς </supplied><gap reason="lost" extent="16" unit="character"/><supplied reason="lost" cert="high"> ταύτην</supplied> 

    <lb n="9"/><supplied reason="lost" cert="high">παραλαβεῖ</supplied><unclear reason="undefined" cert="high">ν</unclear> παρ᾽ ἐμοῦ εἰς θυγατέρα, καὶ ὁμολογῶ παρ<unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">δεδωκέναι αὐτὴν ὑμῖν ἀπὸ τοῦ</supplied> 

    <lb n="10"/><supplied reason="lost" cert="high">νῦν εἰς τὸν ἑξ</supplied>ῆς ἅπαντα χρόνον εἰς θυγατέρα νομίμην, <supplied reason="lost" cert="high">ὥστε ὑμᾶς χορηγοῦντας τὰ δέον</supplied>-

    <lb n="11"/><supplied reason="lost" cert="high">τα χώραν γον</supplied><unclear reason="undefined" cert="high">έ</unclear>ων εἰς θυγατέρα ἀποπληρῶσαι εἰς <unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">ὐτήν, καὶ ἐντεῦθεν μὴ δύνασθαι</supplied> 

    <lb n="12"/><supplied reason="lost" cert="high">με ταύτην ἀ</supplied>ποσπάσαι ἀφ᾽ <hi rend="diaeresis">ὑ</hi>μῶν. εἰ δὲ τοῦτο ποιήσ<supplied reason="lost" cert="high">ω, ὁμολογῶ ὑμῖν ἀποτίσειν</supplied> 

    <lb n="13"/><supplied reason="lost" cert="high">ἅπαντα τὰ ἀ</supplied>ναλώματα λόγῳ τῶν αὐτῆ<unclear reason="undefined" cert="high">ς</unclear> <unclear reason="undefined" cert="high">ἀποτρο</unclear><supplied reason="lost" cert="high">φῶν </supplied><gap reason="lost" extent="20" unit="character"/> 

