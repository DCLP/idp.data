    <lb n="59"/><subst><add place="inline"><choice><corr>τῆ<supplied cert="high" reason="omitted">ς</supplied></corr><sic>τη</sic></choice></add><del rend="corrected" status="unremarkable">τ<gap extent="1" reason="illegible" unit="character"/></del></subst> σχολῆς τον <choice><corr>κουρσώρων</corr><sic>κουρσαρ<unclear cert="high" reason="undefined">ν</unclear></sic></choice> τῆς ἡγεμονικῆς <choice><corr>τάξεως</corr><sic>ταξεος</sic></choice> τῆς Ἀρκαδ<supplied cert="high" reason="lost">ίας υἱὸς τοῦ</supplied> 

    <lb n="60"/>τῆς <choice><corr>μακαρίας</corr><sic>μακαρησας</sic></choice> μνήμης Παύλου ὁ προγεγραμμένος πεποίημ<supplied cert="high" reason="lost">αι τὴν διαθήκην</supplied> 

    <lb n="61"/><choice><corr>νοῶν</corr><sic>νοον</sic></choice> καὶ <choice><corr>φρονῶν</corr><sic>φρονον</sic></choice> καὶ ἐπὶ <unclear cert="high" reason="undefined">γ</unclear>ῆς <choice><corr>βαδίζων</corr><sic>βαζηδον</sic></choice> ἐνστησάμενος κληρονόμο[υς τὴν <unclear cert="high" reason="undefined">ἐκκλησίαν</unclear>] 

    <lb n="62"/>τοῦ <choice><corr>ἁγίου</corr><sic>αγηου</sic></choice> Γεοργήου τοῦ καλουμένου ἄπα Σημεονήου τοῦ <choice><corr>ἡμί<supplied cert="high" reason="lost">σους</supplied></corr><sic>ημη<supplied cert="high" reason="lost">σους</supplied></sic></choice><supplied cert="high" reason="lost"> μέρους τοῦ εἰρη</supplied>-

    <lb n="63"/>μένου ὁλοκλήρου μου οἰκήματος καὶ τὴν <choice><corr>προσκολληθεῖσ<supplied cert="high" reason="lost">άν</supplied></corr><sic>προσσκολοθησ<supplied cert="high" reason="lost">αν</supplied></sic></choice><supplied cert="high" reason="lost"> μοι γυναῖκα Κυρίαν</supplied> 

    <lb n="64"/>τοῦ τετάρτου μέρους τοῦ αὐτοῦ ὁλοκλήρου μου οἴκου καὶ <choice><corr>τῶν</corr><sic>το<unclear cert="high" reason="undefined">ν</unclear></sic></choice> <supplied cert="high" reason="lost">εὑρεθησομένων</supplied> 

    <lb n="65"/>ἐν <choice><corr>τῷ</corr><sic>το</sic></choice> <choice><corr>ἐμῷ</corr><sic>εμο</sic></choice> <choice><corr>οἴκῳ</corr><sic>οικο</sic></choice> <choice><corr>ἱματίων</corr><sic><hi rend="diaeresis">ι</hi>ματηον</sic></choice> <choice><corr>γυναικείων</corr><sic>γυνεκηον</sic></choice> καὶ <choice><corr>κοσμίων</corr><sic>κοσμηον</sic></choice> καὶ πά<supplied cert="high" reason="lost">σης τῆς ἐν τῷ οἰκημα</supplied>-

    <lb n="66"/><supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ί</unclear> μου <hi rend="diaeresis">ὕ</hi>λης ἀπὸ <subst><add place="inline"><choice><corr>κεφαλαίου</corr><sic>κεφαλεου</sic></choice></add><del rend="corrected" status="unremarkable">κεφαλεο<gap extent="1" reason="illegible" unit="character"/></del></subst> <choice><corr>μέχρι</corr><sic>μεχρη</sic></choice> <subst><add place="inline"><choice><corr>ἐλαχίστου</corr><sic>ελαχησμου</sic></choice></add><del rend="corrected" status="unremarkable">ελαχησμο<gap extent="1" reason="illegible" unit="character"/></del></subst> <choice><corr>τινός</corr><sic>τηνος</sic></choice>, καὶ <unclear cert="high" reason="undefined">τ</unclear>[ὴν <unclear cert="high" reason="undefined">εἰρημένην</unclear>] 

    <lb n="67"/><choice><corr>κύρα<supplied cert="high" reason="omitted">ν</supplied></corr><sic>κυρα</sic></choice> Μάνναν τοῦ ἄλλου τετάρτου <subst><add place="inline">μέρους</add><del rend="corrected" status="unremarkable">με<gap extent="1" reason="illegible" unit="character"/>ους</del></subst> ὁλοκλήρου μο<supplied cert="high" reason="lost">υ οἰκήματος</supplied> 

    <lb n="68"/>καὶ τοῦ <subst><add place="inline">φορημένου</add><del rend="corrected" status="unremarkable"><gap extent="1" reason="illegible" unit="character"/>ορημενου</del></subst> <choice><corr>βατελλικίου</corr><sic>βατελληκηου</sic></choice> καὶ <choice><corr>τῶν</corr><sic>τον</sic></choice> <choice><corr>τριῶν</corr><sic>τριον</sic></choice> <num value="3"/> <choice><corr>κοχλιαρί<supplied cert="high" reason="lost">ων</supplied></corr><sic>κοχληαρη<supplied cert="high" reason="lost">ων</supplied></sic></choice>[ καὶ τῶν <unclear cert="high" reason="undefined">δύο</unclear> <num value="2"/> <choice><corr>καμ]-

    <lb n="69"/>ψίων</corr><sic><supplied cert="high" reason="lost">καμ</supplied>ψηον</sic></choice> καὶ τοῦ τρίτου μέρους <choice><corr>τῶν</corr><sic>τον</sic></choice> <choice><corr>ἐμῶν</corr><sic>εμον</sic></choice> <choice><corr>ἱματίων</corr><sic>ιματηον</sic></choice> <choice><corr>χειμονι<supplied cert="high" reason="lost">κῶν</supplied></corr><sic>χημονι<supplied cert="high" reason="lost">κον</supplied></sic></choice><supplied cert="high" reason="lost"> καὶ θεριακῶν,</supplied> 

    <lb n="70"/>τὸ δὲ ἄλλο <choice><corr>δίμοιρον</corr><sic>ζημερον</sic></choice> μέρος <choice><corr>τῶν</corr><sic>τον</sic></choice> <choice><corr>αὐτῶν</corr><sic>αυτον</sic></choice> μου <choice><corr>ἱματίων</corr><sic>ιματηον</sic></choice> κα<unclear cert="high" reason="undefined">τα</unclear><supplied cert="high" reason="lost">λείπω τοῖς εἰρημέ</supplied>-

    <lb n="71"/>νοις Σαμβᾷ καὶ <choice><corr>Ἰουλιανῷ</corr><sic>λουληανου</sic></choice> <choice><corr>τῶν</corr><sic>τον</sic></choice> <choice><corr>κουρσώρων</corr><sic>κουρσορον</sic></choice> τῆς ἐκ μητρὸς τ<unclear cert="high" reason="undefined">ῆ</unclear><supplied cert="high" reason="lost">ς </supplied><gap extent="12" reason="lost" unit="character"/> 

    <lb n="72"/>Ἡραείδος. ἀποκληρονόμους δὲ ποιῶ πάντας τοὺς πρὸς γέ<supplied cert="high" reason="lost">νους τυγχάνον</supplied>-

    <lb n="73"/>τας <choice><corr>καὶ</corr><sic>κε</sic></choice> <choice><corr>τυγχανούσας</corr><sic>νυνχανουσης</sic></choice>, καὶ <choice><corr>συμφωνεῖ</corr><sic>συμφονη</sic></choice> μοι πάντα τὰ προ<supplied cert="high" reason="lost">γεγραμμένα</supplied> 

    <lb n="74"/>καὶ <choice><corr>ὑπέγραψα</corr><sic>υπογραψας</sic></choice> <choice><corr>χειρὶ</corr><sic>χηρη</sic></choice> ἐμῇ. <g type="stauros"/> <handShift new="m3"/> Αὐρήλιος Μηνᾶς  δημ<unclear cert="high" reason="undefined">ό</unclear><supplied cert="high" reason="lost">σιος </supplied><gap extent="5" reason="lost" unit="character"/><supplied cert="high" reason="lost"> υἱὸς</supplied> 

    <lb n="75"/>τοῦ μακαρίου Δανιηλίου μαρτυρῶ τῇδε τῇ <choice><corr>διαθήκῃ</corr><sic>διαθυκη</sic></choice> <unclear cert="high" reason="undefined">ἀκο</unclear><supplied cert="high" reason="lost">ύσας παρὰ Ποῦσι ἀπὸ</supplied> 

    <lb n="76"/>τῆς σχολῆς <choice><corr>τῶν</corr><sic>τομ</sic></choice> <choice><corr>κουρσώρον</corr><sic>κουρσωρος</sic></choice> τοῦ <choice><corr>θεμένου</corr><sic>θυμενου</sic></choice> ὡς πρόκειται. [<handShift new="m4"/><gap extent="12" reason="illegible" unit="character"/>] 

    <lb n="77"/>τῆς ἁγίας καθολικῆς ἐκκλησίας υ<hi rend="diaeresis">ἱ</hi>ὸς τοῦ μακαρίου Γεροντίου μαρ<supplied cert="high" reason="lost">τυρῶ τῇδε τῇ διαθήκῃ</supplied> 

    <lb n="78"/>ἀκούσας παρὰ Ποῦσι κούρσ<unclear cert="high" reason="undefined">ο</unclear>ρος τοῦ διαθεμένου ὡς πρόκειται. <g type="stauros"/> <handShift new="m5"/> Φ<unclear cert="high" reason="undefined">οιβ</unclear><supplied cert="high" reason="lost">άμμων υἱὸς</supplied> 

    <lb n="79"/><subst><add place="inline">Θεοτίμου</add><del rend="corrected" status="unremarkable">θεο<gap extent="4" reason="illegible" unit="character"/>υ</del></subst> μαρτυρῶ τῇδε τῇ διαθήκῃ ἀκούσας παρὰ Ποῦσι κούρσ<supplied cert="high" reason="lost">ορος τοῦ διαθεμένου</supplied> 

    <lb n="80"/>ὡς <expan>πρόκ<ex cert="high">ειται</ex></expan>. <handShift new="m6"/> <g type="chirho"/> <expan>Φλ<ex cert="high">αούιος</ex></expan> Φοιβάμμων πριμικήριος σχολῆς πραικόνων <unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">αρτυρῶ τῇδε τῇ</supplied> 

    <lb n="81"/>διαθήκῃ ἀκούσας παρὰ Ποῦσι κούρσωρος τοῦ <choice><corr>διαθεμένου</corr><sic>διαθημενου</sic></choice> ὡς πρό[κειται. <handShift new="m7"/> <gap extent="6" reason="illegible" unit="character"/>] 

    <lb n="82"/>υ<hi rend="diaeresis">ἱ</hi>ὸς τοῦ τῆς μακαρίας μνήμης Μουσαίου μαρτυρῶ τῇδ<supplied cert="high" reason="lost">ε τῇ διαθήκῃ ἀκούσας</supplied> 

    <lb n="83"/>παρὰ Ποῦσι κούρσωρος τοῦ διαθεμένου ὡς <expan>πρόκ<ex cert="high">ειται</ex></expan>. <handShift new="m8"/> Μην<unclear cert="high" reason="undefined">ᾶ</unclear><supplied cert="high" reason="lost">ς </supplied><gap extent="7" reason="lost" unit="character"/><supplied cert="high" reason="lost"> υἱὸς</supplied> 

    <lb n="84"/>Θέωνος μαρτυρῶ τῇδε τῇ διαθήκῃ ἀκούσας παρὰ Ποῦσι κούρσωρο<unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">οῦ διαθεμένου</supplied> 

    <lb n="85"/>ὡς πρόκειται. <g type="chirho"/> <handShift new="m9"/> Πέτρος <choice><corr>διάκονος</corr><sic>διακωνου</sic></choice> τῆς ἁγίας <choice><corr>ἐκκλησίας</corr><sic>ελισιας</sic></choice> υἱὸς <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">οῦ μακαρίου</supplied> 

    <lb n="86"/><hi rend="diaeresis">Ἰ</hi>ωάννου γεναμένου <choice><corr>πρεσβυτέρου</corr><sic>πρεβητερου</sic></choice> μαρτυρῶ τῇδε τῇ <subst><add place="inline">διαθ<supplied cert="high" reason="lost">ήκῃ</supplied></add><del rend="corrected" status="unremarkable">δεαθ<supplied cert="high" reason="lost">ηκη</supplied></del></subst><supplied cert="high" reason="lost"> ἀκούσας</supplied> 

    <lb n="87"/>παρὰ Ποῦσι κούρσορος τοῦ <choice><corr>διαθεμένου</corr><sic>διαθημενου</sic></choice> ὡς <subst><add place="inline">πρόκιται</add><del rend="corrected" status="unremarkable">πρικιται</del></subst>. </ab></div></div><div n="meta.37875" org="uniform" part="N" sample="complete" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.37875" org="uniform" part="N" sample="complete" subtype="textDate" type="commentary"><p><date notAfter="600" notBefore="501" type="textDate">VI</date></p></div><div n="meta.37875" org="uniform" part="N" sample="complete" subtype="principalEdition" type="bibliography"><listBibl default="NO"><bibl default="NO" subtype="principal" type="publication"><title level="s" type="abbreviated">P.Oxy.</title><biblScope type="volume">16</biblScope><biblScope type="numbers">1901</biblScope></bibl><bibl default="NO" type="Trismegistos"><biblScope type="numbers">37875</biblScope></bibl><bibl default="NO" type="DDbDP"><series>0181</series>; <biblScope type="volume">16</biblScope>; <biblScope type="numbers">1901</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0181;16;1901" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.37875" org="uniform" part="N" sample="complete" subtype="illustrations" type="bibliography"><p>keine</p></div><div n="meta.37875" org="uniform" part="N" sample="complete" subtype="locations" type="history"><p><placeName cert="high" full="yes" type="ancientFindspot">Oxyrhynchos</placeName></p></div><div n="meta.37875" org="uniform" part="N" sample="complete" type="figure"><p><figure href="http://163.1.169.40/cgi-bin/library?e=d-000-00---0POxy--00-0-0-0prompt-10---4---Document---0-1l--1-en-50---20-about---001-001-1-0utfZz-8-00031-001-1-0utfZz-8-00&amp;a=d&amp;c=POxy&amp;cl=CL5.1.14&amp;d=HASH014186c8f57beee261287939"><figDesc>Abbildung im Internet</figDesc></figure></p></div></body></text></TEI.2>