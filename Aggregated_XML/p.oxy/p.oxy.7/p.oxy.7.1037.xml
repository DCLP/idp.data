    <lb n="6"/><supplied cert="high" reason="lost">πόλ</supplied>εως παρὰ Αὐρηλίου Ἱέρακ<supplied cert="high" reason="lost">ο</supplied>ς <supplied cert="high" reason="lost">Ἀ</supplied><unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">α</supplied>σ<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">α</supplied>τιανοῦ 

