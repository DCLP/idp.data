    <lb n="1"/><supplied reason="lost" cert="high">Ἀπο</supplied>λλώνιος Σαραπίωνος <supplied reason="lost" cert="high">τοῦ Σαρα</supplied>-

    <lb n="2"/>πίωνος μητρὸς Σινθεῦτ<supplied reason="lost" cert="high">ος ἀπ᾽ Ὀξυ</supplied>-

    <lb n="3"/>ρύγχων πόλεως Θέωνι Κ<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="6" unit="character"/> 

    <lb n="4"/>τοῦ καὶ Χαιρήμονος μητρὸς Ἀπ<unclear reason="undefined" cert="high">ί</unclear><supplied reason="lost" cert="high">ας ἀπὸ</supplied> 

    <lb n="5"/>τῆς αὐτῆς πόλεως χαίρειν. <supplied reason="lost" cert="high">ὁμολο</supplied>-

    <lb n="6"/>γῶ ἐσχηκέναι παρὰ σοῦ διὰ χ<supplied reason="lost" cert="high">ειρὸς</supplied> 

    <lb n="7"/>ἐν παραθέσει ἀργυρίου Σεβασ<supplied reason="lost" cert="high">τοῦ</supplied> 

    <lb n="8"/>νομίσματος δραχμὰς ἑξακ<supplied reason="lost" cert="high">οσίας</supplied> <num value="600"/>, 

    <lb n="9"/>ἅσπερ ὁπηνίκα ἐὰν αἱρῇ ἀπ<supplied reason="lost" cert="high">οκα</supplied>-

    <lb n="10"/>ταστήσω σοι ἄνευ πάσης ὑπε<supplied reason="lost" cert="high">ρθέ</supplied>-

    <lb n="11"/>σεως <supplied reason="lost" cert="high">καὶ</supplied> <choice><corr>εὑρησιλογίας</corr><sic>ευρησολογ<supplied reason="lost" cert="high">ια</supplied>ς</sic></choice>, εἰ δ<supplied reason="lost" cert="high">ὲ μή</supplied>, 

    <lb n="12"/>ἐκτείσ<supplied reason="lost" cert="high">ω σοι</supplied> κατὰ τὸν τῶν παρ<supplied reason="lost" cert="high">α</supplied>-

    <lb n="13"/>θηκῶ<supplied reason="lost" cert="high">ν νόμον</supplied>, γεινομένης <supplied reason="lost" cert="high">σοι</supplied> 

    <lb n="14"/>τῆς πρ<supplied reason="lost" cert="high">ά</supplied>ξε<supplied reason="lost" cert="high">ως</supplied> ἔκ τε ἐμοῦ καὶ <supplied reason="lost" cert="high">ἐκ</supplied> 

    <lb n="15"/>τῶν ὑπαρχόντων μοι πάντω<supplied reason="lost" cert="high">ν</supplied>. 

    <lb n="16"/>κύρια <choice><corr>τὰ</corr><sic>τη</sic></choice> τῆς παραθήκης γρά<supplied reason="lost" cert="high">μ</supplied>-

    <lb n="17"/>ματα δισσὰ γραφέντα ὑπ᾽ ἐμ<supplied reason="lost" cert="high">οῦ</supplied> 

    <lb n="18"/>τοῦ Ἀπολλωνίου πανταχῇ ἐπιφ<supplied reason="lost" cert="high">ε</supplied>-

    <lb n="19"/>ρόμενα καὶ παντὶ τῷ ὑπὲρ σο<supplied reason="lost" cert="high">ῦ</supplied> 

    <lb n="20"/>ἐπιφέροντι. <expan><ex cert="high">ἔτους</ex></expan> <num value="19">ιθ</num> Αὐτοκρατό<supplied reason="lost" cert="high">ρων</supplied>

    <lb n="21"/>Καισάρων Λουκίου Σεπτιμίου Σεουή<supplied reason="lost" cert="high">ρου</supplied> 

    <lb n="22"/>Περτίνακος Ἀραβ<supplied reason="lost" cert="high">ι</supplied>κοῦ Ἀδιαβην<supplied reason="lost" cert="high">ικοῦ</supplied> 

    <lb n="23"/>Παρθικοῦ Βρεταννικοῦ Μεγι´στο<supplied reason="lost" cert="high">υ</supplied> 

