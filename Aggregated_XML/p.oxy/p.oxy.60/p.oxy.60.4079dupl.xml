    <lb n="3"/>Ἄμμωνο<supplied cert="high" reason="lost">ς</supplied> Ηρωσι<unclear cert="high" reason="undefined">ω</unclear>ς ἀ<supplied cert="high" reason="lost">πὸ τῆς</supplied> 

