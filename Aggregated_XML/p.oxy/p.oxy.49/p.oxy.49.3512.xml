    <lb n="30"/><expan>ἐποικ<ex cert="high">ίου</ex></expan> <hi rend="diaeresis">Ἱ</hi>π<g type="apostrophe"/>πο<unclear reason="undefined" cert="high">ν</unclear>άμου <expan>οἴν<ex cert="high">ου</ex></expan> <expan>σηκ<ex cert="high">ωμάτων</ex></expan> <num value="63">ξγ</num> <g type="stauros"/> </ab></div></div><div n="meta.15659" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.15659" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" value="0492-02-27" notBefore="492" notAfter="492">27. Febr. 492</date></p></div><div n="meta.15659" type="commentary" subtype="mentionedDates" org="uniform" part="N" sample="complete"><head>Erwähnte Daten</head></div><div n="meta.15659" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">P.Oxy.</title><biblScope type="volume">49</biblScope><biblScope type="numbers">3512</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">15659</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0181</series>; <biblScope type="volume">49</biblScope>; <biblScope type="numbers">3512</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0181;49;3512" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.15659" type="bibliography" subtype="corrections" org="uniform" part="N" sample="complete"><head>BL-Einträge nach BL-Konkordanz</head><bibl type="BL" default="NO"><biblScope type="volume">IX</biblScope>, S. <biblScope type="pages">203</biblScope></bibl></div><div n="meta.15659" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p><bibl type="illustration" default="NO">ZPE 30, 1978, Tafel VIII a, b</bibl></p></div><div n="meta.15659" type=