    <lb n="1"/>Τιβερίωι Κλ<supplied reason="lost" cert="high">αυδίωι</supplied> Πα<supplied reason="lost" cert="low">σί</supplied>ων<supplied reason="lost" cert="low">ι</supplied>[ <expan>στρα<ex cert="high">τηγῷ</ex></expan>] 

    <lb n="2"/>παρὰ <supplied reason="lost" cert="high">Σ</supplied>αραπ[ίωνος <gap reason="illegible" extent="unknown" unit="character"/> τῷ <gap reason="illegible" extent="1" unit="character"/> ἔτει] 

    <lb n="3"/>Τιβερ<supplied reason="lost" cert="high">ίου</supplied> Κλαυ<supplied reason="lost" cert="high">δίου Καίσ</supplied>αρος Σεβασ<supplied reason="lost" cert="high">τοῦ Γερμανικοῦ</supplied> 

    <lb n="4"/>Αὐτοκράτορος <gap reason="lost" extent="11" unit="character"/><unclear reason="undefined" cert="high">υ</unclear> εωτέρ<supplied reason="lost" cert="high">ου </supplied><gap reason="lost" extent="3" unit="character"/><gap reason="illegible" extent="2" unit="character"/><unclear reason="undefined" cert="high">ω</unclear>ρα<unclear reason="undefined" cert="high">θ</unclear><gap reason="illegible" extent="4" unit="character"/> 

