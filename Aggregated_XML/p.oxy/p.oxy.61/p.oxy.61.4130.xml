    <lb n="9"/>παρὰ <subst><add place="inline">Αὐρηλίω<unclear cert="high" reason="undefined">ν</unclear></add><del rend="corrected">αυρηλιου</del></subst> <unclear cert="high" reason="undefined">Ἡρακλείδ</unclear>ου <choice><corr>Φιλίσκου</corr><sic>φιλι<unclear cert="high" reason="undefined">σκ</unclear>α</sic></choice> 

    <lb n="10"/>καὶ Οὑρῖτος Ὥ<unclear cert="high" reason="undefined">ρου</unclear> <unclear cert="high" reason="undefined">ἀμφ</unclear><supplied cert="high" reason="lost">οτέρων</supplied> <unclear cert="high" reason="undefined">κωμάρ</unclear>-

    <lb n="11"/>χων κώμης Μ<unclear cert="high" reason="undefined">ουχινά</unclear><supplied cert="high" reason="lost">ξαπ</supplied> <unclear cert="high" reason="undefined">τρί</unclear>-

    <lb n="12"/>του πάγου τοῦ αὐτοῦ ν<unclear cert="high" reason="undefined">ομ</unclear><supplied cert="high" reason="lost">οῦ. δ</supplied><unclear cert="high" reason="undefined">ίδομεν</unclear> 

    <lb n="13"/>καὶ <choice><corr>εἰσα<supplied cert="high" reason="omitted">γ</supplied>γέλ<supplied cert="high" reason="omitted">λ</supplied>ομεν</corr><sic>εισαγελ<unclear cert="high" reason="undefined">ομεν</unclear></sic></choice> <unclear cert="high" reason="undefined">τῷ</unclear> <hi rend="diaeresis">ἰ</hi>δίῳ ἡ<unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ῶν</supplied> 

    <lb n="14"/><handShift new="m1"/> κινδύνῳ εἰς ἐκ<unclear cert="high" reason="undefined">βολίαν</unclear> τῶν 

    <lb n="15"/>διαφερόν<unclear cert="high" reason="undefined">των</unclear> τῇ ἡμετ<unclear cert="high" reason="undefined">έ</unclear>ρᾳ κώ-

    <lb n="16"/>μῃ δημοσίων χωμά<unclear cert="high" reason="undefined">τω</unclear>ν <unclear cert="high" reason="undefined">ἐπὶ</unclear> <unclear cert="high" reason="undefined">το</unclear><supplied cert="high" reason="lost">ῦ</supplied> 

    <lb n="17"/>ἐνεστῶτος <num value="34">λδ</num> <num value="3">γ</num> ἔτους τοὺς 

    <lb n="18"/>ἑξῆς ἐνγεγραμ<unclear cert="high" reason="undefined">μ</unclear>ένου<unclear cert="high" reason="undefined">ς</unclear> 

    <lb n="19"/>ὄντας εὐπόρ<unclear cert="high" reason="undefined">ου</unclear><supplied cert="high" reason="lost">ς</supplied> 

    <lb n="20"/>καὶ <choice><corr>ἐπιτηδείους</corr><sic>επιτηδι<unclear cert="high" reason="undefined">ο</unclear>υς</sic></choice> πρὸς τ<supplied cert="high" reason="lost">ὴν</supplied> 

    <lb n="21"/>χρείαν. <unclear cert="high" reason="undefined">εἰσὶ</unclear> <unclear cert="high" reason="undefined">δέ</unclear>· Αὐρήλ<unclear cert="high" reason="undefined">ι</unclear><supplied cert="high" reason="lost">οι</supplied> 

    <lb n="22"/>ἐκβολε<unclear cert="high" reason="undefined">ῖ</unclear>ς 

    <lb n="23"/>Θώνιος Ἀμόϊτος 

    <lb n="24"/><hi rend="diaeresis">Ἰ</hi>σίδωρος Φιλοξ<unclear cert="high" reason="undefined">ένου</unclear> 

    <lb n="25"/>ἀμφότεροι ἀπὸ τῆς <expan>α<ex cert="high">ὐτῆς</ex></expan> <expan>κώ<ex cert="high">μης</ex></expan>.
<milestone rend="paragraphos" unit="undefined"/>
    <lb n="26"/><handShift new="m2"/> Αὐρήλιοι Ἡρακλείδης 

    <lb n="27"/>Φιλίσκ<unclear cert="high" reason="undefined">ου</unclear> καὶ Ὡρῖς 

    <lb n="28"/>Ὥρου ἐπιδεδώκα<unclear cert="high" reason="undefined">μεν</unclear>. <expan>Αὐρ<ex cert="high">ήλιος</ex></expan> 

