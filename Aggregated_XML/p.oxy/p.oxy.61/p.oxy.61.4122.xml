    <lb n="25"/><handShift new="m3"/> Αὐρήλοι<unclear cert="high" reason="undefined">ς</unclear> Ὡρίων ὑπ<unclear cert="high" reason="undefined">ηρ</unclear><supplied cert="high" reason="lost">έτης</supplied> λογιστοῦ 

    <lb n="26"/><space dim="horizontal" extent="unknown" unit="character"/> <unclear cert="high" reason="undefined">ἐ</unclear>πήνε<hi rend="diaeresis">γ</hi>κα. 

    <lb n="27"/><expan><ex cert="high">ἔτους</ex></expan> <num value="21">κα</num> καὶ <num value="13">ιγ</num> <supplied cert="high" reason="lost">Π</supplied>αῦνι <num value="28">κη</num>. </ab></div><div n="meta.21623" part="N" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.21623" part="N" subtype="textDate" type="commentary"><p><date notAfter="305" notBefore="305" type="textDate" value="0305-06-22">22. Juni 305</date></p></div><div n="meta.21623" part="N" subtype="principalEdition" type="bibliography"><listBibl><bibl subtype="principal" type="publication"><title level="s" type="abbreviated">P.Oxy.</title><biblScope type="volume">61</biblScope><biblScope type="numbers">4122 </biblScope></bibl><bibl type="Trismegistos"><biblScope type="numbers">21623</biblScope></bibl><bibl type="DDbDP"><series>0181</series>; <biblScope type="volume">61</biblScope>; <biblScope type="numbers">4122 </biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0181;61;4122 " targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.21623" part="N" subtype="illustrations" type="bibliography"><p>keine</p></div><div n="meta.21623" part="N" subtype="translations" type="bibliography"><head>Übersetzungen</head><listBibl lang="fr"><head lang="de">Französisch:</head><bibl type="translations">Schubert, Vivre en Égypte (2000), S. 103-104, Nr. 23</bibl></listBibl></div><div n="meta.21623" part="N" subtype="locations" type="history"><p><placeName cert="high" type="ancientFindspot">Oxyrhynchos</placeName></p></div><div n="meta.21623" part="N" type="figure"><p><figure href="http://163.1.169.40/cgi-bin/library?e=d-000-00---0POxy--00-0-0-0prompt-10---4---Document---0-1l--1-en-50---20-about---001-001-1-0utfZz-8-00031-001-1-0utfZz-8-00&amp;a=d&amp;c=POxy&amp;cl=CL5.1.14&amp;d=HASH0a69238fc563fd562b2b0b"><figDesc>Abbildung im Internet</figDesc></figure></p></div></body></text></TEI.2>