    <lb n="7"/>ἐ<unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ι</supplied><unclear cert="high" reason="undefined">δ</unclear><supplied cert="high" reason="lost">έ</supplied><unclear cert="high" reason="undefined">χ</unclear><supplied cert="high" reason="lost">ομα</supplied><unclear cert="high" reason="undefined">ι</unclear> <unclear cert="high" reason="undefined">μ</unclear>ισθώσασ<unclear cert="high" reason="undefined">θα</unclear>ι <supplied cert="high" reason="lost">πρὸς μό</supplied>νον τὸ ἐνεσ<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ὸς</supplied> 

    <lb n="8"/><num value="6">ϛ</num> <expan><ex cert="high">ἔτος</ex></expan> καὶ <num value="5">ε</num> <expan><ex cert="high">ἔτος</ex></expan> ἐκ τῶν ὑπαρ<unclear cert="high" reason="undefined">χ</unclear>όντων σοι ἐν περι<unclear cert="high" reason="undefined">χ</unclear><supplied cert="high" reason="lost">ώ</supplied>-

    <lb n="9"/>μ<unclear cert="high" reason="undefined">α</unclear>τι Πέκτυ <unclear cert="high" reason="undefined">τ</unclear>οῦδε τοῦ νομοῦ ἐν κοίτῃ καλουμένῃ 

