    <lb n="1"/>Αὐρηλίῳ Ἀντιφάτῃ τῷ διασημο<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">άτῳ  </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/><space dim="horizontal" extent="unknown" unit="character"/>   <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>παρὰ κληρονόμων Αὐρηλίας <choice><corr>Στρατονίκη<unclear cert="high" reason="undefined">ς</unclear></corr><sic>στρατονεικης</sic></choice> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>νοέως γυμνασιάρχου βουλευτοῦ τῆς λαμπρᾶς καὶ λα<supplied cert="high" reason="lost">μπροτάτης Ὀξυρυγχιτῶν πόλεως</supplied> 

    <lb n="4"/>Λουκίλλης τῆς καὶ <choice><corr>Θεωνίδος</corr><sic>θεωνειδος</sic></choice> θυγατρὸς Θέωνο<supplied cert="high" reason="lost">ς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>χιτῶν πόλεως. ἐπειδὴ τῇ ἐνεστώσῃ ἡμ<supplied cert="high" reason="lost">έρᾳ </supplied><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="6"/>ἐπιτρόπου ἐπήνεγ<g type="apostrophe"/>κεν ἐπιστολὴν ἐξ ὀνόμα<supplied cert="high" reason="lost">τος </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/>εἰς Μεχεὶρ μῆνα τοῦ ἐνεστῶτος <num value="7">ζ</num> <expan><ex cert="high">ἔτους</ex></expan> καὶ <num value="6">ϛ</num> <expan><ex cert="high">ἔτους</ex></expan> <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/>στουμένων <hi rend="diaeresis">ὑ</hi>φ᾽ ἡμῶν, τῆς δὲ ἐπισ<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">λῆ</unclear><supplied cert="high" reason="lost">ς</supplied> <gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/>δέπω τοῦ πράγματος παρὰ σοὶ γνωσ<unclear cert="high" reason="undefined">θέν</unclear><supplied cert="high" reason="lost">τος </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/>διὰ τοῦ <hi rend="diaeresis">υ</hi><hi rend="diaeresis">ἱ</hi>οῦ Ἡρώδου καί τινα διελάλησας δι<gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/>ἡμᾶς ἀκολούθως τοῖς νόμοις ἐκ<g type="apostrophe"/>καλουμε<unclear cert="high" reason="undefined">ν</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="12"/>Ὁνωρᾶτον, ᾧ παραθησόμεθα τὰ προσόντ<supplied cert="high" reason="lost">α </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/><supplied cert="high" reason="lost">ἀξι</supplied><unclear cert="high" reason="undefined">οῦ</unclear>ντες, ὡς ἐκελεύσθη, τὴν συνήθη υ<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

