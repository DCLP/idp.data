    <lb n="17"/>τῶ<unclear cert="high" reason="undefined">ν</unclear> περιγινομένων <choice><corr>ἰχθύων</corr><sic>ειχθυων</sic></choice>. βεβ<supplied cert="high" reason="lost">αιουμένης</supplied> 

    <lb n="18"/>δ<unclear cert="high" reason="undefined">ὲ</unclear> <unclear cert="high" reason="undefined">ἡ</unclear><supplied cert="high" reason="lost">μῖ</supplied><unclear cert="high" reason="undefined">ν</unclear> <unclear cert="high" reason="undefined">τῆ</unclear><supplied cert="high" reason="lost">ς</supplied> ἐπιδοχῆς π<supplied cert="high" reason="lost">οι</supplied>ησόμεθα τὴν ἄγ<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">αν ἀκωλύ</supplied>-

    <lb n="19"/>τω<supplied cert="high" reason="lost">ς καὶ ἀπο</supplied><unclear cert="high" reason="undefined">δώσ</unclear><supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">μεν</unclear> <unclear cert="high" reason="undefined">τὸν</unclear> <supplied cert="high" reason="lost">φ</supplied><unclear cert="high" reason="undefined">ό</unclear>ρον με<supplied cert="high" reason="lost">τ</supplied>ὰ τὴν <supplied cert="high" reason="lost">ἄγραν.</supplied> 

    <lb n="20"/>κ<unclear cert="high" reason="undefined">υρία</unclear> <unclear cert="high" reason="undefined">ἡ</unclear> <supplied cert="high" reason="lost">ἐπιδοχὴ καὶ ἐ</supplied><unclear cert="high" reason="undefined">περ</unclear>ω<unclear cert="high" reason="undefined">τη</unclear><supplied cert="high" reason="lost">θ</supplied><unclear cert="high" reason="undefined">ε</unclear>ὶς ὡμ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">λόγησα.</supplied> 

    <lb n="21"/><g type="large-parens"/> ἀλ<unclear cert="high" reason="undefined">λ</unclear>ὰ καὶ <gap extent="8" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ε</unclear> ἡμᾶς ἐν <unclear cert="high" reason="undefined">τ</unclear>αῖς θύραις <supplied cert="high" reason="lost">Ματρίνου <expan><ex cert="low">καὶ</ex></expan></supplied> 

    <lb n="22"/>ἐν τοῖς ἀπηλι<supplied cert="high" reason="lost">ωτι</supplied><unclear cert="high" reason="undefined">κ</unclear>οῖς μέρεσι <choice><corr>περιχώ<supplied cert="high" reason="lost">ματος</supplied></corr><sic>περειχ<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">ματος</supplied></sic></choice> 

    <lb n="23"/>Σενεμσεὲ καὶ <supplied cert="high" reason="lost">λιβι</supplied><unclear cert="high" reason="undefined">κ</unclear>οῖς μέρεσι τῶν αὐτῶν <unclear cert="high" reason="undefined">θ</unclear><supplied cert="high" reason="lost">υρῶν</supplied> 

    <lb n="24"/>Ματρίνου. ἐπὶ τῶν δεσποτῶν ἡμῶν Γα<supplied cert="high" reason="lost">λερίου Οὐαλερίου</supplied> 

    <lb n="25"/>Μαξιμιανοῦ καὶ Οὐαλερίου Λικιννιανοῦ Λι<supplied cert="high" reason="lost">κιννίου</supplied> 

    <lb n="26"/>τῶν ἀνικήτων Σεβαστῶν καὶ τῶν δ<supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">σπ</unclear><supplied cert="high" reason="lost">οτῶν ἡμῶν</supplied> 

    <lb n="27"/>Μαξιμίνου καὶ Κωνσταντίνο<supplied cert="high" reason="lost">υ</supplied> τῶν ἐπιφα<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">εστάτων</supplied> 

