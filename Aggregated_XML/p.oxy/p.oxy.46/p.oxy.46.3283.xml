    <lb n="1"/><num value="120">ρκ</num> 

    <lb n="2"/>παρ<supplied reason="lost" cert="high">ὰ</supplied> Θέωνος Θέων<supplied reason="lost" cert="high">ο</supplied>ς τοῦ Θέωνος μητρὸς 

    <lb n="3"/><supplied reason="lost" cert="high">Διογ</supplied>ενοῦτος Διονυσίου ἀπ᾽ Ὀξυρύγχων π<supplied reason="lost" cert="high">ό</supplied>λ<supplied reason="lost" cert="high">ε</supplied>ως. 

