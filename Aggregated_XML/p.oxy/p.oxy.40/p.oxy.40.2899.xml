    <lb n="1"/><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>Αὐρη<unclear cert="high" reason="undefined">λί</unclear><supplied cert="low" reason="lost">ῳ Ἀχιλλεῖ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>νεωκ<supplied cert="high" reason="lost">όρῳ τοῦ μεγάλου</supplied> Σα-

    <lb n="4"/>ράπιδ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ς τῷ <expan>κρ<ex cert="high">ατίστῳ</ex></expan> ὑπομνημ</supplied><unclear cert="high" reason="undefined">ατογρά</unclear>-

    <lb n="5"/>φῳ <unclear cert="high" reason="undefined">καὶ</unclear> <supplied cert="high" reason="lost">τοῖς αἱρεθεῖ</supplied><unclear cert="high" reason="undefined">σι</unclear> ὑ<unclear cert="high" reason="undefined">πὸ</unclear> τῆς 

    <lb n="6"/>κρατίστη<supplied cert="high" reason="lost">ς βο</supplied><unclear cert="high" reason="undefined">υλῆς</unclear> <unclear cert="high" reason="undefined">δια</unclear>κριταῖς 

    <lb n="7"/>παρὰ Αὐρη<unclear cert="high" reason="undefined">λ</unclear><supplied cert="high" reason="lost">ί</supplied>ου Λογ<g type="apostrophe"/>γεί<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">ο</supplied>υ 

    <lb n="8"/>Θέωνος τοῦ Θέω<unclear cert="high" reason="undefined">νος</unclear> <unclear cert="high" reason="undefined">μη</unclear>-

    <lb n="9"/>τρὸς Διογενίδος ἀπ᾽ Ὀξυρ<supplied cert="high" reason="lost">ύ</supplied>γ<g type="apostrophe"/>-

    <lb n="10"/>χων πόλεως τῆς λαμπρᾶς. ἐ-

    <lb n="11"/>πικριθε<unclear cert="high" reason="undefined">ὶ</unclear>ς ἐπ᾽ ἀμφόδ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">υ Κρη</supplied>-

    <lb n="12"/>τικοῦ ἐπὶ τοῦ <num value="4">δ</num> <expan><ex cert="high">ἔτους</ex></expan> <gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="lost" unit="character"/> 

    <lb n="13"/>Ἀλεξάνδρου βασιλείας ἀ<unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">όδη</supplied>-

    <lb n="14"/>μος τυνχάνων καὶ παρε<supplied cert="high" reason="lost">ίθην</supplied> 

    <lb n="15"/>κατ᾽ ἄγνοιαν <hi rend="diaeresis">ὑ</hi><unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ὸ</supplied> τοῦ <unclear cert="high" reason="undefined">φ</unclear><supplied cert="high" reason="lost">υλάρ</supplied>-

    <lb n="16"/>χου. <choice><corr>ἐπεὶ</corr><sic>επι</sic></choice> οὖν ἔδοξεν ἐν <supplied cert="high" reason="lost">τῇ κρα</supplied>-

    <lb n="17"/>τίστῃ βουλ<supplied cert="high" reason="lost">ῇ ὥστε τοὺς ἔχον</supplied>-

    <lb n="18"/>τάς τι δίκαι<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ν τοῦτο ἐντὸς</supplied> 

    <lb n="19"/>τῆς τριμή<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">ου παραθέσθαι</supplied> 

    <lb n="20"/>πρὸς τὸ δια<unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">ριθέντος τοῦ πρά</supplied>-

    <lb n="21"/>γματος ἐ<unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ανορθώσεως τυ</supplied>-

    <lb n="22"/>χεῖν, ἐπιδί<unclear cert="high" reason="undefined">δ</unclear><supplied cert="high" reason="lost">ωμι ταύτην</supplied> 

    <lb n="23"/>τὴν ἀξίωσι<supplied cert="high" reason="lost">ν ὅπως κελεύση</supplied>-

    <lb n="24"/>τε τῷ φυλάρ<unclear cert="high" reason="undefined">χ</unclear><supplied cert="high" reason="lost">ῳ τοῦ δηλουμέ</supplied>-

    <lb n="25"/>νου ἀμφόδ<supplied cert="high" reason="lost">ου ἐφ᾽ οὗ καὶ ἐλειτούρ</supplied>-

    <lb n="26"/>γησα καὶ τὸ <unclear cert="high" reason="undefined">ἡ</unclear><supplied cert="high" reason="lost">μέτερον ὄνομα</supplied> 


    <lb n="27"/>ἐν τοῖς ἀν<supplied cert="high" reason="lost">αγορευομένοις</supplied> 

