    <lb n="16"/><choice><corr>ἀρτάβας</corr><sic><supplied reason="lost" cert="high">αρτ</supplied>αβαι</sic></choice> δύο, <expan><ex cert="high">γίνονται</ex></expan> <expan><ex cert="high">πυροῦ ἀρτάβαι</ex></expan> <num value="2">β</num>.

