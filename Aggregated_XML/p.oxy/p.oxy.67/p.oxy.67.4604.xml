    <lb n="2"/>ὑπατείας Φλα<supplied reason="lost" cert="high">ου</supplied><unclear reason="undefined" cert="high">ί</unclear><supplied reason="lost" cert="high">ω</supplied>ν Ταύρου καὶ Φλ<unclear reason="undefined" cert="high">ω</unclear><supplied reason="lost" cert="high">ρεντίου τῶν λαμπροτάτων, </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="3"/>ἐ<unclear reason="undefined" cert="high">ν</unclear> <unclear reason="undefined" cert="high">τῇ</unclear> <unclear reason="undefined" cert="high">λα</unclear><supplied reason="lost" cert="high">μ</supplied><unclear reason="undefined" cert="high">προτάτῃ</unclear> <unclear reason="undefined" cert="high">Πηλου</unclear>[σιωτῶν <expan><ex cert="high">μητρο</ex></expan>πόλει. Αὐρήλιοι <gap reason="illegible" extent="unknown" unit="character"/> ]

    <lb n="4"/>καὶ Ποτάμμω<unclear reason="undefined" cert="high">ν</unclear> <unclear reason="undefined" cert="high">Ε</unclear><supplied reason="lost" cert="high">ὐτ</supplied>υχίο<supplied reason="lost" cert="high">υ</supplied> <expan><unclear reason="undefined" cert="high">ἄρ</unclear>ξ[<ex cert="low">αντες</ex></expan>] <unclear reason="undefined" cert="high">καὶ</unclear> Μαρρ[ῆς <note lang="en" anchored="yes" place="not specified">?</note> <gap reason="illegible" extent="unknown" unit="character"/>, χρυσῶναι ἐπαρχίας Αὐγουσταμνικῆς <note lang="en" anchored="yes" place="not specified">?</note>,]

    <lb n="5"/>Αὐρηλίῳ Ἡρακλ<supplied reason="lost" cert="high">ε</supplied>ίῳ Ἡρακλείου προέ<unclear reason="undefined" cert="high">δρ</unclear><supplied reason="lost" cert="high">ῳ τῆς Ὀξυρυγχιτῶν πόλεως χαίρειν. ἀκολούθως τοῖς κελευσθεῖσιν ὑπὸ τῆς</supplied>

    <lb n="6"/>ἀρετῆς τοῦ κυρ<supplied reason="lost" cert="high">ί</supplied>ου <choice><corr>ἡμῶν</corr><sic>μου</sic></choice> λαμπροτ<unclear reason="undefined" cert="high">άτο</unclear>[υ ἡγεμόνος <note lang="en" anchored="yes" place="not specified">?</note> <gap reason="illegible" extent="unknown" unit="character"/> ὑπεδεξάμεθα]

    <lb n="7"/>παρὰ σοῦ τὰς <choice><corr>ἀποσταλείσας</corr><sic><unclear reason="undefined" cert="high">αποσταλι</unclear>σας</sic></choice> ὑπὸ <unclear reason="undefined" cert="high">τ</unclear>[ <gap reason="illegible" extent="unknown" unit="character"/> χρυσοῦ λίτρας τέσσαρας<note lang="en" anchored="yes" place="not specified">?</note> <gap reason="illegible" extent="unknown" unit="character"/> σταθμῷ καὶ ἐν νομισματίοις <note lang="en" anchored="yes" place="not specified">?</note>]

    <lb n="8"/>νομίσματα <unclear reason="undefined" cert="high">ἑ</unclear>βδομήκοντα, <expan><ex cert="high">γίνονται</ex></expan> χρυ<add place="supralinear"><unclear reason="undefined" cert="high">σο</unclear><supplied reason="lost" cert="high">ῦ</supplied></add> <expan><unclear reason="undefined" cert="high">λί</unclear><ex cert="high">τραι</ex></expan> <num value="4"><unclear reason="undefined" cert="high">δ</unclear></num> <gap reason="illegible" extent="4" unit="character"/> [ἀπὸ λόγου <note lang="en" anchored="yes" place="not specified">?</note> <gap reason="illegible" extent="unknown" unit="character"/> χρυσοῦ]

    <lb n="9"/><expan>λί<ex cert="high">τρας</ex></expan> <num value="2">β</num> <expan><ex cert="high">οὐγκίας</ex></expan> <num value="11">ια</num> <expan>γρ<ex cert="high">άμματα</ex></expan> <num value="12">ιβ</num> <num value="1/4">δ</num>, τιρώνων τῆς <num value="5"><unclear reason="undefined" cert="high">ε</unclear></num> <unclear reason="undefined" cert="high">ἰνδικτ</unclear>[ίονος <gap reason="illegible" extent="35" unit="character"/> <expan><ex cert="high">γίνονται</ex></expan>]

    <lb n="10"/>αἱ <choice><corr>προκείμεναι</corr><sic>προκιμεν<unclear reason="undefined" cert="high">αι</unclear></sic></choice> <unclear reason="undefined" cert="high">π</unclear>λήρης μόνας ἃς καὶ <unclear reason="undefined" cert="high">λημ</unclear><supplied reason="lost" cert="high">ματίσομεν διὰ τῶν μηνιαίων ἡμῶν λόγων καὶ ἐξεδόμεθά</supplied>

    <lb n="11"/>σοι τὴν ἀποχὴν ἐσχηκότες παρὰ σο̣̂ <unclear reason="undefined" cert="high">τὸ</unclear> <supplied reason="lost" cert="high">ἀντάποχον </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="12"/>ἀποχὴν ἐπιφερομένην πανταχ<unclear reason="undefined" cert="high">ο</unclear>[ῦ <gap reason="illegible" extent="unknown" unit="character"/> Αὐρήλιος <gap reason="illegible" extent="unknown" unit="character"/> χρυσώνης ὑπεδεξάμην]

    <lb n="13"/><supplied reason="lost" cert="high">τ</supplied>ὰς <choice><corr>προκειμένας</corr><sic>προκιμενας</sic></choice> τοῦ χρυσοῦ λίτρας <unclear reason="undefined" cert="high">τέ</unclear>[σσαρας σταθμῷ καὶ ἐν νομισματίοις <note lang="en" anchored="yes" place="not specified">?</note> νομίσματα]

    <lb n="14"/><unclear reason="undefined" cert="high">ἑ</unclear>βδομήκοντα ὡς <choice><corr>πρόκειται</corr><sic>προκιται</sic></choice>. Αὐρήλ<unclear reason="undefined" cert="high">ιο</unclear>[ς Ποτάμμων <note lang="en" anchored="yes" place="not specified">?</note> χρυσώνης <gap reason="illegible" extent="unknown" unit="character"/> συνυπεδεξάμην]

    <lb n="15"/>τοῦ χρυσοῦ λιτρῶν τεσσάρων <unclear reason="undefined" cert="high">οὐγκι</unclear>[ῶν <num value="11">ια</num> γραμμάτων <num value="16">ιϛ</num> <note lang="en" anchored="yes" place="not specified">?</note>. <gap reason="illegible" extent="unknown" unit="character"/> Αὐρήλιος Μαρρῆς <note lang="en" anchored="yes" place="not specified">?</note>]

