    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/> <num value="10"><unclear reason="undefined" cert="high">ι</unclear></num>  <num value="56">νϛ</num>, <expan><ex cert="high">γίνονται</ex></expan> <num value="364">τξδ</num> 

    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/> <num value="1636">Αχλϛ</num> <choice><corr>Ἀπολλώνιος</corr><sic>απολλωνις</sic></choice> <num value="364">τξδ</num> 

    <lb n="6"/><gap reason="lost" extent="unknown" unit="character"/> <expan>λήμ<ex cert="high">ματος</ex></expan> <num value="364">τξδ</num>, <expan><ex cert="high">γίνονται</ex></expan> <choice><corr><expan>ἀναλ<ex cert="high">ώματος</ex></expan></corr><sic>ανηλ</sic></choice> <num value="364">τξδ</num>, <expan>πλ<ex cert="high">ῆρες</ex></expan>. 

