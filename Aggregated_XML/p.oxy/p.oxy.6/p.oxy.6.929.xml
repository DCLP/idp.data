    <lb n="26"/><handShift new="m2"/> <expan>π<ex cert="high">αρὰ</ex></expan> Νεικάνορος. </ab></div></div><div n="meta.28340" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.28340" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" notBefore="101" notAfter="125">frühes II</date></p></div><div n="meta.28340" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Descr.</p></div><div n="meta.28340" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">P.Oxy.</title><biblScope type="volume">6</biblScope><biblScope type="numbers">929</biblScope><biblScope type="side">R</biblScope><biblScope type="parts">Einleitung</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">28340</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0181</series>; <biblScope type="volume">6</biblScope>; <biblScope type="numbers">929</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0181;6;929" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.28340" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p>keine</p></div><div n="meta.28340" type="history" subtype="locations" org="uniform" part="N" sample="complete"><p><placeName type="ancientFindspot" cert="high" full="yes">Oxyrhynchos</placeName></p></div><div n="meta.28339" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.28339" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" notBefore="176" notAfter="300">spätes II - III</date></p></div><div n="meta.28339" type="comment