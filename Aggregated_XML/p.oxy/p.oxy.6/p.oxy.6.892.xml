    <lb n="6"/>Αὐρηλίου Ν<unclear cert="high" reason="undefined">επ</unclear>ωτιανοῦ ᾑρῆσθαί <unclear cert="high" reason="undefined">σε</unclear> <supplied cert="high" reason="lost">εἰς </supplied><gap extent="6" reason="lost" unit="character"/> 

    <lb n="7"/>τῶν ἐνχρ<unclear cert="high" reason="undefined">ῃζό</unclear>ντων ξύλων εἰς <gap extent="3" reason="illegible" unit="character"/>ο<unclear cert="high" reason="undefined">ς</unclear><gap extent="7" reason="lost" unit="character"/> 

