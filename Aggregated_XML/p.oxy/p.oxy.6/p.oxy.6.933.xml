    <lb n="6"/>πάζομ<supplied cert="high" reason="lost">αι</supplied> εὐχόμενος 

    <lb n="7"/>πᾶσι τ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ῖς θεο</supplied>ῖς πε<supplied cert="high" reason="lost">ρὶ τ</supplied><unclear cert="high" reason="undefined">ῆ</unclear><supplied cert="high" reason="lost">ς</supplied> 

    <lb n="8"/><supplied cert="high" reason="lost">σ</supplied><unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">τηρίας σου</supplied> <unclear cert="high" reason="undefined">κ</unclear><gap extent="4" reason="lost" unit="character"/> 

