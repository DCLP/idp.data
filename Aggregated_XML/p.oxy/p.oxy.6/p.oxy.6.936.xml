    <lb n="52"/><supplied reason="lost" cert="high">χρόνοις. </supplied><gap reason="lost" extent="unknown" unit="character"/> </ab></div></div><div n="meta.31325" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.31325" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" notBefore="201" notAfter="300">III</date></p></div><div n="meta.31325" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">P.Oxy.</title><biblScope type="volume">6</biblScope><biblScope type="numbers">936</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">31325</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0181</series>; <biblScope type="volume">6</biblScope>; <biblScope type="numbers">936</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0181;6;936" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.31325" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p>keine</p></div><div n="meta.31325" type="history" subtype="locations" org="uniform" part="N" sample="complete"><p><placeName type="ancientFindspot" cert="high" full="yes">Oxyrhynchos</placeName></p></div></body></text></TEI.2>