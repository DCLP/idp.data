    <lb n="1"/><hi rend="diaeresis">ὑ</hi>πατείας Ο<supplied reason="lost" cert="high">ὐιβίου</supplied> Νεπωτιανοῦ καὶ Τετ<g type="apostrophe"/><unclear reason="undefined" cert="high">τ</unclear>ίου Φακούνδου 

    <lb n="2"/>τῶν λαμπροτάτων Π<supplied reason="lost" cert="high">α</supplied>χὼν <num value="6">ϛ</num>. 

    <lb n="3"/>Φλαου<hi rend="diaeresis">ί</hi><supplied reason="lost" cert="high">ῳ</supplied> <hi rend="diaeresis">Ἰ</hi>ουλιανῷ διοικοῦντι <app type="BL"><lem resp="9.181; cf. 3.133"><unclear reason="undefined" cert="high">συ</unclear>νδικίαν</lem><rdg><unclear reason="undefined" cert="high">ἐκ</unclear>δικίαν</rdg></app> Ὀξυρυγχίτου 

    <lb n="4"/>παρὰ Αὐρηλίας Ἀλλοῦτος Θωνίου ἀπὸ κώμης Τααμπέμο<supplied reason="lost" cert="high">υ</supplied> 

    <lb n="5"/><num value="5">ε</num> πάγου. <choice><corr>ἑσπεριναῖς</corr><sic>εσπερινες</sic></choice> <choice><corr>ὥραις</corr><sic>ωρες</sic></choice> τῇ χθὲς ἡμέρᾳ ἡμέτεροι <choice><corr>χοῖροι</corr><sic>χυροι</sic></choice> 

    <lb n="6"/>δύο <num value="2"/> τὴν ὁρμὴν ποιούμενοι ἐπὶ ἡμέτερον ἔδαφος 

    <lb n="7"/><choice><corr>γενόμενοι</corr><sic>γενωμενοι</sic></choice> ἐν ὑδραγωγίῳ μηχανῆς τῶν ἡμετέρων 

    <lb n="8"/><app type="BL"><lem resp="9.181"><choice><corr>πεδίων</corr><sic>πεδων</sic></choice></lem><rdg>παίδων</rdg></app> καὶ <unclear reason="undefined" cert="high">Π</unclear>αβάνου τινὸς ἀπὸ τῆς αὐτῆς <app type="BL"><lem resp="9.181">κώμης</lem><rdg>κώμης <supplied reason="lost" cert="high">καὶ</supplied></rdg></app>. 

    <lb n="9"/>ὁ <choice><corr>προειρημ<supplied reason="lost" cert="high">ένος</supplied></corr><sic>προ<hi rend="diaeresis">ι</hi>ρημ<supplied reason="lost" cert="high">ενος</supplied></sic></choice><supplied reason="lost" cert="high"> Π</supplied><unclear reason="undefined" cert="high">α</unclear>βᾶνος παρατυχὼν ἔχων μετὰ 

    <lb n="10"/><choice><corr>χεῖρας</corr><sic>χιρας</sic></choice> <app type="BL"><lem resp="9.181">ξοίδιον ἀνελεῖν</lem><rdg><choice><corr>ξύλιον</corr><sic>ξοι<unclear reason="undefined" cert="high">λ</unclear>ιο<unclear reason="undefined" cert="high">ν</unclear></sic></choice> <gap reason="illegible" extent="4" unit="character"/>ιν</rdg></app> τοὺς <choice><corr>χοίρους</corr><sic>χυρους</sic></choice> βουλόμεν<supplied reason="lost" cert="high">ο</supplied>ς 

    <lb n="11"/>καὶ τοῦ<unclear reason="undefined" cert="high">τ</unclear>ο <unclear reason="undefined" cert="high">μ</unclear><gap reason="illegible" extent="3" unit="character"/><gap reason="lost" extent="5" unit="character"/><unclear reason="undefined" cert="high">ω</unclear>ς μηδαμῶς ἀδικηθεὶ<supplied reason="lost" cert="high">ς</supplied> 

    <lb n="12"/>ὑπὸ τῶν <choice><corr>χοίρ<supplied reason="lost" cert="high">ων</supplied></corr><sic>χ<unclear reason="undefined" cert="high">υρ</unclear><supplied reason="lost" cert="high">ων</supplied></sic></choice> <app type="BL"><lem resp="9.181">ἀποσχόμενος</lem><rdg><supplied reason="lost" cert="high">ἀλ</supplied><unclear reason="undefined" cert="high">λὰ</unclear> ἐχόμενο?</rdg></app> τῆς πρὸς τοὺς 

    <lb n="13"/><choice><corr>χοίρους</corr><sic>χυρους</sic></choice> ε<gap reason="illegible" extent="3" unit="character"/><gap reason="lost" extent="2" unit="character"/><supplied reason="lost" cert="high"> ὅτι</supplied> <unclear reason="undefined" cert="high">ἐμοὶ</unclear> ἐπελήλυθαν βουλόμενος 

    <lb n="14"/><app type="BL"><lem resp="9.182"><seg cert="low" part="N"><unclear reason="undefined" cert="low">μο</unclear>ι κατα</seg></lem><rdg><unclear reason="undefined" cert="high">ο</unclear>ικατα</rdg></app> <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="15" unit="character"/><gap reason="illegible" extent="1" unit="character"/>ως <choice><corr>εἰ</corr><sic><hi rend="diaeresis">ι</hi></sic></choice> μὴ ἔκ<g type="apostrophe"/> τινος 

    <lb n="15"/><app type="BL"><lem resp="9.182"><choice><corr><supplied reason="lost" cert="high">προ</supplied>νοίας</corr><sic><supplied reason="lost" cert="high">προ</supplied>νυας</sic></choice></lem><rdg><choice><corr><supplied reason="lost" cert="high">ἀπο</supplied>νοίας</corr><sic><supplied reason="lost" cert="high">απο</supplied>νυα?</sic></choice></rdg></app> τὴν <gap reason="lost" extent="8" unit="character"/><unclear reason="undefined" cert="high">π</unclear><gap reason="illegible" extent="1" unit="character"/> <supplied reason="lost" cert="high">ἀ</supplied><unclear reason="undefined" cert="high">λ</unclear>λ᾽ οὖν τῶν <choice><corr>βοῶν</corr><sic>βωων</sic></choice> 

