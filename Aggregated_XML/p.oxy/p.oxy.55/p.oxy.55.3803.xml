    <lb n="19"/><supplied cert="high" reason="lost">πρ</supplied><unclear cert="high" reason="undefined">ότε</unclear>ρ<supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ν</unclear> <unclear cert="high" reason="undefined">Ἀνου</unclear>θίου καὶ τε<unclear cert="high" reason="undefined">λέ</unclear><supplied cert="high" reason="lost">σω τὸν</supplied> <unclear cert="high" reason="undefined">φόρ</unclear><supplied cert="high" reason="lost">ο</supplied>ν 

    <lb n="20"/><supplied cert="high" reason="lost">τὸν</supplied> προ<supplied cert="high" reason="lost">τ</supplied>ελ<supplied cert="high" reason="lost">ο</supplied>ύμενον <unclear cert="high" reason="undefined">ἀπὸ</unclear> <unclear cert="high" reason="undefined">τῆ</unclear><supplied cert="high" reason="lost">ς νέ</supplied><unclear cert="high" reason="undefined">ας</unclear> <unclear cert="high" reason="undefined">ἑνδεκάτης</unclear> 

    <lb n="21"/><supplied cert="high" reason="lost">ἰνδικ</supplied>τί<supplied cert="high" reason="lost">ω</supplied>νος καὶ τελέ<supplied cert="high" reason="lost">σ</supplied><unclear cert="high" reason="undefined">ω</unclear> <unclear cert="high" reason="undefined">πά</unclear><supplied cert="high" reason="lost">ντα τ</supplied><unclear cert="high" reason="undefined">ὰ</unclear> <unclear cert="high" reason="undefined">ἔκτ</unclear>ακτα <gap extent="3" reason="illegible" unit="character"/> 

    <lb n="22"/><gap extent="2" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ὡς πρ</supplied><unclear cert="high" reason="undefined">όκ</unclear>ιται. Φλαύ<hi rend="diaeresis"><unclear cert="high" reason="undefined">ϊ</unclear></hi>ο<supplied cert="high" reason="lost">ς</supplied> <unclear cert="high" reason="undefined">Κ</unclear>αστρ<gap extent="6" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/>νος 

    <lb n="23"/><gap extent="6" reason="lost" unit="character"/>ν<unclear cert="high" reason="undefined">α</unclear> ἀξιωθεὶ<unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">ἔ</unclear>γραψ<supplied cert="high" reason="lost">α ὑπὲρ αὐτοῦ</supplied> παρόντ<unclear cert="high" reason="undefined">ος</unclear> 

