    <lb n="1"/><supplied cert="high" reason="lost">ἔτους </supplied><gap extent="1" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Αὐ</supplied>τοκρ<supplied cert="high" reason="lost">α</supplied>τόρων Καισάρων Γα<hi rend="diaeresis">ί</hi>ου Οὐιβίου Τρεβωνιανοῦ 

    <lb n="2"/><supplied cert="high" reason="lost">Γάλλου καὶ Γαίο</supplied><unclear cert="high" reason="undefined">υ</unclear> Οὐ<supplied cert="high" reason="lost">ιβί</supplied>ου Ἀφινίου Γάλλου Οὐελδουμιανοῦ Οὐολουσιανοῦ 

    <lb n="3"/><supplied cert="high" reason="lost">Εὐσε</supplied>βῶν Εὐτυχ<supplied cert="high" reason="lost">ῶ</supplied>ν Σεβαστῶν Δαισίου <space dim="horizontal" extent="unknown" unit="character"/> Φαρμοῦθι <space dim="horizontal" extent="unknown" unit="character"/> 

    <lb n="4"/><supplied cert="high" reason="lost">ἐν Ὀξ</supplied>υρύγχων πόλει ἐπὶ Αὐρηλίου Ἀντιπάτρου τοῦ καὶ Διονυσίου 

    <lb n="5"/><supplied cert="high" reason="lost">ἀσχ</supplied>ολουμένου ὠνὴν <choice><corr>ἀγορανομίου</corr><sic>αγορανομειου</sic></choice>. 

    <lb n="6"/><supplied cert="high" reason="lost">ἐπρί</supplied>ατο Αὐρήλιος Ἀσκληπιάδης ὁ καὶ Σαρᾶς Σαραπίωνος <expan>μητρὸ<ex cert="high">ς</ex></expan> 

    <lb n="7"/><supplied cert="high" reason="lost">Λουκ</supplied>ίλλης τῆς καὶ Δημητρίας ἀπ᾽ Ὀξυρύγχων πόλεως ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <num value="32">λβ</num> 

    <lb n="8"/><expan><supplied cert="high" reason="lost">ἄσημ<ex cert="high">ος</ex></supplied></expan> παρὰ Αὐρηλίου Σερήνου τοῦ καὶ Σαραπίωνος Ἀγαθείνου 

    <lb n="9"/><supplied cert="high" reason="lost">μητρὸ</supplied>ς Τα<unclear cert="high" reason="undefined">ποσ</unclear>είριος ἀπὸ τῆς αὐτῆς πόλεως ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <num value="34">λδ</num> <expan>ἀσήμ<ex cert="high">ου</ex></expan>, ἐν ἀ-

    <lb n="10"/><supplied cert="high" reason="lost">γυιᾷ, τὴ</supplied>ν <hi rend="diaeresis">ὑ</hi>πάρχουσαν αὐτῷ ὠνηθεῖσαν <hi rend="diaeresis">ὑ</hi>π᾽ αὐτοῦ κατὰ χρη-

    <lb n="11"/><supplied cert="high" reason="lost">ματισ</supplied>μὸν γενόμενον διὰ τοῦ αὐτοῦ <choice><corr>ἀγορανομίου</corr><sic>αγορανομειου</sic></choice> τῷ <num value="4">δ</num> <expan><ex cert="high">ἔτει</ex></expan> 

    <lb n="12"/><supplied cert="high" reason="lost">Φιλίππ</supplied>ων μηνὶ Φαμενὼθ ἀπὸ τῆς τοῦ ὠνουμένου μητρὸς 

    <lb n="13"/><supplied cert="high" reason="lost">Αὐρηλ</supplied>ίας Λουκίλλης τῆς καὶ Δημητρίας Εὐπόρου τοῦ Διογένους 

    <lb n="14"/><supplied cert="high" reason="lost">μητρὸς</supplied> Ταύριος τῆς καὶ Φιλουμένης ἀπὸ τῆς αὐτῆς πόλεως 

    <lb n="15"/><supplied cert="high" reason="lost">οἰκογε</supplied>νὴν αὐτῆς δούλην ὀνόματι Τερέα ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <num value="21">κα</num> <expan>μελίχρο<ex cert="high">υν</ex></expan> <expan>ο<ex cert="high">ὐλὴ</ex></expan> 

    <lb n="16"/><gap extent="5" reason="lost" unit="character"/><unclear cert="high" reason="undefined">η</unclear> σὺν <choice><corr>ὑποτιτ<supplied cert="high" reason="omitted">θ</supplied>ίῳ</corr><sic><hi rend="diaeresis">υ</hi>ποτιτιω</sic></choice> αὐ<supplied cert="high" reason="lost">τ</supplied>ῆς ἀρρενικῷ βρέφει ὀνόματι 

    <lb n="17"/><gap extent="6" reason="lost" unit="character"/> ἥνπερ δούλην σὺν <supplied cert="high" reason="lost">τ</supplied>ῷ <choice><corr>ὑποτι<supplied cert="high" reason="omitted">τ</supplied>θίῳ</corr><sic><hi rend="diaeresis">υ</hi>ποτιθιω</sic></choice> αὐτόθι παρείλη-

    <lb n="18"/><supplied cert="high" reason="lost">φ</supplied><unclear cert="high" reason="undefined">εν</unclear> <unclear cert="high" reason="undefined">ὁ</unclear> πριάμενος παρὰ τοῦ <supplied cert="high" reason="lost">ἀ</supplied>ποδουμένου ταῦτα τοιαῦτα 

    <lb n="19"/><supplied cert="high" reason="lost">ἀναπόρρ</supplied>ιφα ἐκτὸς ὄντα <hi rend="diaeresis">ἱ</hi>ερ<supplied cert="high" reason="lost">ᾶς</supplied> νόσου καὶ ἐπαφῆς, ἀνα-

    <lb n="20"/><supplied cert="high" reason="lost">κριθείση</supplied><unclear cert="high" reason="undefined">ς</unclear> τῆς Τερεῦτος ὡς διὰ τοῦ προτέρου χρηματισμοῦ 

    <lb n="21"/><supplied cert="high" reason="lost">δηλοῦται</supplied>, τὰς δὲ συμπεφωνημένας πρὸς ἀλλήλους 

    <lb n="22"/><supplied cert="high" reason="lost">ὑπὲρ τῆς α</supplied>ὐτῆς δούλης σὺν τῷ <choice><corr>ὑποτι<supplied cert="high" reason="omitted">τ</supplied>θίῳ</corr><sic><hi rend="diaeresis">υ</hi>ποτιθιω</sic></choice> ἀργυρίου Σεβαστῶ<unclear cert="high" reason="undefined">ν</unclear> 

    <lb n="23"/><supplied cert="high" reason="lost">νομίσμα</supplied>τος δραχμὰς <choice><corr>δισχιλίας</corr><sic>δισχειλιας</sic></choice> <num value="2000"/> ἀπέσχεν ὁ ἀποδόμενος 


    <lb n="24"/><supplied cert="high" reason="lost">Αὐρήλιος</supplied> Σαραπίων ὁ καὶ Σερῆνος παρὰ τοῦ <choice><corr>πριαμένου</corr><sic>αποδομενου</sic></choice> 

    <lb n="25"/><supplied cert="high" reason="lost">Αὐρηλίου Ἀ</supplied>σκληπιάδου τοῦ καὶ Σαρᾶ διὰ χειρός. πωλεῖ καὶ 

    <lb n="26"/><supplied cert="high" reason="lost">βεβαιοῖ ὁ ἀ</supplied>ποδόμενος τὴν αὐτὴν δούλην σὺν τῷ <choice><corr>ὑποτιτ<supplied cert="high" reason="omitted">θ</supplied>ί-

    <lb n="27"/><supplied cert="high" reason="lost">ῳ</supplied></corr><sic>υποτιτι<supplied cert="high" reason="lost">ω</supplied></sic></choice><gap extent="12" reason="lost" unit="character"/> ὀνόματος αὐτοῦ ὡς πρόκειται ἐν ἀ-

    <lb n="28"/><supplied cert="high" reason="lost">γυιᾷ τῇ αὐτῇ</supplied>, περὶ δὲ τοῦ ταῦτα ὀρθῶς καλῶς γεγε-

    <lb n="29"/><supplied cert="high" reason="lost">νῆσθαι ἐπερώτη</supplied><unclear cert="high" reason="undefined">σ</unclear>εν ὁ ὠνούμενος ὡμολόγησεν 

    <lb n="30"/><supplied cert="high" reason="lost">ὁ ἀποδόμενος.</supplied> 

    <lb n="31"/><handShift new="m2"/> <supplied cert="high" reason="lost">Αὐρήλιος Ἀσκληπι</supplied>άδης ὁ καὶ Σαρᾶς ἐπριάμην τὴν δούλην 

    <lb n="32"/><supplied cert="high" reason="lost">ὡς <expan>πρόκ<ex cert="high">ειται</ex></expan>. Αὐρήλιος Σαραπίων ὁ κ</supplied><unclear cert="high" reason="undefined">αὶ</unclear> <unclear cert="high" reason="undefined">Σε</unclear><supplied cert="high" reason="lost">ρῆ</supplied><unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">ος τ</supplied>ὴν <choice><corr>τιμὴ<supplied cert="high" reason="lost">ν</supplied></corr><sic>τειμη<supplied cert="high" reason="lost">ν</supplied></sic></choice><supplied cert="high" reason="lost"> ἔσχο</supplied><unclear cert="high" reason="undefined">ν</unclear> 

    <lb n="33"/><supplied cert="high" reason="lost">ὡς <expan>πρόκ<ex cert="high">ειται</ex></expan> </supplied><gap extent="unknown" reason="lost" unit="character"/> 

