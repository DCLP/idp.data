    <lb n="1"/><supplied cert="high" reason="lost"><g type="stauros"/> βασιλείας τοῦ θειοτάτου καὶ εὐσεβεστάτου ἡμῶν δεσ</supplied><unclear cert="high" reason="undefined">πότου</unclear> Φλαου<hi rend="diaeresis">ΐ</hi>ου <hi rend="diaeresis">Ἰ</hi>ουστινιανοῦ τοῦ αἰωνίου 

    <lb n="2"/><supplied cert="high" reason="lost">Αὐγούστου καὶ Αὐτοκράτορος ἔτους <num value="1">α</num></supplied><num value="7"><unclear cert="high" reason="undefined">ζ</unclear></num>, τοῖς τὸ <num value="4"><unclear cert="high" reason="undefined">δ</unclear></num> μετ<unclear cert="high" reason="undefined">ὰ</unclear> <supplied cert="high" reason="lost">τὴν</supplied> <choice><corr>ὑπατείαν</corr><sic><unclear cert="high" reason="undefined">υπα</unclear>τιαν</sic></choice> Φλαου<hi rend="diaeresis">ΐ</hi>ου Βασιλίου τοῦ <expan>λαμ<ex cert="high">προτάτου</ex></expan> Μ<unclear cert="high" reason="undefined">εχε</unclear>ὶρ <num value="12">ιβ</num>, <expan>ἰνδ<ex cert="high">ικτίωνος</ex></expan> <num value="7">ζ</num>. 

    <lb n="3"/><supplied cert="high" reason="lost">Φλαουΐῳ Ἀπίωνι τῷ ὑπερ</supplied><unclear cert="high" reason="undefined">φυε</unclear>στάτῳ <hi rend="diaeresis">ὑ</hi><supplied cert="high" reason="lost">πάτ</supplied>ῳ ὀρδιναρίῳ γεουχοῦντι καὶ ἐνταῦθα τῇ λαμπρᾷ 

    <lb n="4"/><supplied cert="high" reason="lost">Ὀξυρυγχιτῶν πόλει διὰ Μ</supplied>ηνᾶ <supplied cert="high" reason="lost">οἰ</supplied>κέτου τοῦ ἐπερωτῶντος καὶ προσπορίζοντος τῷ <hi rend="diaeresis">ἰ</hi>δίῳ 

    <lb n="5"/><supplied cert="high" reason="lost">δεσπότῃ τῷ αὐτῷ ὑπε</supplied><unclear cert="high" reason="undefined">ρφ</unclear><supplied cert="high" reason="lost">υ</supplied><unclear cert="high" reason="undefined">εστά</unclear>τῳ ἀνδρὶ τὴν ἀγωγὴν καὶ ἐνοχήν, Αὐρήλιος Σερῆνος 

    <lb n="6"/><supplied cert="high" reason="lost">μυλοκόπος τὴν τέ</supplied>χνην υ<hi rend="diaeresis">ἱ</hi>ὸς Ἠλίο<supplied cert="high" reason="lost">υ ὁ</supplied><unclear cert="high" reason="undefined">ρμ</unclear><supplied cert="high" reason="lost">ώμ</supplied><unclear cert="high" reason="undefined">ενος</unclear> <unclear cert="high" reason="undefined">ἀπὸ</unclear> τῆς Ὀξυρυγχιτῶν πόλεως 

    <lb n="7"/><supplied cert="high" reason="lost">χαίρειν. ὁμολογ</supplied><unclear cert="high" reason="undefined">ῶ</unclear> <unclear cert="high" reason="undefined">δι</unclear>ὰ ταύτης μου τῆς ἐγ<g type="apostrophe"/>γρ<supplied cert="high" reason="lost">ά</supplied><unclear cert="high" reason="undefined">φ</unclear><supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">υ</unclear> ἀσφαλείας ἑτοίμως <choice><corr>ἔχειν</corr><sic>εχω</sic></choice> <unclear cert="high" reason="undefined">ἐργ</unclear>άσασθαι 

    <lb n="8"/><unclear cert="high" reason="undefined">ἀπὸ</unclear> <gap extent="1" reason="illegible" unit="character"/><gap extent="5" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/> <unclear cert="high" reason="undefined">ἐ</unclear>πὶ τ<unclear cert="high" reason="undefined">ὸ</unclear>ν <unclear cert="high" reason="undefined">τῆς</unclear> <unclear cert="high" reason="undefined">ζ</unclear>ωῆς μ<supplied cert="high" reason="lost">ο</supplied>υ χρόνον ε<unclear cert="high" reason="undefined">ἰς</unclear> πάντας τοὺς μυλ<unclear cert="high" reason="undefined">αί</unclear>ους <choice><corr>στροβίλους</corr><sic>στροβιλ<g type="apostrophe"/>λους</sic></choice> τε 

    <lb n="9"/>κ<supplied cert="high" reason="lost">α</supplied>ὶ καλάθους το<unclear cert="high" reason="undefined">ὺ</unclear>ς ὄντας ἐν ἑκάσ<supplied cert="high" reason="lost">τῳ</supplied> τόπῳ τῆς <hi rend="diaeresis">ὑ</hi>μετέρας οὐσίας ἐν <choice><corr>τῷ</corr><sic>τε</sic></choice> κριβανείῳ <unclear cert="high" reason="undefined">τ</unclear>ε 

    <lb n="10"/>καὶ πᾶσιν τοῖς κτήμασιν καὶ ἐν ἐλαι<supplied cert="high" reason="lost">ου</supplied>ργίοις τῆς <hi rend="diaeresis">ὑ</hi>μετέρας <hi rend="diaeresis">ὑ</hi>περφυείας ἀόκνως 

    <lb n="11"/>καὶ ἀκαταγνώστως εἰς τὸ μηδε<supplied cert="high" reason="lost">μ</supplied>ίαν μέμψιν ἢ ἀμέλειαν ἢ κατάγνωσιν ἢ <choice><corr>ῥαθυμία<supplied cert="high" reason="omitted">ν</supplied></corr><sic>ραθυμια</sic></choice> 

    <lb n="12"/>τινὰ περὶ ἐμὲ γενέσθαι δίχα ἀρ<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ωστ</supplied>ίας καὶ πόνου τινὸς δέξασθαί με παρὰ τῆς 

    <lb n="13"/><hi rend="diaeresis">ὑ</hi>μετέρας <hi rend="diaeresis">ὑ</hi>περφυείας <hi rend="diaeresis">ὑ</hi>πὲρ ἑκάστου <choice><corr>στροβίλου</corr><sic>στροβιλ<g type="apostrophe"/>λου</sic></choice> νέου καὶ καλάθου νέου 

    <lb n="14"/>χρυσοῦ νομισμάτια δύο ἥμισ<supplied cert="high" reason="lost">υ <num value="2  1/2 "/> ἰ</supplied>διωτικῷ ζυγῷ νομιτ<supplied cert="high" reason="lost">ε</supplied>υόμενα, ἐμοῦ λαμβάνοντος 

    <lb n="15"/>τοὺς παλαιοὺς λίθους, καὶ μη<gap extent="10" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/> ἀποστῆναι ἀπὸ τῆς <hi rend="diaeresis">ὑ</hi>μετέρας <supplied cert="high" reason="lost">χρείας</supplied> 

    <lb n="16"/>ἢ ἀπὸ τῶν ὀφειλόντων γενέ<supplied cert="high" reason="lost">σθα</supplied><unclear cert="high" reason="undefined">ι</unclear> <supplied cert="high" reason="lost">ἔ</supplied><unclear cert="high" reason="undefined">ργ</unclear>ων. εἰ δὲ <supplied cert="high" reason="lost">τοῦ</supplied>το ποιήσω, ἐπὶ τῷ με δοῦναι 

    <lb n="17"/>τῇ <hi rend="diaeresis">ὑ</hi>μετέρᾳ <hi rend="diaeresis">ὑ</hi>περφυείᾳ λόγῳ προστίμου χρυσοῦ νομισμάτια εἴκοσι τέσ<g type="apostrophe"/>σαρα <num value="24"/>. εἰ δὲ 

    <lb n="18"/>ἡ <hi rend="diaeresis">ὑ</hi>μετέρα <hi rend="diaeresis">ὑ</hi>περφυεία διὰ τῶν αὐτῆς διοικητῶν ἐκβάλῃ με ἐκτὸς καταγνώσεως 

    <lb n="19"/>ἢ <choice><corr><unclear cert="high" reason="undefined">ῥ</unclear>αθυμία<supplied cert="high" reason="omitted">ς ἢ</supplied></corr><sic><unclear cert="high" reason="undefined">ρ</unclear>αθυμια</sic></choice> ο<unclear cert="high" reason="undefined">ἱ</unclear>ασδήποτε αἰτίας, <hi rend="diaeresis">ὑ</hi>ποκεῖσθαι καὶ αὐτῇ τῷ <hi rend="diaeresis">ἴ</hi>σῳ προστίμῳ. <hi rend="diaeresis">ὑ</hi>ποθέμενος 

    <lb n="20"/><unclear cert="high" reason="undefined">εἰς</unclear> <unclear cert="high" reason="undefined">τὸ</unclear> δίκ<unclear cert="high" reason="undefined">αιον</unclear> τούτου τοῦ συναλ<g type="apostrophe"/>λάγματος ἅπαντά μου τὰ <hi rend="diaeresis">ὑ</hi>πάρχοντα καὶ <hi rend="diaeresis">ὑ</hi>πάρξοντα ὅσα τε νῦν ἔχω 

    <lb n="21"/><unclear cert="high" reason="undefined">καὶ</unclear> <supplied cert="high" reason="lost">ὅσα ἄλλα μ</supplied>ετὰ ταῦτα ἐπικτήσομαι <hi rend="diaeresis">ἰ</hi>δικῶς καὶ γενικῶς ἐνεχύρου λόγῳ καὶ <hi rend="diaeresis">ὑ</hi>ποθήκης δικαίῳ. 

    <lb n="22"/>κύριο<supplied cert="high" reason="lost">ν τὸ συ</supplied>νά<unclear cert="high" reason="undefined">λλ</unclear>αγμα δι<unclear cert="high" reason="undefined">σσ</unclear>ὸν γραφὲν καὶ ἐπερωτηθεὶς ὡμολόγησα. <g type="chirho"/> <handShift new="m2"/> Αὐρήλιος Σερῆνος, 

    <lb n="23"/>μυλοκ<supplied cert="high" reason="lost">όπ</supplied><unclear cert="high" reason="undefined">ος</unclear> <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ὴ</supplied><unclear cert="high" reason="undefined">ν</unclear> τέχνην, υ<hi rend="diaeresis">ἱ</hi>ὸς Ἠλίου ὁ προγεγραμμένος πεποίημαι τοῦτο τὸ συνάλλαγμα 

    <lb n="24"/><unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">ατ</supplied><unclear cert="high" reason="undefined">ὰ</unclear> <unclear cert="high" reason="undefined">τὸν</unclear> <unclear cert="high" reason="undefined">π</unclear>ρο<unclear cert="high" reason="undefined">κ</unclear>είμενον τρόπον καὶ συμφωνεῖ μοι πάντα ὡς πρόκειται. Αὐρήλιος 

    <lb n="24"/><gap extent="5" reason="lost" unit="character"/><supplied cert="high" reason="lost"> υἱὸς τ</supplied><unclear cert="high" reason="undefined">οῦ</unclear> μακαρίου <hi rend="diaeresis">Ἰ</hi>σὰκ <expan>ἀξιωθ<ex cert="high">εὶς</ex></expan> ἔγραψα <hi rend="diaeresis">ὑ</hi>πὲρ αὐτοῦ ἀγραμμάτου ὄντος. <g type="chirho"/> 

