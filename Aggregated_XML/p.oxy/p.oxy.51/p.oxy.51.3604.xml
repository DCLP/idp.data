    <lb n="1"/><gap extent="unknown" reason="lost" unit="line"/> 

    <lb n="1"/><unclear cert="high" reason="undefined">α</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>ἀπὸ <expan>κώ<ex cert="high">μης</ex></expan> <gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ε</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>ε<gap extent="2" reason="illegible" unit="character"/>ονων ὀμνύ<unclear cert="high" reason="undefined">ω</unclear> <supplied cert="high" reason="lost">τὴν Μάρκου</supplied> 

    <lb n="4"/>Αὐρηλίου Σεουήρου <supplied cert="high" reason="lost">Ἀντωνίνου</supplied> 

    <lb n="5"/>Παρθικοῦ Μεγίστου Βρε<supplied cert="high" reason="lost">ταννικοῦ</supplied> 

    <lb n="6"/>Μεγίστου Γερμανικοῦ <supplied cert="high" reason="lost">Μεγίστου</supplied> 

    <lb n="7"/>Εὐσεβοῦς Σεβαστοῦ τ<supplied cert="high" reason="lost">ύχην ἅμα τοῖς</supplied> 

    <lb n="8"/>ἐξ ἀρχόντων α<supplied cert="high" reason="lost">ἱρεθεῖσι</supplied> 

    <lb n="9"/>εἰς τοῦτο συνεξελέσθαι καὶ <unclear cert="high" reason="undefined">συν</unclear>-

    <lb n="10"/>καταστῆσαι ὄνους <expan>πεμπ<unclear cert="high" reason="undefined">ομ</unclear><ex cert="high">ένους</ex></expan> εἰς 

    <lb n="11"/>τὴν ε<unclear cert="high" reason="undefined">ὐκτ</unclear>αιοτάτην <expan>ἐπιδη<unclear cert="high" reason="undefined">μ</unclear><ex cert="high">ίαν</ex></expan> 

    <lb n="12"/>τοῦ κυρίου ἡμῶν Αὐτοκράτορος 

    <lb n="13"/>Σεουήρου Ἀντωνίνου, οὓς καὶ 

    <lb n="14"/>ἀποκαταστήσω καὶ <choice><corr>παραδώσω</corr><sic>παραδοσω</sic></choice> 

    <lb n="15"/>εἰς τὸ ἐν μηδενὶ <expan>μεμφθ<ex cert="high">ῆναι</ex></expan> ἢ ἔνοχος 

    <lb n="16"/>εἴην τῷ ὅρκῳ. παρέσχον δὲ 

    <lb n="17"/>ἐνγυητὴν εἰς τοῦτο <choice><corr>Αὐρήλιο<supplied cert="high" reason="omitted">ν</supplied></corr><sic>αυρηλιο</sic></choice> 

    <lb n="18"/><unclear cert="high" reason="undefined">Π</unclear>ωλίωνα Λ<gap extent="3" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">το</unclear>ς ἀπὸ <supplied cert="high" reason="omitted">τῆς</supplied> 

    <lb n="19"/><expan>α<ex cert="high">ὐτῆς</ex></expan> <expan>κώ<ex cert="high">μης</ex></expan> παρόν<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">α</supplied> <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">α</supplied>ὶ <expan>εὐδοκ<ex cert="high">οῦντα</ex></expan> 

    <lb n="20"/>ἐπὶ πᾶσι τοῖς <expan>προκ<ex cert="high">ειμένοις</ex></expan>. <expan>ἐγρ<ex cert="high">άφη</ex></expan> <expan>ἐ<ex cert="high">πακολουθοῦντος</ex></expan> <expan>Α<ex cert="high">ὐρηλίου</ex></expan> Μ<unclear cert="high" reason="undefined">αμερ</unclear>τί-

    <lb n="21"/>νου τοῦ καὶ <expan>Πτολεμ<ex cert="high">αίου</ex></expan> <expan>ὑ<ex cert="high">πηρέτου</ex></expan>. καί εἰσι 

    <lb n="22"/><expan>Α<ex cert="high">ὐρήλιοι</ex></expan> Πακλῆς <expan><ex cert="high">ἐτῶν</ex></expan> <num value="40">μ</num> <expan>οὐ<ex cert="high">λὴ</ex></expan> <gap extent="4" reason="illegible" unit="character"/> 

    <lb n="23"/><expan>ἀντικ<ex cert="high">νημίῳ</ex></expan> <expan>δεξ<ex cert="high">ιῷ</ex></expan>, 

    <lb n="24"/>Πωλίων <expan><ex cert="high">ἐτῶν</ex></expan> <num value="45">με</num> <expan>οὐ<ex cert="high">λὴ</ex></expan> <expan><unclear cert="high" reason="undefined">ἀν</unclear><ex cert="high">τικνημίῳ</ex></expan> <expan>ἀριστ<ex cert="high">ερῷ</ex></expan>. 

    <lb n="25"/><expan><ex cert="high">ἔτους</ex></expan> <num value="24">κδ</num> Αὐτοκράτορος Καίσαρος 

    <lb n="26"/>Μάρκου Αὐρηλίου Σεουήρου Ἀντωνίνου 

    <lb n="27"/>Παρθικοῦ Μεγίστου Βρεταννικοῦ Μεγίστου Γερμανικοῦ 

    <lb n="28"/>Μεγίστου Εὐσεβοῦς Σεβαστοῦ, Ἁθὺρ <num value="30">λ</num>. </ab></div><div n="meta.12597" part="N" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.12597" part="N" subtype="textDate" type="commentary"><p><date notAfter="215" notBefore="215" type="textDate" value="0215-11-27">27. Nov. 215</date></p></div><div n="meta.12597" part="N" subtype="general" type="commentary"><p> </p></div><div n="meta.12597" part="N" subtype="principalEdition" type="bibliography"><listBibl><bibl subtype="principal" type="publication"><title level="s" type="abbreviated">P.Oxy.</title><biblScope type="volume">51</biblScope><biblScope type="numbers">3604</biblScope></bibl><bibl type="Trismegistos"><biblScope type="numbers">12597</biblScope></bibl><bibl type="DDbDP"><series>0181</series>; <biblScope type="volume">51</biblScope>; <biblScope type="numbers">3604</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0181;51;3604" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.12597" part="N" subtype="illustrations" type="bibliography"><p><bibl type="illustration">keine</bibl></p></div><div n="meta.12597" part="N" subtype="locations" type="history"><p><geogName cert="high" type="nome">Arsinoites</geogName>, <geogName cert="high" key="Aegyptus" type="ancientRegion">Egypt</geogName></p></div><div n="meta.12597" part="N" type="figure"><p><figure href="http://163.1.169.40/cgi-bin/library?e=d-000-00---0POxy--00-0-0-0prompt-10---4---Document---0-1l--1-en-50---20-about---001-001-1-0utfZz-8-00031-001-1-0utfZz-8-00&amp;a=d&amp;c=POxy&amp;cl=CL5.1.14&amp;d=HASH8dc59e16dde3c5da6f84a8"><figDesc>Abbildung im Internet</figDesc></figure></p></div></body></text></TEI.2>