    <lb n="2"/>ἀπὸ <expan>κώ<ex cert="high">μης</ex></expan> <gap reason="illegible" extent="1" unit="character"/><unclear reason="undefined" cert="high">ε</unclear><gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>ε<gap reason="illegible" extent="2" unit="character"/>ονων ὀμνύ<unclear reason="undefined" cert="high">ω</unclear> <supplied reason="lost" cert="high">τὴν Μάρκου</supplied> 

    <lb n="4"/>Αὐρηλίου Σεουήρου <supplied reason="lost" cert="high">Ἀντωνίνου</supplied> 

    <lb n="5"/>Παρθικοῦ Μεγίστου Βρε<supplied reason="lost" cert="high">ταννικοῦ</supplied> 

    <lb n="6"/>Μεγίστου Γερμανικοῦ <supplied reason="lost" cert="high">Μεγίστου</supplied> 

    <lb n="7"/>Εὐσεβοῦς Σεβαστοῦ τ<supplied reason="lost" cert="high">ύχην ἅμα τοῖς</supplied> 

    <lb n="8"/>ἐξ ἀρχόντων α<supplied reason="lost" cert="high">ἱρεθεῖσι</supplied> 

    <lb n="9"/>εἰς τοῦτο συνεξελέσθαι καὶ <unclear reason="undefined" cert="high">συν</unclear>-

    <lb n="10"/>καταστῆσαι ὄνους <expan>πεμπ<unclear reason="undefined" cert="high">ομ</unclear><ex cert="high">ένους</ex></expan> εἰς 

