    <lb n="35"/>προσαγορεύω <choice><corr>Φοιβάμμωνα</corr><sic>φο<unclear cert="high" reason="undefined">ι</unclear>βαμμ<unclear cert="high" reason="undefined">ονα</unclear></sic></choice> <choice><corr>τὸν</corr><sic><unclear cert="high" reason="undefined">τ</unclear>ων</sic></choice> <choice><corr>γεωργὸν</corr><sic>γεοργον</sic></choice> 

    <lb n="36"/>καὶ <choice><corr>Φιλοξέναν</corr><sic>φιλοξεν<unclear cert="high" reason="undefined">α</unclear></sic></choice> τ<unclear cert="high" reason="undefined">ὴν</unclear> <choice><corr>θυγατέρα</corr><sic>θυγατεραν</sic></choice> Θε<unclear cert="high" reason="undefined">ο</unclear>φα-

    <lb n="37"/>νίου κ<unclear cert="high" reason="undefined">α</unclear>ὶ <choice><corr>ὅλους</corr><sic>ο<unclear cert="high" reason="undefined">λι</unclear></sic></choice> ἐν τῇ <choice><corr>οἰκίᾳ</corr><sic>υκια</sic></choice> 

    <lb n="38" rend="perpendicular"/> κυρίῳ μου τιμιωτάτῳ πατρὶ Ζ<unclear cert="high" reason="undefined">ω</unclear><supplied cert="high" reason="lost">ί</supplied><unclear cert="high" reason="undefined">λῳ</unclear> <gap extent="unknown" reason="lost" unit="character"/> 

