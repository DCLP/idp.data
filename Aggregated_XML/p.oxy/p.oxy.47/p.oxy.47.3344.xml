    <lb n="1"/>Διδύμῳ <expan>στρ<ex cert="high">ατηγῷ</ex></expan> <expan>Ὀξ<ex cert="high">υρυγχίτου</ex></expan>. 

    <lb n="2"/>Ὀννῶφρις Πατσίβθεως <expan>μητρὸ<ex cert="high">ς</ex></expan> 

    <lb n="3"/>Σινθεῦτος καταγινόμενος ἐν τῷ 

    <lb n="4"/>Ληνῶν<supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">ς</unclear> ἐποικίῳ εἰσδοθεὶς ὑπὸ 

    <lb n="5"/>τοῦ τῶν <supplied reason="lost" cert="high">τ</supplied>όπων <expan>κωμογρ<ex cert="high">αμματέως</ex></expan> εἰς νομοφυ-

    <lb n="6"/>λακίαν ὀμνύω τὴν τῶν <expan>κυρίω<ex cert="high">ν</ex></expan> 

    <lb n="7"/>Αὐτοκρατόρων Σεουήρου καὶ <expan>Ἀντων<ex cert="high">ίνου</ex></expan> 

    <lb n="8"/>καὶ Γέτα Καίσαρος τύ<unclear reason="undefined" cert="high">χ</unclear><supplied reason="lost" cert="high">η</supplied>ν εὐθέως 

    <lb n="9"/>ἀντιλήμψεσθαι τῆς δηλουμένης 

    <lb n="10"/>χρε<supplied reason="lost" cert="high">ί</supplied><unclear reason="undefined" cert="high">ας</unclear> <unclear reason="undefined" cert="high">καὶ</unclear> ἐκτελέσειν ταύτην ἐν-

    <lb n="11"/>φ<unclear reason="undefined" cert="high">ανὴς</unclear> <unclear reason="undefined" cert="high">ὢν</unclear> καὶ προσκαρτερῶν <supplied reason="lost" cert="high">εἰς</supplied> 

    <lb n="12"/>τὸ <unclear reason="undefined" cert="high">ἐ</unclear><supplied reason="lost" cert="high">ν</supplied> <unclear reason="undefined" cert="high">μη</unclear>δενὶ μεμφθῆ<unclear reason="undefined" cert="high">να</unclear>ι, ἢ ἔ-

    <lb n="13"/>νοχος εἴην τῷ ὅρκῳ. π<unclear reason="undefined" cert="high">αρ</unclear><supplied reason="lost" cert="high">έσ</supplied>χον 

    <lb n="14"/>δὲ ἐμαυτοῦ ἐνγυητὴν 

    <lb n="15"/>Παυσεῖριν Παήσιος μητρὸς 

    <lb n="16"/>Θα<unclear reason="undefined" cert="high">ή</unclear>σιος καταγεινόμενον 

    <lb n="17"/>ἐν τῷ αὐτῷ ἐποικίῳ παρόν-

    <lb n="18"/>τα καὶ εὐδοκοῦντα. <expan><ex cert="high">ἔτους</ex></expan> <num value="16">ιϛ</num> 

    <lb n="19"/>Αὐτοκρατόρων Καισάρων <expan>Λουκίο<ex cert="high">υ</ex></expan> 

    <lb n="20"/>Σεπτιμίου Σεουήρου Εὐσεβοῦς 

    <lb n="21"/>Περτίνακος Ἀραβικοῦ <expan>Ἀδιαβην<ex cert="high">ικοῦ</ex></expan> 

    <lb n="22"/>Παρθικοῦ Μεγίστου καὶ Μάρκου 

    <lb n="23"/><supplied reason="lost" cert="high">Α</supplied>ὐρη<supplied reason="lost" cert="high">λί</supplied>ου Ἀντωνίνου 

    <lb n="24"/>Ε<unclear reason="undefined" cert="high">ὐσε</unclear><supplied reason="lost" cert="high">β</supplied>οῦς Σεβ<supplied reason="lost" cert="high">α</supplied>στῶν καὶ 

    <lb n="25"/>Πουβλίου Σεπτιμίου Γέτα 

    <lb n="26"/>Καίσ<supplied reason="lost" cert="high">α</supplied><unclear reason="undefined" cert="high">ρ</unclear>ος Σ<unclear reason="undefined" cert="high">ε</unclear><supplied reason="lost" cert="high">β</supplied><unclear reason="undefined" cert="high">α</unclear>στοῦ, Θὼθ <num value="3">γ</num>. 

    <lb n="27"/><handShift new="m2"/> Ὀννῶφρις <subst><add place="inline">Πατσίβιτιος</add><del rend="corrected" status="unremarkable">πα<gap reason="illegible" extent="1" unit="character"/>σβιτιος</del></subst> <subst><add place="inline">ὤ-

    <lb n="28"/>μοσα</add><del rend="corrected" status="unremarkable">ομοσα</del></subst> <supplied reason="lost" cert="high">τ</supplied><unclear reason="undefined" cert="high">ὸν</unclear> <unclear reason="undefined" cert="high">ὅρκον</unclear> καὶ <unclear reason="undefined" cert="high">ἐκ</unclear>τε-

    <lb n="29"/>λέσω τὴ<unclear reason="undefined" cert="high">ν</unclear> <unclear reason="undefined" cert="high">χρ</unclear>είαν ὡς πρό-

    <lb n="30"/>κ<supplied reason="lost" cert="high">ε</supplied>ιται. Παυσεῖρις Παήσιος 

    <lb n="31"/><choice><corr>ἐγγυῶμαι</corr><sic>εγγυ<g type="apostrophe"/>ωμε</sic></choice> τὸν Ὀννῶφριν 

    <lb n="32"/>ἐκτελοῦντα τὴ<unclear reason="undefined" cert="high">ν</unclear> χρείαν 

    <lb n="33"/>ὡς πρόκειται. Πτ<supplied reason="lost" cert="high">ο</supplied>λεμαῖος 

    <lb n="34"/>Διονυσίου ἔγραψα ὑπὲρ 

    <lb n="35"/>αὐτῶν μὴ εἰδότων 

    <lb n="36"/>γράμματα. 
