    <lb n="3"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Ν</supplied><unclear cert="high" reason="undefined">ότου</unclear> <expan><unclear cert="high" reason="undefined">Κρη</unclear><ex cert="high">πῖδος</ex></expan> <num value="2">β</num> 

    <lb n="4"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Π</supplied>λατίας <num value="4">δ</num> 

    <lb n="5"/><gap extent="unknown" reason="lost" unit="character"/> <expan>Μ<supplied cert="high" reason="lost">υ</supplied>ροβαλά<ex cert="high">νου</ex></expan> <num value="4">δ</num> 

    <lb n="6"/><gap extent="unknown" reason="lost" unit="character"/> Κρητικοῦ <num value="2">β</num> 

