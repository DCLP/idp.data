    <lb n="1"/><handShift new="m2"/> Αὐρηλίῳ Πασίωνι τῷ καὶ Ἀπολλωνίῳ γυμ<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">α</supplied>-

    <lb n="2"/>σιαρχήσαντι ἐνάρχῳ πρυτάν<add place="supralinear">ε</add>ι βουλευτῇ τ<unclear cert="high" reason="undefined">ῆ</unclear><supplied cert="high" reason="lost">ς</supplied> 

    <lb n="3"/>Ὀξυρυγχειτῶν πόλεως. 

    <lb n="4"/>παρὰ <unclear cert="high" reason="undefined">Σ</unclear><supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">π</unclear>τιμίου Εὐδαίμονος γυμνασιά<supplied cert="high" reason="lost">ρχου</supplied> 

    <lb n="5"/>βουλε<supplied cert="high" reason="lost">υτο</supplied><unclear cert="high" reason="undefined">ῦ</unclear> τῆς αὐτῆς πόλεως <hi rend="diaeresis">υ</hi>ἱοῦ Σεπ<supplied cert="high" reason="lost">τιμίου</supplied> 

    <lb n="6"/>Σερήν<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">υ</supplied> <unclear cert="high" reason="undefined">ἐ</unclear>ξηγητεύσαντος καὶ πρυτα<supplied cert="high" reason="lost">νεύ</supplied>-

    <lb n="7"/>σαντο<supplied cert="high" reason="lost">ς</supplied> τῆς αὐτῆς πόλεως. ἐπ<supplied cert="high" reason="lost">εὶ παρὰ</supplied> 

    <lb n="8"/>τὰ γε<unclear cert="high" reason="undefined">νό</unclear>μενά μοι <hi rend="diaeresis">ὑ</hi>πὸ τῆς κρατίστης <unclear cert="high" reason="undefined">β</unclear><supplied cert="high" reason="lost">ουλῆς</supplied> 

    <lb n="9"/>ψηφίσματα ἐφ᾽ οἷς πεποίημαι τῆς <unclear cert="high" reason="undefined">ἀν</unclear><supplied cert="high" reason="lost">αδο</supplied>-

    <lb n="10"/>χῆς τ<unclear cert="high" reason="undefined">ῆς</unclear> γυμνασιαρχίας ὅροις <unclear cert="high" reason="undefined">πρὸς</unclear> <unclear cert="high" reason="undefined">τὰς</unclear> <supplied cert="high" reason="lost">δυ</supplied>-

    <lb n="11"/>νάμεις <app type="alternative"><lem>μου</lem><rdg>μου <g rend="extension" type="filler"/></rdg></app> ὠνόμασάς μ<unclear cert="high" reason="undefined">ε</unclear> <unclear cert="high" reason="undefined">σ</unclear>ήμερ<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ν ἥτις</supplied> 

    <lb n="12"/>ἔστιν <supplied cert="high" reason="lost"><num value="1">α</num></supplied><num value="6">ϛ</num> ἐν τῇ κρατίστῃ βουλῇ εἰς ε<unclear cert="high" reason="undefined">ὐθη</unclear><supplied cert="high" reason="lost">νιαρ</supplied>-

    <lb n="13"/>χίας ἡμέρας            π<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ρ᾽</supplied> α<unclear cert="high" reason="undefined">ὐ</unclear>τὸ <unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ὲν</supplied> 

    <lb n="14"/>ἐπὶ <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ῶ</supplied>ν <hi rend="diaeresis">ὑ</hi>πομνημάτων τῆς βου<unclear cert="high" reason="undefined">λ</unclear><supplied cert="high" reason="lost">ῆς</supplied> 

    <lb n="15"/>ἔκστασίν σοι προεβ<unclear cert="high" reason="undefined">α</unclear>λ<unclear cert="high" reason="undefined">ό</unclear>μην καὶ νῦν δ<supplied cert="high" reason="lost">ὲ</supplied> 

    <lb n="16"/>διὰ τῶνδε τῶν <choice><corr>βιβλιδίων</corr><sic>βιβ<unclear cert="high" reason="undefined">λε</unclear>ιδιων</sic></choice> τὴν ἔκστ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">σίν</supplied> 

    <lb n="17"/>σοι προσφέρω παντ<supplied cert="high" reason="lost">ὸ</supplied>ς τοῦ ὑπάρχον<supplied cert="high" reason="lost">τός</supplied> 

    <lb n="18"/>μοι π<unclear cert="high" reason="undefined">ό</unclear>ρου ἐπὶ τῷ <unclear cert="high" reason="undefined">σὲ</unclear> <unclear cert="high" reason="undefined">ἀ</unclear>ντιλαβόμεν<supplied cert="high" reason="lost">ον</supplied> 

    <lb n="19"/>τ<unclear cert="high" reason="undefined">ῶν</unclear> <hi rend="diaeresis"><unclear cert="high" reason="undefined">ὑ</unclear></hi>π<supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">ρ</unclear>χόντων μοι ἀποπληρ<unclear cert="high" reason="undefined">ῶ</unclear><supplied cert="high" reason="lost">σαι</supplied> 

    <lb n="20"/>ὑπὲρ ἐμοῦ τήν τε γυμνα<unclear cert="high" reason="undefined">σιαρ</unclear><supplied cert="high" reason="lost">χίαν</supplied> 

    <lb n="21"/>καὶ εὐθηνιαρχίαν καὶ τὰ ἑπό<unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ενα</supplied> 

    <lb n="22"/>αὐτῇ ἀναλώματα πάντα <unclear cert="high" reason="undefined">ἔ</unclear><supplied cert="high" reason="lost">τι δὲ</supplied> 

    <lb n="23"/>καὶ <unclear cert="high" reason="undefined">τὰ</unclear> ὀφ<add place="supralinear">ε</add>ιλόμενα <hi rend="diaeresis">ὑ</hi>π᾽ ἐμοῦ <choice><corr>σιτικ<supplied cert="high" reason="lost">ὰ</supplied></corr><sic>σει<unclear cert="high" reason="undefined">τ</unclear>ι<unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">α</supplied></sic></choice> 

    <lb n="24"/>καὶ ἀργ<unclear cert="high" reason="undefined">υ</unclear>ρικὰ τ<unclear cert="high" reason="undefined">ελέσ</unclear>ματα <hi rend="diaeresis">ὑ</hi>πέρ τε <supplied cert="high" reason="lost">ὧν</supplied> 

    <lb n="25"/>κέκτημαι <hi rend="diaeresis">ὑ</hi><unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">α</supplied>ρχόντων καὶ <unclear cert="high" reason="undefined">οὗ</unclear> <unclear cert="high" reason="undefined">ἔχ</unclear><supplied cert="high" reason="lost">ω</supplied>

    <lb n="26"/>ἐν μισθώσει μ<unclear cert="high" reason="undefined">έρ</unclear><supplied cert="high" reason="lost">ου</supplied><unclear cert="high" reason="undefined">ς</unclear> οὐσίας πρό<supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ερον</unclear> 

    <lb n="27"/>Κλαυ<unclear cert="high" reason="undefined">δ</unclear>ίου Συρ<unclear cert="high" reason="undefined">ί</unclear>ωνος <choice><corr>αἰωνογυμνασιά<supplied cert="high" reason="lost">ρ</supplied>-

    <lb n="28"/>χου</corr><sic>εωνογυμνασ<unclear cert="high" reason="undefined">ια</unclear><supplied cert="high" reason="lost">ρ</supplied>χου</sic></choice> τῆς λαμπροτάτης Ἀλεξαν<unclear cert="high" reason="undefined">δρ</unclear><supplied cert="high" reason="lost">εί</supplied>-

    <lb n="29"/>ας. πρὸς ἅπερ τελέσματα πάντα 

    <lb n="30"/>ὁ ἐκβὰς οἶνος ἀπὸ τῶν χωρίων κα<supplied cert="high" reason="lost">τε</supplied>-

    <lb n="31"/>νεγυ<unclear cert="high" reason="undefined">ήθη</unclear> <hi rend="diaeresis">ὑ</hi>πὸ τῶν δεκαπρώτω<unclear cert="high" reason="undefined">ν</unclear>. 

    <lb n="32"/>καὶ ἐ<unclear cert="high" reason="undefined">π</unclear>ερωτηθεὶ<unclear cert="high" reason="undefined">ς</unclear> ὡμολόγησα. <g rend="extension" type="filler"/> 

    <lb n="33"/><expan><ex cert="high">ἔτους</ex></expan> <num value="6">ϛ</num> <unclear cert="high" reason="undefined">Α</unclear>ὐτοκρατ<unclear cert="high" reason="undefined">ό</unclear>ρων Καισά<supplied cert="high" reason="lost">ρ</supplied>ων Μάρκ<supplied cert="high" reason="lost">ων</supplied> 

    <lb n="34"/><hi rend="diaeresis">Ἰ</hi>ουλ<supplied cert="high" reason="lost">ίω</supplied>ν Φιλίππων Εὐ<unclear cert="high" reason="undefined">σ</unclear>εβῶν Εὐ<unclear cert="high" reason="undefined">τ</unclear>υχῶ<supplied cert="high" reason="lost">ν</supplied> 

    <lb n="35"/>Σεβ<supplied cert="high" reason="lost">ασ</supplied><unclear cert="high" reason="undefined">τ</unclear>ῶν Ἁθὺρ ἑκκαιδεκάτῃ. 
