    <lb n="25"/><unclear cert="high" reason="undefined">σε</unclear> <unclear cert="high" reason="undefined">Π</unclear>αῦλ<unclear cert="high" reason="undefined">ο</unclear>ν εἶ<unclear cert="high" reason="undefined">ναι</unclear> περὶ τ<unclear cert="high" reason="undefined">ούτ</unclear>ου. <unclear cert="high" reason="undefined">κύρ</unclear>ια τὰ γρ<unclear cert="high" reason="undefined">άμ</unclear>ματα δ<unclear cert="high" reason="undefined">ισ</unclear>σ<unclear cert="high" reason="undefined">ὰ</unclear> γραφέντα 

    <lb n="26"/>πρὸς τὸ ἑκάτ<unclear cert="high" reason="undefined">ερ</unclear><supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ν</unclear> ἔχειν μοναχόν. <supplied cert="high" reason="omitted">καὶ</supplied> ἀλλή<unclear cert="high" reason="undefined">λ</unclear><supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">υ</unclear>ς <subst><add place="inline"><expan>ἐπερωτήσαντ<ex cert="high">ες</ex></expan></add><del rend="corrected" status="unremarkable">επερ<unclear cert="high" reason="undefined">α</unclear>τησαντ</del></subst> ἀλλήλοις 

    <lb n="27"/>ὡμολογήσαμεν. <expan><ex cert="high">ἔτους</ex></expan> <num value="18">ιη</num> καὶ <expan><ex cert="high">ἔτους</ex></expan> <num value="17">ιζ</num> <unclear cert="high" reason="undefined">τῶν</unclear> κυρίων ἡμῶν 

