    <lb n="28"/>τη<unclear cert="high" reason="undefined">ν</unclear> ἐ<unclear cert="high" reason="undefined">πι</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="29"/>καὶ <gap extent="2" reason="illegible" unit="character"/>θ<gap extent="1" reason="illegible" unit="character"/>[ <gap extent="unknown" reason="illegible" unit="character"/> <choice><corr>ἀ]-

    <lb n="30"/>πόστειλον</corr><sic><supplied cert="high" reason="lost">α</supplied>ποστιλον</sic></choice> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="31"/>τινας <unclear cert="high" reason="undefined">ἀπο</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="32"/>ἀποχὰς ἐμο<supplied cert="high" reason="lost">ῦ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="33,ms"/>τί ἕκαστος <choice><corr>παρέχει</corr><sic>παρεχι</sic></choice>. ἐρρῶσθαί σε εὔχομαι. 
