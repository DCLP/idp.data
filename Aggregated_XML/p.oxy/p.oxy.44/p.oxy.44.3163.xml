    <lb n="13"/><handShift new="m3"/> <unclear reason="undefined" cert="high">Σισοῦ</unclear>χος <choice><corr>σεσημείωμαι</corr><sic>σεσημειωμε</sic></choice> 

    <lb n="14"/>ἀρτάβας τεσσεράκοντα 

    <lb n="15"/>ἓξ <choice><corr>χοίνικας</corr><sic>χυνικες</sic></choice> <choice><corr>ὀκτώ</corr><sic>οκτο</sic></choice>. <handShift new="m4"/> <expan>Πτολεμ<unclear reason="undefined" cert="high">αῖ</unclear><ex cert="high">ος</ex></expan> 

