    <lb n="11"/><expan><unclear reason="undefined" cert="high">χοί</unclear><ex cert="high">νικας</ex></expan> <unclear reason="undefined" cert="high">ὀκτ</unclear>ώ, <expan><ex cert="high">γίνονται</ex></expan> <expan><ex cert="high">πυροῦ</ex></expan> <expan><ex cert="high">ἀρτάβαι</ex></expan> <num value="46"><unclear reason="undefined" cert="high">μ</unclear>ϛ</num> <expan>χ<ex cert="high">οίνικες</ex></expan> <num value="8">η</num>. <handShift new="m2"/> Παράις 

