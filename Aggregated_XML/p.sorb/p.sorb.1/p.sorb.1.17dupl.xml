    <lb n="1"/><handShift new="m1"/> <supplied cert="high" reason="lost">βασιλεύοντ</supplied>ος Πτολεμαίου τοῦ Πτολεμαίου, <unclear cert="high" reason="undefined">ἔτους</unclear> 

    <lb n="2"/><supplied cert="high" reason="lost">ἐνάτου καὶ</supplied> εἰκο<supplied cert="high" reason="lost">στοῦ, ἐφ᾽ ἱ</supplied>ερέως Ἀντιόχ<supplied cert="high" reason="lost">ου τοῦ Κέββα</supplied> 

    <lb n="3"/><supplied cert="high" reason="lost">Ἀλεξάνδρου καὶ θεῶν Ἀδελφῶν, κανηφόρου Ἀρσινόης Φιλα</supplied>-

    <lb n="4"/><supplied cert="high" reason="lost">δέλφου Δημονίκης τῆς Φίλωνος, μηνὸς Δίου, ἐν Μ</supplied><unclear cert="high" reason="undefined">ερ</unclear><supplied cert="high" reason="lost">μέρθοις</supplied> 

    <lb n="5"/><supplied cert="high" reason="lost">τοῦ Ὀξυρυγχίτου.  ἐδάνεισεν Πυθο</supplied>κλῆς Ἱπποδάμ<supplied cert="high" reason="lost">ου, Ἀμ</supplied>-

    <lb n="6"/><supplied cert="high" reason="lost">μωνιεύς, γραμματεὺς τ</supplied>ῆς Νικάδ<supplied cert="high" reason="lost">α</supplied> ἱππαρχίας, Σπο<supplied cert="high" reason="lost">κῆι</supplied> 

    <lb n="7"/>Βίθυος Θραικὶ τῆς <supplied cert="high" reason="lost">ἐπιγο</supplied>νῆ<supplied cert="high" reason="lost">ς</supplied> ἱππ<supplied cert="high" reason="lost">εῖ</supplied>, ἀπὸ τοῦ <supplied cert="high" reason="lost">βασιλι</supplied>-

