    <lb n="28"/>τὰ <gap reason="illegible" extent="4" unit="character"/><gap reason="illegible" extent="2" unit="character"/> κομισ<unclear reason="undefined" cert="high">θέ</unclear>ντ<unclear reason="undefined" cert="high">α</unclear> <unclear reason="undefined" cert="high">μο</unclear>ι γράμμα-

    <lb n="29"/>τα <unclear reason="undefined" cert="high">πτ</unclear>ε<unclear reason="undefined" cert="high">ρ</unclear>αι ἀνέγνων ἅπερ προλα-

