r><text><body><head lang="en"><date>AD326</date>
<placeName cert="high">Oxyrhynchus</placeName></head><div lang="grc" part="N" type="edition"><ab part="N">
    <lb n="1"/>ὑπατείας τῶν δεσποτῶν ἡμῶ<supplied cert="high" reason="lost">ν Κ</supplied>ωνσταντίνου τοῦ <expan>Σ[εβ<ex cert="high">αστοῦ</ex></expan> τὸ <num value="7">ζ</num>] <unclear cert="high" reason="undefined">κα</unclear>ὶ Κωνσταντίνου [τοῦ ἐπιφανεστάτου Καίσαρος τὸ <num value="1">α</num>, <gap extent="unknown" reason="illegible" unit="character"/> ]<foreign lang="la">. </foreign>

    <lb n="2"/>Αὐρήλιοι Ὡρίων καὶ Εὐδαίμων <supplied cert="high" reason="lost">καὶ</supplied> Εὐλόγιος οἱ <choice><corr>τρεῖς</corr><sic>τρις</sic></choice> <num value="3"/> ἐκ πατ<supplied cert="high" reason="lost">ρὸς Δ</supplied>ιονυσίου Διοσκουρί<supplied cert="high" reason="lost">δου </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ὁ προ</supplied>-

    <lb n="3"/>δηλούμενος ἡμῶν πατὴρ Διον<supplied cert="high" reason="lost">ύσιο</supplied>ς τὸν βίον διεξ<unclear cert="high" reason="undefined">ῆ</unclear>λθεν, <choice><corr>ἡμεῖς</corr><sic>ημις</sic></choice> δὲ οἱ τούτου <choice><corr>παῖδες</corr><sic>πεδαις</sic></choice> <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> εὐ</supplied>-

    <lb n="4"/><unclear cert="high" reason="undefined">τρε</unclear>πίσ<unclear cert="high" reason="undefined">αι</unclear> ἡμᾶς ἀποσχέσθαι τῆ<supplied cert="high" reason="lost">ς</supplied> πρὸς ἀλλήλους <choice><corr>φιλονικίας</corr><sic>φιλονηκειας</sic></choice> καὶ διάλυσιν ηκ<unclear cert="high" reason="undefined">ε</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>ἔδοξε<unclear cert="high" reason="undefined">ν</unclear> <supplied cert="high" reason="lost">ἡ</supplied>μᾶς τοὺς <choice><corr>τρεῖς</corr><sic>τρις</sic></choice> <num value="3"/> ἐπὶ τῶν ἑ<supplied cert="high" reason="lost">α</supplied>υτῶν μερῶν τὴν διάλυσιν ποιήσασθαι πρὸς ἑα[υτοὺς <gap extent="unknown" reason="illegible" unit="character"/> Ὡρίωνι<note lang="en" place="not specified">?</note>] 

    <lb n="6"/>καὶ Εὐδαίμωνι ἀργυρίου τάλαντα <supplied cert="high" reason="lost">ἑβ</supplied>δομήκοντα <choice><corr>πέντε</corr><sic>πεται</sic></choice> <num value="75"/> πρὸς τὸ <choice><corr>ἔχειν</corr><sic>εχιμ</sic></choice> <choice><corr>με</corr><sic>μαι</sic></choice> τοῦ μέρ<supplied cert="high" reason="lost">ους </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/>ἀρουρῶν τὰς μὲν οὔσας ἐπ᾽ ὀνόμ<supplied cert="high" reason="lost">α</supplied>τος τοῦ πατρὸς περὶ κώμην Σενέπτα <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> πα</supplied>-

    <lb n="8"/><choice><corr>ραχωρεῖν</corr><sic><supplied cert="high" reason="lost">πα</supplied>ραχωριν</sic></choice> <choice><corr>ὅσον</corr><sic><unclear cert="high" reason="undefined">ο</unclear>σων</sic></choice> ἐὰν ἢ <choice><corr>ἡμεῖς</corr><sic>ημις</sic></choice> δύο <num value="2"/> τε Ὡρίων καὶ Εὐδαίμων πρὸς τὴν τ<unclear cert="high" reason="undefined">ρ</unclear>ίτη<unclear cert="high" reason="undefined">ν</unclear> <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/><unclear cert="high" reason="undefined">τ</unclear>οῦ ἐκδικ<unclear cert="high" reason="undefined">ουμέν</unclear>ου ὑφ᾽ ἡμῶν μ<unclear cert="high" reason="undefined">έ</unclear><supplied cert="high" reason="lost">ρο</supplied>υς ἥμισυ μέρος, ὅσον ἐ<unclear cert="high" reason="undefined">ὰ</unclear>ν ἐπὶ δὲ τῶν ἀρουρ<supplied cert="high" reason="lost">ῶν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/><unclear cert="high" reason="undefined">σ</unclear><gap extent="3" reason="illegible" unit="character"/><gap extent="9" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">αιτ</unclear><gap extent="25" reason="lost" unit="character"/>ν <choice><corr>ἥμισυ</corr><sic>ημισου</sic></choice> <unclear cert="high" reason="undefined">πρ</unclear>οσω<unclear cert="high" reason="undefined">π</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ερισ</unclear><gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">συ</unclear><gap extent="3" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

