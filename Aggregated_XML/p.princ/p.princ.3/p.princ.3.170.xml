    <lb n="1"/><g type="stauros"/> ἐκέλευσεν ὁ ἐμὸς δεσπότης ὅτι πέμπω διὰ τὰ καμ<unclear cert="high" reason="undefined">ήλ</unclear><supplied cert="high" reason="lost">ια</supplied> 

    <lb n="2"/>οὐκ ἐπέμψατε <choice><corr>καὶ</corr><sic>καν</sic></choice> νῦν καταξιώσατε πέμψαι δι᾽ <unclear cert="high" reason="undefined">αὐ</unclear><supplied cert="high" reason="lost">τῶν </supplied><gap extent="3" reason="lost" unit="character"/> 

    <lb n="3"/>εἰς ταύτην τὴν κώμην καὶ ἔλαβεν ἄλλα καμήλια <gap extent="3" reason="lost" unit="character"/> 

    <lb n="4"/>μὴ οὖν ἀμελήσατε τὸ παραυτὰ πέμψαι δι᾽ αὐτὰ ἐπι<gap extent="4" reason="illegible" unit="character"/> 

    <lb n="5"/>ἐπὶ τοῦ παρόντος τάδε κέντρια τοῦ ὀκ<unclear cert="high" reason="undefined">ν</unclear>ολ<unclear cert="high" reason="undefined">αδ</unclear><gap extent="2" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/> 

    <lb n="6"/>παραυτὰ καταξιώσατε πέμψαι τόδε πιτ<g type="apostrophe"/>τάκιο<supplied cert="high" reason="lost">ν </supplied><gap extent="4" reason="lost" unit="character"/> 

    <lb n="7"/>καταξιώσατε πέμψαι ἵνα λάβω τὸ ὁλοκότ<g type="apostrophe"/>τινο<supplied cert="high" reason="lost">ν</supplied> 

    <lb n="8"/><gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/> τὸ πιτ<g type="apostrophe"/>τάκ<supplied cert="high" reason="lost">ιον </supplied><gap extent="10" reason="lost" unit="character"/> 

    <lb n="9"/><gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/> τὸ ὁλοκότ<g type="apostrophe"/>τινον παραυτὰ <unclear cert="high" reason="undefined">κ</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/> 
