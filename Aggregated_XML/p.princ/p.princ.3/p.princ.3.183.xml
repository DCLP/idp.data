    <lb n="1"/><supplied cert="high" reason="lost">ὑπατείας Φλαουίων Ἀμαν</supplied>τίου καὶ Ἀλβίνου τῶν <expan>λαμπρ<ex cert="high">οτάτων</ex></expan> 

    <lb n="2"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τοῦ ἐπ</supplied>ιτρόπου Φλαουίου Θεοδώρου 

    <lb n="3"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>τα ἐκ λοιπάδων κατατρέχοντα 

    <lb n="4"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Φ</supplied>λαούιος Φαυστῖνος ἐξάκτορσιν 

    <lb n="5"/><gap extent="unknown" reason="lost" unit="character"/>ου <choice><corr>ἐξαποστεῖλαι</corr><sic>εξαποστιλαι</sic></choice> τὰ ἐκ λοιπάδων 

    <lb n="6"/><gap extent="unknown" reason="lost" unit="character"/>εως τῆς τρίτης παρελθούσης 

    <lb n="7"/><supplied cert="high" reason="lost">ἰνδικτίωνος </supplied><gap extent="unknown" reason="lost" unit="character"/>ος πρὸς τὴν ἐμὴν καθοσίωσιν 

    <lb n="8"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">β</unclear><gap extent="2" reason="lost" unit="character"/>ιων ὃ προύταξεν τῶν 

    <lb n="9"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ρ</unclear>ίων τῶν πρόσφατον 

    <lb n="10"/><gap extent="unknown" reason="lost" unit="character"/> πρὸς ὑμᾶς γέγραφα 

    <lb n="11"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">λοτοντον</unclear> <choice><corr>ἐξαποστεῖλαι</corr><sic>εξαποστιλαι</sic></choice> 

    <lb n="12"/><gap extent="unknown" reason="lost" unit="character"/>ι τῶν ἑαυτῶν ὑπαρχόν-

    <lb n="13"/><supplied cert="high" reason="lost">των </supplied><gap extent="unknown" reason="lost" unit="character"/>γραμ<supplied cert="high" reason="lost">ματ</supplied>εύσαντες σπουδάσατε 

    <lb n="14"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">μπ</unclear>ερισιν ἀποσταλέντα ἰνδικὰ ἐκ πεμπ<unclear cert="high" reason="undefined">τ</unclear>-

    <lb n="15"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἔρ</supplied>ρωσθε τὸ <unclear cert="high" reason="undefined">καὶ</unclear> <gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="illegible" unit="character"/> 

    <lb n="16"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τῆ</supplied>ς <num value="4">δ</num> <expan>ἰνδικ<ex cert="high">τίωνος</ex></expan> καὶ τὰ ἐκ λοιπαζόν-

    <lb n="17"/><supplied cert="high" reason="lost">των </supplied><gap extent="unknown" reason="lost" unit="character"/>εας ἐπὶ τοσούτῳ χρόνῳ <abbr>δεδραμ</abbr> 

    <lb n="18"/><gap extent="unknown" reason="lost" unit="character"/> χρόνον <choice><corr cert="low">ἀπεσταλκέναι</corr><sic>αποσταλκιται</sic></choice> εἰς τὴν Ἀλεξανδρέων 

    <lb n="19"/><supplied cert="high" reason="lost">πόλιν </supplied><gap extent="unknown" reason="lost" unit="character"/><num value="500">φ</num> ἐξαπέσταλκε τοὺς <choice><corr>κ<unclear cert="high" reason="undefined">ατα</unclear>ναγκάσαντας</corr><sic>κ<unclear cert="high" reason="undefined">ατα</unclear>νακασαντας</sic></choice> 

    <lb n="20"/><gap extent="unknown" reason="lost" unit="character"/> εἰς τὴν Ἀλεξανδρέων ἀκολούθως τῷ 

    <lb n="21"/><gap extent="unknown" reason="lost" unit="character"/><unclear cert="high" reason="undefined">αν</unclear>ολα τὰ εἴδη <choice><corr>ἐξαποστεῖλαι</corr><sic>εξαποστιλαι</sic></choice> <choice><corr>προστέτακται</corr><sic>προσταιτακται</sic></choice> 
