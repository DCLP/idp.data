    <lb n="15"/>ῥητ<supplied cert="high" reason="lost">ίνη</supplied>ς                          <expan>μν<ex cert="high">αῖ</ex></expan> <num value="2">β</num> <gap extent="unknown" reason="lost" unit="character"/> 

