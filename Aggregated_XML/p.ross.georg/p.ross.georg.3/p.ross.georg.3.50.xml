    <lb n="1"/><unclear reason="undefined" cert="high">η</unclear><gap reason="lost" extent="11" unit="character"/>ων τ<gap reason="lost" extent="14" unit="character"/> 

    <lb n="2"/>Ἐ<supplied reason="lost" cert="high">π</supplied>εὶφ <num value="26"><unclear reason="undefined" cert="high">κ</unclear>ϛ</num> δευτέρας <expan>ἰν<ex cert="high">δικτίονος</ex></expan> ἐν <expan>Ἀρ<ex cert="high">σινόῃ</ex></expan> <g type="stauros"/> 

