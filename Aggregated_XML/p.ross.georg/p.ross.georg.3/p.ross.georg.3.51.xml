    <lb n="1"/>[<g type="chirho"/> ἐν ὀν]ό<unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">α</supplied>τ<supplied cert="high" reason="lost">ι τοῦ κυρίου καὶ δ</supplied>εσπότου <expan>Ἰησο<ex cert="high">ῦ</ex></expan> 

    <lb n="2"/><supplied cert="high" reason="lost">Χριστοῦ</supplied> τοῦ θεοῦ καὶ <supplied cert="high" reason="lost">σωτ</supplied>ῆ<unclear cert="high" reason="undefined">ρ</unclear>ος ἡμῶν βασιλείας 

    <lb n="3"/><supplied cert="high" reason="lost">τοῦ εὐσ</supplied>εβεστάτου καὶ φ<supplied cert="high" reason="lost">ι</supplied>λ<supplied cert="high" reason="lost">αν</supplied>θρώπ<supplied cert="high" reason="lost">ου</supplied> ἡμῶν <expan>δεσπότο<ex cert="high">υ</ex></expan> 

    <lb n="4"/><supplied cert="high" reason="lost">καὶ μεγί</supplied>στου εὐεργέτου <expan>[Φλ<ex cert="high">αυίου</ex>]</expan> <unclear cert="high" reason="undefined">Ἡρ</unclear><supplied cert="high" reason="lost">ακ</supplied>λείου τοῦ <expan>αἰωνίο<ex cert="high">υ</ex></expan> 

    <lb n="5"/>Αὐγ<supplied cert="high" reason="lost">ούστ</supplied>ου καὶ Αὐτοκρ<supplied cert="high" reason="lost">άτορος ἔ</supplied>τους <expan>εἰκοστο<ex cert="high">ῦ</ex></expan> 

    <lb n="6"/><space dim="horizontal" extent="unknown" unit="character"/> <supplied cert="high" reason="lost">Φα</supplied>μενὼθ [<gap extent="2" reason="illegible" unit="character"/> τετάρτης <expan>ἰ<ex cert="high">ν</ex>δ<ex cert="high">ικτίονος</ex></expan>] ἐν <expan>Ἀρ[σιν<ex cert="high">όῃ</ex>]</expan> 

    <lb n="7"/>Α<supplied cert="high" reason="lost">ὐ</supplied>ρήλιος Μηνᾶς υἱὸς <supplied cert="high" reason="lost">Ἀπολλῶ</supplied> <unclear cert="high" reason="undefined">ἀ</unclear>πὸ ἐποικίου <unclear cert="high" reason="undefined">Κ</unclear>υρᾶς 

    <lb n="8"/>Μ<supplied cert="high" reason="lost">αρί</supplied>ας μετ᾽ ἐγγυήτου ἄ<supplied cert="high" reason="lost">πα Ἀπολ</supplied><unclear cert="high" reason="undefined">λ</unclear>ῶ υἱοῦ Βίκτορος 

    <lb n="9"/>ἀπὸ ἐποικί<supplied cert="high" reason="lost">ου</supplied> Κυρ<unclear cert="high" reason="undefined">ίο</unclear>υ Θεο<supplied cert="high" reason="lost">ῦ</supplied> τ<supplied cert="high" reason="lost">ῷ λαμπρ</supplied>οτ<supplied cert="high" reason="lost">ά</supplied><unclear cert="high" reason="undefined">τ</unclear>ῳ Ἄππα Κύρῳ 

    <lb n="10"/>ὀσ<unclear cert="high" reason="undefined">τι</unclear>αρίῳ υἱῷ τοῦ μακαρ<supplied cert="high" reason="lost">ίου Θ</supplied>εοδώρου ἀπὸ τῆς 

    <lb n="11"/>Ἀρσινοιτῶν πόλεως <expan>χ<ex cert="high">αίρειν</ex></expan>. ὁ<supplied cert="high" reason="lost">μ</supplied>ολογῶ <supplied cert="high" reason="lost">μ</supplied>εμι<unclear cert="high" reason="undefined">σθῶσ</unclear>θαι 

    <lb n="12"/>παρὰ <supplied cert="high" reason="lost">σ</supplied>οῦ τὸ δια<supplied cert="high" reason="lost">φ</supplied>έρον σοι ἐν π<supplied cert="high" reason="lost">ε</supplied>δίῳ τ<supplied cert="high" reason="lost">οῦ αὐ</supplied>το<unclear cert="high" reason="undefined">ῦ</unclear> 

    <lb n="13"/>ἐποικίου Κυρᾶς Μ<supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ίας ἐν κλήρῳ</supplied> <unclear cert="high" reason="undefined">κ</unclear>αλουμένῳ 

    <lb n="14"/>τοῦ υἱοῦ Ἰούλου ἀμ<supplied cert="high" reason="lost">πελικὸν χωρίον ἀρού</supplied><unclear cert="high" reason="undefined">ρη</unclear>[ς <expan>τ]ετάρτ<ex cert="high">ου</ex></expan> 

    <lb n="15"/>πλέον ἔλαττο<supplied cert="high" reason="lost">ν με</supplied>τ<supplied cert="high" reason="lost">ὰ παν</supplied>τ<supplied cert="high" reason="lost">ὸς αὐ</supplied>τ<supplied cert="high" reason="lost">οῦ</supplied> <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">οῦ δι</supplied>καίου 

    <lb n="16"/>ἐφ᾽ ὅσον χρόνον βο<supplied cert="high" reason="lost">ύ</supplied>λει ἀπὸ καρπ<unclear cert="high" reason="undefined">ῶν</unclear> τ<supplied cert="high" reason="lost">ῆ</supplied><unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">σ</unclear>ὺ<unclear cert="high" reason="undefined">ν</unclear> <expan>θ<ex cert="high">εῷ</ex></expan> τετάρτης <expan>ἰ<ex cert="high">ν</ex>δ<ex cert="high">ικτίονος</ex></expan> 

    <lb n="17"/><supplied cert="high" reason="lost">π</supplied>ρὸς τό με <choice><corr>ἀπεργάσ<supplied cert="high" reason="omitted">ασ</supplied>θαι</corr><sic>απεργασθαι</sic></choice> ε<unclear cert="high" reason="undefined">ἰ</unclear><supplied cert="high" reason="lost">ς</supplied> τὸ αὐτὸ χωρ<unclear cert="high" reason="undefined">ίο</unclear>ν καὶ πᾶσαν 

    <lb n="18"/><supplied cert="high" reason="lost">ἀ</supplied>μπελουργικὴν ἐργασίαν ἐν<supplied cert="high" reason="lost">δεί</supplied>ξασθ<supplied cert="high" reason="lost">αι</supplied> ἐν <supplied cert="high" reason="lost">τ</supplied>ούτῳ <supplied cert="high" reason="lost">δ</supplied>εόντως 

    <lb n="19"/>καὶ ἀκαταγνώστως καὶ ἀ<unclear cert="high" reason="undefined">καταφ</unclear><supplied cert="high" reason="lost">ρ</supplied>ο<supplied cert="high" reason="lost">ν</supplied><unclear cert="high" reason="undefined">ή</unclear>τ<supplied cert="high" reason="lost">ω</supplied><unclear cert="high" reason="undefined">ς</unclear> <unclear cert="high" reason="undefined">ἐπι</unclear>σκ<gap extent="1" reason="lost" unit="character"/> 

    <lb n="20"/>σὺν <expan>θ<ex cert="high">εῷ</ex></expan> περιγινομένης ἐξ αὐτ<supplied cert="high" reason="lost">οῦ</supplied> ῥύσεως τοῦ <unclear cert="high" reason="undefined">οἴνου</unclear> εν<unclear cert="high" reason="undefined">ε</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/> 

    <lb n="21"/>διαφέρειν σοι μέρη δύ<supplied cert="high" reason="lost">ο</supplied> <num value="2"/> κ<supplied cert="high" reason="lost">αὶ ἐμο</supplied>ὶ μέρ<supplied cert="high" reason="lost">ο</supplied>ς ἕν <num value="1"/>. ἔ<supplied cert="high" reason="lost">σ</supplied>χον δὲ 

    <lb n="22"/><unclear cert="high" reason="undefined">εἰ</unclear>ς ἀπεργασίαν <choice><corr>ξοΐδα</corr><sic>ξο<hi rend="diaeresis">ι</hi>δ<supplied cert="high" reason="lost">α</supplied>ν</sic></choice> μία[ν <num value="1"/>. εἰ δ]ὲ <unclear cert="high" reason="undefined">π</unclear>ε<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ι</supplied>φρονήσω εἰς τὴν 

    <lb n="23"/>ἀπεργασίαν <unclear cert="high" reason="undefined">τοῦ</unclear> αὐτοῦ χ<supplied cert="high" reason="lost">ωρίου</supplied> ἢ <supplied cert="high" reason="lost">κατ</supplied><unclear cert="high" reason="undefined">α</unclear>γνώσθω εἰς <expan>βλάβη<ex cert="high">ν</ex></expan> 

    <lb n="24"/>καρποῦ αὐτοῦ ἐπὶ τῷ σε <unclear cert="high" reason="undefined">ἀπαγαγ</unclear>έσθαι με ἐξ αὐτοῦ δίχ<unclear cert="high" reason="undefined">α</unclear> 

    <lb n="25"/>γεωρ<unclear cert="high" reason="undefined">γ</unclear>ί<supplied cert="high" reason="lost">ας</supplied> καὶ <choice><corr>ἐὰν</corr><sic><unclear cert="high" reason="undefined">καν</unclear></sic></choice> φθάσω τὸ<unclear cert="high" reason="undefined">ν</unclear> <supplied cert="high" reason="lost">καιρὸν τ</supplied>ῆς τρ<supplied cert="high" reason="lost">ύ</supplied>γης. 

    <lb n="26"/>ἡ μίσθωσις κυρί<supplied cert="high" reason="lost">α</supplied> καὶ <expan>ἐ<supplied cert="high" reason="lost">πε</supplied><unclear cert="high" reason="undefined">ρ</unclear><ex cert="high">ωτηθεὶς</ex></expan> <expan>[ὡμ<ex cert="high">ολόγησα</ex>]</expan> <g type="chirho"/> Αὐρήλιος 

    <lb n="27"/>Μηνᾶς υἱὸς Ἀπολλῶ στοιχεῖ μοι πάντα ὡς πρόκειται. 

