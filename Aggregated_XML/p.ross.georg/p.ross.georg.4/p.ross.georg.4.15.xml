    <lb n="1"/>περὶ τῆς ζημίας τῶν ὑπουργ<supplied reason="lost" cert="high">ῶν τῆς διοικήσεώς σου ἁπτομένων ἀπὸ</supplied> 

    <lb n="2"/>κληρονομίας τινῶν τελ<supplied reason="lost" cert="high">ευ</supplied>τ<supplied reason="lost" cert="high">ώντων εὐπόρων προσώπων</supplied> 

    <lb n="3"/>αὐτῆς<g type="apostrophe"/> καὶ ὁ διαπραττόμενος <supplied reason="lost" cert="high">ἐν</supplied> τ<supplied reason="lost" cert="high">ούτοις παρ᾽ ἅπερ ἐθεματίσαμεν</supplied> 

    <lb n="4"/>ὑπάρχει ὑπαί<g type="apostrophe"/>τιος μεγάλη<supplied reason="lost" cert="high">ς ἀν</supplied>ταπο<supplied reason="lost" cert="high">δόσεως ὑπὲρ παρακοῆς. λοιπὸν</supplied> 

    <lb n="5"/>μὴ δόξῃ σοι ἅψασθαι ἀπὸ κληρο<supplied reason="lost" cert="high">νομίας τῶν τοιούτων προσώπων</supplied> 

    <lb n="6"/>ἕως ἑνὸς μιλιαρισίου καὶ μόνου καὶ καταχ<supplied reason="lost" cert="high">ρῆσαι εἴς τι τῶν δημοσίων</supplied> 

    <lb n="7"/>στίχων. ἐὰν δὲ τελευτήσῃ τ<supplied reason="lost" cert="high">ις τῶν παγαρχιτῶν γράψον ἡμῖν περὶ <expan>αὐτ<ex cert="high">οῦ</ex></expan></supplied> 

    <lb n="8"/>σαφηνίζων ἐν τοῖς αὐτοῖς γράμμασιν <supplied reason="lost" cert="high">τά τε γῄδια αὐτοῦ καὶ τὸ πόσον</supplied> 

    <lb n="9"/>ὃ <choice><corr>ἐχείρισεν</corr><sic>εχειρησεν</sic></choice> καὶ<g type="apostrophe"/> τοὺς κληρονόμ<supplied reason="lost" cert="high">ους</supplied> τ<supplied reason="lost" cert="high">ῆς οὐσίας αὐτοῦ φανεροποιῶν ἡμῖν</supplied> 

