    <lb n="18"/><gap extent="19" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ς παρὰ τὰ <choice><corr>ἀπειρημένα</corr><sic><supplied cert="high" reason="lost">τ</supplied>α<unclear cert="high" reason="undefined">πι</unclear>ρη<supplied cert="high" reason="lost">μ</supplied>ενα</sic></choice> α<unclear cert="high" reason="undefined">ὐ</unclear><supplied cert="high" reason="lost">τ</supplied>ῶν ποιούντω<unclear cert="high" reason="undefined">ν</unclear> <gap extent="4" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/> ἄδελφε 

    <lb n="19"/><gap extent="4" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐπέμφθη πρὸ δέκα <num value="10"/> καλανδῶν</supplied> Φεβραρ<supplied cert="high" reason="lost">ίω</supplied><unclear cert="high" reason="undefined">ν</unclear> <add place="supralinear">Τῦβι <num value="28">κη</num></add> Βραδουᾷ καὶ <supplied cert="high" reason="lost">Ο</supplied>ὐάρ<unclear cert="high" reason="undefined">ῳ</unclear> <supplied cert="high" reason="lost">ὑπ</supplied><unclear cert="high" reason="undefined">άτ</unclear>οις. <expan><ex cert="high">ἔτους</ex></expan> <num value="23"><unclear cert="high" reason="undefined">κγ</unclear></num> Ἀντω<supplied cert="high" reason="lost">νίνου</supplied>. </ab></div></div><div n="meta.12890" part="N" type="description"><p><rs cert="high" type="material">Papyrus</rs></p></div><div n="meta.12890" part="N" subtype="textDate" type="commentary"><p><date notAfter="160" notBefore="160" type="textDate" value="0160-01-30">30. Jan. 160</date></p></div><div n="meta.12890" part="N" subtype="general" type="commentary"><p>Zur Datierung vgl. ZPE 107, 1995, S. 83.</p></div><div n="meta.12890" part="N" subtype="mentionedDates" type="commentary"><head>Erwähnte Daten</head><p>Z. 5-6: <date type="mentioned" value="0160-01-24">24. Jan. 160</date></p></div><div n="meta.12890" part="N" subtype="principalEdition" type="bibliography"><listBibl><bibl subtype="principal" type="publication"><title level="s" type="abbreviated">P.Ross. Georg.</title><biblScope type="volume">2</biblScope><biblScope type="numbers">26</biblScope></bibl><bibl type="Trismegistos"><biblScope type="numbers">12890</biblScope></bibl><bibl type="DDbDP"><series>0197</series>; <biblScope type="volume">2</biblScope>; <biblScope type="numbers">26</biblScope></bibl></listBibl><p>Perseus links: <xref evaluate="all" from="ROOT" href="0197;2;26" targOrder="Y" to="DITTO" type="Perseus">Text in Perseus</xref></p></div><div n="meta.12890" part="N" subtype="illustrations" type="