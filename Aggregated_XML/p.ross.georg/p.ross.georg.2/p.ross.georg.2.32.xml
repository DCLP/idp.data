<text><body><head lang="en"><date>IIspc</date>
<placeName cert="high">Arsinoiton Polis, Krokodilopolis, Ptolemais Euergetis</placeName></head><div lang="grc" part="N" type="edition"><ab part="N">
    <lb n="1"/><gap extent="unknown" reason="lost" unit="character"/><abbr>δ</abbr> 

    <lb n="2"/><space dim="horizontal" extent="unknown" unit="character"/> 

    <lb n="1"/><supplied cert="high" reason="lost">ἔτου</supplied>ς ἐνάτου Αὐτοκράτορο<supplied cert="high" reason="lost">ς Καίσαρος </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> μηνὸς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/><supplied cert="high" reason="lost">ἐν Πτ</supplied>ολεμα<hi rend="diaeresis">ί</hi>δι Εὐεργέτιδι τ<supplied cert="high" reason="lost">οῦ Ἀρσινοείτου νομοῦ. ὁμολογεῖ Ἰσιδώρα τοῦ δεῖνος </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ὡς <expan><ex cert="high">ἐτῶν</ex></expan> </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> οὐλὴ </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/><gap extent="3" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀ</supplied>ριστερᾶι μετὰ κυρίου τ<supplied cert="high" reason="lost">οῦ δεῖνος </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ὡς <expan><ex cert="high">ἐτῶν</ex></expan> </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> οὐ</supplied>-

    <lb n="4"/><supplied cert="high" reason="lost">λὴ πή</supplied>χει δεξιῶι Μαρεῖτι Λευ<gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> πεπρακέναι τὴν Ἰσιδώραν κατὰ τήν</supplied>-

    <lb n="5"/><supplied cert="high" reason="lost">δε τ</supplied>ὴν ὁμολογίαν ἀπὸ τοῦ <supplied cert="high" reason="lost">νῦν ἐπὶ τὸν ἅπαντα χρόνον ἀπὸ τοῦ ὑπάρχοντος τῆι</supplied> 

    <lb n="6"/><supplied cert="high" reason="lost">Ἰσι</supplied>δώραι ἀγοραστοῦ ἡμίσο<supplied cert="low" reason="lost">υς μέρους οἰκίας </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> καὶ τῶν</supplied> 

    <lb n="7"/><supplied cert="high" reason="lost">συν</supplied>κυρόντων πάντων ἐν <supplied cert="high" reason="lost">μίᾳ <num value="1"/> σφραγῖδι </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/><gap extent="2" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τ</supplied>οῦ δρόμου τοῦ Σούχου βου<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/><gap extent="3" reason="lost" unit="character"/> ἐπ᾽ ἀμφοτέρων δὲ τῶν <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/><gap extent="3" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/> Ἡρώδου, μετάξυ δὲ τῶ<supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀπέχειν δὲ τὴν ισιδώ</supplied>-

    <lb n="11"/><supplied cert="high" reason="lost">ραν</supplied> τὴν συνκεχωρημ<unclear cert="high" reason="undefined">έ</unclear>ν<supplied cert="high" reason="lost">ην τιμὴν ἀργυρίου δραχμὰς </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="12"/><gap extent="3" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐ</supplied>κ πλήρους διὰ τῆ<supplied cert="high" reason="lost">ς ἐν </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> τραπέζης καὶ βεβαιώσειν τὴν ὁμολογοῦ</supplied>-

    <lb n="13"/><supplied cert="high" reason="lost">σαν κ</supplied>αὶ τοὺς παρ᾽ αὐτῆ<supplied cert="high" reason="lost">ς πάσῃ βεβαιώσει </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="14"/><gap extent="unknown" reason="lost" unit="character"/>αν<gap extent="2" reason="illegible" unit="character"/> φ<unclear cert="high" reason="undefined">ακο</unclear><gap extent="unknown" reason="lost" unit="character"/> 

