    <lb n="2"/><supplied cert="high" reason="lost">παρὰ </supplied><gap extent="8" reason="lost" unit="character"/>σ<gap extent="2" reason="lost" unit="character"/>ης Σαραπίωνος ἀσ<unclear cert="high" reason="undefined">τ</unclear>ῆς <gap extent="17" reason="lost" unit="character"/> 

    <lb n="3"/><gap extent="3" reason="lost" unit="character"/><supplied cert="high" reason="lost"> πρὸς τοὺς ἀ</supplied>δ<supplied cert="high" reason="lost">ε</supplied>λφούς μου καὶ πρὸς Ἑρμίαν ἀγ<supplied cert="high" reason="lost">οραστὴν </supplied><gap extent="7" reason="lost" unit="character"/> 

    <lb n="4"/><gap extent="12" reason="lost" unit="character"/> πράσεως αὐτοῦ πλεῖσταί μου ἀ<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">τικαταστάσεις</supplied> 

    <lb n="5"/><supplied cert="high" reason="lost">ἐγένοντο ἐπὶ </supplied><gap extent="1" reason="lost" unit="character"/><unclear cert="high" reason="undefined">ω</unclear>ρηιου <choice><corr>Κελεάριος</corr><sic>κελεαρις</sic></choice> γενομένου ὑπ<supplied cert="high" reason="lost">ομνηματογράφου</supplied> 

    <lb n="6"/><supplied cert="high" reason="lost">καὶ ἐπὶ τῶν κε</supplied>κριμένων, ὃς ἀνάδαστον τὸν ἀ<supplied cert="high" reason="lost">γορασμὸν ε᾿ποίη</supplied>-

    <lb n="7"/><supplied cert="high" reason="lost">σεν τῆς τιμῆς</supplied> <unclear cert="high" reason="undefined">φ</unclear>ανείσης ἀδότου κατὰ τὴν γενο<supplied cert="high" reason="lost">μένην ἐξέτασιν. ὁ</supplied> 

