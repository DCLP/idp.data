    <lb n="12"/><supplied cert="low" reason="lost">στάσεως</supplied><gap extent="4" reason="lost" unit="character"/> τοῦ ἀγοραστοῦ Ἑρμία ἐκ προκηρύξε<supplied cert="high" reason="lost">ως </supplied><gap extent="9" reason="lost" unit="character"/> 

    <lb n="13"/><gap extent="6" reason="lost" unit="character"/><supplied cert="high" reason="lost"> βιβλί</supplied>διον εἰς τὸ <supplied cert="high" reason="lost">δί</supplied>κας <choice><corr>ἡμῖν</corr><sic>ημειν</sic></choice> εἰσάγειν <gap extent="11" reason="lost" unit="character"/> 

    <lb n="14"/><gap extent="11" reason="lost" unit="character"/>σθαι καὶ κυρ<supplied cert="high" reason="lost">ω</supplied>θεὶς ὁ Ἑρμίας κατας<gap extent="11" reason="lost" unit="character"/> 

    <lb n="15"/><gap extent="8" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐξ ἧ</supplied>ς ἔσχομ<unclear cert="high" reason="undefined">εν</unclear> πρὸς αὐτὸν ἀντικατα<supplied cert="high" reason="lost">στάσεως τῇ δε</supplied>-

    <lb n="16"/><supplied cert="high" reason="lost">δομένῃ προθ</supplied>εσμίᾳ τὴν <choice><corr>τιμὴν</corr><sic><supplied cert="high" reason="lost">τ</supplied>ειμην</sic></choice> πᾶσαν οὐκ ἐθέ<supplied cert="low" reason="lost">λων δοῦναι </supplied><gap extent="2" reason="lost" unit="character"/> 

    <lb n="17"/><gap extent="11" reason="lost" unit="character"/> ἑξῆς κατὰ <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ὰ</supplied> <unclear cert="high" reason="undefined">κ</unclear>ριθέντα καὶ διὰ τοῦτο μ<gap extent="11" reason="lost" unit="character"/> 

    <lb n="18"/><gap extent="5" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐνέτυχ</supplied>ον πλειστάκις τῷ αὐτῷ ὑπομ<unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">ηματογράφῳ</supplied> 

    <lb n="19"/><gap extent="11" reason="lost" unit="character"/> ὃς ἐκέλευσεν τοῖς περὶ αὐτὸν ὑπ<supplied cert="high" reason="lost">ηρέταις </supplied><gap extent="4" reason="lost" unit="character"/> 

    <lb n="20"/><supplied cert="high" reason="lost">ἐπειδὴ ὁ Ἑρμία</supplied>ς καταφρονήσας τῶν κριθέντων α<gap extent="11" reason="lost" unit="character"/> 

    <lb n="21"/><gap extent="10" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐ</supplied><unclear cert="high" reason="undefined">π</unclear>οίησε, μ<unclear cert="high" reason="undefined">ὴ</unclear> ἀρκεσθεὶς δὲ ἐπὶ τούτοι<unclear cert="high" reason="undefined">ς</unclear> <gap extent="6" reason="lost" unit="character"/><supplied cert="high" reason="lost"> οὐ</supplied> 

    <lb n="22"/><supplied cert="high" reason="lost">τὰς τυχούσα</supplied>ς ὕβρεις μοι παρέχει σὺν τοῖς συνερ<supplied cert="high" reason="lost">γοῦσιν αὐτῷ</supplied> 

    <lb n="23"/><supplied cert="high" reason="lost">ἀδελφοῖς καὶ</supplied> ἑτέροις, ἀξιῶ σε τὸν κύριον ἐλεῆσ<supplied cert="high" reason="lost">αί με ἀδικουμέ</supplied>-

    <lb n="24"/><supplied cert="high" reason="lost">νην ὑπὸ τῶν κακ</supplied>οπρ<unclear cert="high" reason="undefined">α</unclear>γ<unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ό</supplied><unclear cert="high" reason="undefined">νω</unclear>ν ἀ<supplied cert="high" reason="lost">ν</supplied><unclear cert="high" reason="undefined">θ</unclear>ρώπων δι<gap extent="11" reason="lost" unit="character"/> 

    <lb n="25"/><gap extent="4" reason="lost" unit="character"/><supplied cert="high" reason="lost"> οὐκ ἐμμε</supplied><unclear cert="high" reason="undefined">ν</unclear>όντ<unclear cert="high" reason="undefined">ων</unclear> <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">οῖς κ</supplied><unclear cert="high" reason="undefined">ρ</unclear>ιθεῖσι καὶ κελεῦσ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ι </supplied><gap extent="10" reason="lost" unit="character"/> 

    <lb n="26"/><gap extent="13" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἱν᾽ ὦ εὐερ</supplied>γετημένη. 

    <lb n="27"/><space dim="horizontal" extent="unknown" unit="character"/> <handShift new="m2"/> διὰ Ἀπίωνος ἐπι<supplied cert="high" reason="lost">δέδοται</supplied>. 

