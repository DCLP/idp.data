    <lb n="9"/><gap extent="3" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀκολούθως</supplied> τοῖς γενομένοις ὑπομνηματ<supplied cert="high" reason="lost">ισμοῖς </supplied><gap extent="8" reason="lost" unit="character"/> 

    <lb n="10"/><gap extent="11" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/>ντων καὶ ἐπὶ τοῦ κρατίστου ὑπ<supplied cert="high" reason="lost">ομνηματογρά</supplied>-

