    <lb n="1"/>ὅλη<unclear cert="high" reason="undefined">ς</unclear> <choice><corr>γείτονες</corr><sic><supplied cert="high" reason="lost">γι</supplied><unclear cert="high" reason="undefined">τονες</unclear></sic></choice> <unclear cert="high" reason="undefined">νότ</unclear><supplied cert="high" reason="lost">ου </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> βορρᾶ </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀπηλιώτου </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>γῆ Ἁρσιήσιος τοῦ Πεήιτος, ἧς κρατεῖ Πεμσάις Ὥρου, λιβὸς ὄρος καὶ ἀπὸ τῆς <unclear cert="low" reason="undefined">ὑ</unclear>π<supplied cert="low" reason="lost">αρχούσης</supplied><gap extent="6" reason="lost" unit="character"/> 

    <lb n="3"/>οἴκοι ἀπὸ τοῦ ἐπιβάλλοντος αὐτῶι ἡμίσους τὸ <expan><ex cert="high"><unclear cert="high" reason="undefined">τέταρτον</unclear></ex></expan> μέρος τῆς οὔσης ἐν τῆι ἀπὸ ἀπ<supplied cert="high" reason="lost">ηλιώτου </supplied><gap extent="6" reason="lost" unit="character"/><supplied cert="high" reason="lost"> γείτονες</supplied> 

    <lb n="4"/>τῆς οἰκίας νότου ὁ λοιπὸς ψιλὸς τόπος αὐτοῦ Ὥρου, βορρᾶ οἰκία Πασούχου τοῦ Ψενσν<supplied cert="low" reason="lost">έως </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="low" reason="lost"> ἀπηλιώτου </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>ἄλλο ἥμισυ τῆς οἰκίας, ἣ<supplied cert="high" reason="lost">ν</supplied> πέπρακεν Πανομοῦτι, λιβὸς οἰκία Πελίσιος, ὁμοίως δὲ καὶ τὰ ἴδια <gap extent="8" reason="lost" unit="character"/><supplied cert="high" reason="lost"> σὺν</supplied> 

    <lb n="6"/>τῆι ἄλληι συνσκευῆι καὶ τὴν λοιπὴν γεωργικὴν ἀποσκευὴν πᾶσαν ἢ οἱ ἂν ὦσι <supplied cert="high" reason="lost">γείτονες πάντοθεν </supplied><gap extent="7" reason="lost" unit="character"/> 

