    <lb n="16"/>και ἄνευ βλάβους.  θέλησον οὖν <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> δη</supplied>-

    <lb n="17"/>λῶσαι <choice><corr>ἡμῖν</corr><sic>ημειν</sic></choice> τὸ τῆς καταστάσεώ<supplied reason="lost" cert="low">ς σου καὶ εἰ ἀμε</supplied>-

    <lb n="18"/>ριμνεῖς εἰς ἅπαντα <subst><add place="inline">τα</add><del rend="corrected" status="unremarkable">ντα</del></subst> κατὰ σὲ <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="19"/>ἀμεριμνῶς ὦμεν καὶ <unclear reason="undefined" cert="high">δ</unclear>ι<unclear reason="undefined" cert="high">α</unclear>ν<gap reason="illegible" extent="4" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/>καθὼς καὶ ἡμεῖς ὑπὲρ ἁπάντω<supplied reason="lost" cert="high">ν τῶν ἐν τῷ νο</supplied>-

    <lb n="21"/>μῷ σοι <choice><corr>γράφομεν</corr><sic>γραφωμεν</sic></choice> <supplied reason="lost" cert="high">δ</supplied>ή.  τί καὶ ἐδήλ<supplied reason="lost" cert="high">ωσας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="22"/>τερου τῶν <choice><corr>χοίρων</corr><sic>χυρων</sic></choice>; <choice><corr>ὑπέλειψα<supplied reason="omitted" cert="high">ς</supplied></corr><sic>υπελιψα</sic></choice> προσδοκ<supplied reason="lost" cert="high">ῶντα παρά</supplied> 

    <lb n="23"/>σου <choice><corr>σι<supplied reason="omitted" cert="high">τεί</supplied>αν</corr><sic>σια<hi rend="subscript">ν</hi></sic></choice> μὴ ἐχόντων ἡμῶν κριθὰς <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="24"/>μ<unclear reason="undefined" cert="high">ήτ</unclear>ε ἀπαντήσας μήτε <choice><corr>ἀπόκρισι<supplied reason="lost" cert="high">ν</supplied></corr><sic>αποκρισει<supplied reason="lost" cert="high">ν</supplied></sic></choice> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="25"/><choice><corr>τάχιον</corr><sic>ταχειον</sic></choice> οὖν ἀπάντησον πρὸς ἡμ<supplied reason="lost" cert="high">ᾶς ἵνα μετὰ</supplied> 

    <lb n="26"/>χαρᾶς σε ἀπολάβωμεν καὶ εὐθυμήσ<supplied reason="lost" cert="high">ωμέν</supplied> 

    <lb n="27"/>σε. προσαγορεύει σε ἡ καλὴ καὶ σεμνοτάτ<supplied reason="lost" cert="high">η σύμβιος</supplied> 

    <lb n="28"/>καὶ πάντες οἱ ἀδελφοί σου κατ᾽ ὄνομα ολ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="29"/>λκ ὁρίσαι καὶ ἐκδεχόμενοι τῷ ἐλαίῳ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="30"/>καὶ οὐδὲ εἰς <choice><corr>φαγεῖν</corr><sic>φαγαι</sic></choice> <unclear reason="undefined" cert="high">τ</unclear><gap reason="lost" extent="5" unit="character"/><unclear reason="undefined" cert="high">δ</unclear>ειν.  ἐρρωμε<supplied reason="lost" cert="high">νον σε</supplied> 

    <lb n="31"/>προκόπτειν Φιλόξε<supplied reason="lost" cert="high">νε εὔχομαι</supplied> 

    <lb n="32"/>χρόνοις πολλοῖς. 

