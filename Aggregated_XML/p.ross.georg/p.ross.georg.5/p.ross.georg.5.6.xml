    <lb n="11"/><unclear reason="undefined" cert="high">δ</unclear>εύτερόν <choice><corr>σοι</corr><sic>συ</sic></choice> γέγρ<supplied reason="lost" cert="high">αφα </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> οὐδε</supplied>-

    <lb n="12"/>μίαν <choice><corr>ἀπόκρισι<supplied reason="lost" cert="high">ν</supplied></corr><sic>αποκρισει<supplied reason="lost" cert="high">ν</supplied></sic></choice> <gap reason="lost" extent="unknown" unit="character"/> 

