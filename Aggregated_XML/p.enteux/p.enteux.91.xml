    <lb n="1"/><supplied reason="lost" cert="high">βα</supplied>σιλεῖ Πτολεμαίωι χαίρειν <gap reason="lost" extent="12" unit="character"/><supplied reason="lost" cert="high"> ἀδικο</supplied>ῦμαι ὑπὸ Αὐτοβούλου <expan><ex cert="high">ἑκατονταρούρου</ex></expan> τῆς <num value="3">γ</num> <supplied reason="lost" cert="high">ἱππαρχίας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="lost" extent="2" unit="character"/>ναχέως. συνεγδόμενος γὰρ <gap reason="lost" extent="18" unit="character"/>ατρὸς τῆς γυναικὸς ἧι συνοικῶ κ<supplied reason="lost" cert="high">ατὰ συγγραφὴν</supplied> 

    <lb n="3"/><supplied reason="lost" cert="high">συ</supplied>νοικισίου τὴν κειμένην <gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="16" unit="character"/><supplied reason="lost" cert="high"> δε</supplied>δωκότος εἰς τὸ αὑτοῦ μέρος πρ<supplied reason="lost" cert="high">όβατα </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><supplied reason="lost" cert="high">ἐπ</supplied>ίποκα καὶ τὰ <unclear reason="undefined" cert="high">εἰ</unclear>ς τὴν <unclear reason="undefined" cert="high">π</unclear><gap reason="illegible" extent="2" unit="character"/>μ<gap reason="lost" extent="13" unit="character"/><supplied reason="lost" cert="high"> Σεμθέ</supplied>α τὸν ποιμένα ἐκέλευσε ἐγλεξα<gap reason="lost" extent="unknown" unit="character"/> 

