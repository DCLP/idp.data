    <lb n="6"/><gap reason="lost" extent="unknown" unit="character"/>τωι <gap reason="illegible" extent="3" unit="character"/><supplied reason="lost" cert="high">Αὐ</supplied>τοβουλ<gap reason="lost" extent="5" unit="character"/><supplied reason="lost" cert="high"> τῶ</supplied>ν νομῶν τ<unclear reason="undefined" cert="high">η</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> παρ</supplied>ὰ τὸ μὴ ἰσχύειν με <supplied reason="lost" cert="high">τ</supplied><unclear reason="undefined" cert="high">ὰς</unclear> νομὰ<supplied reason="lost" cert="high">ς </supplied><gap reason="lost" extent="6" unit="character"/><supplied reason="lost" cert="high"> ἔ</supplied>γραψε δέ μοι α<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><gap reason="lost" extent="14" unit="character"/>κλήσειν κατὰ ταῦτα ἔδωκα δέ σοι καὶ πρ<supplied reason="lost" cert="high">ότε</supplied>ρ<supplied reason="lost" cert="high">ον ἔντευ</supplied>ξιν εἰς τὸ σὸ<supplied reason="lost" cert="high">ν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><gap reason="lost" extent="7" unit="character"/><supplied reason="lost" cert="high"> κ</supplied>αταπλεῦσαί με εἰς τὴν πόλιν ἐπὶ τὴν παράστασιν τὴν 

