    <lb n="1"/>βασιλεῖ Πτολεμαίωι χαίρειν Πτολεμαῖος, Μακεδὼν τ<gap reason="illegible" extent="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἀδικοῦμαι</supplied> 

    <lb n="2"/>ὑπὸ Ὥρου ἰ<supplied reason="lost" cert="high">σ</supplied>ιονόμου καὶ Θοτορταίου καὶ Παθει<gap reason="illegible" extent="2" unit="character"/> <add place="supralinear">καὶ <unclear reason="undefined" cert="high">Π</unclear>αλου<unclear reason="undefined" cert="high">τ</unclear><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/></add> <supplied reason="lost" cert="high">κατοι</supplied>κούν<supplied reason="lost" cert="high">των </supplied><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" extent="1" unit="character"/>ν<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> Ἀρσι</supplied>-

    <lb n="3"/>νοίτου νο<supplied reason="lost" cert="high">μοῦ</supplied>. τηρήσαντές με τῆι <num value="3">γ</num> το<supplied reason="lost" cert="high">ῦ </supplied><gap reason="lost" extent="unknown" unit="character"/>ε<unclear reason="undefined" cert="high">μι</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>ὅρκον ευ<gap reason="lost" extent="unknown" unit="character"/>ας<gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" extent="1" unit="character"/>οτει<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>νεμομ<gap reason="lost" extent="8" unit="character"/>ην συνδιώξαντες α<gap reason="lost" extent="unknown" unit="character"/>εχο<gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>πικοῖς <unclear reason="undefined" cert="high">δι</unclear><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" extent="1" unit="character"/> ἐνευροκόπησαν ταῦτα <gap reason="lost" extent="unknown" unit="character"/><unclear reason="undefined" cert="high">των</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>παραπορευ<supplied reason="lost" cert="high">ο</supplied>μένοις τισὶν εἰς τὰ Ὀξόρυγχα ἔδωκαν <gap reason="lost" extent="unknown" unit="character"/><unclear reason="undefined" cert="high">κ</unclear>όψαι τη<gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" extent="1" unit="character"/>μν<unclear reason="undefined" cert="high">ι</unclear><gap reason="illegible" extent="1" unit="character"/>ν<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>ἐν τῶι πεδίωι· ὕστερον δὲ αὐτὴν εὕρομεν ὑπὸ τῶν <supplied reason="lost" cert="high">κυνῶν καὶ τ</supplied>ῶν κοράκω<supplied reason="lost" cert="high">ν διαβεβρ</supplied>ωμένην, 

    <lb n="9"/>ἣν καὶ τοῖς φυλακίταις καὶ ἄλλοις πλέοσιν ἐπεδείξα<unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">εν. δέομ</supplied>αι οὖν σου, βασ<supplied reason="lost" cert="high">ιλεῦ, εἴ σ</supplied>οι δοκεῖ, 

    <lb n="10"/>προστάξαι Διοφάνει τῶι στρατηγῶι γράψαι Μοσ<supplied reason="lost" cert="high">χίωνι</supplied> τῶι ἐν <supplied reason="lost" cert="high">Ὀ</supplied>ξορύγχοις ἐπι<supplied reason="lost" cert="high">στάτ</supplied>ηι ἀποστεῖ-

    <lb n="11"/>λαι τοὺς ἐγκεκλημένους ὑπό μου πρὸς αὐτόν, ἵνα ἐπισκέψηται περὶ το<supplied reason="lost" cert="high">ύτων</supplied> καί, ἐὰν ἦι 

    <lb n="12"/>τὰ διὰ τῆς ἐντεύξεως ἀληθῆ, πραχθῆ<supplied reason="lost" cert="high">ν</supplied>αι αὐτοὺς παραχρῆμα καὶ ἀποδοῦ<supplied reason="lost" cert="high">ναί</supplied> μοι 

    <lb n="13"/>τ<supplied reason="lost" cert="high">ὴ</supplied>ν προδεδηλωμένην τιμὴν. τούτου γὰρ γενομένου, βασιλεῦ, ἐπὶ σὲ κ<supplied reason="lost" cert="high">ατα</supplied>φυγών, 

    <lb n="14"/>τ<supplied reason="lost" cert="high">ὸν</supplied> κοινὸν εὐεργέτην καὶ βοηθόν, <supplied reason="lost" cert="high">τεύ</supplied>ξομαι τῆς παρὰ σου βοηθείας. 

    <lb n="15"/>εὐτύχει. 

    <lb n="16"/><space extent="unknown" unit="character" dim="horizontal"/> 

    <lb n="16"/><handShift new="m2"/> Μοσχίωνι. <expan>μά<ex cert="high">λιστα</ex></expan> <expan>δι<ex cert="high">άλυσον</ex></expan> <supplied reason="lost" cert="high">αὐτούς· εἰ δὲ μή, <expan>ἀπό<ex cert="high">στειλον</ex></expan></supplied> ὅπως ἐπὶ τοῦ <expan>κοινοδι<ex cert="high">κίου</ex></expan> <expan>δι<ex cert="high">ακριθῶσιν</ex></expan>. 

