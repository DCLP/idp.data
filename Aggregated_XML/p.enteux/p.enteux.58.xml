    <lb n="1"/>βασιλεῖ Πτολεμαίωι χαίρειν Λύκος. ἀδικο<supplied reason="lost" cert="high">ῦμαι ὑπὸ Πετεσίου </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> μισθω</supplied>-

    <lb n="2"/>σάμενος γὰρ τὸν Θεοκλ<supplied reason="lost" cert="high">έους κλῆρον </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>τὴν συγγραφὴν πρὸς αὐτ<supplied reason="lost" cert="high">ὸν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>εργώμεθα, τοῦ δὲ σπό<supplied reason="lost" cert="high">ρου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>ἀλλὰ κατεσπείραμεν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>μενον ἀνάλωμα καὶ συν<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>πλεονέκτης ὢν προεστ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>διασαφῆσαι ἀμφοτερ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>τοῦ κλήρου, ἐν δὲ τῶι <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>ὑπὸ Λυκομήδους εἰς κ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>μετροῦν αὐτῶι σπέρμα <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>διον κατῆγον, ὁ δὲ αἰς<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>σπέρμα οὐδὲ τὰ ζ<unclear reason="undefined" cert="high">εύ</unclear>γ<supplied reason="lost" cert="high">η </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/>μεμέτρηκεν εἰς τὸ βασ<supplied reason="lost" cert="high">ιλικὸν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/>τοῦ <expan><ex cert="low">ἑκατονταρούρου</ex></expan>. δέομαι οὖ<supplied reason="lost" cert="high">ν σου, βασιλεῦ, </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>γράψαι Μοσχίωνι τῶι <supplied reason="lost" cert="high">ἐπιστάτηι </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἐπι</supplied>-

    <lb n="17"/>σκεψάμενον τὴν συγ<unclear reason="undefined" cert="high">γ</unclear><supplied reason="lost" cert="high">ραφὴν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/>Πετέσιον τὸν τελ<gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="19"/>σησάμου καὶ ἐὰν ἦι <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/>δοῦναί με αὐτῶι τ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="21"/>τοῦ σησάμου τὸ ἐπιβ<supplied reason="lost" cert="high">άλλον </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> εἰς τὸν λοι</supplied>-

    <lb n="22"/>πὸν χ<unclear reason="undefined" cert="high">ρ</unclear>όνον τῆς μισθώ<supplied reason="lost" cert="high">σεως. </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="23"/>τεύξομαι τοῦ δικαίου. <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="24"/><space extent="unknown" unit="character" dim="horizontal"/> 

    <lb n="24"/><handShift new="m2"/> Μοσχίωνι. <supplied reason="lost" cert="high">μ</supplied>άλιστα μὲν διάλυσον αὐτούς· εἰ δὲ μή, <supplied reason="lost" cert="high">πρὸς ἡμᾶς <expan>ἀπό<ex cert="high">στειλον</ex></expan> <expan>ὅπ<ex cert="high">ως</ex></expan> <expan>ἐπι<ex cert="high">σκεψώμεθα</ex></expan></supplied>. 

    <lb n="25"/><expan><ex cert="high">ἔτους</ex></expan> <num value="25">κε</num>, Λώιου <supplied reason="lost" cert="high"><num value="26">κϛ</num>, Χοίαχ <num value="13">ιγ</num></supplied>. 
