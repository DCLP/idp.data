    <lb n="1"/><supplied cert="high" reason="lost">βασιλεῖ Πτολεμαίωι χαίρειν </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀδικοῦμαι ὑπὸ</supplied> Πάσιτος τοῦ <gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/><gap extent="unknown" reason="lost" unit="character"/> η῞ ἐστιν ἐν τῶι δ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/><gap extent="unknown" reason="lost" unit="character"/> σπόρον τὴν ἄρουρ<supplied cert="high" reason="lost">αν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

