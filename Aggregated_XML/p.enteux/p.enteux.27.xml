    <lb n="2"/><supplied reason="lost" cert="high">Λίβυς</supplied> ναύκληρος πε<supplied reason="lost" cert="high">ρὶ τ</supplied><unclear reason="undefined" cert="high">ο</unclear>ῦ 

    <lb n="3"/><supplied reason="lost" cert="high">γεμι</supplied>σθῆναι αὐτοῦ τ<supplied reason="lost" cert="high">ὸ πλο</supplied>ῖ<supplied reason="lost" cert="high">ο</supplied>ν 

    <lb n="4"/><supplied reason="lost" cert="high">ἐκ τ</supplied>οῦ νομοῦ. </ab></div></div><div n="meta.3302" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.3302" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" value="-0222-01-28" notBefore="-222" notAfter="-222">28. Jan. 222 v.Chr.</date></p></div><div n="meta.3302" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">P.Enteux.</title><biblScope type="numbers">27</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">3302</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0106</series>; ; <biblScope type="numbers">27</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0106;;27" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.3302" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p>keine</p></div><div n="meta.3302" type="bibliography" subtype="otherPublications" org="uniform" part="N" sample="complete"><head>Andere Publikationen</head><listBibl default="NO"><bibl type="publication" subtype="other" default="NO">P.Lille II 11</bibl><bibl type="publication" subtype="other" default="NO">W.Chr. 442</bibl><bibl type="publication" subtype="other" default="NO">C.Pap. Hengstl 139</bibl></listBibl></div><div n="meta.3302" type="history" subtype="locations" org="uniform" part="N" sample="complete"><p><placeName type="ancientFindspot" cert="high" full="yes">Magdola</placeName> (<geogName type="nome" cert="high" full="yes">Arsinoites</geogName>), <geogName type="ancientRegion" key="Aegyptus" cert="high" full="yes">Egypt</geogName></p></div><div n="meta.3302" type="figure" org="uniform" part="N" sample=