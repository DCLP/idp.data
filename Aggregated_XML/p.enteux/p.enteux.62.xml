    <lb n="1"/>βασιλεῖ Πτολεμαίωι χαίρειν Ἀδάμας Λιβάνου, Σαμαρίτης τ<gap extent="unknown" reason="lost" unit="character"/> 


    <lb n="2"/>τοῦ παρὰ <unclear cert="high" reason="undefined">Φ</unclear>ιλίτου γραμματέως τῶν μισθοφόρων ἱππέων ἐμισθω<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>τοῦ ὑπηρέτου τῶν μισθοφόρων τὸν σπόρον τὸν εἰς τὸ <num value="26">κϛ</num> <expan><ex cert="high">ἔτος</ex></expan>, ἐκφ<supplied cert="high" reason="lost">ορι </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>κατὰ συνγραφὴν μισθώσεως ἣν συνεγραψάμην αὐτῶι, πρὸ τοῦ δὲ <unclear cert="high" reason="undefined">τ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>συνέβη τῶι Ἀρτεμιδώρωι ἀπὸ τῆς γραμματείας ἀποσταθῆναι <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="6"/><gap extent="2" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Ἀ</supplied>μμώνιο<unclear cert="high" reason="undefined">ν</unclear> <supplied cert="high" reason="lost">ὑ</supplied>φ᾽ ὧν ἠναγκάσθην δοῦναι τῶν ἐκφορίων τ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="7"/><gap extent="2" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="12" reason="lost" unit="character"/> μην<supplied cert="high" reason="lost">ὸ</supplied>ς Αἰγ<supplied cert="high" reason="lost">υπτ</supplied>ίων Τῦβι <num value="9">θ</num>, παραγεν<unclear cert="high" reason="undefined">ομ</unclear><gap extent="unknown" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="8"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ε αὐτῶι τὰ ἐκφορια <gap extent="1" reason="lost" unit="character"/>ατε<unclear cert="high" reason="undefined">μ</unclear><gap extent="4" reason="lost" unit="character"/> μισθώσεων <gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/>ων. ἀξιῶ οὖ<supplied cert="high" reason="lost">ν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="9"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> προστά</supplied>ξαι Διοφάνει τῶι στρατηγῶι, ἀνακαλεσάμενον Νουμήνιον τὸν <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> κα</supplied><unclear cert="high" reason="undefined">ὶ</unclear> Χαρμίδην τὸν ἐπιστάτην καὶ Ἀμμώνιον τὸν πράκτορα, ἐπισκέψ<supplied cert="high" reason="lost">ασθαι </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="11"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἀ</supplied>ποδοθῆναι καὶ ὧι ἂν συνκρ<unclear cert="high" reason="undefined">ίνηι</unclear> <unclear cert="high" reason="undefined">ἀπο</unclear>δοθῆναι ὑπ<gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="12"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> μὴ συκο</supplied>φαντηθῶ ἀπραγμ<unclear cert="high" reason="undefined">ον</unclear><gap extent="4" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">ω</unclear>ν, ἀλλ᾽ ἐπὶ σὲ καταφ<supplied cert="high" reason="lost">υγὼν </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="13"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> φι</supplied>λανθρωπίας ὦ τετευχώς. <space dim="horizontal" extent="unknown" unit="character"/>  <supplied cert="high" reason="lost">εὐτύχει</supplied>. 

    <lb n="14"/><space dim="horizontal" extent="unknown" unit="character"/> 

