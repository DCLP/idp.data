    <lb n="1"/>βασιλεῖ Πτολεμαίωι χαίρειν Σπερχίδης μ<add place="supralinear"><unclear reason="undefined" cert="high">ε</unclear></add><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>Πετοσούχου. <subst><add place="inline">μεμισθωμένου</add><del rend="corrected" status="unremarkable"><del rend="erasure" status="unremarkable">υπο</del>μεμισθωμενου</del></subst> γάρ μου <unclear reason="undefined" cert="high">π</unclear><gap reason="lost" extent="unknown" unit="character"/> 


    <lb n="3"/>προειρημένην κώμην, ἣν κατέσπειρα σικ<supplied reason="lost" cert="high">ύωι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>ἐκφόρια καὶ σύμ<supplied reason="lost" cert="high">β</supplied>ολον ἀποχῆς ἔχοντος ε<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>εἰς τὸ σικυήρατον μετ᾽ ἄλλων τινῶν ε<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>αὐτοῦ οὐθένα λόγον ποιησάμενοι δ<unclear reason="undefined" cert="high">ης</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>ἐμοῦ δὲ <unclear reason="undefined" cert="high">β</unclear>ο<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="6" unit="character"/> τά τε σοῦ <del rend="erasure" status="unremarkable"><gap reason="illegible" extent="1" unit="character"/></del> βασιλεῦ κ<unclear reason="undefined" cert="high">α</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>οὕτως ἀφῆκέν με ἐν τῆι κώμηι. ταῦτα δὲ ἔ<supplied reason="lost" cert="high">πραξεν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>δέομαι οὖν σου, βασιλεῦ, προστάξαι Διοφάνει τῶι <supplied reason="lost" cert="high">στρατηγῶι γράψαι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>τεταγμένωι ἐν τῆι Θεμίστου μερίδι ἀποστεῖλ<supplied reason="lost" cert="high">αι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>γενέσθαι μοι τὸ δίκαιον καθότι ἂν Διοφάνης σ<supplied reason="lost" cert="high">υγκρίνηι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><space extent="unknown" unit="character" dim="horizontal"/> 

    <lb n="12"/><handShift new="m2"/> Διοσκουρίδει. μάλιστα μὲν διάλυσον αὐ<supplied reason="lost" cert="high">τούς· εἰ δὲ μή, πρὸς ἡμᾶς <expan>ἀπό<ex cert="high">στειλον</ex></expan> <expan>ὅπ<ex cert="high">ως</ex></expan> <expan>ἐπι<ex cert="high">σκεψώμεθα</ex></expan></supplied>. 
