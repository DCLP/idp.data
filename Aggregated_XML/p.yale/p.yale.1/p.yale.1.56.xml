    <lb n="4"/><space dim="horizontal" extent="unknown" unit="character"/> 

    <lb n="5"/><supplied cert="high" reason="lost">βασιλεὺς Πτολε</supplied>μαῖος ὁ ἐπικαλούμενο<supplied cert="high" reason="lost">ς Ἀλέξανδρος καὶ</supplied> 

    <lb n="6"/><supplied cert="high" reason="lost">βασίλισσα</supplied> Βερ<supplied cert="high" reason="lost">ενί</supplied>κη ἡ ἀδελφὴ Χάρμω<supplied cert="high" reason="lost">ι χαίρειν.</supplied> 

    <lb n="7"/><gap extent="9" reason="lost" unit="character"/><gap extent="3" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/>ξαι τὸ ὑπ<supplied cert="high" reason="lost">ο</supplied>τετ<supplied cert="high" reason="lost">α</supplied>γμέν<supplied cert="high" reason="lost">ον πρόσταγμα</supplied> 

    <lb n="8"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> μεταγρα</supplied>φὲν τοῖς Ἑλληνικοῖς καὶ ἐγχω<supplied cert="high" reason="lost">ρίοις γράμμασιν</supplied> 

