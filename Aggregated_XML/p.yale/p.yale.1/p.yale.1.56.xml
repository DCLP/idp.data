    <lb n="2"/><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> ἐν</supplied>τὸς τῶν ὡρισμ<supplied cert="high" reason="lost">ένων </supplied><gap extent="unknown" reason="lost" unit="character"/> 

