    <lb n="1"/><gap extent="unknown" reason="lost" unit="character"/> Εὐδαιμονίδι τῆι <gap desc="vestiges" extent="unknown" reason="illegible" unit="character"/> 

    <lb n="2"/>ἀδε<supplied cert="high" reason="lost">λ</supplied>φῆι πλεῖστα <choice><corr>χαίρειν</corr><sic>χαιριεν</sic></choice> 

