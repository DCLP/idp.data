    <lb n="1"/>ὑπατείας τοῦ δ<supplied cert="high" reason="lost">εσ</supplied>πότ<supplied cert="high" reason="lost">ου ἡ</supplied>μῶν Γρ<supplied cert="high" reason="lost">ατιαν</supplied>οῦ τοῦ αἰωνίου Αὐγούστου 

    <lb n="2"/>τὸ <num value="2">β</num> καὶ Φλαυί<supplied cert="high" reason="lost">ου Πρό</supplied>β<supplied cert="high" reason="lost">ου τ</supplied>οῦ λαμ<supplied cert="high" reason="lost">προτάτο</supplied>υ ἐ<supplied cert="high" reason="lost">πά</supplied>ρχου τοῦ ἱεροῦ 

    <lb n="3"/>πραιτωρίου <supplied cert="high" reason="lost">Με</supplied><unclear cert="high" reason="undefined">σορ</unclear><supplied cert="high" reason="lost">ὴ</supplied> <num value="29">κθ</num>. 

    <lb n="4"/>Φλαυίῳ Ἰσιδώρῳ <expan><unclear cert="high" reason="undefined">ὀ</unclear>φφ<ex cert="high">ικιαλίῳ</ex></expan> <expan>τάξ<ex cert="high">εως</ex></expan> ἡ<supplied cert="high" reason="lost">γεμον</supplied>ίας Θηβαίδος 

    <lb n="5"/>παρὰ Αὐρηλ<supplied cert="high" reason="lost">ίου</supplied> Ἀπο<supplied cert="high" reason="lost">λλ</supplied>ωνίου <app type="BL"><lem resp="1.208">Β<supplied cert="high" reason="lost">ησαπ</supplied>όλλωνος</lem><rdg>β<supplied cert="high" reason="lost">ησᾶ Ἀπ</supplied>ολλων<supplied cert="high" reason="lost">ί</supplied>ο<supplied cert="high" reason="lost">υ</supplied></rdg></app> <expan>ἄρξ<ex cert="high">αντος</ex></expan> Πανὸς <expan><unclear cert="high" reason="undefined">π</unclear>όλ<ex cert="high">εως</ex></expan>. 

    <lb n="6"/>ὁμολογῶ ὀμνὺς τὴ<supplied cert="high" reason="lost">ν</supplied> θείαν καὶ οὐ<supplied cert="high" reason="lost">ρ</supplied>άνιον τύχην τῶν 

    <lb n="7"/>τὰ πάντα νικώντω<supplied cert="high" reason="lost">ν</supplied> δεσποτῶν ἡμῶν αἰωνίων 

    <lb n="8"/>Αὐγούστων Οὐαλεντινιαν<supplied cert="high" reason="lost">ο</supplied>ῦ καὶ Οὐάλεντος καὶ Γρατιανοῦ 

    <lb n="9"/>ἑκουσίως καὶ αὐθ<supplied cert="high" reason="lost">α</supplied>ιρέτως ἐγγ<supplied cert="high" reason="lost">υᾶ</supplied>σθαι Πανίσκον 

    <lb n="10"/>Μακαρίου ἐπι<unclear cert="high" reason="undefined">μελ</unclear>ητὴν ἐσθῆ<supplied cert="high" reason="lost">τ</supplied>ος <choice><corr>τρεισκαιδεκάτης</corr><sic>τρισκαιδεκατης</sic></choice> 

    <lb n="11"/>ἰνδικτίωνος κελευσθέντα <choice><corr>καθιέναι</corr><sic>κατιεναι</sic></choice> εἰ<supplied cert="high" reason="lost">ς</supplied> τὴν λαμπρο-

    <lb n="12"/>τάτην Ἀλεξάν<supplied cert="high" reason="lost">δ</supplied>ρεια<supplied cert="high" reason="lost">ν</supplied> <supplied cert="high" reason="omitted">ἐπὶ τῷ αὐτὸν κατιέναι εἰς τὴν αὐτὴν Ἀλεξάνδρειαν</supplied> καὶ μὴ <app type="BL"><lem resp="1.208">ἀπο<supplied cert="high" reason="lost">λ</supplied>είπεσθαί</lem><rdg>ἀπο<supplied cert="high" reason="lost">γ</supplied>είνεσθαί</rdg></app> σου 

    <lb n="13"/><unclear cert="high" reason="undefined">ἕω</unclear><supplied cert="high" reason="lost">ς</supplied> <unclear cert="high" reason="undefined">ἐὰ</unclear>ν τ<supplied cert="high" reason="lost">ὰ</supplied> κατ᾽ αὐτὸ<unclear cert="high" reason="undefined">ν</unclear> <supplied cert="high" reason="lost">πέρ</supplied><unclear cert="high" reason="undefined">ατ</unclear>ος <unclear cert="high" reason="undefined">τύ</unclear><supplied cert="high" reason="lost">χ</supplied>ῃ εἴτε ἐπὶ τῆς Ἀλεξαν-

    <lb n="14"/>δρ<supplied cert="high" reason="lost">έ</supplied>ων ε<supplied cert="high" reason="lost">ἴτε</supplied> ἐν τῇ <app type="BL"><lem resp="1.208">ἐπα<supplied cert="high" reason="lost">ν</supplied>όδῳ</lem><rdg><unclear cert="high" reason="undefined">χώ</unclear>ρ<supplied cert="high" reason="lost">ᾳ</supplied> <gap extent="2" reason="illegible" unit="character"/>ω<gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/></rdg></app> ἐπὶ τὸ δικαστήριον, 

    <lb n="15"/>εἰ δὲ <app type="BL"><lem resp="1.208"><choice><corr>λιποτακτήσῃ</corr><sic>λειπ<supplied cert="high" reason="lost">ο</supplied>τακτηση</sic></choice></lem><rdg>υ<gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/>τακτ<unclear cert="high" reason="undefined">η</unclear>σ<unclear cert="high" reason="undefined">ῃ</unclear></rdg></app> καὶ μὴ π<supplied cert="high" reason="lost">α</supplied>ραστήσω <expan><supplied cert="high" reason="lost">α<ex cert="high">ὐτὸν</ex></supplied></expan>, ἐγὼ 

    <lb n="16"/>αὐτὸς <app type="BL"><lem resp="1.208">τὸν</lem><rdg>τ<unclear cert="high" reason="undefined">ὰ</unclear></rdg></app> ὑπὲρ αὐτοῦ <app type="BL"><lem resp="1.208">λόγον</lem><rdg><unclear cert="high" reason="undefined">ἐν</unclear> π<supplied cert="high" reason="lost">α</supplied>ντ<unclear cert="high" reason="undefined">ὶ</unclear></rdg></app> <unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">οι</supplied>ήσω ἢ ἔ<supplied cert="high" reason="lost">ν</supplied>οχος 

    <lb n="17"/><supplied cert="high" reason="lost">ἔ</supplied><unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">ι</unclear> τῶι θείῳ ὅρκῳ <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">αὶ τ</supplied>ῷ ἐπηρτημένῳ 

    <lb n="18"/>τούτω<unclear cert="high" reason="undefined">ι</unclear> κ<supplied cert="high" reason="lost">ιν</supplied>δύνῳ καὶ <expan>ἐπερ<ex cert="high">ωτηθεὶς</ex></expan> <expan>ὡμολόγη<ex cert="high">σα</ex></expan>. 

    <lb n="19"/><handShift new="m2"/> Αὐρήλιος Ἀ<supplied cert="high" reason="lost">πο</supplied>λλώνιος <app type="BL"><lem resp="1.208">Βησαπ<supplied cert="high" reason="lost">ό</supplied>λλω<supplied cert="high" reason="lost">ν</supplied>ος</lem><rdg><unclear cert="high" reason="undefined">Β</unclear>ησᾶ <supplied cert="high" reason="omitted">ἄπὸ</supplied> <unclear cert="high" reason="undefined">Π</unclear><supplied cert="high" reason="lost">α</supplied>νὸ<supplied cert="high" reason="lost">ς πόλ</supplied><unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">ω</supplied>σ</rdg></app> ὁ <choice><corr>προκείμενος</corr><sic>προκιμενας</sic></choice> 

    <lb n="20"/><choice><corr>ἐγγυῶμαι</corr><sic>ενγυ<unclear cert="high" reason="undefined">ω</unclear>μεν</sic></choice> τὸν <expan>προκ<ex cert="high">είμενον</ex></expan> Πανί<supplied cert="high" reason="lost">σ</supplied>κο<supplied cert="high" reason="lost">ν ὡ</supplied>ς <expan>πρόκ<ex cert="high">ειται</ex></expan>. 
