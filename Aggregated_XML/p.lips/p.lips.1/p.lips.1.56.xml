    <lb n="20"/><supplied cert="high" reason="lost">παρ</supplied>αστήσω, ἐγὼ αὐτὸς τὸν ὑπὲρ α<supplied cert="high" reason="lost">ὐτοῦ</supplied> <unclear cert="high" reason="undefined">λόγ</unclear><supplied cert="high" reason="lost">ον</supplied> 

