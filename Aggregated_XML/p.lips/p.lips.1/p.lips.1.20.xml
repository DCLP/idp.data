    <lb n="23"/><supplied reason="lost" cert="high">ὡ</supplied>ς πρόκειται. <expan><supplied reason="lost" cert="high">Α</supplied>ὐρ<ex cert="high">ήλιος</ex></expan> Ἑρμῆς Σ<supplied reason="lost" cert="high">α</supplied><unclear reason="undefined" cert="high">μ</unclear>βᾶ 

