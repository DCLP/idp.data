    <lb n="1"/>Φλαυίοις Ἰσιδώρῳ καὶ Θεοδώρῳ 

    <lb n="2"/><expan>ὀφ<ex cert="high">φικιαλίοις</ex></expan> τάξεως ἡγεμονίας <choice><corr>Θηβαίδος</corr><sic>θη<unclear cert="high" reason="undefined">ι</unclear>βαιδα</sic></choice> 

    <lb n="3"/>παρὰ Αὐρηλίου <app type="BL"><lem resp="1.209">Λεοντίου Μασκουλείνου</lem><rdg><unclear cert="high" reason="undefined">Κ</unclear>υντίου Μόσχου</rdg></app> 

    <lb n="4"/>ἀπό κεφαλα<supplied cert="high" reason="lost">ι</supplied>ωτῶν <choice><corr>τρεισκαιδεκάτης</corr><sic>τρισκαιδεκατης</sic></choice> 

    <lb n="5"/>ἰνδικτ<supplied cert="high" reason="lost">ί</supplied>ωνος. ὁμολογῶ ὀμνὺς τὴν θείαν 

    <lb n="6"/>καὶ <choice><corr>οὐρανία<supplied cert="high" reason="omitted">ν</supplied></corr><sic>ουρ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">ν</supplied>ια</sic></choice> τύχην τῶν <choice><corr>πάντα</corr><sic>παντων</sic></choice> <choice><corr>νικ<supplied cert="high" reason="omitted">ώ</supplied>ντων</corr><sic>ν<unclear cert="high" reason="undefined">ικντων</unclear></sic></choice> 

    <lb n="7"/>δεσποτῶν ἡμῶν <choice><corr>Οὐαλεντινιανοῦ</corr><sic>ουαλεντινιανιανου</sic></choice> 

    <lb n="8"/>καὶ Οὐάλεντος καὶ Γρατιανοῦ τῶν αἰωνίων 

    <lb n="9"/>Αὐγούστων τὴν <choice><corr>ἐμφάνειαν</corr><sic>ενφανιαν</sic></choice> τὴ<unclear cert="high" reason="undefined">ν</unclear> <choice><corr>ἐμα<supplied cert="high" reason="omitted">υ</supplied>τοῦ</corr><sic><unclear cert="high" reason="undefined">ε</unclear>ματου</sic></choice> 

    <lb n="10"/>ποιήσ<unclear cert="high" reason="undefined">α</unclear>σθαι τῇ τάξει, ἐπειδὴ <app type="BL"><lem resp="1.209">ἐκελεύσθη</lem><rdg><expan><unclear cert="high" reason="undefined">ἀκολούθ</unclear><ex cert="high">ως</ex></expan> <unclear cert="high" reason="undefined">τοῖς</unclear></rdg></app> 

    <lb n="11"/><app type="BL"><lem resp="1.209">πέρας <supplied cert="high" reason="lost">σ</supplied>ταθῆναι</lem><rdg>προσ<supplied cert="high" reason="lost">τε</supplied>ταγμένοισ</rdg></app> ὑπ<supplied cert="high" reason="lost">ὸ</supplied> τ<unclear cert="high" reason="undefined">ῆ</unclear>ς ἐξουσίας τ<supplied cert="high" reason="lost">ο</supplied>ῦ 

    <lb n="12"/>κυρίου μ<supplied cert="high" reason="lost">ο</supplied>υ τ<supplied cert="high" reason="lost">ο</supplied>ῦ λαμπροτάτου 

    <lb n="13"/>ἡγεμόνος Ἰουλίου <app type="BL"><lem resp="1.209"><seg cert="low" part="N"><supplied cert="high" reason="omitted">Εὐβουλίου</supplied> Ἰουλιαν<supplied cert="high" reason="omitted">οῦ</supplied> <hi rend="diaeresis"><seg cert="low" part="N">Ἰ</seg></hi>ουβιανοῦ</seg></lem><rdg>Ἰουλιανι<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">υλ</supplied>ιανου</rdg></app> 

    <lb n="14"/>τῆς <choice><corr>κεφαλαιωτίας</corr><sic>κ<unclear cert="high" reason="undefined">αι</unclear>φαλαιωτιας</sic></choice> ἕνεκεν <gap extent="2" reason="illegible" unit="character"/>μοι 

    <lb n="15"/><gap extent="1" reason="lost" unit="character"/><unclear cert="high" reason="undefined">π</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/>ασθ<unclear cert="high" reason="undefined">αι</unclear> ἐ<gap extent="1" reason="illegible" unit="character"/><gap extent="2" reason="lost" unit="character"/> <unclear cert="high" reason="undefined">ἕως</unclear> ἂν τὰ κ<unclear cert="high" reason="undefined">ατ</unclear>᾽ <choice><corr>ἐμὲ</corr><sic><unclear cert="high" reason="undefined">εμοι</unclear></sic></choice> 

    <lb n="16"/><unclear cert="high" reason="undefined">πέ</unclear>ρ<unclear cert="high" reason="undefined">ατ</unclear><supplied cert="high" reason="lost">ος</supplied> τ<unclear cert="high" reason="undefined">ύχῃ</unclear>, <unclear cert="high" reason="undefined">εἰ</unclear> <unclear cert="high" reason="undefined">δὲ</unclear> <supplied cert="high" reason="lost">μ</supplied>ὴ, ἔνοχος ἔσομαι 

    <lb n="17"/><supplied cert="high" reason="lost">τ</supplied>ῷ θεί<supplied cert="high" reason="lost">ῳ</supplied> ὅρκῳ καὶ τῷ περὶ τούτου 

    <lb n="18"/><supplied cert="high" reason="lost">κ</supplied>ινδ<supplied cert="high" reason="lost">ύνῳ καὶ</supplied> <unclear cert="high" reason="undefined">ἐπ</unclear><supplied cert="high" reason="lost">ε</supplied>ρωτηθεὶς ὡμολόγησα. 

    <lb n="19"/><supplied cert="high" reason="lost">ὑ</supplied>πατεί<supplied cert="high" reason="lost">α</supplied>ς Δομιτίου <supplied cert="high" reason="lost">Μ</supplied>οδέστου καὶ 

    <lb n="20"/><supplied cert="high" reason="lost">Ἀ</supplied>ρ<unclear cert="high" reason="undefined">ε</unclear>νθ<supplied cert="high" reason="lost">ίου τῶ</supplied>ν λαμπρότατων. 

    <lb n="21"/><supplied cert="high" reason="lost">Α</supplied>ὐρήλι<supplied cert="high" reason="lost">ος</supplied> <app type="BL"><lem resp="1.209">Λεόντιος Μασκουλείνου</lem><rdg><unclear cert="high" reason="undefined">Κύ</unclear>ντιος Μόσχου <unclear cert="high" reason="undefined">ὁ</unclear> <expan><unclear cert="high" reason="undefined">προκ</unclear><ex cert="high">είμενοσ</ex></expan></rdg></app> 

    <lb n="22"/><gap extent="4" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/> <unclear cert="high" reason="undefined">ὤμοσα</unclear> τὸν ὅρκον ὡς <expan>πρόκ<ex cert="high">ειται.</ex></expan> 
