    <lb n="11"/>ἐπιμελ<unclear cert="high" reason="undefined">η</unclear><supplied cert="high" reason="lost">τοῦ ἐ</supplied><unclear cert="high" reason="undefined">σθ</unclear><supplied cert="high" reason="lost">ῆ</supplied>το<supplied cert="high" reason="lost">ς</supplied> <choice><corr>τρεισκαιδεκάτης</corr><sic>τρισ<supplied cert="high" reason="lost">κα</supplied>ιδεκατης</sic></choice> 

    <lb n="12"/><expan>ἰνδικ<ex cert="high">τίονος</ex></expan> <unclear cert="high" reason="undefined">κελευσθ</unclear><supplied cert="high" reason="lost">ε</supplied><unclear cert="high" reason="undefined">ῖ</unclear><supplied cert="high" reason="lost">σα</supplied><unclear cert="high" reason="undefined">ν</unclear> <supplied cert="high" reason="lost">ἅ</supplied><unclear cert="high" reason="undefined">μα</unclear> σοι <choice><corr>κατιέναι</corr><sic>κατ<g type="apostrophe"/>ιεναι</sic></choice> 

    <lb n="13"/>ἐπὶ τὴν <unclear cert="high" reason="undefined">λαμπροτ</unclear><supplied cert="high" reason="lost">ά</supplied><unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">η</supplied>ν <app type="BL"><lem resp="1.208; cf. 8.171">μεγαλόπολιν</lem><rdg><unclear cert="high" reason="undefined">μητρό</unclear>πολιν</rdg></app> 

