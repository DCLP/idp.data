    <lb n="1"/><expan><ex cert="high">ἔτους</ex></expan> <num value="2">β</num> Τιβερίου Καίσαρος. 
<expan><unclear cert="high" reason="undefined">ἐ</unclear>κδ[όσι<ex cert="high">μον</ex></expan> <expan><unclear cert="high" reason="undefined">Πα</unclear><supplied cert="high" reason="lost">χ<ex cert="high">ὼν</ex></supplied></expan> <num value="10">ι</num>.

    <lb n="2"/>Ἔτους δευτέρου Τιβερίου Καίσαρος Σεβ<supplied cert="high" reason="lost">α</supplied>στοῦ μη-

    <lb n="3"/>νὸς <choice><corr>Πανέμου</corr><sic>πανημου</sic></choice> δεκάτ<unclear cert="high" reason="undefined">η</unclear>ι Παχ<unclear cert="high" reason="undefined">ὼ</unclear>ν δεκάτῃ 

    <lb n="4"/>ἐν Τεβτύνι τῆς Πολέμωνος μερίδος

    <lb n="5"/>τοῦ <unclear cert="high" reason="undefined">Ἀρσι</unclear>ν<unclear cert="high" reason="undefined">οεί</unclear>του νομοῦ. <expan>ὁμο<unclear cert="high" reason="undefined">λογ</unclear><ex cert="high">οῦσιν</ex></expan> 
Μαρσισοῦχος

    <lb n="6"/>Μαρεψήμιος Πέρσης τῆς <expan>ἐπιγον<ex cert="high">ῆς</ex></expan> ὡς ἐτῶν πεν-

    <lb n="7"/>τήκοντα <num value="50"/> οὐλὴ ἀντίχειρι δεξιᾷ καὶ <supplied cert="high" reason="omitted">ἡ</supplied>

    <lb n="8"/><supplied cert="high" reason="omitted">τού</supplied>του γυνὴ Ταμαρρῆς Μάρωνος ὡς ἐτῶν τριάκον-

    <lb n="9"/>τα <num value="30"/> οὐλὴ ὀφ<unclear cert="high" reason="undefined">ρύ</unclear>ι ἀριστερᾷ Περσίνη <supplied cert="high" reason="lost">μ</supplied>ετὰ 
κυρίου τοῦ

    <lb n="10"/>προγεγραμμένου ἀνδρὸς ἀλλήλων ἔγγυ<unclear cert="high" reason="undefined">ο</unclear>ι εἰς

    <lb n="11"/>ἔκτεισιν ἔχιν παρὰ <choice><corr>Κολλεύτιος τῆς Ψοίφιος</corr><sic>κολλεύτις της ψοιφος</sic></choice>

    <lb n="12"/>ὡς ἐτῶν εἴκοσι δύο <num value="22"/> οὐλὴ ἀ<unclear cert="high" reason="undefined">στ</unclear><supplied cert="high" reason="lost">ρ</supplied>αγά<unclear cert="high" reason="undefined">λω</unclear>ι 
δεξιῷ

    <lb n="13"/>μετὰ κυρίου τοῦ πατρὸς αὐτῆς Ψοίφος τοῦ Πν<unclear cert="high" reason="undefined">εβ</unclear>-

    <lb n="14"/>τύ<unclear cert="high" reason="undefined">νεω</unclear>ς ὡς ἐτῶν τεσσαράκοντα <num value="40"/> οὐλὴ 
<expan>μετ<ex cert="high">ώπῳ</ex></expan> ἐξ <expan>ἀρ<ex cert="high">ιστερᾶς</ex></expan>

    <lb n="15"/>τὰς τοῦ ἀργυρίου ἐπισήμου νομίσματος κεφαλαίου

    <lb n="16"/>δραχμὰς <choice><corr>εἰκοσιτέσσαρας</corr><sic>εικοσιτεσσαρες</sic></choice> <num value="24"/>, ἀντὶ δὲ τῶν τούτων <expan><unclear cert="high" reason="undefined">τ</unclear>ό<ex cert="high">κων</ex></expan>

    <lb n="17"/><choice><corr>συγκεχωρηκέναι</corr><sic>συνκωχωρηκεναι</sic></choice> τῇ Κολλεῦτι <choice><corr>ἐνοικήσειν</corr><sic>ενοικησιν</sic></choice> ἕως

    <lb n="18"/><expan>μηνὸ<ex cert="high">ς</ex></expan> Παῦνι ἕκτης δευτέρου <expan><ex cert="high">ἔτους</ex></expan> 
Τιβερίου Καίσαρος Σεβαστοῦ

    <lb n="19"/>ἐν τῷ ἡμίσει <choice><corr>μέρει</corr><sic>μερ<unclear cert="high" reason="undefined">ους</unclear></sic></choice> τῆς κοινῆς καὶ ἀδιαιρέτου <expan>οἰκί<ex cert="high">ας</ex></expan>

    <lb n="20"/>ἐν Τεβτύνι. καὶ <choice><corr>χρήσεται</corr><sic>χρησηται</sic></choice> κατὰ κοινὸν ταύτῃ οἰκίᾳ

    <lb n="21"/>πᾶσι χρη<unclear cert="high" reason="undefined">στη</unclear>ρίοις, ἕως μετὰ τὸν χρὸν<supplied cert="high" reason="lost">ον</supplied> ἀποδώσο<unclear cert="high" reason="undefined">υσι</unclear> <unclear cert="high" reason="undefined">τὸ</unclear>

    <lb n="22"/>ἀργύριον. <expan>ὑ<unclear cert="high" reason="undefined">π</unclear><supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">γρα</unclear><ex cert="high">φεὺς</ex></expan> τοῦ 
<expan>ὁμο<ex cert="high">λογοῦντος</ex></expan> <supplied cert="high" reason="lost">Π</supplied><unclear cert="high" reason="undefined">ατύνις</unclear> <unclear cert="high" reason="undefined">Φα</unclear>λαού<unclear cert="high" reason="undefined">τος</unclear> ὡς

    <lb n="23"/>ἐτῶν τριάκοντα πέντε <num value="35"/> οὐλὴ ταρσῷ 
<expan>ἀριστερ<ex cert="high">ῷ</ex></expan>,

    <lb n="24"/>καὶ τῆς <expan>Κολλεύτιο<ex cert="high">ς</ex></expan> Μα<supplied cert="high" reason="omitted">ρ</supplied>σισοῦχος <choice><corr>Μαρεψήμιος</corr><sic>μαρεψημιου</sic></choice> ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <num value="30">λ</num>

    <lb n="25"/><handShift new="m2"/> Κολλεῦχις <choice><corr>Ψοίφιος</corr><sic><unclear cert="high" reason="undefined">ψ</unclear>οιφις</sic></choice>· <choice><corr>γέγονε εἴς με</corr><sic>γεγοναι εις μαι</sic></choice>. ἔγραψεν ὑπὲρ αὐτῆς

    <lb n="26"/>Μαρσισοῦχ<unclear cert="high" reason="undefined">ος</unclear> <choice><corr>Μαρεψήμιος</corr><sic>μαρεψημιου</sic></choice>.

    <lb n="27"/><handShift new="m3"/><g type="filled-circle"/> Μαρσισοῦχος <choice><corr>Μαρεψήμιος</corr><sic>μαρεψημις</sic></choice> Πέρσης τῆς ἐπιγονῆς

    <lb n="28"/>καὶ ἡ τούτου γυνὴ Ταμαρῆς Μάρωνος Περ-

    <lb n="29"/><unclear cert="high" reason="undefined">σί</unclear>νη μετὰ <choice><corr>κυρίου</corr><sic>κυρυου</sic></choice> τοῦ προγεγραμμένου ἑα-

    <lb n="30"/><supplied cert="high" reason="lost">υτ</supplied>ῆς ἀνδρὸς ἀλλήλων <choice><corr>ἔγγυοι εἰς ἔκ-

    <lb n="31"/>τισιν</corr><sic>ενγυο<unclear cert="high" reason="undefined">ι</unclear> <sic>ει</sic> εις εκ<supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ε</unclear>ισιν</sic></choice> ὁμολογοῦμεν <choice><corr>ἔχειν</corr><sic>εχιν</sic></choice> παρὰ Κολλεύθ<supplied cert="high" reason="lost">ιος</supplied>

    <lb n="32"/>τῆς Ψοί<unclear cert="high" reason="undefined">φ</unclear>ειος μετὰ τοῦ <choice><corr>κυρίου</corr><sic>κυρυου</sic></choice> αὐτοῦ

    <lb n="33"/><supplied cert="high" reason="lost">Ψοίφιος τ</supplied>οῦ πατρὸς αὐτ<supplied cert="high" reason="lost">ῆς ἀργυ</supplied><unclear cert="high" reason="undefined">ρί</unclear><supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">υ</unclear>

    <lb n="34"/><supplied cert="high" reason="lost">ἐπισήμου νομίσ</supplied><unclear cert="high" reason="undefined">ματο</unclear><supplied cert="high" reason="lost">ς κεφαλαίου δραχμὰ</supplied><unclear cert="high" reason="undefined">ς</unclear> εἰ<unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">οσι-</supplied>

    <lb n="35"/><supplied cert="high" reason="lost">τέσσαρας <num value="24"/>, ἀντὶ δὲ τ</supplied>ῶν <choice><corr>τόκων ἐνοικήσειν</corr><sic>τωκων ενοικησιν</sic></choice> τὴ<supplied cert="high" reason="lost">ν</supplied>

    <lb n="36"/><supplied cert="high" reason="lost">Κολλεῦτιν</supplied> καὶ τοὺς παρ᾽ αὐτῆς ἕω<supplied cert="high" reason="lost">ς</supplied>

    <lb n="37"/><supplied cert="high" reason="lost">Παῦνι ἕκτης τοῦ</supplied> ἐνεστῶτος δευτέρου 

    <lb n="38"/><supplied cert="high" reason="lost">ἔτους Τιβερίου Καίσαρος Σεβαστοῦ</supplied>

    <lb n="39"/><supplied cert="high" reason="lost">ἐν τῷ ἡμίσει</supplied> <choice><corr>μέρει</corr><sic>μερ<unclear cert="high" reason="undefined">η</unclear></sic></choice> <unclear cert="high" reason="undefined">τῆ</unclear>ς κοινῆς καὶ

    <lb n="40"/><supplied cert="high" reason="lost">ἀδιαιρέτου οἰ</supplied><unclear cert="high" reason="undefined">κ</unclear>ίας ἐν Τεβτύνι κα<unclear cert="high" reason="undefined">ὶ</unclear>

    <lb n="41"/><supplied cert="high" reason="lost">μετὰ τὸν χρόν</supplied>ον <choice><corr>ἀποδώσομεν</corr><sic>απωδωσωμεν</sic></choice> τὰς ἐπ<unclear cert="high" reason="undefined">ι</unclear>-

    <lb n="42"/><supplied cert="high" reason="lost">σήμου ἀ</supplied>ργυρίου δραχμὰς εἰκοσι-

    <lb n="43"/><supplied cert="high" reason="lost">τέσσαρας <num value="24"/>. ἔγρα</supplied><unclear cert="high" reason="undefined">ψ</unclear>εν <unclear cert="high" reason="undefined">ὑπ</unclear>ὲ<unclear cert="high" reason="undefined">ρ</unclear> αὐτ<unclear cert="high" reason="undefined">ῶν</unclear>

    <lb n="44"/><supplied cert="high" reason="lost">Πατῦνις Φαλαοῦτος</supplied> <unclear cert="high" reason="undefined">διὰ</unclear> <unclear cert="high" reason="undefined">τὸ</unclear> μὴ εἰ-

    <lb n="45"/><supplied cert="high" reason="lost">δέναι γράμματα.</supplied>
