    <lb n="1"/><expan><ex cert="high">ἔτους</ex></expan> <num value="2">β</num> Τιβερίου Καίσαρος. 
<expan><unclear reason="undefined" cert="high">ἐ</unclear>κδ[όσι<ex cert="high">μον</ex></expan> <expan><unclear reason="undefined" cert="high">Πα</unclear><supplied reason="lost" cert="high">χ<ex cert="high">ὼν</ex></supplied></expan> <num value="10">ι</num>.

    <lb n="2"/>Ἔτους δευτέρου Τιβερίου Καίσαρος Σεβ<supplied reason="lost" cert="high">α</supplied>στοῦ μη-

    <lb n="3"/>νὸς <choice><corr>Πανέμου</corr><sic>πανημου</sic></choice> δεκάτ<unclear reason="undefined" cert="high">η</unclear>ͅ Παχ<unclear reason="undefined" cert="high">ὼ</unclear>ν δεκάτῃ 

    <lb n="4"/>ἐν Τεβτύνι τῆς Πολέμωνος μερίδος

    <lb n="5"/>τοῦ <unclear reason="undefined" cert="high">Ἀρσι</unclear>ν<unclear reason="undefined" cert="high">οεί</unclear>του νομοῦ. <expan>ὁμο<unclear reason="undefined" cert="high">λογ</unclear><ex cert="high">οῦσιν</ex></expan> 
Μαρσισοῦχος

    <lb n="6"/>Μαρεψήμιος Πέρσης τῆς <expan>ἐπιγον<ex cert="high">ῆς</ex></expan> ὡς ἐτῶν πεν-

    <lb n="7"/>τήκοντα <num value="50"/> οὐλὴ ἀντίχειρι δεξιᾷ καὶ <supplied reason="omitted" cert="high">ἡ</supplied>

    <lb n="8"/><supplied reason="omitted" cert="high">τού</supplied>του γυνὴ Ταμαρρῆς Μάρωνος ὡς ἐτῶν τριάκον-

    <lb n="9"/>τα <num value="30"/> οὐλὴ ὀφ<unclear reason="undefined" cert="high">ρύ</unclear>ι ἀριστερᾷ Περσίνη <supplied reason="lost" cert="high">μ</supplied>ετὰ 
κυρίου τοῦ

    <lb n="10"/>προγεγραμμένου ἀνδρὸς ἀλλήλων ἔγγυ<unclear reason="undefined" cert="high">ο</unclear>ι εἰς

    <lb n="11"/>ἔκτεισιν ἔχιν παρὰ <choice><corr>Κολλεύτιος τῆς Ψοίφιος</corr><sic>κολλεύτις της ψοιφος</sic></choice>

    <lb n="12"/>ὡς ἐτῶν εἴκοσι δύο <num value="22"/> οὐλὴ ἀ<unclear reason="undefined" cert="high">στ</unclear><supplied reason="lost" cert="high">ρ</supplied>αγά<unclear reason="undefined" cert="high">λω</unclear>ͅ 
δεξιῷ

    <lb n="13"/>μετὰ κυρίου τοῦ πατρὸς αὐτῆς Ψοίφος τοῦ Πν<unclear reason="undefined" cert="high">εβ</unclear>-

    <lb n="14"/>τύ<unclear reason="undefined" cert="high">νεω</unclear>ς ὡς ἐτῶν τεσσαράκοντα <num value="40"/> οὐλὴ 
<expan>μετ<ex cert="high">ώπῳ</ex></expan> ἐξ <expan>ἀρ<ex cert="high">ιστερᾶς</ex></expan>

    <lb n="15"/>τὰς τοῦ ἀργυρίου ἐπισήμου νομίσματος κεφαλαίου

    <lb n="16"/>δραχμὰς <choice><corr>εἰκοσιτέσσαρας</corr><sic>εικοσιτεσσαρες</sic></choice> <num value="24"/>, ἀντὶ δὲ τῶν τούτων <expan><unclear reason="undefined" cert="high">τ</unclear>ό<ex cert="high">κων</ex></expan>

    <lb n="17"/><choice><corr>συγκεχωρηκέναι</corr><sic>συνκωχωρηκεναι</sic></choice> τῇ Κολλεῦτι <choice><corr>ἐνοικήσειν</corr><sic>ενοικησιν</sic></choice> ἕως

    <lb n="18"/><expan>μηνὸ<ex cert="high">ς</ex></expan> Παῦνι ἕκτης δευτέρου <expan><ex cert="high">ἔτους</ex></expan> 
Τιβερίου Καίσαρος Σεβαστοῦ

    <lb n="19"/>ἐν τῷ ἡμίσει <choice><corr>μέρει</corr><sic>μερ<unclear reason="undefined" cert="high">ους</unclear></sic></choice> τῆς κοινῆς καὶ ἀδιαιρέτου <expan>οἰκί<ex cert="high">ας</ex></expan>

    <lb n="20"/>ἐν Τεβτύνι. καὶ <choice><corr>χρήσεται</corr><sic>χρησηται</sic></choice> κατὰ κοινὸν ταύτῃ οἰκίᾳ

    <lb n="21"/>πᾶσι χρη<unclear reason="undefined" cert="high">στη</unclear>ρίοις, ἕως μετὰ τὸν χρὸν<supplied reason="lost" cert="high">ον</supplied> ἀποδώσο<unclear reason="undefined" cert="high">υσι</unclear> 

    <lb n="22"/>ἀργύριον. <expan>ὑ<unclear reason="undefined" cert="high">π</unclear><supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">γρα</unclear><ex cert="high">φεὺς</ex></expan> τοῦ 
<expan>ὁμο<ex cert="high">λογοῦντος</ex></expan> <supplied reason="lost" cert="high">Π</supplied><unclear reason="undefined" cert="high">ατύνις</unclear> λαού<unclear reason="undefined" cert="high">τος</unclear> ὡς

    <lb n="23"/>ἐτῶν τριάκοντα πέντε <num value="35"/> οὐλὴ ταρσῷ 
<expan>ἀριστερ<ex cert="high">ῷ</ex></expan>,

    <lb n="24"/>καὶ τῆς <expan>Κολλεύτιο<ex cert="high">ς</ex></expan> Μα<supplied reason="omitted" cert="high">ρ</supplied>σισοῦχος <choice><corr>Μαρεψήμιος</corr><sic>μαρεψημιου</sic></choice> ὡς <expan><ex cert="high">ἐτῶν</ex></expan> <num value="30">λ</num>

    <lb n="25"/><handShift new="m2"/> Κολλεῦχις <choice><corr>Ψοίφιος</corr><sic><unclear reason="undefined" cert="high">ψ</unclear>οιφις</sic></choice>· <choice><corr>γέγονε εἴς με</corr><sic>γεγοναι εις μαι</sic></choice>. ἔγραψεν ὑπὲρ αὐτῆς

    <lb n="26"/>Μαρσισοῦχ<unclear reason="undefined" cert="high">ος</unclear> <choice><corr>Μαρεψήμιος</corr><sic>μαρεψημιου</sic></choice>.

    <lb n="27"/><handShift new="m3"/><g type="filled-circle"/> Μαρσισοῦχος <choice><corr>Μαρεψήμιος</corr><sic>μαρεψημις</sic></choice> Πέρσης τῆς ἐπιγονῆς

    <lb n="28"/>καὶ ἡ τούτου γυνὴ Ταμαρῆς Μάρωνος Περ-

    <lb n="29"/><unclear reason="undefined" cert="high">σί</unclear>νη μετὰ <choice><corr>κυρίου</corr><sic>κυρυου</sic></choice> τοῦ προγεγραμμένου ἑα-

    <lb n="30"/><supplied reason="lost" cert="high">υτ</supplied>ῆς ἀνδρὸς ἀλλήλων <choice><corr>ἔγγυοι εἰς ἔκ-

    <lb n="31"/>τισιν</corr><sic>ενγυο<unclear reason="undefined" cert="high">ι</unclear> <sic>ει</sic> εις εκ<supplied reason="lost" cert="high">τ</supplied><unclear reason="undefined" cert="high">ε</unclear>ισιν</sic></choice> ὁμολογοῦμεν <choice><corr>ἔχειν</corr><sic>εχιν</sic></choice> παρὰ Κολλεύθ<supplied reason="lost" cert="high">ιος</supplied>

    <lb n="32"/>τῆς Ψοί<unclear reason="undefined" cert="high">φ</unclear>ειος μετὰ τοῦ <choice><corr>κυρίου</corr><sic>κυρυου</sic></choice> αὐτοῦ

    <lb n="33"/><supplied reason="lost" cert="high">Ψοίφιος τ</supplied>οῦ πατρὸς αὐτ<supplied reason="lost" cert="high">ῆς ἀργυ</supplied><unclear reason="undefined" cert="high">ρί</unclear><supplied reason="lost" cert="high">ο</supplied><unclear reason="undefined" cert="high">υ</unclear>

    <lb n="34"/><supplied reason="lost" cert="high">ἐπισήμου νομίσ</supplied><unclear reason="undefined" cert="high">ματο</unclear><supplied reason="lost" cert="high">ς κεφαλαίου δραχμὰ</supplied><unclear reason="undefined" cert="high">ς</unclear> εἰ<unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">οσι-</supplied>

    <lb n="35"/><supplied reason="lost" cert="high">τέσσαρας <num value="24"/>, ἀντὶ δὲ τ</supplied>ῶν <choice><corr>τόκων ἐνοικήσειν</corr><sic>τωκων ενοικησιν</sic></choice> τὴ<supplied reason="lost" cert="high">ν</supplied>

    <lb n="36"/><supplied reason="lost" cert="high">Κολλεῦτιν</supplied> καὶ τοὺς παρ᾽ αὐτῆς ἕω<supplied reason="lost" cert="high">ς</supplied>

    <lb n="37"/><supplied reason="lost" cert="high">Παῦνι ἕκτης τοῦ</supplied> ἐνεστῶτος δευτέρου 

    <lb n="38"/><supplied reason="lost" cert="high">ἔτους Τιβερίου Καίσαρος Σεβαστοῦ</supplied>

    <lb n="39"/><supplied reason="lost" cert="high">ἐν τῷ ἡμίσει</supplied> <choice><corr>μέρει</corr><sic>μερ<unclear reason="undefined" cert="high">η</unclear></sic></choice> <unclear reason="undefined" cert="high">τῆ</unclear>ς κοινῆς καὶ

    <lb n="40"/><supplied reason="lost" cert="high">ἀδιαιρέτου οἰ</supplied><unclear reason="undefined" cert="high">κ</unclear>ίας ἐν Τεβτύνι κα<unclear reason="undefined" cert="high">ὶ</unclear>

    <lb n="41"/><supplied reason="lost" cert="high">μετὰ τὸν χρόν</supplied>ον <choice><corr>ἀποδώσομεν</corr><sic>απωδωσωμεν</sic></choice> τὰς ἐπ<unclear reason="undefined" cert="high">ι</unclear>-

    <lb n="42"/><supplied reason="lost" cert="high">σήμου ἀ</supplied>ργυρίου δραχμὰς εἰκοσι-

    <lb n="43"/><supplied reason="lost" cert="high">τέσσαρας <num value="24"/>. ἔγρα</supplied><unclear reason="undefined" cert="high">ψ</unclear>εν <unclear reason="undefined" cert="high">ὑπ</unclear>ὲ<unclear reason="undefined" cert="high">ρ</unclear> αὐτ<unclear reason="undefined" cert="high">ῶν</unclear>

    <lb n="44"/><supplied reason="lost" cert="high">Πατῦνις Φαλαοῦτος</supplied> <unclear reason="undefined" cert="high">διὰ</unclear>  μὴ εἰ-

    <lb n="45"/><supplied reason="lost" cert="high">δέναι γράμματα.</supplied>
