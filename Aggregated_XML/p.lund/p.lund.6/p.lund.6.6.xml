    <lb n="1"/>ἀντίγραφον <supplied reason="lost" cert="high">διαθήκης</supplied> <expan><ex cert="high">ἔτους</ex></expan> <num value="31">λα</num> Αυ<gap reason="illegible" extent="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" extent="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἀγορα</supplied>-

    <lb n="2"/>νόμου. <choice><corr>τάδε</corr><sic>τατε</sic></choice> διέθετο ν<supplied reason="lost" cert="high">οοῦσ</supplied><unclear reason="undefined" cert="high">α</unclear> καὶ φρονοῦσα ι α δ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>τοῦ Θέωνος ἀπὸ τῆς αὐτῆς κώμης <gap reason="illegible" extent="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" extent="4" unit="character"/> <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> ἐὰν δὲ ἐπὶ ταύτῃ τελευτή</supplied>-

