    <lb n="8"/>ὀκτώ. <expan><ex cert="high">γίνονται</ex></expan> <expan><ex cert="high">δραχμαὶ</ex></expan> <num value="528">φ<unclear cert="high" reason="undefined">κη</unclear></num> <gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="illegible" unit="character"/> 

    <lb n="9"/>ἐπαγο<supplied cert="high" reason="lost">μ</supplied>ένων πέμπτηι <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">οῦ</supplied> 

    <lb n="10"/>ἐνεστῶτος ἔτου<supplied cert="high" reason="lost">ς</supplied> <gap extent="3" reason="illegible" unit="character"/>κ<unclear cert="high" reason="undefined">α</unclear><gap extent="4" reason="illegible" unit="character"/>ι<gap extent="1" reason="illegible" unit="character"/> 

    <lb n="11"/>καὶ γράφομαι τῆι αὐτῆι <unclear cert="high" reason="undefined">θ</unclear>υγ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">τρ</supplied><unclear cert="high" reason="undefined">ί</unclear> 

    <lb n="12"/>σου διὰ δημοσίου τὴν κα<unclear cert="high" reason="undefined">θ</unclear>ή-

    <lb n="13"/>κουσαν τοῦ γάμου συγγραφήν. 

    <lb n="14"/>ἐὰ<unclear cert="high" reason="undefined">ν</unclear> δὲ πρὶν <choice><corr>γ<supplied cert="high" reason="omitted">ρ</supplied>άψωμαι</corr><sic>γαψωμαι</sic></choice> α<supplied cert="high" reason="lost">ὐ</supplied>-

    <lb n="15"/>τὴν συμβῆι <supplied cert="high" reason="lost">ἡμ</supplied>ᾶς ἐκ διαφο<supplied cert="high" reason="lost">ρᾶς</supplied> 

    <lb n="16"/>ἀπαλλαγῆναι <unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">α</supplied><unclear cert="high" reason="undefined">ὶ</unclear> <unclear cert="high" reason="undefined">η</unclear><gap extent="4" reason="illegible" unit="character"/><gap extent="4" reason="illegible" unit="character"/> 

    <lb n="17"/>βούληται τῆς φερνῆς ἀπα<unclear cert="high" reason="undefined">ίτ</unclear><supplied cert="high" reason="lost">ησιν</supplied> 

    <lb n="18"/>ποιεῖσθαι, ἀποδώσω τὰς 

    <lb n="19"/><expan>ἀργ<ex cert="high">υρίου</ex></expan> <expan><ex cert="high">δραχμὰς</ex></expan> <num value="528">φκη</num> ἐν ἡμέραις <unclear cert="high" reason="undefined">ἑ</unclear><supplied cert="high" reason="lost">ξ</supplied><unclear cert="high" reason="undefined">ή</unclear>-

    <lb n="20"/>κοντα <num value="60"/>, ἀφ᾽ ἧς ἐὰν ἀπα<supplied cert="high" reason="lost">ιτ</supplied><unclear cert="high" reason="undefined">η</unclear>θ<unclear cert="high" reason="undefined">ῆ</unclear><supplied cert="high" reason="lost">ι</supplied>, 

    <lb n="21"/>χωρὶς πάσ<unclear cert="high" reason="undefined">η</unclear><supplied cert="high" reason="lost">ς</supplied> ὑπερθέσε<supplied cert="high" reason="lost">ως ἐξόν</supplied>-

    <lb n="22"/>τος σοι ἔχειν <choice><corr><supplied cert="high" reason="lost">τ</supplied>ὴ<supplied cert="high" reason="omitted">ν</supplied></corr><sic><supplied cert="high" reason="lost">τ</supplied>η</sic></choice> διάλυσιν <supplied cert="high" reason="lost">ἐν τῆι</supplied> 

    <lb n="23"/>αὐτῆι <choice><corr>διατιμήσει</corr><sic>διατειμησει</sic></choice> <unclear cert="high" reason="undefined">κατὰ</unclear> 

    <lb n="24"/>τὴν διατίμησιν τῆς ὑπα<supplied cert="high" reason="lost">ρχ</supplied>-

    <lb n="25"/>ούσης περὶ σέ. ἐὰν δὲ <supplied cert="high" reason="lost">μὴ</supplied> 

    <lb n="26"/>ποιῶ ὡς γέγραπται, ἐκ<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">είσω</supplied> 

    <lb n="27"/>τὸ προκ<unclear cert="high" reason="undefined">ε</unclear>ίμενον <unclear cert="high" reason="undefined">κ</unclear>ε<unclear cert="high" reason="undefined">φ</unclear>άλαιον μεθ᾽ ἡ-

    <lb n="28"/>μιολίας τῆς πράξ<supplied cert="high" reason="lost">εώς σοι οὔσης</supplied> 

    <lb n="29"/>ἔκ τε ἐμοῦ καὶ ἐκ <unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ῶν ὑπαρχόντων</supplied> 

    <lb n="30"/>μοι πάντων <choice><corr>καθάπερ</corr><sic>καταπερ</sic></choice> <choice><corr>ἐκ</corr><sic>εγ</sic></choice> δίκ<supplied cert="high" reason="lost">ης.</supplied> 

    <lb n="31"/>κυρία ἡ ἰδιόγραφος σ<unclear cert="high" reason="undefined">υγ</unclear>γ<supplied cert="high" reason="lost">ρ</supplied>α<unclear cert="high" reason="undefined">φ</unclear><supplied cert="high" reason="lost">ή.</supplied> 

