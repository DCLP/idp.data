    <lb n="6"/><supplied cert="high" reason="lost">ἔτου</supplied>ς δεκ<unclear cert="high" reason="undefined">ά</unclear><supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ου</unclear> <expan><unclear cert="high" reason="undefined">Αὐτ</unclear><ex cert="high">οκράτορος</ex></expan> <expan><unclear cert="high" reason="undefined">Ἀντ</unclear>ων<unclear cert="high" reason="undefined">εί</unclear><ex cert="high">νου</ex></expan> 

