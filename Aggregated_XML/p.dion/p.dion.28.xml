    <lb n="1"/><supplied cert="high" reason="lost">ἔτους </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"><foreign lang="la">,</foreign> ἐν Ἑρμοῦ πόλει τῆς Θηβαΐδος</supplied> 

    <lb n="2"/><supplied cert="high" reason="lost">ὁμολογεῖ Δίδυμος </supplied><gap extent="unknown" reason="lost" unit="character"/><supplied cert="high" reason="lost"> Διονυσίωι Κεφαλᾶτος</supplied> 

    <lb n="3"/>Π<unclear cert="high" reason="undefined">έρσηι</unclear> <supplied cert="high" reason="lost">τῆς ἐπιγονῆς καὶ τῆι μητρὶ αὐ</supplied>τοῦ <supplied cert="high" reason="lost">Δημητρίαι</supplied> 

    <lb n="4"/>ἀπέχει<unclear cert="high" reason="undefined">ν</unclear> π<unclear cert="high" reason="undefined">α</unclear>ρ᾽ <unclear cert="high" reason="undefined">α</unclear>ὐτ<unclear cert="high" reason="undefined">ῶ</unclear>ν τὰς <num value="250">σ<unclear cert="high" reason="undefined">ν</unclear></num> <supplied cert="high" reason="lost">ἀρ</supplied><unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">άβα</supplied>ς, 

    <lb n="5"/>τὸ δάνει<supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ν</unclear> ὃ ἐδάνε<supplied cert="high" reason="lost">ι</supplied>σεν αὐτοῖς κατὰ συγγραφὴν 

    <lb n="6"/>δανείου <unclear cert="high" reason="undefined">τ</unclear>ὴν κειμ<unclear cert="high" reason="undefined">έ</unclear>νην διὰ τ<unclear cert="high" reason="undefined">ο</unclear>ῦ ἐν Ἑρμοῦ <expan>πόλ<ex cert="high">ει</ex></expan> 

    <lb n="7"/>τῆς Θηβαΐ<unclear cert="high" reason="undefined">δ</unclear>ος ἀγορα<unclear cert="high" reason="undefined">ν</unclear>ομίου τοῦ <num value="7">ζ</num> <expan><ex cert="high">ἔτους</ex></expan> Θῶυθ <num value="9">θ</num>. 

    <lb n="8"/>καὶ μήτ᾽ αὐτὸν Δίδ<unclear cert="high" reason="undefined">υ</unclear>μον μη<unclear cert="high" reason="undefined">δ</unclear>᾽ <unclear cert="high" reason="undefined">ἄλ</unclear>λον ὑπὲρ 

    <lb n="9"/>αὐτοῦ ἐπελεύσεσθ<unclear cert="high" reason="undefined">α</unclear>ι ἐπὶ <unclear cert="high" reason="undefined">Δ</unclear>ιο<unclear cert="high" reason="undefined">ν</unclear>ύσιον <unclear cert="high" reason="undefined">κ</unclear>αὶ 

    <lb n="10"/>Δημητ<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">ί</supplied><unclear cert="high" reason="undefined">α</unclear>ν μη<unclear cert="high" reason="undefined">δ</unclear>᾽ <supplied cert="high" reason="lost">ἐ</supplied>πὶ τοὺς παρ᾽ αὐτῶν 

    <lb n="11"/>περὶ τοῦ προκει<unclear cert="high" reason="undefined">μέ</unclear>νου πυροῦ οὗ ἀ<unclear cert="high" reason="undefined">π</unclear>έχει, 

    <lb n="12"/>μηδὲ πε<unclear cert="high" reason="undefined">ρ</unclear>ὶ μέρους αὐτοῦ, παρευρέσει μηδε-

    <lb n="13"/>μίᾳ· ἐὰν <unclear cert="high" reason="undefined">δὲ</unclear> ἐπέλθῃ, ἥ τ᾽ ἔφοδος Διδύ<unclear cert="high" reason="undefined">μ</unclear>ωι 

    <lb n="14"/>καὶ τῶι ὑ<unclear cert="high" reason="undefined">π</unclear>ὲρ αὐτοῦ ἐπιπορευομ<unclear cert="high" reason="undefined">έν</unclear>ωι 

    <lb n="15"/>ἄκυρος ἔσ<supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ω</unclear> καὶ προσαποτεισάτω <supplied cert="high" reason="lost">Δ</supplied><unclear cert="high" reason="undefined">ί</unclear>δυμος 

    <lb n="16"/><unclear cert="high" reason="undefined">Δ</unclear>ιονυσίω<supplied cert="high" reason="lost">ι καὶ Δημ</supplied><unclear cert="high" reason="undefined">η</unclear>τρίᾳ ἢ τοῖς παρ᾽ <unclear cert="high" reason="undefined">αὐ</unclear>τῶν, 

    <lb n="17"/>καθ᾽ ὅτι ἂ<unclear cert="high" reason="undefined">ν</unclear> ἐπέλθῃ, ἐπίτιμον <expan>χα<ex cert="high">λκοῦ</ex></expan> <expan><ex cert="high"><unclear cert="high" reason="undefined">τάλαντα</unclear></ex></expan> <num value="125">ρκε</num> 

    <lb n="18"/>καὶ τὸ βλάβος <unclear cert="high" reason="undefined">κ</unclear>αὶ μηθὲν ἧσσον ἡ ὁμ<unclear cert="high" reason="undefined">ολο</unclear>γία 

    <lb n="19"/>ἥδε κυρ<unclear cert="high" reason="undefined">ία</unclear> ἔσ<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">ω</supplied>. 
