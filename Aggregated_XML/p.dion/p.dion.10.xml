    <lb n="1"/>Νικάνορι καὶ τοῖς συ<unclear cert="high" reason="undefined">νφ</unclear>υλακίταις Κιρκᾶ 

    <lb n="2"/>παρὰ Πα<unclear cert="high" reason="undefined">ήσ</unclear>ιος τοῦ καὶ <gap extent="1" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/>γης Κεφαλᾶτος τῶν <unclear cert="high" reason="undefined">ἐ</unclear><supplied cert="high" reason="lost">κ</supplied> τῆς 

    <lb n="3"/>Ἀκώριος <choice><corr>Λίβυος</corr><sic>λιβυς</sic></choice> τῶν <supplied cert="high" reason="lost">Δ</supplied>ημητρίου ἱππέων μισθοφόρων. 

    <lb n="4"/>τῆι νυ<unclear cert="high" reason="undefined">κ</unclear>τὶ τῆι φερ<unclear cert="high" reason="undefined">ού</unclear>σηι εἰς τὴν <num value="14">ιδ</num> τοῦ Ἁθὺρ τοῦ <num value="9">θ</num> <expan><ex cert="high">ἔτους</ex></expan> 

    <lb n="5"/>ἐπιβαλόν<unclear cert="high" reason="undefined">τ</unclear>ες τιν<unclear cert="high" reason="undefined">ὲς</unclear> <add place="supralinear">λῃ<supplied cert="high" reason="lost">σ</supplied>τικῷ τρόπωι</add> <supplied cert="high" reason="lost">εἰς</supplied> <unclear cert="high" reason="undefined">τὸν</unclear> ὑπάρχοντά <unclear cert="high" reason="undefined">μ</unclear><supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ι</unclear> <supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ἶκ</unclear><supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">ν</unclear> <del rend="erasure" status="unremarkable">π</del> 

    <lb n="6"/>περὶ τὴν αὐτὴν <unclear cert="high" reason="undefined">ἐ</unclear>ξ<supplied cert="high" reason="lost">έ</supplied>δυσαν τοὺς ἐνοικοῦ<unclear cert="high" reason="undefined">ν</unclear>τ<unclear cert="high" reason="undefined">ας</unclear> <unclear cert="high" reason="undefined">κ</unclear>αὶ 

    <lb n="7"/>τὰ πρὸς τὴ<unclear cert="high" reason="undefined">ν</unclear> γεωργικὴ<unclear cert="high" reason="undefined">ν</unclear> κατασκευὴν καὶ τὰ <choice><corr>ἄλλα</corr><sic>ταλλα</sic></choice> τὰ ἐν τῶι 

    <lb n="8"/>αὐτῶι τόπωι ᾤχον<supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">ο</unclear> ἔχοντες, ὧν τὸ καθ᾽ ἓν ὑπόκειται. 

    <lb n="9"/><unclear cert="high" reason="undefined">ἐπε</unclear>ὶ οὖν ὑπο<unclear cert="high" reason="undefined">λα</unclear>μβάνω <unclear cert="high" reason="undefined">τὰ</unclear> τῆς ἐπιθέσεως <unclear cert="high" reason="undefined">γε</unclear>γονέναι 

    <lb n="10"/><gap extent="1" reason="illegible" unit="character"/> Κωννῶτος <choice><corr>Κυνοπολίτου</corr><sic>κυνοπ<supplied cert="high" reason="lost">ο</supplied><unclear cert="high" reason="undefined">λ</unclear>ειτου</sic></choice>, καταγεινομέν<unclear cert="high" reason="undefined">ο</unclear>υ δ᾽ ἐν <unclear cert="high" reason="undefined">τ</unclear>ῆι 

    <lb n="11"/><unclear cert="high" reason="undefined">Α</unclear>ἴθωνος, κ<unclear cert="high" reason="undefined">α</unclear>ὶ <unclear cert="high" reason="undefined">ἑ</unclear>τέρ<unclear cert="high" reason="undefined">ων</unclear> <supplied cert="high" reason="lost">σὺ</supplied><unclear cert="high" reason="undefined">ν</unclear> <unclear cert="high" reason="undefined">α</unclear>ὐτῶι, οὓς τ<unclear cert="high" reason="undefined">ὰ</unclear> <supplied cert="high" reason="lost">ὀ</supplied><unclear cert="high" reason="undefined">ν</unclear><supplied cert="high" reason="lost">όμα</supplied><unclear cert="high" reason="undefined">τα</unclear> 

    <lb n="12"/>ἀγνοῶι, ἐπιδίδω<unclear cert="high" reason="undefined">μ</unclear>ι ὑμῖν τὴν προσα<unclear cert="high" reason="undefined">γ</unclear>γελίην 

    <lb n="13"/>ὅπως οἱ α<unclear cert="high" reason="undefined">ἴ</unclear>τιοι ἀναζ<unclear cert="high" reason="undefined">η</unclear>τηθέντες ἐξαποσταλῶ<unclear cert="high" reason="undefined">σι</unclear> 

    <lb n="14"/>ἐπὶ τὸν στ<unclear cert="high" reason="undefined">ρ</unclear>ατηγόν, <supplied cert="high" reason="lost">ἵν</supplied><unclear cert="high" reason="undefined">α</unclear> ἐμοὶ μὲν <supplied cert="high" reason="omitted">τὰ</supplied> διαπεφωνημένα 

