    <lb n="16"/>κολουθούντ<unclear cert="high" reason="undefined">ω</unclear>ν· τούτου <unclear cert="high" reason="undefined">δ</unclear>ὲ γενομένου ἔσομαι αν 

    <lb n="17"/><gap extent="1" reason="lost" unit="character"/><gap extent="2" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">λ</unclear>λη<gap extent="1" reason="illegible" unit="character"/>ε<gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/>                   <supplied cert="high" reason="lost">ε</supplied>ὐτύχ<unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">ι</supplied>. 

    <lb n="18"/><expan><ex cert="high"><unclear cert="high" reason="undefined">ἔτους</unclear></ex></expan> <supplied cert="high" reason="lost"><num value="9">θ</num> Ἁ</supplied><unclear cert="high" reason="undefined">θὺρ</unclear>  <num value="16">ιϛ</num> 

    <lb n="19"/>ἔστιν δὲ <supplied cert="high" reason="lost">τὸ κ</supplied>αθ᾽ ἕν· 

    <lb n="20"/><unclear cert="high" reason="undefined">Σ</unclear>ωτιόγχι<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ς</supplied>· <choice><corr>χιτὼν</corr><sic>κιτων</sic></choice> <unclear cert="high" reason="undefined">κ</unclear>αὶ ἱμάτιον καὶ <choice><corr>σάκκος</corr><sic>σακκον</sic></choice>, 

    <lb n="21"/>Πλῆνις <unclear cert="high" reason="undefined">ὁ</unclear> <unclear cert="high" reason="undefined">ἀ</unclear>δελφός· <choice><corr>χιτών</corr><sic>κιτων</sic></choice>, ἄροτρον <num value="1">α</num>, ζυ<unclear cert="high" reason="undefined">γ</unclear><supplied cert="high" reason="lost">ὸ</supplied>ν <num value="1">α</num>, 

