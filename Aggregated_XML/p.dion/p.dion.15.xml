    <lb n="1"/>βασιλευόντων Κλεοπάτρας <del rend="erasure" status="unremarkable"><unclear reason="undefined" cert="high">θ</unclear></del> καὶ Πτολεμαίου <choice><corr>θεῶ<supplied reason="omitted" cert="high">ν</supplied></corr><sic>θεω</sic></choice> Φιλομητό<unclear reason="undefined" cert="high">ρ</unclear><supplied reason="lost" cert="high">ων</supplied> 

    <lb n="2"/>Σωτήρων ἔτους ὀγδόου, ἐφ᾽ ἱερέως τοῦ ὄντος ἐν <supplied reason="omitted" cert="high">Ἀλεχανδρείαι</supplied> Ἀλεξάνδρου κ<supplied reason="lost" cert="high">αὶ θεῶν Σω</supplied>-

    <lb n="3"/>τήρων καὶ θεῶν Ἀδελφῶν καὶ θεῶν Εὐεργετῶν καὶ θεῶν Φ<supplied reason="lost" cert="high">ιλοπατόρων</supplied> 

    <lb n="4"/>καὶ θεῶν Ἐπιφα<unclear reason="undefined" cert="high">ν</unclear>ῶ<unclear reason="undefined" cert="high">ν</unclear> ὶ <unclear reason="undefined" cert="high">θεο</unclear>ῦ <unclear reason="undefined" cert="high">Ε</unclear>ὐ<unclear reason="undefined" cert="high">π</unclear>άτορος καὶ θεοῦ Φιλομήτορος κ<unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">ὶ θεοῦ Νέου</supplied> 

    <lb n="5"/>Φιλοπάτορος καὶ θεοῦ Εὐεργέ<unclear reason="undefined" cert="high">το</unclear>υ καὶ <choice><corr>θεῶ<supplied reason="omitted" cert="high">ν</supplied></corr><sic>θεω</sic></choice> Φιλομητόρων Σωτήρων, <supplied reason="lost" cert="high">ἱεροῦ πώ</supplied>-

    <lb n="6"/>λου Ἴσιδος μητρὸς θεῶν μεγάλης, στεφανηφόρου βασιλίσσης Κλε<unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">πάτρας</supplied> 

    <lb n="7"/>θεᾶς Φιλομήτορος Σωτείρας Δικαιοσύνης Νικηφόρου, ἀθλοφόρου Βερε<supplied reason="lost" cert="high">νίκης</supplied> 

    <lb n="8"/>Εὐεργέτιδος, φωσφόρου βασιλίσσης Κλεοπάτρας θεᾶς Φιλομήτορος <supplied reason="lost" cert="high">Σωτείρας</supplied> 

    <lb n="9"/>Δικαιοσ<unclear reason="undefined" cert="high">ύ</unclear>νης Νι<unclear reason="undefined" cert="high">κη</unclear>φόρου, <unclear reason="undefined" cert="high">καν</unclear>ηφόρου Ἀρσινόης <unclear reason="undefined" cert="high">Φ</unclear>ιλα<unclear reason="undefined" cert="high">δέλ</unclear>φο<unclear reason="undefined" cert="high">υ</unclear>, ἱ<unclear reason="undefined" cert="high">ερ</unclear>είας β<supplied reason="lost" cert="high">ασιλίσσης</supplied> 

    <lb n="10"/>Κλεοπάτρας θεᾶς Φιλομήτορος Σωτείρας Δικαιοσύνης Νικηφόρου, <unclear reason="undefined" cert="high">ἱ</unclear><supplied reason="lost" cert="high">ερείας Ἀρσι</supplied>-

    <lb n="11"/>νόης Φιλοπάτορος τῶν ὄντ<unclear reason="undefined" cert="high">ων</unclear>    Ἀ<unclear reason="undefined" cert="high">λε</unclear>ξανδρείαι, μ<unclear reason="undefined" cert="high">η</unclear><supplied reason="lost" cert="high">νὸς Δύστρου</supplied> 

    <lb n="12"/>ἑκκαιδεκάτῃ, Τῦβι ἑκκαιδ<unclear reason="undefined" cert="high">ε</unclear>κάτῃ, ἐν Τήνει <unclear reason="undefined" cert="high">τῆι</unclear>  ώριος τ<supplied reason="lost" cert="high">οῦ Μωχίτου</supplied> 

    <lb n="13"/>τόπου, νομοῦ δ᾽ Ἑρμοπολίτ<unclear reason="undefined" cert="high">ου</unclear>. <supplied reason="lost" cert="high">ἐδάνεισεν Διονύσιος</supplied> <unclear reason="undefined" cert="high">Ἀ</unclear>σκληπιάδ<unclear reason="undefined" cert="high">ο</unclear><supplied reason="lost" cert="high">υ Πέρσης</supplied> 

    <lb n="14"/>τῶν Ἀπολλοφάνου καὶ <unclear reason="undefined" cert="high">Ἐξακ</unclear><supplied reason="lost" cert="high">ῶντος κατοίκων ἱπ</supplied><unclear reason="undefined" cert="high">π</unclear>έων <unclear reason="undefined" cert="high">Δ</unclear>ιον<supplied reason="lost" cert="high">υσίωι Κεφα</supplied>-

    <lb n="15"/>λᾶτος Πέρσηι τῆς ἐπιγονῆς πυροῦ ἀρτάβας εἴκοσι ἓξ δίμοι<unclear reason="undefined" cert="high">ρ</unclear><supplied reason="lost" cert="high">ον <num value="26  2/3 "/>, ἃς</supplied> 

    <lb n="16"/>ἐκτεταμίευται παρ᾽ αὐτοῦ ἐξ οἴκου, τόκων ἡμιολίων, ἀρτάβ<unclear reason="undefined" cert="high">α</unclear><supplied reason="lost" cert="high">ις δέκα</supplied> 

    <lb n="17"/>τρισὶ τρίτωι <num value="13  1/3 "/>· τὰς δὲ πάσας πυροῦ ἀρτάβας τεσσαράκοντα <num value="40"/> ἀποδ<supplied reason="lost" cert="high">ότω</supplied> 

    <lb n="18"/>ὁ δεδα<unclear reason="undefined" cert="high">ν</unclear>εισμένος Διονυσίωι ἐν μηνὶ Λωίωι ὅς ἐστιν Παῦνι <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">οῦ ὀγδόου</supplied> 

    <lb n="19"/>ἔτους, πυρὸν νέον καθαρὸν καὶ ἄδολον, μέτρωι τῶι πρὸς <unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">ὸ</supplied> χ<supplied reason="lost" cert="high">αλκοῦν,</supplied> 

    <lb n="20"/>καθεσταμένας ἐπὶ τὸν <sic>καθεσταμένας ἐπὶ τὸν</sic> κατὰ τὴν Ἀκώ<supplied reason="lost" cert="high">ριος ὅρμον,</supplied> 

    <lb n="21"/>ἄνευ δίκης καὶ κρίσεως καὶ πάσης εὑρησιλογίας·   ἐὰν δὲ μὴ ἀ<supplied reason="lost" cert="high">ποδῷ</supplied> 

    <lb n="22"/>καθ᾽ ἃ γέγραπται, ἀποτεισάτω ὁ δεδανεισμένος Διονυσίωι τιμ<supplied reason="lost" cert="high">ὴν ἑκάσ</supplied>-

    <lb n="23"/>της ἀρτ<unclear reason="undefined" cert="high">ά</unclear>βης χαλκοῦ δρ<unclear reason="undefined" cert="high">α</unclear>χμ<unclear reason="undefined" cert="high">ὰ</unclear>ς τρισχιλίας <num value="3000"/> καὶ εἰς τὸ βασιλικὸν ἱερὰ<supplied reason="lost" cert="high">ς ἀργυρίου</supplied> 

    <lb n="24"/>τοῦ <subst><add place="inline">Πτολεμαϊκοῦ</add><del rend="corrected" status="unremarkable">πτολεμαιουου</del></subst> νομίσματος <subst><add place="inline">δραχμὰς</add><del rend="corrected" status="unremarkable">χραχμας</del></subst> <unclear reason="undefined" cert="high">μηθ</unclear><supplied reason="lost" cert="high">ὲν ἧσσον·</supplied> 

    <lb n="25"/>ἡ δὲ πρᾶξις ἔστω Διονυσίωι τοῦ πυροῦ καὶ τῶν κατὰ τὴν <supplied reason="lost" cert="high">συγγραφὴν</supplied> 

    <lb n="26"/>πάντων ἔκ τε <add place="supralinear">αὐτοῦ</add> Διονυσίου τοῦ δεδανεισμένου καὶ ἐκ τῶν <unclear reason="undefined" cert="high">ὑ</unclear><supplied reason="lost" cert="high">παρχόν</supplied>-

    <lb n="27"/>των αὐτῶι πάντων καθ᾽ ἅπερ <choice><corr>ἐκ</corr><sic>εγ</sic></choice> δίκης· ἡ συγγραφὴ ἥδε κυρί<unclear reason="undefined" cert="high">α</unclear> <supplied reason="lost" cert="high">ἔστω</supplied>. 

    <lb n="28"/>μάρτυρες· Πτολεμαῖος Ἀναξαγόρου Μιλήσιος, Ἀρίμ<unclear reason="undefined" cert="high">μας</unclear> <supplied reason="lost" cert="high">σίου *χαρισ</supplied>-

    <lb n="29"/>τήριος, Ἀγήνωρ Βαρκαίου Λακεδαιμόνιος, Διονύσιος Πτολεμαίο<unclear reason="undefined" cert="high">υ</unclear> <supplied reason="lost" cert="high">Μακεδὼν</supplied> 

    <lb n="30"/>ὑπηρέτης <choice><corr>τάγματος</corr><sic>προσταγματος</sic></choice>, Εὐμένης Πολυκράτου Πέρσης, <supplied reason="lost" cert="high">Ἀπολλώνιος</supplied> 

    <lb n="31"/>Πολυκράτου Πέρσης.         συγγραφοφύλαξ· Πτο<supplied reason="lost" cert="high">λεμαῖος</supplied>. 

    <lb n="32"/><handShift new="m2"/> Δι<unclear reason="undefined" cert="high">ο</unclear>νύσιος <unclear reason="undefined" cert="high">Κε</unclear>φ<unclear reason="undefined" cert="high">αλᾶτος</unclear> Πέρσης τῆς ἐπιγονῆς   ἔχω <supplied reason="lost" cert="high">τὰς</supplied> 

    <lb n="33"/><unclear reason="undefined" cert="high">τ</unclear>οῦ πυροῦ ἀρτ<unclear reason="undefined" cert="high">άβα</unclear>ς τεσσαράκοντα <num value="40"/> καθ᾽ ὅτι πρόκει<unclear reason="undefined" cert="high">τ</unclear><supplied reason="lost" cert="high">αι</supplied> 

    <lb n="34"/>καὶ ποιήσω κατ<unclear reason="undefined" cert="high">ὰ</unclear> τὰ <choice><corr>ἄλλα</corr><sic><unclear reason="undefined" cert="high">ταλλα</unclear></sic></choice> <unclear reason="undefined" cert="high">τὰ</unclear> γεγραμμένα καὶ δέδ<supplied reason="lost" cert="high">ω</supplied><unclear reason="undefined" cert="high">κα</unclear> <supplied reason="lost" cert="high">κυρίαν</supplied> 

    <lb n="35"/><supplied reason="lost" cert="high">Πτολεμαίωι</supplied>. 

    <lb n="36"/><handShift new="m3"/> <supplied reason="lost" cert="high">Πτολεμαῖος ἔχω κυρίαν</supplied>. 
</ab></div><div type="textpart" n="r,int" org="uniform" part="N" sample="complete"><ab part="N">

