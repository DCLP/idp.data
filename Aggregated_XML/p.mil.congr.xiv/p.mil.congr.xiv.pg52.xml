    <lb n="1"/><unclear reason="undefined" cert="high">ἔ</unclear>τους τετ<unclear reason="undefined" cert="high">άρ</unclear>του Αὐτ<unclear reason="undefined" cert="high">οκ</unclear>ράτορ<unclear reason="undefined" cert="high">ος</unclear> Κ<unclear reason="undefined" cert="high">αίσα</unclear>ρος Μάρκου Αὐρηλίου Σεουήρου 

    <lb n="2"/><unclear reason="undefined" cert="high">Ἀ</unclear>λεξάνδρου Εὐσεβοῦς <unclear reason="undefined" cert="high">Ε</unclear><supplied reason="lost" cert="high">ὐ</supplied>τ<unclear reason="undefined" cert="high">υ</unclear>χο<unclear reason="undefined" cert="high">ῦς</unclear> Σεβαστοῦ μηνὸς Τῦβι <choice><corr>πέμπτῃ</corr><sic>πεμτη</sic></choice> 

    <lb n="3"/>καὶ εἰκάδι ἐν Νείλου πό<unclear reason="undefined" cert="high">λει</unclear> ῇ μητροπόλει. ὁμολογεῖ Αὐρήλιος 

    <lb n="4"/>Κεφάλων Σεμθ<unclear reason="undefined" cert="high">έ</unclear>ως μη<unclear reason="undefined" cert="high">τ</unclear>ρὸς Διονυσαρίου ἀπὸ Νείλου πόλεως 

    <lb n="5"/>ὡς ἐτῶν πεντήκοντα δύο ἄσημος πεπρακέναι κατὰ 

    <lb n="6"/>τήνδε τὴν ὁμολογίαν Αὐρηλίῳ Ἀπίωνι Χαιρᾶ μητρὸς 

    <lb n="7"/>Μεθυσίου γυμνασιαρχήσαντι ἀρχιερατεύσαντι 

    <lb n="8"/>τῶν κυρίων Σεβαστῶν τῆς αὐτῆς πόλεως ἀπὸ τῶν 

    <lb n="9"/>κατηντηκότων εἰς αὐτὸν ἀπὸ κλη<unclear reason="undefined" cert="high">ρ</unclear>ονομίας τοῦ ὁμογνη-

    <lb n="10"/>σίου αὐτοῦ ἀδελφοῦ Αὐρηλίου Κολλούθου δούλην ὀνό-

    <lb n="11"/><unclear reason="undefined" cert="high">μα</unclear>τι Σω<unclear reason="undefined" cert="high">τη</unclear>ρίδα ὡς ἐτῶν ἐννέα <num value="9"/> ἄσημον τιμῆς τῆς 


    <lb n="12"/>συμπεφωνημένης πρὸς ἀλλήλους ἀργυρίου Σεβαστῶν 

    <lb n="13"/>νομ<unclear reason="undefined" cert="high">ίσμ</unclear>ατος δραχμῶν <choice><corr>χιλίων</corr><sic>χιλειων</sic></choice> ἑξακοσίων <num value="1600"/> ἃς καὶ 

    <lb n="14"/><supplied reason="lost" cert="high">αὐ</supplied><unclear reason="undefined" cert="high">τόθι</unclear> <supplied reason="lost" cert="high">π</supplied>ρο<unclear reason="undefined" cert="high">σ</unclear>ομολ<unclear reason="undefined" cert="high">ογεῖ</unclear>  <supplied reason="lost" cert="high">ε</supplied><unclear reason="undefined" cert="high">φ</unclear><supplied reason="lost" cert="high">ά</supplied><unclear reason="undefined" cert="high">λων</unclear> πεσχηκέ<unclear reason="undefined" cert="high">ν</unclear>αι παρὰ 

    <lb n="15"/>τοῦ <unclear reason="undefined" cert="high">Ἀπί</unclear>ω<supplied reason="lost" cert="high">ν</supplied><unclear reason="undefined" cert="high">ο</unclear>ς πλήρεις ἀ<unclear reason="undefined" cert="high">ρι</unclear><supplied reason="lost" cert="high">θμοῦ <expan><ex cert="high">δραχμὰς</ex></expan> <num value="1600">Αχ</num></supplied> 

    <lb n="16"/><unclear reason="undefined" cert="high">τ</unclear><gap reason="illegible" extent="2" unit="character"/><unclear reason="undefined" cert="high">ρ</unclear>ος <unclear reason="undefined" cert="high">το</unclear> απ<unclear reason="undefined" cert="high">ο</unclear> το<unclear reason="undefined" cert="high">υ</unclear><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="high"> καὶ παρέξεσθαι</supplied> 

    <lb n="17"/>τὴν δ<unclear reason="undefined" cert="high">ού</unclear>λην Σωτηρ<unclear reason="undefined" cert="high">ί</unclear><supplied reason="lost" cert="high">δα ταύτην τοιαύτην ἀνα</supplied>-

    <lb n="18"/><choice><corr>πόριφον</corr><sic><supplied reason="lost" cert="high">ανα</supplied>πορριφον</sic></choice> χωρὶς ἱε<supplied reason="lost" cert="high">ρᾶς νόσου καὶ ἐπαφῆς. κυριεύειν οὖν</supplied> 

    <lb n="19"/>καὶ ἐγ<supplied reason="lost" cert="high">κρ</supplied>ατεῖν κα<unclear reason="undefined" cert="high">ὶ</unclear> <supplied reason="lost" cert="high">διοικεῖν αὐτῆς τὸν Ἀπίωνα</supplied> 

    <lb n="20"/>καὶ <unclear reason="undefined" cert="high">ἐπιτε</unclear>λεῖν περὶ αὐτῆς ὃν ἐ<unclear reason="undefined" cert="high">ὰ</unclear><supplied reason="lost" cert="high">ν αἱρῆται τρόπον,</supplied> 

    <lb n="21"/>ἐξου<supplied reason="lost" cert="high">σίας</supplied> οὐκέτι οὔσης τῷ Κεφάλωνι <unclear reason="undefined" cert="high">μ</unclear><supplied reason="lost" cert="high">ηδ᾽ ἄλλῳ ὑπὲρ</supplied> 

    <lb n="22"/>αὐτο<supplied reason="lost" cert="high">ῦ ἐ</supplied>πελεύσεσθαι ἐφ᾽ ἣν πέπρα<unclear reason="undefined" cert="high">κεν</unclear> <supplied reason="lost" cert="high">πίωνι</supplied> 

    <lb n="23"/>δού<unclear reason="undefined" cert="high">λ</unclear><supplied reason="lost" cert="high">η</supplied>ν <choice><corr>Σωτηρίδα</corr><sic>σωτηριδαν</sic></choice> ἀνακριθεῖσαν ὑπὸ τοῦ νομοῦ 

    <lb n="24"/>βασιλ<supplied reason="lost" cert="high">ικ</supplied>οῦ γραμματέως διαδεχομένου τὴν στρατηγί-

    <lb n="25"/>αν Αὐ<supplied reason="lost" cert="high">ρη</supplied>λίου Ὠριγένους τῇ προκειμένῃ ἡμέρᾳ, 

    <lb n="26"/><unclear reason="undefined" cert="high">ἀλλὰ</unclear> <supplied reason="lost" cert="high">α</supplied>ὶ τὸν ἐπελευσόμενον ἢ ἐ<supplied reason="lost" cert="high">μ</supplied><unclear reason="undefined" cert="high">πο</unclear><supplied reason="lost" cert="high">ιη</supplied>σόμενον 

    <lb n="27"/>τ<unclear reason="undefined" cert="high">αύ</unclear><supplied reason="lost" cert="high">τη</supplied><unclear reason="undefined" cert="high">ς</unclear> ἢ μέρους χάρ<unclear reason="undefined" cert="high">ιν</unclear> <supplied reason="lost" cert="high">πά</supplied><unclear reason="undefined" cert="high">να</unclear><supplied reason="lost" cert="high">γκον</supplied> <unclear reason="undefined" cert="high">ἀ</unclear>π<unclear reason="undefined" cert="high">ο</unclear>στήσειν 

    <lb n="28"/>αὐτὸ<supplied reason="lost" cert="high">ν</supplied> Κεφάλωνα παρα<unclear reason="undefined" cert="high">χ</unclear><supplied reason="lost" cert="high">ρῆμα το</supplied><unclear reason="undefined" cert="high">ῖς</unclear> <supplied reason="lost" cert="high">δ</supplied>ίοις ἑαυτοῦ 

    <lb n="29"/>ἀναλ<unclear reason="undefined" cert="high">ώ</unclear>μασι βεβαιοῦντα π<supplied reason="lost" cert="high">άσῃ βεβαι</supplied>ώσει. ἡ πρᾶσις 

    <lb n="30"/>κυρία. περὶ δὲ τοῦ ταῦτα ὀ<supplied reason="lost" cert="high">ρ</supplied><unclear reason="undefined" cert="high">θ</unclear><supplied reason="lost" cert="high">ῶς καὶ καλ</supplied>ῶς 

    <lb n="31"/><supplied reason="lost" cert="high">γε</supplied><unclear reason="undefined" cert="high">γε</unclear><supplied reason="lost" cert="high">νῆσθαι</supplied> <unclear reason="undefined" cert="high">ἐ</unclear><supplied reason="lost" cert="high">περωτηθεὶς</supplied> ὁ <supplied reason="lost" cert="high">Κ</supplied><unclear reason="undefined" cert="high">εφ</unclear>ά<supplied reason="lost" cert="high">λων ὡμολόγησε.</supplied> </ab></div><div n="meta.18109" type="description" org="uniform" part="N" sample="complete"><p><rs type="material" cert="high">Papyrus</rs></p></div><div n="meta.18109" type="commentary" subtype="textDate" org="uniform" part="N" sample="complete"><p><date type="textDate" value="0225-01-20" notBefore="225" notAfter="225">20. Jan. 225</date></p></div><div n="meta.18109" type="commentary" subtype="general" org="uniform" part="N" sample="complete"><p>Vgl. Kruse, Der Königliche Schreiber, S. 1011f., Anm. 253.</p></div><div n="meta.18109" type="commentary" subtype="mentionedDates" org="uniform" part="N" sample="complete"><head>Erwähnte Daten</head></div><div n="meta.18109" type="bibliography" subtype="principalEdition" org="uniform" part="N" sample="complete"><listBibl default="NO"><bibl type="publication" subtype="principal" default="NO"><title level="s" type="abbreviated">SB</title><biblScope type="volume">14</biblScope><biblScope type="numbers">11277</biblScope></bibl><bibl type="Trismegistos" default="NO"><biblScope type="numbers">18109</biblScope></bibl><bibl type="DDbDP" default="NO"><series>0165</series>; ; <biblScope type="numbers">pg52</biblScope></bibl></listBibl><p>Perseus links: <xref type="Perseus" href="0165;;pg52" evaluate="all" from="ROOT" targOrder="Y" to="DITTO">Text in Perseus</xref></p></div><div n="meta.18109" type="bibliography" subtype="illustrations" org="uniform" part="N" sample="complete"><p><bibl type="illustration" default="NO">Aegyptus 54, 1974, Tav. XII</bibl></p></div><div n="meta.18109" type="bibliography