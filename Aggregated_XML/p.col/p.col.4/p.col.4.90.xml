    <lb n="23"/>καὶ τοῦ <num value="3">γ</num> <expan><ex cert="high">ἔτους</ex></expan> ἀπὸ Ἁθὺρ ἕως Τῦβι, <expan>μην<ex cert="high">ῶν</ex></expan> <num value="3">γ</num> <expan>ὀψώνι<ex cert="high">ον</ex></expan> <expan><ex cert="high">δραχμαὶ</ex></expan> <num value="15">ιε</num> 

