    <lb n="11"/>καὶ <choice><corr>ἱματισμὸς</corr><sic>ιματισμον</sic></choice> <expan><ex cert="high">ἐτῶν</ex></expan> <num value="3">γ</num>, ὡς τοῦ ἐνιαυτοῦ <expan><ex cert="high">δραχμαὶ</ex></expan> <num value="20">κ</num>, <expan><ex cert="high">γίνονται</ex></expan> <expan><ex cert="high">δραχμαὶ</ex></expan> <num value="60">ξ</num>, 

