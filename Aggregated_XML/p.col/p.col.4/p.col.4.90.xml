    <lb n="1"/>ὑπόμνημα Ζηνοδώρου. ε<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>νος τῶν παίδων <supplied cert="high" reason="lost">τ</supplied>ῶν πρότ<supplied cert="high" reason="lost">ερον </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>τῶν κατὰ Φιλαδέλφειαν <gap extent="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/><gap extent="4" reason="illegible" unit="character"/><gap extent="1" reason="illegible" unit="character"/> πρὸς <unclear cert="high" reason="undefined">τ</unclear>ῶι ταμ<supplied cert="high" reason="lost">ιείωι </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>μετὰ τοῦ βασιλικοῦ <expan>γρ<ex cert="high">αμματέως</ex></expan> καὶ τοῦ <expan>κωμογρ<ex cert="high">αμματέως</ex></expan> πρ<gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>σιδήρου καὶ τῶν λοιπῶν τῶν παρακομιζομένω<supplied cert="high" reason="lost">ν</supplied> 

    <lb n="6"/>εἰς Φιλαδέλφειαν συνέ<unclear cert="high" reason="undefined">φ</unclear>η διαφωνῆσαι δι<unclear cert="high" reason="undefined">κ</unclear><supplied cert="high" reason="lost">έλ</supplied>-

    <lb n="7"/>λας <num value="15">ιε</num>. Εὐκλῆς δέ με ὁ οἰκονόμος ἀναγκά<supplied cert="high" reason="lost">ζει</supplied> 

    <lb n="8"/>ἀποκαταστῆσαι μόνον. ἐμοὶ δὲ προσοφείλε<unclear cert="high" reason="undefined">τ</unclear><supplied cert="high" reason="lost">αι</supplied> 

