    <lb n="6"/><expan>μη<ex cert="high">τρὸς</ex></expan> <expan>Ταπάιτ<ex cert="high">ος</ex></expan> ἀπὸ κώμης Καραν<supplied cert="high" reason="lost">ί</supplied>δος τοῦ <expan>Ἀρσι<ex cert="high">νοίτου</ex></expan> <expan>νομ<ex cert="high">οῦ</ex></expan> 

