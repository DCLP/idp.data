    <lb n="19"/>δι᾽ ἐπισ<supplied cert="high" reason="lost">τ</supplied><unclear cert="high" reason="undefined">α</unclear>λμάτων, δι᾽ <unclear cert="high" reason="undefined">ἑ</unclear><supplied cert="high" reason="lost">νὸς <num value="1"/> μὲν αὐτοῦ κ</supplied><unclear cert="high" reason="undefined">εχ</unclear><supplied cert="high" reason="lost">ρονισμένο</supplied><unclear cert="high" reason="undefined">υ</unclear> εἰς τὸ διεληλυθὸς <num value="1">α</num> <expan><ex cert="high">ἔτος</ex></expan> Μεσορὴ

    <lb n="20"/>ἐπα<supplied cert="high" reason="lost">γομέ</supplied>νων <num value="5">ε</num>, το<unclear cert="high" reason="undefined">ῦ</unclear> δ᾽ ἑ<supplied cert="high" reason="lost">τέρο</supplied><unclear cert="high" reason="undefined">υ</unclear> σοῦ <unclear cert="high" reason="undefined">εἰ</unclear>ς τὸ <supplied cert="high" reason="lost">ἐν</supplied><unclear cert="high" reason="undefined">ε</unclear><supplied cert="high" reason="lost">σ</supplied>τὸ<supplied cert="high" reason="lost">ς <num value="2">β</num></supplied> <expan><ex cert="high">ἔτος</ex></expan> Ἁθὺρ <num value="8">η</num>, γράφω σοι.

    <lb n="21"/><gap extent="unknown" reason="lost" unit="character"/> ἐρρῶ<unclear cert="high" reason="undefined">σ</unclear><supplied cert="high" reason="lost">θαί σ</supplied><unclear cert="high" reason="undefined">ε</unclear> εὔχομαι, φίλτατε.

