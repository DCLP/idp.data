    <lb n="1"/><unclear cert="high" reason="undefined">μετα</unclear>ξὺ τοῦ δεῖνο<unclear cert="high" reason="undefined">ς</unclear> <supplied cert="high" reason="lost">τοῦ</supplied> <unclear cert="high" reason="undefined">δ</unclear><supplied cert="high" reason="lost">εῖνος</supplied> 

    <lb n="2"/>ἐ<unclear cert="high" reason="undefined">ν</unclear>κ<unclear cert="high" reason="undefined">α</unclear>λοῦντος καὶ το<unclear cert="high" reason="undefined">ῦ</unclear> <supplied cert="high" reason="lost">δεῖνος ἐγ</supplied>-

